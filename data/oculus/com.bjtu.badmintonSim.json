junliang.github.io/badmintonxr/privacy_policy.html", "developer_terms_of_service_url": null, "publisher_name": "Chunjun Liang", "support_website_url": null, "website_url": "https://chunjunliang.github.io/badmintonxr", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "ko", "zh_CN", "es"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER"], "id": "25037437762541525"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sin2-2.oculuscdn.com/v/t64.5771-25/39031555_1513022456009865_8607747565322085322_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=G1ae4qhEf74Q7kNvwEcZaCW&_nc_oc=AdlZFWhk2xI7AC9N0c3lZZIqjUSgXcPblGVHd_H9_vg9hxCQSajWZddxwMORzyio5a8&_nc_zt=3&_nc_ht=scontent-sin2-2.oculuscdn.com&oh=00_AfnySxyNX9Sr1EPdMem_dJACMTnQ734AgRZKfrWTOUv52Q&oe=6943F507"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Badminton Battle", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Sports", "id": "2776542286000603"}], "release_date": 1687447562, "lastRevision": {"nodes": [{"id": "6942168885828356", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "6942168869161691", "is_first_party": false, "enables_in_app_ads": false, "is_concept": false, "platform": "ANDROID_6DOF"}, "iarc_cert": {"