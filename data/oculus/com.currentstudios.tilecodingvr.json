ler": null, "video_trailer_thumbnail": null, "display_name": "Tile Coding", "keywords": [], "long_description": "Tile Coding is a virtual space where young coders can experiment and solve problems to acquire algorithmic thinking and coding skills. Coding, while being among the most important literacies in modern society, is often regarded as an arduous process as well as a real learning challenge for younger students. The virtual space of Tile Coding offers an alternative approach to teaching and learning algorithmic thinking and coding while aiming to make the subject appealing to younger students. The virtual lab is ideal for Problem-Based Learning (PBL) scenarios where students actively collaborate and seek solutions to open-ended problem situations.", "short_description": "Tile Coding is an innovative app designed to facilitate the learning of fundamental programming concepts in an engaging and accessible manner for children."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.currentstudios.com/privacy", "developer_terms_of_service_url": null, "publisher_name": "Current Studios", "support_website_url": null, "website_url": "https://www.currentstudios.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1043107547362485"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "27443548311926050"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/38974698_1103291878