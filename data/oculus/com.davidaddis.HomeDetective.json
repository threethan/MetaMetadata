": [], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "VideoCover.png", "id": "650169846939595", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/39031645_650169850272928_8852918538355627650_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=aCYj1ubBGBYQ7kNvwHZwMrQ&_nc_oc=Admx6z4h5Yprohq23sPw4SLcnRcajPZ9HjCrEgyReTWmzKfvCcA71ZRK8ePIL3ezs4g&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfIwGAx9a4_zgMG32hRiRk3h3Q6Iq2GmeELC3l2w03L5hw&oe=68370D95"}, "display_name": "Home Detective", "keywords": [], "long_description": "Use your own living room to solve the crime! Using passthrough, the crime scene is overlaid onto your play area, leaving you free to explore and find clues. Use the patent-pending Residual Heat Scanner to detect the criminals' recent movements, and then dust for fingerprints or try to find evidence left behind. With your wits, and your trusty sidekick Steve Dobbins' snarky comments, you're sure to get to the bottom of these confusing crimes.", "short_description": "Work-from-home detectives, we need you! Some baffling crimes require your skills to solve, fortunately from the comfort of your living room. Use passthrough to overlay the crime scene on your own environment, and interact with the detective tools using hand tracking or controllers to find all the essential tools."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.davidaddis.com/?page_id=61616", "developer_terms_of_service_url": null, "publisher_name": "David Addis", "support_website_url": null, "website_url": "https://www.davidaddis.com/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "377978390272490"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recomm