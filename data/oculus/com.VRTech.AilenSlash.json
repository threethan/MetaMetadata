 dreamed of becoming!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://vrtech.com.vn/PrimacyGameVR/PRIVACY_POLICY.htm", "developer_terms_of_service_url": null, "publisher_name": "VRtech Studio", "support_website_url": null, "website_url": "https://vrtech.com.vn/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "2710427532541308"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "28843527308593899"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "2305491209836875"}]}, "baseline_offer": {"id": "2305491209836875"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1740326400, "status": "PUBLISHED", "id": "2305491209836875"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "28953991860880776", "version": "0.1.1"}, "id": "1007424428105850", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "28843527281927235", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "28953991860880776"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1007424428105850"}]}, "change_log": null, "created_date": 1740132322, "id": "289539918608