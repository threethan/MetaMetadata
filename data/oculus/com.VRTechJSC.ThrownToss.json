_oc=AdkLC1PMLGJcn9yKmcCYh6nApZJ61Wiy98wNQAk-6kcCyYbBhPDpg1jEJOz97gZC4Sk&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_AfqUk9bnrGvcGCKnMxrl9anyBN3DNo9tR8WHXKcGBa_7jg&oe=695CF78F"}, {"file_name": "Screenshots_1.png", "id": "1770432533691421", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/38974628_1770432537024754_5636662912625262115_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=xlZnKT7fEiwQ7kNvwE6Kjgi&_nc_oc=AdmLB79LZlwOCg5_DbJdRcnkvDsx7Uw6yBSeA2PcOsHjlwdp5kdz9NhxeE3OEqQJ8ow&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfrT0MlR2tNDeMBnFUMprig87iJMpQMw0o9C_uyjj3z0JQ&oe=695CFDE9"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Cover Image - Video.png", "id": "588727970778899", "uri": "https://scontent-sin2-1.oculuscdn.com/v/t64.5771-25/57568683_588727974112232_7157595735585792244_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=1V5bseZAAVEQ7kNvwH9RJff&_nc_oc=AdmWZ0wn4vQ4lpFGaRBY8eVfruQ4N52vGHN2L-WjeXf5s6x6uKyQeI6PhWbogXwQZz8&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_AfohrNSwmLtCEGaIZPH6-h5FEHITb1MVIOeIj5zrVbT85w&oe=695D140E"}, "display_name": "Throw n Toss", "keywords": ["throwing", "toss", "casual"], "long_description": "Dive into the thrilling world of Throw n Toss, a captivating VR casual game. In this immersive experience, you'll embark on a journey through exciting levels in the Campaign Mode, where conquering challenges unlocks special rewards.\nUnleash your throwing skills in the Throw Anything feature, where you can toss a wide range of random objects to rack up high scores. Each level presents unique items, making every throw a new adventure!\nChallenge your precision in the Ring Toss mode, where you aim to land rings on targets for bonus points.\nAs you progress through the game, you'll earn coins in both modes, allowing you to upgrade your experience. Get ready to throw, toss, and triumph in Throw n Toss!", "short_description": "In Throw and Toss, every throw counts! Whether you're competing for high scores or simply enjoying a friendly game with family and friends, the vibrant worlds and engaging mechanics ensure hours of fun. Grab your VR headset, warm up your throwing arm, and get ready to hit those checkpoints like never before! Embrace the thrill of tossing and become the ultimate champion in this exhilarating VR experience!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.privacypolicies.com/live/931bda30-41d8-41b9-b478-4141b7bb2e9a", "developer_terms_of_service_url": null, "publisher_name": "VRTech Studio", "support_website_url": null, "website_url": "https://vrtech.com.vn/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1592192974313489"}, {"id": "377978390272490"}, {"id": "331529188132278"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "28144609651820068"}}]}, "scheduled_offers": {"count": 4, "nodes": [{"status": "PUBLISHED", "id": "4610520019175656"}, {"status": "PUBLISHED", "id": "1800436390892950"}, {"status": "PUBLISHED", "id": "1259705785680467"}, {"status": "PUBLISHED", "id": "607059565069117"}]}, "baseline_offer": {"id": "1259705785680467"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1771005600, "status": "PUBLISHED", "id": "4610520019175656"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "27999338383013863", "version": "0.1.0"}, "id": "931709088316374", "channel