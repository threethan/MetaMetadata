istic physics and sounds!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://soaringrocstudio.net/policy", "developer_terms_of_service_url": null, "publisher_name": "Soaring Roc Studio", "support_website_url": null, "website_url": "https://soaringrocstudio.net/", "category": null, "comfort_rating": null, "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}, {"id": "377978390272490"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "7518910234871358"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "196821590192894"}]}, "baseline_offer": {"id": "196821590192894"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1716305460, "status": "PUBLISHED", "id": "196821590192894"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "7523865867709128", "version": "0.1.1"}, "id": "1110617466702442", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "7505917049504010", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "7523865867709128"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1110617466702442"}]}, "change_log": null, "created_date": 1716312495, "id": "7523865867709128", "version": 