_name": "HoloATC", "keywords": [], "long_description": "This app show live air traffic from a number of airports all around the world. It is indented to be used with hand tracking only. It uses. Initially you are asked select an airport, then select a place for the airport center. Press fingers and thumb together to place the airport. Then you can watch air traffic. More options are in the hand menu (hold hand up with palm towards you).\nYou can also see aircraft pictures by looking at a plane for 3 seconds, and select a single airplane to follow. This will be shared in all headsets watching this airport.", "short_description": "Watch live air traffic control from a host of airports all over the world, right in your living room!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.schaikweb.net/HoloATC/holoatc_pp_en.html", "developer_terms_of_service_url": null, "publisher_name": "Joost van Schaik", "support_website_url": null, "website_url": "https://localjoost.github.io/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "726368587961803"}, {"id": "393222478507326"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["HAND_TRACKING"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "5781639948570872"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "872128970706622"}]}, "baseline_offer": {"id": "872128970706622"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1675703460, "status": "PUBLISHED", "id": "872128970706622"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__