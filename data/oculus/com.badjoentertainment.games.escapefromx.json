s game redefines VR UX."}]}, "developer_name": null, "developer_privacy_policy_url": "https://badjoentertainment.com/efx/Privacy%20Policy.html", "developer_terms_of_service_url": "https://badjoentertainment.com/efx/TermsAndConditions.html", "publisher_name": "Badjo Entertainment", "support_website_url": "https://badjoentertainment.com/efx/support/SupportLandingPage.html", "website_url": "https://badjoentertainment.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_FEW", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "331529188132278"}, {"id": "644354873106712"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8564242210350999"}}]}, "scheduled_offers": {"count": 6, "nodes": [{"status": "PUBLISHED", "id": "716075420896895"}, {"status": "PUBLISHED", "id": "1712459623001509"}, {"status": "PUBLISHED", "id": "1235289384918765"}, {"status": "PUBLISHED", "id": "973125461015639"}, {"status": "PUBLISHED", "id": "1550935305624317"}, {"status": "PUBLISHED", "id": "1076143280639815"}]}, "baseline_offer": {"id": "1076143280639815"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1751392800, "status": "PUBLISHED", "id": "716075420896895"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "9669554649819744", "version": "0.7.0"}, "id": "899930055489673", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary",