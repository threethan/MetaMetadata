ite VR Training", "developer_privacy_policy_url": "https://elitevrtraining.com/privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "Elite VR Training", "support_website_url": null, "website_url": "https://www.elitevrtraining.com/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "514087707868786"}, {"id": "1043107547362485"}], "external_subscription_type": "REQUIRED", "genres": ["SIMULATION", "EDUCATIONAL"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "923882978771196"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "1123534412518509"}, {"status": "PUBLISHED", "id": "578487327429983"}]}, "baseline_offer": {"id": "1123534412518509"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1736108520, "status": "PUBLISHED", "id": "1123534412518509"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "10053567694656542", "version": "2.0.3"}, "id": "168767911784674", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "4546486248698075", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "10053567694656542"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "168767911784674"}]}, "change_log": null, "created_date": 17398433