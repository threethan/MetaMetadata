 formal panel interviews.   "}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.holopundits.com/privacy-policy.html", "developer_terms_of_service_url": null, "publisher_name": "Holo Pundits", "support_website_url": null, "website_url": "https://www.holopundits.com/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "514087707868786"}], "external_subscription_type": "REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["HAND_TRACKING"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "271444722163646"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "652593730221935"}]}, "baseline_offer": {"id": "652593730221935"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1691503240, "status": "PUBLISHED", "id": "652593730221935"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "9847679905296637", "version": "1.0"}, "id": "2948735865257312", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "6717971874934138", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "9847679905296637"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "2948735865257312"}]}, "change_log": null, "created_date": 1743587494, "id": "9847679905296637", "version": "1.0", "version_code": 19, "release_status": "RELEASED", "status"