": null, "video_trailer_thumbnail": {"file_name": "2560z1440fit (2).png", "id": "1430983358282138", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/473391785_1430983361615471_3838589695186036432_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=PGh0Ebn9tv0Q7kNvwFpAC8t&_nc_oc=Adm-ZJZPQ2TOK2dObiLTUwAuQRwVlsVRqow2A796RekZHifq3AozqGpNZ-8kVAwYaRg&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfT7wux5xG9vgywbSVGpxWZM_esBMYlCh7bBzFm1FvtJ1A&oe=6876483D"}, "display_name": "Be Fit & Mindful", "keywords": ["fitness", "workout", "training", "mindful", "mindfulness"], "long_description": "**Smash Stress, Sculpt Yourself: VR's Unconventional Workout!**\n\n**Feeling the pressure?** Grab a baseball bat, boxing gloves, or even a shovel!  This innovative VR experience lets you unleash your inner athlete and smash away stress in a fun and effective way, all surrounded by the calming beauty of the virtual woods.\n\n**More than just stress relief:**\n\n- **Unleash the Fury:** Smash balls with a variety of weapons, each offering a satisfying and cathartic experience.\n- **Personalized Workout:** Choose the tool that best suits you, from boxing gloves to sledgehammers.\n- **Physical & Fun:** Burn calories and relieve tension with different engaging VR workouts.\n\n**Transform stress relief into a dynamic experience!**  Head to the virtual ball smasher and say goodbye to pent-up frustration. It's a healthier way to cope with life's pressures, leaving you feeling energized and balanced.\n\n**Take a swing, feel the tension melt away, and discover a unique approach to stress relief and overall wellness!**", "short_description": "Stressed? Escape to the virtual woods & smash away your worries in this fun VR workout!  Unleash your inner athlete with boxing gloves, baseball bats & more!  Each satisfying smash melts tension & burns calories.  Forget the gym, this unique stress relief experience offers a personalized workout in a calming forest setting. Take a swing, breathe in the fresh VR air and discover a healthier, more balanced you!  "}]}, "developer_name": null, "developer_privacy_policy_url": "https://artisoft.pl/policies/bs_v6.html", "developer_terms_of_service_url": null, "publisher_name": "Artisoft", "support_website_url": "https://fitness.artisoft.pl/support.html", "website_url": "https://fitness.artisoft.pl/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "810139357995560"}, {"id": "2776542286000603"}, {"id": "478089301798119"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "7861460137310874"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/473391814_984023883440549_1302621950375121162_n.png?stp=dst-png_s360x360&_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=fO3PCpVDD3IQ7kNvwGgjsR7&_nc_oc=Adln6BLQavxSBC_gVzc8odL97BYchZ_hP8AHhB2Naw5vGkrsup9nnwK535tnTT6X1y4&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfRta4GqLuWPifj3lFM4UjabmsfxZNyzNWoxnIyIJRn5lA&oe=68765563"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Be Fit & Mindful", "platform": "ANDROID_6DOF", "context_category": {"context_category": "APPS", "id": "362692211442873"}, "context_topics": [{"topic_name": "Fitness & Wellness", "id": "810139357995560"}, {"topic_name": "Sports", "id": "2776542286000603"}, {"topic_name": "Medical", "id": "478089301798119"}], "release_date": 1699900999, "lastRevision": {"nodes": [{"id": "523