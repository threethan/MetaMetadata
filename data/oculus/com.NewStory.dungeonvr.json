_oc=Adlz-Nw4ulCMjX-vextjiyR2LgBYUE85x9_sHWOcUqenZJTHV3nB3WVnH4T__zvCtns&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_AfX1oTnxApSzLAISR18Q-mpx-KnkHVRXSeAy7_NilWefug&oe=68B970B9"}, {"file_name": "screenshot1.png", "id": "1127579675838465", "uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/491820093_1127579679171798_2677043666339594417_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=EfyVQ82mvnoQ7kNvwHATiqP&_nc_oc=Adkbfw-jcw8Sp7ncMwNkt3cmA7BiLXYilJGbC58X2I1BVaLd6KorI1Oifpkutq6CHOA&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfUrO-b2WgJlUFxy31tYxHVrpiNDqHFHoMVlz7GrCth0WA&oe=68B971DE"}, {"file_name": "screenshot4.png", "id": "1212304633804613", "uri": "https://scontent-sin6-2.oculuscdn.com/v/t64.5771-25/473394082_1212304640471279_5131118070059765112_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=2LKBd-JjyawQ7kNvwHdnJj8&_nc_oc=AdlH8bmqa64viufZs9x531m6dH6s4hkx71QLuKRBHaLsPeWHwJOOAIrTmcHIsk97qAc&_nc_zt=3&_nc_ht=scontent-sin6-2.oculuscdn.com&oh=00_AfVE9F1tqTzsBLJPnZh_sGyoI8WzZsECRMj_rsziBfHDQA&oe=68B96CE5"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Landscape_2560x1440.png", "id": "1030416448999878", "uri": "https://scontent-sin2-2.oculuscdn.com/v/t64.5771-25/473391921_1030416452333211_1658634763125926847_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=RA2sFBjnvDsQ7kNvwErDqX2&_nc_oc=Adk8SeaiWrilOtLkzT2n_PGGja3ue-xEYLJPsILvj8CTDPU6kkI9-AOGtBv850iBMOY&_nc_zt=3&_nc_ht=scontent-sin2-2.oculuscdn.com&oh=00_AfVjz8BirymcRUHLdZlZecZ0iMYHKmRL978r81r7nzYu1g&oe=68B973C4"}, "display_name": "Big White Room", "keywords": ["room", "physics", "action"], "long_description": "Welcome to Big White Room. A place with no color... just endless white space and a whole lot of satisfyingly realistic physics objects. Your goal? Experiment, build, interact, and maybe find a way out.\n\nThis isn't about complex riddles; it's a hands-on VR physics playground. Get creative with our awesome physics engine \u2013 stack crates impossibly high, build wobbly towers, send spheres flying, and see how things satisfyingly tumble and crash. Feel the realistic weight and motion as you manipulate objects in immersive VR.\n\nAnd yeah, you're not entirely alone. Odd figures watch your every move. Feel free to give 'em a good smack if they're creeping you out \u2013 the physics makes it surprisingly fun!\n\nUnleash your creativity and just have fun messing around with physics in Big White Room. See what you can build, what you can break!\n\nFeatures:\n\nEngaging VR physics sandbox set in a stark, minimalist white world.\nHighly realistic and fun physics engine \u2013 perfect for experimenting and creative play.\nBuild, stack, toss, and manipulate objects with intuitive VR controls.\nEncounter strange, smackable watchers.\nDiscover emergent ways to play and interact within the environment.\n", "short_description": "Mess around with awesome physics in a strange white VR world. Build, toss, smack!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://newstory-inc.com/privacypolicy", "developer_terms_of_service_url": null, "publisher_name": "New Story Inc", "support_website_url": null, "website_url": "https://newstory-inc.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1990104051447093"}, {"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "28986605497653988"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://sc