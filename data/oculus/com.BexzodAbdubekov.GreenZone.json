h=00_AfMdM6by-nPJe6U78hivBUd1xB44xMJTiUGM6K3XxMsfmw&oe=685E7D3D"}, {"file_name": "sc3.png", "id": "1137471304069427", "uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/38984445_1137471307402760_7212815590231567740_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=6vT346JP2jsQ7kNvwEumiHu&_nc_oc=AdnTiT2gLU0MK6prxlAYAke6Kc7hB4aOEeTHi79Qi8s3ouGOn6G9b2yI1pps3ycX-qQ&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_AfMQp2aEddVWAkzuIb7lHfQRNr_Awg18O-NIoflYL0LQRA&oe=685E9C6A"}, {"file_name": "sc5.png", "id": "969196391551875", "uri": "https://scontent-ams4-1.oculuscdn.com/v/t64.5771-25/75520131_969196394885208_7115778398273723767_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=zAL2m_CD7okQ7kNvwE3lvIT&_nc_oc=Adme2RALKMPMQLXFEhYh0UZbXpibYF8j9o1Oi-ZXdoUGlUMX1A3p_SM4h-kfqMn-Tos&_nc_zt=3&_nc_ht=scontent-ams4-1.oculuscdn.com&oh=00_AfNg-NHTalEw8qoRXEusMrMEkUPBA-boHmalJR9x4Xd7gQ&oe=685E951E"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "25601440.png", "id": "1009902807136482", "uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/39031425_1009902810469815_1845567791388563259_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=YfiOyIrFriEQ7kNvwFAG1EH&_nc_oc=AdlpjKCsYPFXUQdre-70JSL0OSn3gAsYJ3yYHNJzChm_ZM0Y70CMuLdNm8KAUKnluSo&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_AfOlwzcn9VcpcyLw4rHLGB7XLivUmZ_U63TIrJRpmyUWfw&oe=685E7864"}, "display_name": "Green Zone", "keywords": ["defense", "Zombie", "Shooter", "Survival", "Action"], "long_description": "Immerse yourself in a post-apocalyptic world filled with toxic zombies in this thrilling Meta Quest game. Players must navigate through a variety of environments, each presenting unique challenges and threats. Equip yourself with a diverse array of weapons, from firearms to explosives, and strategically place mines and barriers to fortify defenses. As you progress, you'll need to defend cities from waves of relentless zombies, using every tool and tactic at your disposal. Master the art of survival, adapt to changing scenarios, and unlock new levels and equipment to enhance your fighting capabilities. This intense VR experience not only tests your combat skills but also your ability to strategize under pressure. Prepare for an adrenaline-pumping adventure that keeps you on the edge of your seat!", "short_description": "Survive amidst toxic zombies in diverse locations in this new Meta Quest game! Defend cities using an arsenal of weapons, mines, and barriers. Discover unique tactics and strategies to fight for your life!"}]}, "developer_name": "Bexzod Abdubekov", "developer_privacy_policy_url": "https://www.facebook.com/permalink.php?story_fbid=122108972456316995&id=61559509877826", "developer_terms_of_service_url": null, "publisher_name": "Bexzod Abdubekov", "support_website_url": null, "website_url": "https://www.facebook.com/61559509877826/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "644354873106712"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9807585509351482"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ams4-1.oculuscdn.com/v/t64.5771-25/75348041_1640617176520941_2891858986587112468_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=RZo2s8RG6jwQ7kNvwG5VqHh&_nc_oc=AdlQJzCfXZgrOxtsUdwWC_Yx_b70fhx5_nOI5vsW9Hn6mnnMGUc1gXHcdUk1BMeeJYM&_nc_zt=3&_nc_ht=scontent-ams4-1.oculuscdn.com&oh=00_AfPOF7q7R6_2RMJ0Xf-vJtKmRpyO55k7kJjXQlLB0CPSlQ&o