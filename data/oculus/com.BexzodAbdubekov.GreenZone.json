h=00_AfO6gTMPuMSm8b1XQjArHoJFM02Z12-6o_mc_JW-W7E58g&oe=686271BD"}, {"file_name": "sc3.png", "id": "1137471304069427", "uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/38984445_1137471307402760_7212815590231567740_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=yOJp9khSfw8Q7kNvwH56zoq&_nc_oc=AdnI-R4XqUZuwkRijIOUV7LbZa7XGs4s7tkQcg8-NXAufo5tY_DcI5Vi4jRnLijykGY&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AfMzfG7mOO_6GQSsmUdMp0zvgq721KtGVG8ncBXvPw2L7g&oe=686290EA"}, {"file_name": "sc5.png", "id": "969196391551875", "uri": "https://scontent-lhr8-2.oculuscdn.com/v/t64.5771-25/75520131_969196394885208_7115778398273723767_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=mqDR2QO5Kn4Q7kNvwFDRWdq&_nc_oc=AdmAcFNL-tYRg5WYzOTe7I9ayYbd2ZY4ccaEBYmlmMakcN2sEFHQQHMqZv2MNIYcGjc&_nc_zt=3&_nc_ht=scontent-lhr8-2.oculuscdn.com&oh=00_AfPBFz5II5XlVkcDErfib9CIs-xaE6k5WCva-1EgPsBqYA&oe=6862899E"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "25601440.png", "id": "1009902807136482", "uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/39031425_1009902810469815_1845567791388563259_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=l43q4qU7saIQ7kNvwEbrC3p&_nc_oc=Adk6VpFYutZNDe5y0EN3ykNPZ2JpVNG0XN-OUSocXeWyZmJBSIa6jeYvzHQiGqyMpmQ&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AfN1V5XhIp7vHT40v5CXFf2CoPVda-hCzHsEplnt9x2qjQ&oe=68626CE4"}, "display_name": "Green Zone", "keywords": ["defense", "Zombie", "Shooter", "Survival", "Action"], "long_description": "Immerse yourself in a post-apocalyptic world filled with toxic zombies in this thrilling Meta Quest game. Players must navigate through a variety of environments, each presenting unique challenges and threats. Equip yourself with a diverse array of weapons, from firearms to explosives, and strategically place mines and barriers to fortify defenses. As you progress, you'll need to defend cities from waves of relentless zombies, using every tool and tactic at your disposal. Master the art of survival, adapt to changing scenarios, and unlock new levels and equipment to enhance your fighting capabilities. This intense VR experience not only tests your combat skills but also your ability to strategize under pressure. Prepare for an adrenaline-pumping adventure that keeps you on the edge of your seat!", "short_description": "Survive amidst toxic zombies in diverse locations in this new Meta Quest game! Defend cities using an arsenal of weapons, mines, and barriers. Discover unique tactics and strategies to fight for your life!"}]}, "developer_name": "Bexzod Abdubekov", "developer_privacy_policy_url": "https://www.facebook.com/permalink.php?story_fbid=122108972456316995&id=61559509877826", "developer_terms_of_service_url": null, "publisher_name": "Bexzod Abdubekov", "support_website_url": null, "website_url": "https://www.facebook.com/61559509877826/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "644354873106712"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9807585509351482"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/75348041_1640617176520941_2891858986587112468_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=114xFLMFYv0Q7kNvwF3uj5H&_nc_oc=AdmeW9Wd3grRs3EDBHcbfJ9__siPdJYe5GqiFqZf2jKlqLTo8j8OMa36BJUoKL4egzM&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AfNxYMkNf6EfVgCSG0PdGpKKMt3owbhEtCjI8HIFwrQKLg&o