h=00_AfiCaDJx4p82qy7FCwJAETn9SWklLfD88R88oz-v3Pq27A&oe=691860BD"}, {"file_name": "sc3.png", "id": "1137471304069427", "uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/38984445_1137471307402760_7212815590231567740_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Ps8m4K1r8wAQ7kNvwF5vjoq&_nc_oc=AdkYmGvLuxPAJ2vCBeObp-VyE8ahXzvvBzVq_TCtAskla2InkMn7d5vXecYEmeeY1H0&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AfgwHcB8XF6IrccOiR-xmJQWGJa8kwte1QxNKnvfzWo72w&oe=69187FEA"}, {"file_name": "sc5.png", "id": "969196391551875", "uri": "https://scontent-lhr8-2.oculuscdn.com/v/t64.5771-25/75520131_969196394885208_7115778398273723767_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=4KA6Y72DccsQ7kNvwFyzVGZ&_nc_oc=AdkEgjAgiPwxbiW_ILOR3LyCWZuitvHns4sQSNQ9KGxazzO6peqS8zqPxcWxRMCHiVc&_nc_zt=3&_nc_ht=scontent-lhr8-2.oculuscdn.com&oh=00_Afjfi3CSM5SYxO0yY2SIk1AiV2t9tkVJeTPt4SEqwqwNjw&oe=6918789E"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "25601440.png", "id": "1009902807136482", "uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/39031425_1009902810469815_1845567791388563259_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=HMEIWgzOQyMQ7kNvwFnL17n&_nc_oc=AdlHeLrTRtB10EYe_ptITWDVEedp7tF4dhp6KI1oNUL2wJkjITkCaGJTAcuSZNx2ANY&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AfgP3jeSMLxxOgXbla6WIjXyygZASrKUx4zdzpdXtTWW1g&oe=69185BE4"}, "display_name": "Green Zone", "keywords": ["defense", "Zombie", "Shooter", "Survival", "Action"], "long_description": "Immerse yourself in a post-apocalyptic world filled with toxic zombies in this thrilling Meta Quest game. Players must navigate through a variety of environments, each presenting unique challenges and threats. Equip yourself with a diverse array of weapons, from firearms to explosives, and strategically place mines and barriers to fortify defenses. As you progress, you'll need to defend cities from waves of relentless zombies, using every tool and tactic at your disposal. Master the art of survival, adapt to changing scenarios, and unlock new levels and equipment to enhance your fighting capabilities. This intense VR experience not only tests your combat skills but also your ability to strategize under pressure. Prepare for an adrenaline-pumping adventure that keeps you on the edge of your seat!", "short_description": "Survive amidst toxic zombies in diverse locations in this new Meta Quest game! Defend cities using an arsenal of weapons, mines, and barriers. Discover unique tactics and strategies to fight for your life!"}]}, "developer_name": "Bexzod Abdubekov", "developer_privacy_policy_url": "https://www.facebook.com/permalink.php?story_fbid=122108972456316995&id=61559509877826", "developer_terms_of_service_url": null, "publisher_name": "Bexzod Abdubekov", "support_website_url": null, "website_url": "https://www.facebook.com/61559509877826/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "644354873106712"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9807585509351482"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/75348041_1640617176520941_2891858986587112468_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Pn7Rmkzq7AAQ7kNvwFUYVVp&_nc_oc=AdkGNh3FQQYvOixocZYIqvKxUAF5Kcjv7QbpiKXhgEhBfk4shM1nkaTZTWZWA5cA8vc&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AfgRtbPsth50YMBm7d3m5eYko_nQRyNSS0K9H-PO0cOMKA&o