h=00_AflJHC0q4T88RqWRxwVADV9xW1kl3js4WjKejvXI1p0deg&oe=6946853D"}, {"file_name": "sc3.png", "id": "1137471304069427", "uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/38984445_1137471307402760_7212815590231567740_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=jvlV803TTHwQ7kNvwFc4Jvo&_nc_oc=AdmU8qsbJuZweYSwtjlqD7Q4PLhEt6kCR29lBt5Drv79g2iUqflHeaxFlsEyxb1ZdKY&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AfmlNzh7z7p5IFsE4LOlgKCrau_kLQic6M5PbUzFWBUvXQ&oe=6946A46A"}, {"file_name": "sc5.png", "id": "969196391551875", "uri": "https://scontent-lhr8-2.oculuscdn.com/v/t64.5771-25/75520131_969196394885208_7115778398273723767_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=4Xe5FkrR9xsQ7kNvwHD4OfB&_nc_oc=Adlt3_lmvzYComNBbK_hkM7oX8NI6AWQzQxS9BtAQqwEwi7VTnNB03_OI7ut6x5zHGw&_nc_zt=3&_nc_ht=scontent-lhr8-2.oculuscdn.com&oh=00_AflmmJBO1tpOVhV6_v233Iqv5ffzlXSEWpZwAYLm3DGTuw&oe=69469D1E"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "25601440.png", "id": "1009902807136482", "uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/39031425_1009902810469815_1845567791388563259_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Dwhf4syJXvMQ7kNvwFWx_Q5&_nc_oc=AdkI7qKes0eYUzh8VhCV6zGbdxpOswwGAOHhi6UtgRe_z4y_zCxzEP36LyG6-P9ItU8&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AflQMvkI3-rjODe2t10HvQRrCDbUolU2A0gMadzvpw9YMQ&oe=69468064"}, "display_name": "Green Zone", "keywords": ["defense", "Zombie", "Shooter", "Survival", "Action"], "long_description": "Immerse yourself in a post-apocalyptic world filled with toxic zombies in this thrilling Meta Quest game. Players must navigate through a variety of environments, each presenting unique challenges and threats. Equip yourself with a diverse array of weapons, from firearms to explosives, and strategically place mines and barriers to fortify defenses. As you progress, you'll need to defend cities from waves of relentless zombies, using every tool and tactic at your disposal. Master the art of survival, adapt to changing scenarios, and unlock new levels and equipment to enhance your fighting capabilities. This intense VR experience not only tests your combat skills but also your ability to strategize under pressure. Prepare for an adrenaline-pumping adventure that keeps you on the edge of your seat!", "short_description": "Survive amidst toxic zombies in diverse locations in this new Meta Quest game! Defend cities using an arsenal of weapons, mines, and barriers. Discover unique tactics and strategies to fight for your life!"}]}, "developer_name": "Bexzod Abdubekov", "developer_privacy_policy_url": "https://www.facebook.com/permalink.php?story_fbid=122108972456316995&id=61559509877826", "developer_terms_of_service_url": null, "publisher_name": "Bexzod Abdubekov", "support_website_url": null, "website_url": "https://www.facebook.com/61559509877826/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "644354873106712"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9807585509351482"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/75348041_1640617176520941_2891858986587112468_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=jB29TYRl00wQ7kNvwGR5lUs&_nc_oc=Admd-tausMUo2uAnKaKWNaQw_D50tuiX3AECKf0rPuBSkoFuRnQ3F2m-TvVaRLP9jMQ&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_Afl7nOJP4YX9guGSezqjuglOxHNOzUmLTt2wSlVVkTd0MA&o