oh=00_AfHg6dF8NxgoyVhxi41VgtG6TIYfqafzMrNmK5AKGDixXA&oe=680385BD"}, {"file_name": "sc3.png", "id": "1137471304069427", "uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/38984445_1137471307402760_7212815590231567740_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=pB5831x6l-IQ7kNvwGhop_g&_nc_oc=AdkYtPpxJsxO4kx795FHzx4RQgQDYmuo3zNlDrKJ6VGclQWJetOfFXI9skxmU_e8GHk&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AfFqPH0oNtAsLcAHsU2y0EEwnV3cVELVc97OZtct8CE-RA&oe=6803A4EA"}, {"file_name": "sc5.png", "id": "969196391551875", "uri": "https://scontent-lhr8-2.oculuscdn.com/v/t64.5771-25/75520131_969196394885208_7115778398273723767_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=UwvEi36QVXoQ7kNvwEbGPIY&_nc_oc=AdnSdqqkdE6WVzUpNWhs2a4ZATm0HBHOmj_Z3waxjcbfW7yNkXOzcmHLckaNpXAIRCo&_nc_zt=3&_nc_ht=scontent-lhr8-2.oculuscdn.com&oh=00_AfEGJNyzibZpHMLP8q0c1AV2IRLFd3MLMkFfpIMKYwWUOQ&oe=68039D9E"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "25601440.png", "id": "1009902807136482", "uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/39031425_1009902810469815_1845567791388563259_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=oCJOoM020HkQ7kNvwEjiRhO&_nc_oc=AdlBGlZDTeqW_Al-wjOAH08SFL1i0D12RaylefHqq6vpBEPV__aDuQVfYxtSiUOZwjI&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AfF8en8Pt6eQp9T6U3EaBcPu8YHjY4_tHmKF6aNlSA88Mw&oe=680380E4"}, "display_name": "Green Zone", "keywords": ["defense", "Zombie", "Shooter", "Survival", "Action"], "long_description": "Immerse yourself in a post-apocalyptic world filled with toxic zombies in this thrilling Meta Quest game. Players must navigate through a variety of environments, each presenting unique challenges and threats. Equip yourself with a diverse array of weapons, from firearms to explosives, and strategically place mines and barriers to fortify defenses. As you progress, you'll need to defend cities from waves of relentless zombies, using every tool and tactic at your disposal. Master the art of survival, adapt to changing scenarios, and unlock new levels and equipment to enhance your fighting capabilities. This intense VR experience not only tests your combat skills but also your ability to strategize under pressure. Prepare for an adrenaline-pumping adventure that keeps you on the edge of your seat!", "short_description": "Survive amidst toxic zombies in diverse locations in this new Meta Quest game! Defend cities using an arsenal of weapons, mines, and barriers. Discover unique tactics and strategies to fight for your life!"}]}, "developer_name": "Bexzod Abdubekov", "developer_privacy_policy_url": "https://www.facebook.com/permalink.php?story_fbid=122108972456316995&id=61559509877826", "developer_terms_of_service_url": null, "publisher_name": "Bexzod Abdubekov", "support_website_url": null, "website_url": "https://www.facebook.com/61559509877826/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "644354873106712"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8512367302206649"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/75348041_1640617176520941_2891858986587112468_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=spChs_GKigMQ7kNvwHp2Nlv&_nc_oc=AdlqWFWTyWzYD6UB3FmD6vb-OrDgm2gDzwcsF2P_Bke7a4yrv0joVOWIKNa2svq7NlY&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AfE12vDEHzbtjZ7cCeld-OrKE-tl7SIWOaAbQdUNrKHi9