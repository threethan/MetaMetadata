_trailer_thumbnail": {"file_name": "Landscape_2560_1440.png", "id": "857828942470516", "uri": "https://scontent-sin2-2.oculuscdn.com/v/t64.5771-25/38974800_857828945803849_726516360446674022_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=3GVHiYle8zIQ7kNvgEN3yZI&_nc_oc=AdhmjjgYL4j56FaPclqM84N95ljVxm1m13iT6c8kov1kgEibF-YDERkylAgVD_lA2Ys&_nc_zt=3&_nc_ht=scontent-sin2-2.oculuscdn.com&oh=00_AYF5moUCTP4Iq1QNmoPm6qLk41BqhzUX6zx_lVga0hOHlA&oe=67DD365C"}, "display_name": "RealCarrom", "keywords": [], "long_description": "\"Welcome to 'Real Carrom', where tradition meets modern gaming. Engage in one of the most beloved board games with a virtual reality enhancement.\n\n\ud83d\udd33 Game Modes:\n\nBlack and White: Stay true to the classic Carrom game, where you're assigned either black or white coins and must pocket them before your opponent.\nFreestyle: Flexibility is key. Pocket any coin and rack up those points against your competition.\n\ud83d\udd33 Single Player:\nTest your skills against our AI opponents. Whether you're playing solo, partnering with an AI, or challenging multiple AIs, there's a level for everyone.\n\n\ud83d\udd33 Multiplayer Magic:\nExperience multiplayer like never before. Play one-on-one, team up with a friend against two AIs, or embrace the challenge of player + AI versus player + AI matches.\n\n\ud83d\udd33 Immersive Features:\n\nVoice Chat: Communicate with your opponents in real-time.\nOculus Avatars: Customize and flaunt your unique avatar.\nLip-Sync: Experience enhanced realism with lip-sync features.\nRediscover the joy of Carrom, whether you're a seasoned player or a newcomer. With intuitive controls, lifelike interactions, and multiple game modes, 'Real Carrom' promises endless fun and challenges. Ready to strike?\"", "short_description": "Experience authentic Carrom with a digital twist! Dive into single or multiplayer modes, challenge AI, or team up with friends, all while enjoying voice chat, Oculus avatars, and lip-sync. Choose from 'Black and White' or 'Freestyle' modes and showcase your Carrom mastery."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.visionxr.co.in/realcarromprivacypolicy", "developer_terms_of_service_url": null, "publisher_name": "VisionXR", "support_website_url": null, "website_url": "https://www.visionxr.co.in/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1676485315806614"}, {"id": "275629210202852"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER", "SINGLE_USER"], "id": "568607247948184"}}]}, "scheduled_offers": {"count": 6, "nodes": [{"status": "PUBLISHED", "id": "1617660675779159"}, {"status": "PUBLISHED", "id": "914174957535368"}, {"status": "PUBLISHED", "id": "931594398405834"}, {"status": "PUBLISHED", "id": "1177056870219298"}, {"status": "PUBLISHED", "id": "1036172730769671"}, {"status": "PUBLISHED", "id": "197434310056127"}]}, "baseline_offer": {"id": "1036172730769671"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1744995600, "status": "PUBLISHED", "id": "1617660675779159"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "9412548908776608", "version": "0.7"}, "id": "819144115719198", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "5492017687496436", "is_concep