_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AflzPL5vz3u8kXg6_tXCw7GNVJh6Kh2wxoGe56U9sDe0iA&oe=6958FC23"}, {"file_name": "migrated_image.png", "id": "2905155546227966", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/11162680_792407794257234_4366628464556507136_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=zd8YI0VDlfkQ7kNvwF2IXIV&_nc_oc=AdmkIIt0dtjiW0J2_mM010Sipc5EdcQyae7oSpoJumzq2x7O22k53WAg2qw2OkcLkC4&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_Afkd6RRb37HWoyTD80OPinP5XvMldPdnfiIl9qqSqKxEpg&oe=6958F746"}, {"file_name": "migrated_image.png", "id": "642589573193888", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/11162810_1390764774311350_2285142954769121280_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=piAG--deQbIQ7kNvwHo71dc&_nc_oc=Adn9VldjT0iuReUdxQi_An9NwRtTCfFJqKwc5oV57-nQwOPh_HJY9wB9XZ3COc0M9GA&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AflgNKCR7eZD-8MHaDpXrgI76Zu3f-p7mY8Beb2Q-4o2Sg&oe=695910C6"}, {"file_name": "migrated_image.png", "id": "552565832036791", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/11162700_1864451723803223_9012485783956750336_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=AZWdT-mZK38Q7kNvwHvIDGw&_nc_oc=Adn5152CgNvjMRgGuttz4hXB-cENzI2_XT0LgWlvqeiifvepzwedfWPHLk_dsME9T6s&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_Afni4OXVCLru9ezkTPcYS8s9bIqfkRLSDkh6eBANEffV-g&oe=6958F8C0"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Ace Academy: Black Flight", "keywords": ["war", "plane", "combat", "black", "flight"], "long_description": "Experience unmatched gameplay, graphics and narrative as you climb into the cockpit of the most aggressive WWI dogfighting game on mobile. You will fly with real squadrons, over actual historical locations, as you ascend the ranks to become a World War One Ace. You will fight for King and Country from the shores of England and Belgium to Flanders and the Western Front.\n\nUnlock, upgrade and customize the leading aircraft of WWI as you attack airships, rail lines, convoys, and enemy bombers. Test your abilities as you go head to head with top German Jastas in Fokker and Albatross fighters.\nExperience a rich historical narrative, based on accurate WWI people, locations and events. Discover the major battles and theatres that made WWI one of the most gruelling wars ever fought!", "short_description": "Ace Academy: Black Flight will pin you to your seat as you experience WWI dogfighting like never before.\n\nEpic aerial combat in stunning visual environments\n\nFully customizeable aircraft, including enhanced performance, weaponry and paint schemes\n\nNarrative-driven experience with accurate historical timelines and worlds to explore"}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.blackflightgame.com/privacy/", "developer_terms_of_service_url": "https://www.blackflightgame.com/tos/", "publisher_name": "Illumination Games", "support_website_url": null, "website_url": "https://www.blackflightgame.com/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": null, "genres": ["ACTION", "ADVENTURE", "SHOOTER", "SIMULATION", "STRATEGY"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "fr_CA", "fr"], "supported_input_devices": ["GAMEPAD"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "621173349108773"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/11162693_100844157170301_8835602903105273856_n.png?stp=dst-png_s360x360&_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=hWTWsysRNq4Q7kNvwEHp75J&_nc_oc=Adm28trYmaR6g_ZldKrrt43dTDrtT2asMrge1sqJlGFpxfxbvr8ZzJ6bXTAIFNMVZ0g&_nc_zt=3&_nc_ht=scont