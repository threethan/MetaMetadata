ge the color progressively."}]}, "developer_name": null, "developer_privacy_policy_url": "https://midicreationsvr.blogspot.com/p/privacy-policy-updated-at-2021-09-17.html", "developer_terms_of_service_url": null, "publisher_name": "Midi Creations", "support_website_url": null, "website_url": "https://midicreationsvr.blogspot.com/p/who-are-we-indie-virtual-reality-video.html", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "434961926215971"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "24098809486451018"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1286445813106952"}]}, "baseline_offer": {"id": "1286445813106952"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1753881600, "status": "PUBLISHED", "id": "1286445813106952"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "24111775885154378", "version": "1.09"}, "id": "1104587908434470", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "24095265096805457", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "24111775885154378"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1104587908434