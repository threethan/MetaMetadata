d watch your wealth grow! Manage resources, unlock new opportunities, and strategize to dominate the market. With immersive VR mechanics and engaging gameplay, Boss Market challenges you to rise from a startup to a corporate giant. Are you ready to become the ultimate business tycoon? Play now and rule the market!"}]}, "developer_name": "Esti Soft", "developer_privacy_policy_url": "https://drive.google.com/file/d/1nRyME7ipZKdK4JPdh6Aza7RPKWnEWmNI/view", "developer_terms_of_service_url": null, "publisher_name": "Esti Soft", "support_website_url": null, "website_url": "https://www.esti.tr/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1676485315806614"}, {"id": "331529188132278"}, {"id": "1023770785722696"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH", "GAMEPAD"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "28652871920964679"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-sin11-2.oculuscdn.com/v/t64.5771-25/473391814_1949877368880752_2771369787864022544_n.png?stp=dst-png_s360x360&_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=HXN59f_qVc0Q7kNvgEaWzX0&_nc_oc=Adj2q4w9fmCWbBN5XosU-MtSFeGkyjoC4euoC4an6lYXzhPHflZBcPGnjueLXz141-Q&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_AYGCEq3iVnxyvv_PAlwOC2jKDaULgy7c8-4m6KfBLNu6-Q&oe=67DBE873"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Boss Market", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "274332603