rm yourself with stilts and use your quick reflexes and problem-solving skills to complete levels!\n(This is a demo version of the game.)"}]}, "developer_name": "Rekt Games", "developer_privacy_policy_url": "https://sites.google.com/view/stilt-privacy-notice", "developer_terms_of_service_url": null, "publisher_name": "MeKiwi", "support_website_url": null, "website_url": "https://stiltgame.com/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": "NOT_REQUIRED", "genres": ["ACTION", "ADVENTURE", "ARCADE"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "7632636206797244"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sin2-3.oculuscdn.com/v/t64.5771-25/38974491_863292728446922_7903663429858675821_n.png?stp=dst-png_s360x360&_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=PfZ-9qIZ1o4Q7kNvwHY3tSO&_nc_oc=Adls7FO72RBTcb85rLFMSzyO7uNPkds1aZI_vLoe8tFLn21qOv3RHawLDOB8NsKvg-g&_nc_zt=3&_nc_ht=scontent-sin2-3.oculuscdn.com&oh=00_AfUUl0rGapMKK7cgl6idkMr2_DQyLctdQhSe1B977cwMdA&oe=68A3150F"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Stilt Demo", "platform": "ANDROID_6DOF", "context_category": null, "context_topics": [], "release_date": 1677837600, "lastRevision": {"nodes": [{"id": "6121848947875985", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "6121848934542653", "is_first_party": false, "enables_in_app_ads": false, "is_concept": false, "platform": "ANDROID_6DOF"}, "iarc_