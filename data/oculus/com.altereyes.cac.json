_ohc=aZ4d6YCyhAQQ7kNvgGez058&_nc_oc=AdgBLOFm1dKaJuHesj74Y_VOmEBh25_y8O9I8aclNK_PVxfwiFspePqmgeEv1lawu4U&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AYCeJSn2K7wzTggMooDX4axSLLZZ1FWBSYwhgA0cR-leWw&oe=67AB2108"}, {"file_name": "Fantasy_Pack.png", "id": "1287548819265246", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/38982593_1287548822598579_8780483558394124917_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Kb8xWxrzumAQ7kNvgFP-qJQ&_nc_oc=AdiKgE_ZurU1xY9vlYr_ZLnkI09c-JfXumtDRfRsXj3bVB45wbGjI_tu6nBCo7hUDRY&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYBFcb_mS86rZqRaf7IpRKRzPOvvgInzAn2OqZ5H-jhghg&oe=67AB46EA"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Trailer_Cover_Image_2560_X_1440.png", "id": "3796886113899694", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/39031396_3796886120566360_7833945356597784028_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Y_wjiac2w0YQ7kNvgE2WhJe&_nc_oc=Adigp_G8KiqoBmv81IYnouTizcC8iMkhYwQhawE0Hm47KUm8ImxvflS7YE94If1LOCE&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYCaPhIBhBR2xL9BN0V0zm1NKsDxjbOzzHv4tQdE8lpmQQ&oe=67AB26AE"}, "display_name": "Color-A-Cube", "keywords": ["voxel", "coloring", "relax", "chill", "painting"], "long_description": "**Dive** into a **relaxing**, **color-filled** escape with Color A Cube. This immersive coloring experience lets you bring voxel worlds to life, one cube at a time. Discover unique themes and models, with new content inspired by our community!\n\n{560028636645784}\n**Claim** your first pack **for free!** Download now and choose from:\n\n- Fantasy Pack\n- Animal Pack\n- Sci-Fi Pack\n\nEach of these packs has unique and fun models for you to paint!\n\n{1143127904042401}{8799122136832134}\n**Unwind** and **relax** as you paint vibrant voxel environments and characters. Enjoy the process, discover hidden details, and find calm in every voxelated corner.\n{426619937211884}{1080121970080651}\nWith the Meta Quest 3 lineup, you can choose to play in either Mixed Reality (MR) or Virtual Reality (VR). Paint your models your way\u2014whether you're sitting or standing, the experience is tailored to your comfort and creativity!\n{481204747741603}\n\nWe\u2019re dedicated to keeping the game fresh, exciting, and constantly improving shaped in part by your valuable feedback!\n\n- A steady stream of new content\n- Difficulty levels\n- New modes\n- Diorama's to showcase your creations\n- Extended shop\n- More to be revealed!\n\n# Why Early Access?\n\nWe\u2019re launching in early access to involve our community in shaping the game. Your feedback will help us refine features, add content, and create the best experience possible! Join our Discord server to share your ideas, report bugs, and participate in discussions.\n", "short_description": "Dive into a relaxing, color-filled escape with Color A Cube. This immersive coloring experience lets you explore and bring voxel worlds to life, one cube at a time."}]}, "developer_name": "AlterEyes", "developer_privacy_policy_url": "https://bigshots-vr.com/ColorACube/privacy.html", "developer_terms_of_service_url": "https://bigshots-vr.com/ColorACube/tos.html", "publisher_name": "AlterEyes", "support_website_url": null, "website_url": "https://altereyes.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "377978390272490"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9058823247547549"}, "review_messages": [""]}