, "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/75500286_312906725136614_1043827207362831468_n.png?_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=4GmsIV5-gm8Q7kNvwH-hfHn&_nc_oc=AdltM3J6BsjVeY5CQ4RL2MTI-zBOzwjnG_oCBZhovmFp4YI6lMku-0Uc5KMS9-n9ylg&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfmPoHF4Dga7M2-PfVeqHy3EMJooBJpXFT0tA9CWyx63Mw&oe=6952726B"}, "display_name": "Contractors Showdown", "keywords": ["BR", "FPS", "BattleRoyale", "military", "simulation"], "long_description": "Contractors Showdown delivers an immersive military-styled VR First-Person Shooter experience. Engage in heart-pounding combat as you, along with 59 other players, navigate a vast and detailed 16 square-kilometer map. The game centers around a thrilling Battle Royale game mode, where strategic prowess and combat skills are paramount.  Gear up for intense battles, dynamic team play, and a visually stunning VR environment that thrusts players into the forefront of military actions.", "short_description": "Contractors Showdown delivers an immersive military-styled VR First-Person Shooter experience that centers around a thrilling Battle Royale game mode, where strategic prowess and combat skills are paramount."}]}, "developer_name": "Caveman Game Studio", "developer_privacy_policy_url": "https://www.contractorsvr.com/privacy", "developer_terms_of_service_url": null, "publisher_name": "Caveman Game Studio", "support_website_url": null, "website_url": "https://www.contractorsvr.com/showdown", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "3744040075834813"}, {"id": "644354873106712"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": nu