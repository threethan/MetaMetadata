oice-vr.com/", "category": "ENTERTAINMENT", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1008279844328894"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ART_CREATIVITY", "DOCUMENTARY_HISTORY", "NARRATIVE"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "697619261645962"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "437912464606282"}]}, "baseline_offer": {"id": "437912464606282"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1687604400, "status": "PUBLISHED", "id": "437912464606282"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "6324222590969594", "version": "1.3.4.4"}, "id": "452759492574753", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "3778279212230624", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "6324222590969594"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": []}, "change_log": null, "created_date": 1759155589, "id": "24787607474204492", "version": "2.0.5", "version_code": 209, "release_status": "PENDING", "status": "DRAFT", "binary_test_results": {"nodes": []}, "obb_binary": {"id": "24787605737537999"}, "asset_files": {"count": 3}, "debug_symbols": {"count": 0}, "__isAppBinaryWithFileAsset": "AndroidBinary"}, {"__typename": "AndroidBinary