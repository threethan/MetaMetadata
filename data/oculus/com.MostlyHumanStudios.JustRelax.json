l, "developer_privacy_policy_url": "http://www.mostlyhumanstudios.com/", "developer_terms_of_service_url": "http://www.mostlyhumanstudios.com/", "publisher_name": "Mostly Human Studios", "support_website_url": null, "website_url": "http://www.mostlyhumanstudios.com/", "category": "ENTERTAINMENT", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": null, "genres": ["CASUAL", "EDUCATIONAL", "EXPLORATION", "SIMULATION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["TOUCHPAD"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "667700724421775"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/12533842_267869890309388_3513781773571981312_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=-1c8EaDVHeMQ7kNvwH-xiKe&_nc_oc=AdkCKnHC8wvAGTtf4CnJd5m_5oxZj5wzXtGcpBq6FkOtNJ9EhkJXrGWVXJAcE-rlAOg&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AfZnalvMxE4ra1x3_wr4PCAvCnIZecYdutwGv6Mw32rk9A&oe=68E255F6"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Just Relax: VR Paradise", "platform": "ANDROID", "context_category": null, "context_topics": [], "release_date": 1487750400, "lastRevision": {"nodes": [{"id": "799122286855004", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "1025349094212046", "is_first_party": false, "enables_in_app_ads": false, "is_concept": false, "platform": "ANDROID"}, "iarc_cert": {"id": "747579022058269"}, "iarc_email": null, "msrp": null, "translations": {"nodes": [{"images": {"nodes": [{"file_name": "migrated_image.png", "id":