_zt=3&_nc_ht=scontent-sin6-1.oculuscdn.com&oh=00_AflHZhkNgEFRKOS24V9zOrjZgsLvWrL1ZxYasO7yQZD3dQ&oe=693820C8"}, {"file_name": "migrated_image.png", "id": "1045745515793820", "uri": "https://scontent-sin6-1.oculuscdn.com/v/t64.5771-25/11162732_345186819225360_8463322181634359296_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=37Eew92uhFEQ7kNvwGNuvLl&_nc_oc=AdlANh9e3_fRISvYOfbvd4g2OIyfcPFC4uefcTyRODMHnABgxOEvzpFtT0ct1dnJKpQ&_nc_zt=3&_nc_ht=scontent-sin6-1.oculuscdn.com&oh=00_Afmd-Qc1t0_Fkh1vi6L3B2IXlfSgzjsvgDCwfo78fsNXSw&oe=693803E7"}, {"file_name": "migrated_image.png", "id": "208565006909328", "uri": "https://scontent-sin6-1.oculuscdn.com/v/t64.5771-25/11162741_265666307269022_936834578889310208_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=llfhztTn9hMQ7kNvwFo778n&_nc_oc=AdkoAgcxN686-vlt7nPyIlJ7poEFCqsU8cbapn1dY-aBKzndRq13xiMivkxAeLrjdrk&_nc_zt=3&_nc_ht=scontent-sin6-1.oculuscdn.com&oh=00_AfluMdtgasOKCvFlZ8FvmgretzVoxqtibcvPIwoDob7AqA&oe=6938101E"}, {"file_name": "migrated_image.png", "id": "224407572087016", "uri": "https://scontent-sin11-2.oculuscdn.com/v/t64.5771-25/11162726_114415772612159_1957637005366001664_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=g93gycvIi2kQ7kNvwG4gMvZ&_nc_oc=Adnv2LPb7c8e9rFf01GIfCjgskLi4hKQ_OevHdw1K4rX-6X3--adQKYRUac193H6VZ4&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_Afm9KjLX8MysGRgiYTafsr4G3W2YHw2G2jRScCN7y27Ilg&oe=6937FBB0"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "AFFECTVR", "keywords": ["empathy"], "long_description": "Sometimes referred to as and Empathy Machine, Virtual Reality Video has the power to change lives - both of the viewer and of the subject. We make it easy to find amazing content that holds the power to do exactly that, and have a blast while doing it.\n\nTravel to far off lands, voyage under the sea or put yourself in the shoes of someone who has not been given all the advantages of the first world - and experience the power of immersive film making and the empathy it can create. \n\nWe aim to provide a platform for our story telling partners to share films that have the power to create good in the world, and a way for viewers to both entertain and educate themselves, while affecting real change. \n\nExplore. Excite. Affect Change.", "short_description": "An Immersive Media Plaform For Social Change - Affect Yourself, Affect the World\n\nWe provide our audience with a curated feed of the most spectacular Virtual Reality films that will entertain, while simultaneously educating and providing concrete ways to affect change within themselves and in the world."}]}, "developer_name": null, "developer_privacy_policy_url": "http://underexposedagency.com/affectvr/privacy_policy.html", "developer_terms_of_service_url": "http://underexposedagency.com/affectvr/termsofservice.html", "publisher_name": "Affect[VR]", "support_website_url": null, "website_url": "http://www.brianhockensteincinema.com/affect-vr/", "category": "ENTERTAINMENT", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": null, "genres": ["ADVENTURE", "EDUCATIONAL", "NARRATIVE", "MOVIE", "ACTION"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["TOUCHPAD"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "361860895773085"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sin11-2.oculuscdn.com/v/t64.5771-25/11162763_110432226339779_2842953696352927744_n.png?stp=dst-png_s360x360&_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=OHs97_qaX5cQ7kNvwFXwhMf&_nc_oc=AdkAjwtbtq3XenIyvQzlOqHJdecmvakP-mnTmnTUrzste-FGO0th7ZCkKKK3YmXNfpI&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_AfnGi42_QRUSf5MU700z4bwFv-OO1bTQ3F_srKMRaNrITQ&