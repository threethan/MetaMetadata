to smash some balls in VR? Black Hole Pool is the ultimate Pool/Billiard VR experience with the most accurate physics and great graphics. "}]}, "developer_name": "Pixel Works", "developer_privacy_policy_url": "https://gamend.io/pp/bhp_pp.html", "developer_terms_of_service_url": null, "publisher_name": "Pixel Works", "support_website_url": null, "website_url": "https://www.facebook.com/blackholepool", "category": "ENTERTAINMENT", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}, {"id": "331529188132278"}, {"id": "1676485315806614"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["SPORTS"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER"], "id": "1395390931218456"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ord5-3.oculuscdn.com/v/t64.5771-25/39003233_2457321541101818_5206774126976051875_n.png?stp=dst-png_s360x360&_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=NaYc_kClKWIQ7kNvwG8a7oa&_nc_oc=AdmC28hHGsS88NwbgZ_m2QF-2So3MmlsK4IJUvEcpj5bAJ2PAyvlj59RQTv-touBEZQ&_nc_zt=3&_nc_ht=scontent-ord5-3.oculuscdn.com&oh=00_AfuJr3vEiXUIZs8ap-efMu-i7OSCNIzyJAo55r6gz451Gg&oe=698B3435"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Black Hole Pool", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Sports", "id": "2776542286000603"}, {"topic_name": "Simulation", "id": "331529188132278"}, {"topic_name": "Tabletop", "id": "1676485315806614"}], "release_d