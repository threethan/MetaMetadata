oper_terms_of_service_url": null, "publisher_name": "Delightly Creative", "support_website_url": null, "website_url": "http://delightlycreative.com/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": "NOT_REQUIRED", "genres": ["CASUAL", "PUZZLE"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": [], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "511451483638734"}, "review_messages": ["Hello,\n\nCongratulations! Your submission is ready to publish in the Oculus Store.\n\nYour app will be released on 1/9/2019 @ 10AM PT for free under the Games section of the mobile store.  Let us know if you would like a different date otherwise we will proceed with launch  Please review all binaries in the STORE channel to make sure the proper supported devices are listed.  Please do not update your online services (such as your back-end servers) during your launch week.\n \nOnce your submission is published, you can access analytics here: (https://developer.oculus.com/apps/).\n \nThanks,\n \nThe Oculus Store Team\n", "Hi,\n\nThanks again for making those updates.  We just need the hero asset and small landscape to be consistent with the rest of the assets and we should be able to release.", "Hi,\n\nThis is still not sufficient.  As mentioned before, if you have a plain colored background it will look a lot cleaner.", "Hi,\n\nAlthough your application is ready to publish your assets don't look polished and will deter users from engaging with your app.\n\nPlease update your assets using this guideline: https://static.oculus.com/documents/oculus-store-art-guidelines.pdf\n\nMy first suggestion to improve this asset would be to remove the background as it is too busy with your logo.\n\nOnce the update is completed, we will email you with your launch date.", ""]}]}, "cover_square_image": {"uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/38974495_339628510158909_114171556556439552_n.png?stp=dst-png_s360x360&_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=SF2HNjjhqTcQ7kNvgFi_eoT&_nc_oc=AdgDIKNX-9us2eBPILqqyMo8Vgqi2PKizFHTAPsxyDZ1p9CSnVaTK4rEiJSLCVHyafU&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYAKTnYR6AmdGeSbW4vLnlbffN2FNPMDTauwXzfhSkTBog&oe=67B70129"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Balls and Hoops", "platform": "ANDROID", "context_category": null, "context_topics": [], "release_date": 1547053200, "lastRevision": {"nodes": [{"id": "339116103523106", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "2075736015780362", "is_first_party": false, "enables_in_app_ads": false, "is_concept": false, "platform": "ANDROID"}, "iarc_cert": {"id": "2161698294086450"}, "iarc_email": null, "msrp": null, "translations": {"nodes": [{"images": {"nodes": [{"file_name": "migrated_image.png", "id": "4309709592388217", "image_type": "APP_IMG_COVER_PORTRAIT", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/38974706_737751553249140_3435557847742545920_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=kclCVpAOEKEQ7kNvgHGYO3z&_nc_oc=AdiMJ9f47xpdsN4BXFxigH1ODLIQjSRkGxHmSLT_bs7eSLz8kNwHvdEsk1wiUwGPwKI&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AYDDwvXpB62eMsXWGrICo9NB-Y6xrbAy5kdwfG5REgZNfg&oe=67B71A56"}, {"file_name": "migrated_image.png", "id": "2329251384034499", "image_type": "APP_IMG_ICON", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/38974460_2165092887083291_1863132056813305856_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=1LKnjZ_EXMcQ7kNvgEjN2PA&_nc_oc=Adgsz8GBz7y9OBz32_zCvwah7KZXPYmkMr4Zhi97jiFNYznx6zIVt38n7WYtxjclkCY&_n