, and enjoy the special atmosphere with us."}]}, "developer_name": null, "developer_privacy_policy_url": "http://megogo.net/ru/rules", "developer_terms_of_service_url": "http://megogo.net/ru/rules", "publisher_name": "MEGOGO LLC", "support_website_url": "https://megogohelp.userecho.com/", "website_url": "https://megogo.net/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "434961926215971"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "ru"], "supported_input_devices": ["HAND_TRACKING", "OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8449760058378681"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1706922386722876"}]}, "baseline_offer": {"id": "1706922386722876"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1727168820, "status": "PUBLISHED", "id": "1706922386722876"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "8845715092116507", "version": "2.3.1"}, "id": "2103664403362130", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "8449760035045350", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "8845715092116507"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "2103664403362130"}]}, "change_log":