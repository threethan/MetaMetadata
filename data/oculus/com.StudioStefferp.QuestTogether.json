_oc=AdkUK771OjxgiS7K5SnCUl1IZB_qMtg1cDszEV5rUlyP6buzV1Z93SyRjOCRqgKQHUM&_nc_zt=3&_nc_ht=scontent-sin6-1.oculuscdn.com&oh=00_AfqrKsjdwPabDvwHCnYW9O3OgxibNkEvgytgb6mtJREkeg&oe=6964D205"}, {"file_name": "ss05.png", "id": "1195708815323225", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/507883442_1195708818656558_245947397122507213_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=qS1wMI-3-tgQ7kNvwF4PuI8&_nc_oc=AdkjOHe_VPmyJDwE9uLYdH6G_fFuPVBbkfZe5Diip43GpemP4MEiavRpjBDf_SAu9Dk&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfrMjuBL5wx__KyAhb76aBHo_zMigyS_qqWJAHIdPsY58g&oe=6964CFA3"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "CoverImageSafeAreaTemplateLandscape copy.png", "id": "1968993627221764", "uri": "https://scontent-sin11-2.oculuscdn.com/v/t64.5771-25/491827282_1968993630555097_8255122959147714982_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=tvgcKzBQsoIQ7kNvwGgtl58&_nc_oc=AdlqSSc-htwedJzJoeVRijfaYnIR7G4XiKDGmpLRYFiGhTgGzyCm0DhnAQXZFAhOFzY&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_AfoloepbTYdE1fqwMCEnhRh22hgH2AzX0pYb1y4rT4J6kQ&oe=6964F15E"}, "display_name": "QUEST TOGETHER", "keywords": [], "long_description": "Take up your sword and shield and go on a quest with your friends! \n\nQUEST TOGETHER is a social VR MMORPG where you can hang out and play together. As a new adventurer, you get stationed in a small town that's been suffering from recent monster attacks. Take up quests and defend the town from monsters and help out with the villagers' requests. Meet other adventurers in town, make new friends, team up together for adventure, or partake in some PVP matches!  \n\n - Enjoy online multiplayer in VR  and make new friends, or invite your existing ones\n- Party-based combat with up to 4 players per battle \n- For the competitive Heroes, enjoy a PVP mode in the arena to earn new gear and rewards\n- Customize your character with a variety of options and items\n- Unlock new weapons, armor sets, outfits and gear by playing\n- Strategize with your party by dividing roles: melee, ranged, support, etc.\n- Work your way up from small fry goblins to large bosses that won't go down without a fight", "short_description": "Take up your sword and shield and go on a quest with your friends in QUEST TOGETHER, the VR MMORPG. Rise up the ranks in the adventurers guild, hang out with your fellow questers, make new friends, team up together for adventure, or partake in some PVP matches!"}]}, "developer_name": "StudioQT", "developer_privacy_policy_url": "https://questtogethervr.com/privacy", "developer_terms_of_service_url": null, "publisher_name": "StudioQT", "support_website_url": null, "website_url": "https://questtogethervr.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2691890164460465"}, {"id": "821934236779161"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER"], "id": "26332290803026208"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/491823366_877124624823343_8705413590659770775_n.png?stp=dst-png_s360x360&_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=FlpFBgUdmcEQ7kNvwHFSQ0E&_nc_oc=Adlp_8NWEZmmH57SwG32jXxD6-cL5YQ5nDe_-ccA5ZmsCXWA7GayNG4pgWeqC1e6wKQ&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfrNNbKyc0SfkjV0jSiUaSQ-2GJa2M9XLOJrQZqzbc7dkA&oe=6964F09A"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "QUEST TOGETHER", "platform": "