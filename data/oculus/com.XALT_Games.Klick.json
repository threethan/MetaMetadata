, {"file_name": "4.png", "id": "2408464942863131", "uri": "https://scontent-sin6-2.oculuscdn.com/v/t64.5771-25/473393432_2408464946196464_5117368587959372767_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=NJJErBYLwdsQ7kNvwFFfoBA&_nc_oc=Adl4CZ6mOvkQVHTLpeJ8velz2AIJwD36Sudn7Viyihv12bF0djTWkwx8BxJ6CWg7Ru8&_nc_zt=3&_nc_ht=scontent-sin6-2.oculuscdn.com&oh=00_AfoUKkUQlz_Fafrft_tVO5Q1ESJrhx1aJs1Yu-T2-yn_Nw&oe=6970A7F1"}, {"file_name": "1.png", "id": "631256663110807", "uri": "https://scontent-sin2-1.oculuscdn.com/v/t64.5771-25/473391937_631256666444140_5526436203422352506_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=kO2RbsZLk7QQ7kNvwGWmhV_&_nc_oc=Adm9XTwWgUb4TbtAg1tqDqab4F09nNfVVMQk961DQvJXYTJVVyA_6asCIxg-hDHCjhE&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_AfqBzqZH1pHs9wTqhgNWxMjC1kZA-THqGf9hbBQDn7fNew&oe=6970AAD1"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Landscape - 2560 x 1440px.png", "id": "674109361695253", "uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/473391937_674109365028586_8423212186245884846_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=tWhoqrRROKUQ7kNvwH-n8LP&_nc_oc=AdmIJpC8L6r5fuhGkLx_0CK9BBv5mn19qoAF8nXwmO4OzlDcUVOhhFMJmWtv1yO-DmU&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfoPqcZydEcBS0HlCpSKv7aZcVaxv6se13RJM13tp7jhFw&oe=6970CE84"}, "display_name": "Klick", "keywords": ["Puzzle", "Casual", "Minimal", "Logic", "Sequence"], "long_description": "Klick is a minimal, mind-bending VR puzzle experience where every solution is just one sequence away. With over 200 handcrafted puzzles spread across five unique sets, Klick challenges you to think logically, observe patterns, and press buttons in the correct order to progress.\nFrom simple taps to complex logic chains, each level is a test of precision, memory, and deduction. Whether you're chasing the right shape, matching pairs, balancing numbers, or following light-up patterns, Klick turns pressing buttons into a deeply satisfying puzzle journey.\nNow with Mixed Reality support, you can bring the puzzles into your real-world space\u2014seamlessly blending logic and immersion.\nClean. Clever. Klick..", "short_description": "Klick is a minimal, mind-bending VR puzzle game with over 200 handcrafted challenges. Press buttons in the right order, follow patterns, match shapes, balance numbers, and more. Test your logic, memory, and precision across five unique puzzle sets. Now with Mixed Reality support, bring the puzzles into your world for a seamless, immersive experience.\nClean. Clever. Klick."}]}, "developer_name": null, "developer_privacy_policy_url": "https://github.com/nitinkumar25195/PrivacyPolicy/blob/main/Klick_Privacy_Policy", "developer_terms_of_service_url": null, "publisher_name": "XALT Labs ", "support_website_url": null, "website_url": "https://www.xaltlabs.in/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "377978390272490"}, {"id": "1592192974313489"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9865007913566645"}}]}, "scheduled_offers": {"count": 7, "nodes": [{"status": "PUBLISHED", "id": "851794444266871"}, {"status": "PUBLISHED", "id": "1193013226193954"}, {"status": "PUBLISHED", "id": "1282099853547309"}, {"status": "PUBLISHED", "id": "1392518428677333"}, {"status": "PUBLISHED", "id": "762906649441772"}, {"status": "PUBLISHED", "id": "1853925192060903"}, {"status": "PUBLISHED", "id": "1927231067809733"}]}, "baseline_offer": {"id": "7629