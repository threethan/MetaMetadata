irtual reality.\n\ud83d\ude86 Features:\nRealistic train driving physics and controls\nImmersive city environments and railway tracks\nPassenger pickup and drop-off challenges\nMultiple train models to drive\nDesigned for VR immersion on Meta Quest\nBecome the ultimate Railway Driver and experience the most authentic train simulator in VR.", "short_description": "Experience realistic city railway driving in this immersive train simulator."}]}, "developer_name": null, "developer_privacy_policy_url": "https://sites.google.com/view/trainsimulator-privacy-policy/home", "developer_terms_of_service_url": null, "publisher_name": "Horizon Hub", "support_website_url": null, "website_url": "https://sites.google.com/view/horizonhubcom/home", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "24404677405860778"}}]}, "scheduled_offers": {"count": 5, "nodes": [{"status": "PUBLISHED", "id": "4069486133315245"}, {"status": "PUBLISHED", "id": "2626584101037158"}, {"status": "PUBLISHED", "id": "3154274918085311"}, {"status": "PUBLISHED", "id": "800192752490634"}, {"status": "PUBLISHED", "id": "24228008033487923"}]}, "baseline_offer": {"id": "24228008033487923"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1771005600, "status": "PUBLISHED", "id": "4069486133315245"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", 