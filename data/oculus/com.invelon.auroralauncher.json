 take notes and measurements... and much more! and much more!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.auroracloud.app/terms/view/site-terms/", "developer_terms_of_service_url": "https://www.auroracloud.app/terms/view/site-terms/", "publisher_name": "Invelon Technologies", "support_website_url": null, "website_url": "https://www.auroracloud.tech/", "category": null, "comfort_rating": null, "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1819021644882647"}], "external_subscription_type": "REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "es", "it"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "25919450384367307"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "3985700151702543"}, {"status": "PUBLISHED", "id": "7618598214842817"}]}, "baseline_offer": {"id": "3985700151702543"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1741640520, "status": "PUBLISHED", "id": "3985700151702543"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "26806176449028025", "version": "1.1.2"}, "id": "839429618058334", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "25759247957054218", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "26806176449028025"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"cha