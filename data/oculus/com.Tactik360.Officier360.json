, "video_trailer_thumbnail": null, "display_name": "Officer360", "keywords": ["Firefighter", "firefighting", "training", "emergency", "fire"], "long_description": "The fire incident management simulator is an immersive solution designed for fire safety officers. It recreates realistic fire interventions by offering a 360\u00b0 view of the building to assess. Users can plan firefighting and rescue strategies, coordinate firefighter teams, and track operational progress in real-time using a command table. This simulator enhances crisis management, tactical, and leadership skills through varied and interactive scenarios, promoting quick and effective decision-making in emergency situations.\"\n\nThis emphasizes the key features while highlighting the practical benefits for users.", "short_description": "Our incident management simulator for fire safety officers allows for a 360\u00b0 assessment around the building, strategy and tactics planning, and firefighter team coordination. With a command table to monitor real-time operations, this simulator provides an immersive environment for effective fire incident management."}]}, "developer_name": null, "developer_privacy_policy_url": "https://tactik360.com/politique-de-confidentialite/", "developer_terms_of_service_url": null, "publisher_name": "Tactik 360", "support_website_url": null, "website_url": "http://www.tactik360.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "514087707868786"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "fr_BE", "fr_CA", "fr", "en_US", "en_CA", "en_AU"], "supported_input_devices": ["OCULUS_TOUCH"], "suppo