_nc_ohc=hc7B2OTHbP0Q7kNvwEh4odz&_nc_oc=Admza-dTkFk3FXq3BAKwq6sJDyKcoZZeeS5lG7gF0UztCztxfMFGUzwp4nwfKh2Vcu8&_nc_zt=3&_nc_ht=scontent-sjc6-1.oculuscdn.com&oh=00_AfW1R-C9RgwQwdxfm7TEenpSm9g6K3RlmVSILI-yry4EzA&oe=6895E948"}, {"file_name": "screen-05.png", "id": "1071700544544178", "uri": "https://scontent-sjc6-1.oculuscdn.com/v/t64.5771-25/57569458_1071700547877511_3894759084831309614_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=zmyzTIz70E4Q7kNvwEu5k_V&_nc_oc=AdkLN7yB-7Ffo0Yk44XDt2ZDN8t6nRYJhKBb8QTifLqc88rfc_hIeAiOERnSyrGKsms&_nc_zt=3&_nc_ht=scontent-sjc6-1.oculuscdn.com&oh=00_AfUBvDWee_PrpLo41s_XMmA53Yo4JA6oyS8T_zln5RVeFQ&oe=6895F6D8"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Trailor-cover.png", "id": "1928607067623101", "uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/39031493_1928607070956434_5898141224729612957_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=amKEcPoRwYIQ7kNvwF6ezNJ&_nc_oc=AdmM424NILFmOaGMgkdr1b6pOAvXh4TTPj27PqVkrezw3Qf4VIpGCu0Ae1YoYsOExao&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfWQm3WqFWpczRFbnxbqMoxOjyhkBQdWkGFvIdGxdQiGag&oe=6895EFED"}, "display_name": "OVERAY STUDIO", "keywords": ["Productivity", "VirtualOffice", "MultiMonitor"], "long_description": "OVERAY STUDIO is not just another virtual workspace; it's a fusion of immersive experience and practicality, tailored for both professionals and enthusiasts. At its core, this platform provides access to essential features completely free of charge. Users can effortlessly integrate up to five virtual monitors, enjoying the freedom to customize their spatial layout or opt for our handy preset configurations. The software seamlessly syncs with Windows, ensuring an automatic match with your monitor layout for a frictionless experience.\n\nBoasting an intuitive user interface, controlling your spatially arranged monitors becomes second nature. Paired with user-friendly passthrough capabilities, you can immerse yourself in a variety of virtual spaces provided at no additional cost, turning mundane tasks into engaging experiences.\n\nWhile OVERAY STUDIO is currently in its beta phase and there may be areas awaiting refinement, your support fuels our passion. We're committed to enhancing the platform, with upcoming features set to redefine the PC-VR integration landscape. Dive into the future of productivity with OVERAY STUDIO, and be a part of this pioneering journey.", "short_description": "Enjoy your elevated virtual workspace with OVERAY STUDIO \u2013 Your free gateway to an immersive VR-PC integration."}]}, "developer_name": "Overay Inc.", "developer_privacy_policy_url": "https://overay.com/docs/privacy", "developer_terms_of_service_url": "https://overay.com/docs/service", "publisher_name": "Overay Inc.", "support_website_url": "https://overaystudio.com/faq", "website_url": "https://www.overay.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1063584910766706"}, {"id": "393222478507326"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH", "HAND_TRACKING"], "supported_player_modes": ["SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "24591053000501964"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/39031592_1409948483296012_1701633509505912064_n.png?stp=dst-png_s360x360&_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=JRJ4fqie3AAQ7kNvwEaiM-k&_nc_oc=Adkch246ftl9m6gvJ_6gw_HZdMOo8mkQR3W9SU7BaJyV5iJWLZ6v8Us7bCjsRNao33A&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfUGpzCLl1NtEX_o2FkKnasjCBNJTDxs3v