! "}]}, "developer_name": null, "developer_privacy_policy_url": "https://newstory-inc.com/privacypolicy", "developer_terms_of_service_url": null, "publisher_name": "New Story Inc", "support_website_url": null, "website_url": "https://newstory-inc.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}, {"id": "2691890164460465"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8895127967270081"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-atl3-1.oculuscdn.com/v/t64.5771-25/38974892_616839927463237_4336679162718266100_n.png?stp=dst-png_s360x360&_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=abMyKlwaRdkQ7kNvwESRLpF&_nc_oc=Adlz1fc22M26MWgScl_JjkqJXiApC8vGwsqFbBMwu8dfPLcJDopFRRY_wTKGfs7RKOA&_nc_zt=3&_nc_ht=scontent-atl3-1.oculuscdn.com&oh=00_AfmnbRMY55_NNPCNQOFPxxdcFrqYZ9SiYbiaiZyhCnmuKg&oe=6951357D"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Blast Smash", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Simulation", "id": "331529188132278"}, {"topic_name": "Adventure", "id": "2691890164460465"}], "release_date": 1735352040, "lastRevision": {"nodes": [{"id": "8895128003936744", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "8895127937270084", "is_firs