's? Enjoy this VR port of a classic RPG from the Intellivision console."}]}, "developer_name": null, "developer_privacy_policy_url": "http://www.solomonslair.com/privacy.html", "developer_terms_of_service_url": "http://www.solomonslair.com/terms.html", "publisher_name": "RetrovisionVR", "support_website_url": null, "website_url": "http://www.solomonslair.com/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": "NOT_REQUIRED", "genres": ["ADVENTURE", "EXPLORATION", "RPG"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["TOUCHPAD"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "364763448403512"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/38974783_250851755787804_6117847666639503360_n.png?stp=dst-png_s360x360&_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=stoK6R2Zx7wQ7kNvwG7N7H-&_nc_oc=Adk3Y0eHzq7Zgt6uXY7dukxSZ2eTs4s4oBhIqhxW8rMMl5ZfM8a9zKfKYhI0tj9FFk0&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_AfNONU8-CeJpwr4etAXIYU92bT9d7IqhkjUFecns2jJnHg&oe=6852AF75"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Minotaur", "platform": "ANDROID", "context_category": null, "context_topics": [], "release_date": 1493190000, "lastRevision": {"nodes": [{"id": "1202169136531410", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "1283593541663549", "is_first_party": false, "enables_in_app_ads": false, "is_concept": false, "platform": "ANDROID"}, "iarc_cert": {"id": "1131164643648696"}, "iarc_email": null, "msrp": null, "translations"