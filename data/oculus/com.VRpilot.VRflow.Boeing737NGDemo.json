_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=W1XtEPImln4Q7kNvwHgEyg1&_nc_oc=AdmbGSXmxvtvKqB8LG_TUu_ZzKWS9NL7J9WEPb9_L6m1U6mhTxK7qelEuUmu4N2UIPI&_nc_zt=3&_nc_ht=scontent-hkg1-1.oculuscdn.com&oh=00_AfImODoSzOpFv5evAImbJ_e_Hk_ZxBVZTduIce5N0o2HMw&oe=683B0310"}, {"file_name": "Screen2_VRflowA320_Demo_2560x1440.png", "id": "810495789668544", "uri": "https://scontent-hkg1-2.oculuscdn.com/v/t64.5771-25/39031440_810495793001877_5087889788123839018_n.png?_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=98ZOsiJGW6AQ7kNvwHMnLpr&_nc_oc=AdkXrBQdz8LxzTR6FXIMCFq2Nu6kMvfREyJjAt_WWeK8B2lxjMSxqp-MF8y1UkPkzMU&_nc_zt=3&_nc_ht=scontent-hkg1-2.oculuscdn.com&oh=00_AfKFublzvYEPxwH_OxF8GGpRfC_3RyA_AmbhmTTz81Fndw&oe=683AD586"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "VRflow Oculus Store trailer_thumb.png", "id": "520187469404198", "uri": "https://scontent-hkg1-2.oculuscdn.com/v/t64.5771-25/38982487_520187472737531_6395151425899141695_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=sJxEU573W5oQ7kNvwFfKiQj&_nc_oc=Adn1-fLS_Tm8pcCXDCJN_3CATtb4KRx6MknlWGHwKx9gFhwlCLXDhJj9OoiCGO3oA-E&_nc_zt=3&_nc_ht=scontent-hkg1-2.oculuscdn.com&oh=00_AfJpN-PUaedKiSYi2OYsA8gvEP2DU-n-yWewBhckDor_2w&oe=683ADC32"}, "display_name": "VRflow B737NG Demo", "keywords": [], "long_description": "VRflow B737NG is the interactive cockpit procedure trainer for pilots, used by airlines worldwide. Train procedures in training scenarios that incorporate detailed aircraft behavior including sounds, instruments and cockpit layout.\n\nVRflow incorporates numerous features for efficient learning and aircraft familiarization, ideal for type ratings. Train with or without visual help, track your errors, see your training time, train with an automatic crewmember and much more.\n\nThis demo version only includes the Preflight Procedure \u2013 First Officer procedure.\n\nBuy the full version for access to a total of 14 normal procedures and 3 emergency/abnormal procedures. Explore Mode, where you can explore the cockpit and learn about systems, is also included in the full version.", "short_description": "This demo version contains one procedure and does not have the Explore Mode feature.\n\nVRflow B737NG is the interactive cockpit procedure trainer for pilots, used by airlines workdwide. VRflow incorporates advanced VR learning technologies, providing an extremely efficient learning tool for pilots doing their initial or recurring type rating training.\n\nAll purchases of VRflow will be upgraded as we add more procedures, fix bugs and make VRflow better."}]}, "developer_name": null, "developer_privacy_policy_url": "http://vrpilot.aero/privacy", "developer_terms_of_service_url": "https://vrpilot.aero/terms-of-service", "publisher_name": "VRpilot", "support_website_url": null, "website_url": "http://vrpilot.aero/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "514087707868786"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["FLYING", "EDUCATIONAL", "UTILITY"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "783587172750039"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-hkg1-2.oculuscdn.com/v/t64.5771-25/38974613_169933971864154_1051306663704616938_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=h0IsqJHn3t8Q7kNvwGJ9Qiq&_nc_oc=AdnhpbrxxLPk4keJM8yloAU9dESrM8877rh5y3VrYPEMO3CIbQFRDfQ-l3VDo7DzZ-c&_nc_zt=3&_nc_ht=scontent-hkg1-2.oculuscdn.com&oh=00_AfL0uNall1ekwSuztgZq_ecPxCDyByHUv0JCdcWTtZr0gw&oe=683AE37F"}, "is_concept": false, "is_test": false, "is_trial_offer_valid