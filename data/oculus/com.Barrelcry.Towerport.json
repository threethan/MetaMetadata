"}]}, "developer_name": null, "developer_privacy_policy_url": "https://barrelcry.com/privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "Barrelcry", "support_website_url": null, "website_url": "https://barrelcry.com/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "275629210202852"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["STRATEGY", "ACTION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "776717013567651"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "609255620605567"}, {"status": "PUBLISHED", "id": "446570603912741"}]}, "baseline_offer": {"id": "609255620605567"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1662995640, "status": "PUBLISHED", "id": "609255620605567"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "29473349982313763", "version": "0.3"}, "id": "1108424596427487", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "7805743412834399", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "29473349982313763"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1108424596427487"}]}, "change_log": null, "created_date": 17479082