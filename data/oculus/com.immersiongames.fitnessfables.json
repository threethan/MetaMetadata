ots": [{"file_name": "Fitness Fables _Screen03.png", "id": "557779836863316", "uri": "https://scontent-sin11-2.oculuscdn.com/v/t64.5771-25/57566690_557779843529982_2176958166530982049_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=aWX2sO1sDTQQ7kNvwGkvV1e&_nc_oc=Adli_R7_YSfoovSTIoFQARtCgeu3XBJtmZHFIkQPtiwZiqrxDiEOlKe1Ng4amvVezxE&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_AfFWFICrmN9qznVgZaf38qrBGvgOnywttviF2HeAZ_go2g&oe=680F7302"}, {"file_name": "Fitness Fables _Screen01.png", "id": "1017928296768780", "uri": "https://scontent-sin6-2.oculuscdn.com/v/t64.5771-25/57566680_1017928303435446_5168109478840329162_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=4Tsu_SBB3jQQ7kNvwFSKr1t&_nc_oc=AdkU5E4I3YQ884nl43SPRmzoOHNskaFDbwKGcMyBcrZIrp_0W0dRKLD_41pAHwdUD7g&_nc_zt=3&_nc_ht=scontent-sin6-2.oculuscdn.com&oh=00_AfFxjMnO5jHBeedfXn1mRCUsqZHHY2dBIkArrA57dUDKqw&oe=680F799C"}, {"file_name": "Fitness Fables _Screen05.png", "id": "1106902934104332", "uri": "https://scontent-sin11-2.oculuscdn.com/v/t64.5771-25/39035387_1106902937437665_1830145943394226629_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=lX2DqOfar3gQ7kNvwF3P2vx&_nc_oc=AdkLJBRZsRMq3SWVnxzqFHxeRTH3DtvAgmKXeYwwShKJ_1Ig-wInAGh2ggq_HlO3YiE&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_AfEBOnLd_Xy7qxu5sxRVZb8t-x3At-IVYfcgmitb4-rAaw&oe=680F6BB1"}, {"file_name": "Fitness Fables _Screen04.png", "id": "450863567726764", "uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/75462959_450863571060097_160294925395243725_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=CksS-FLgOLEQ7kNvwFHlJnI&_nc_oc=Adll2y8T9F4snTUKDr_TyeXiqhejfnBSaqPynvr6aV3-D17bTMCMp5r5NnnNRtFCcnE&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfEKyTGmKO2FaOcvYgWs-EhYfciDRypj8556lHzKiu2jdg&oe=680F69B9"}, {"file_name": "Fitness Fables _Screen02.png", "id": "1767400787431601", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/57566690_1767400794098267_2041132177250147872_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=0HoBPKtiesgQ7kNvwHsrCmh&_nc_oc=Adl78Po0OEydW2kXmuia4dLAHz9iQYAgEmB6fv32zN243Ze0cBCjHmScKk-sSvTHrR4&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfGBPI55JqtuH7YabHup4vMX47OeacSrdSbwnYsHg3_5zQ&oe=680F57EB"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Trailer_Cover_B.png", "id": "2320134108364817", "uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/43997708_2320134111698150_6193331455269072673_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=IBF1M9K1Y8AQ7kNvwEekMj0&_nc_oc=AdmE2rW1Y_aJaDc-WxaAXbavmLrbE7MhxtdxQ56tZ4URsYvlq5jmBmCM7erD7FZ63fU&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfHGONwyddHovo4dw16chq2a-FtTvGcxEkr0bJLe7R53xw&oe=680F7D27"}, "display_name": "Fitness Fables", "keywords": ["workout", "training", "combat", "fantasy", "Adventure"], "long_description": "Step into a world where fitness becomes an adventure. Fitness Fables turns your workouts into story-driven quests, combining fun challenges with beautiful, immersive environments. It\u2019s not just about staying active\u2014it\u2019s about enjoying a fitness journey.\n{2084049595368528}\n\ud83c\udfae **80 IMMERSIVE LEVELS AWAIT**\n\nDive into a magical journey with 80 levels, featuring 16 boss fights, 16 bonus challenges, and 48 varied traversal stages that keep you active and engaged.\n\n\ud83d\udd25 **24 HOURS OF GAMEPLAY**\n\nExperience up to 24 hours of workouts tailored to your fitness level, from light activity to intense challenges.\n\n\ud83d\udcaa **24 EXERCISES, FULL-BODY CHALLENGE**\n\nMaster exercises like Power Squats, Rope Swings, Sky Press, Battle Ropes, and much more to build strength, endurance, and coordination across different muscle groups.\n\n\u2694\ufe0f **INTENSE BOSS BATTLES**\n\nFace 16 intense bosses, blending fitness and fantasy with moves like Power Bow and Battle Drums to restore balance to Alterterra.\n\n\ud83c\udf0d **16 BONUS LEVELS FOR EXTRA CHALLENGE**\n\nTest your limits in 16 optional bonus levels that reward dedication and perseverance.\n{932722882196334}\n\ud83c\udfcb\ufe0f **FITNESS ADVENTURE FOR ALL**\n\nWith scalable exercises and adaptable gameplay, Fitness Fables welcomes beginners and challenges fitness enthusiasts.\n\n\u2728 **TRAVERSE THE WORLDS OF ALTERTERRA**\n\nTravel through stunning worlds like Terragon, Frostia, Ignis Prime, Aquarion, and Zypheron. Engage, and active.\n\nStep into Fitness Fables a place where movement fuels your story, challenges push you further! Are you ready?\n", "short_description": "\ud83c\udfcb\ufe0f FITNESS MEETS FANTASY IN VR\nStep into Fitness Fables, where workouts become epic adventures!\n\ud83d\udd25 FOR ALL FITNESS LEVELS\nStart at your pace and level up as you grow stronger.\n\ud83c\udfae STORY-DRIVEN QUESTS\nJoin the Magic Academy and save the realms through heroic workouts.\n\ud83d\udcaa FULL-BODY ENGAGEMENT\nBuild strength and stay energized with dynamic exercises.\n\ud83c\udfc6 TRACK YOUR PROGRESS\nEarn rewards, unlock achievements, and make fitness an epic journey!\nTurn your workouts into a magical journey!\n"}]}, "developer_name": null, "developer_privacy_policy_url": "https://immersion.games/game-privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Immersion Games S.A.", "support_website_url": null, "website_url": "https://www.fitnessfablesvr.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "1592192974313489"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_U