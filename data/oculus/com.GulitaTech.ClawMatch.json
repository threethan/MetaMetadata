_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=4oqgyNbluGMQ7kNvwGwm7x7&_nc_oc=AdlqQchv6Z9jasZBMzNYkxmyi2ejIU2oIVt1oU3AQ7pTyrKwqEfS8aaS9w-M4LBM4js&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_Afccg72WfBVaCarLPuZx-vm9XCrRwSHGQRyo9BpWDHI2Jg&oe=6908AD9D"}, "display_name": "Claw Match", "keywords": ["ClawMachine", "Simulator", "3Match", "Puzzle", "ClawMachineSimulator"], "long_description": "Claw Match combines the authentic claw machine experience with match-3 gameplay, taking you into a creative world of claw machines. In this game, you\u2019ll operate the claw machine with precision while using match-3 mechanics to tackle tasks, offering a fresh strategy and exciting gameplay!\n\nGame Features:\n\t1.\tClaw Machine Simulator: Operate the claw machine, and feel every movement of the claw.\n\t2.\tUnique Match-3 Experience: Combine claw machine gameplay with match-3 elements for a new elimination challenge.\n\t3.\tFun Creative Levels: Each level is uniquely designed to enhance the claw machine fun, providing a rich gaming experience.\n\t4.\tHundreds of Fun Objects: From cute plush toys to creative items, see how many interesting objects you can grab.\n\t5.\tRealistic Physical Feedback: With a precise physics engine and controller vibrations, experience every detail of the claw machine action.\n\nCreative Levels:\n\t\u2022\tDolls: Grab various adorable animal plush toys and unlock more.\n\t\u2022\tYammy Sushi: Feel like you\u2019re in a conveyor belt sushi restaurant. Grab your favorite sushi.\n\t\u2022\tFruit World: Grab delicious fruits and complete match-3 tasks in a vibrant fruit world.\n\t\u2022\tMini Cars: Grab moving mini cars on a racetrack.\n\t\u2022\tCasino: Grab lucky coins in a casino-themed claw machine.\n\t\u2022\tAnimals vs Zombies: Protect your animal friends by fending off zombies while grabbing toys and solving match-3 puzzles.", "short_description": "Claw Match combines the authentic claw machine experience with match-3 gameplay, transporting you into a creative world of claw machines. In this game, you not only need to operate the claw machine with precision, but also use match-3 mechanics to continuously challenge tasks, offering a fresh strategy and exciting gameplay!\n"}]}, "developer_name": "Gulita Studio", "developer_privacy_policy_url": "https://www.gulita.net/ClawMatch/privacypolicy.html", "developer_terms_of_service_url": null, "publisher_name": "Gulita Stuido", "support_website_url": null, "website_url": "https://www.gulita.net/ClawMatch/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "377978390272490"}, {"id": "331529188132278"}, {"id": "1676485315806614"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9730649656966203"}}]}, "scheduled_offers": {"count": 9, "nodes": [{"status": "PUBLISHED", "id": "26067696982820660"}, {"status": "PUBLISHED", "id": "1986197815480652"}, {"status": "PUBLISHED", "id": "1464959401295452"}, {"status": "PUBLISHED", "id": "1209921727246181"}, {"status": "CANCELLED", "id": "1021262316121232"}, {"status": "CANCELLED", "id": "703129065609455"}, {"status": "CANCELLED", "id": "653568404313238"}, {"status": "PUBLISHED", "id": "1192312198943270"}, {"status": "PUBLISHED", "id": "1376496970166379"}]}, "baseline_offer": {"id": "1376496970166379"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1765821600, "status": "PUBLISHED", "id": "26067696982820660"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename"