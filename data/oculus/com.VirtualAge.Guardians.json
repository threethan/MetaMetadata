 "is_initial_release": false, "name_with_fallback": "v29", "pdp_metadata_with_fallback": {"version": 29, "application": {"is_concept": false, "latest_released_pdp_metadata": {"id": "31434983549449062"}, "release_status": "RELEASED", "id": "5380153758692064"}, "id": "31434983549449062", "release_status": "RELEASED", "status": "APPROVED"}, "release_date": null, "administration_status": "APPROVED", "is_first": false, "release_status": "RELEASED", "release_type": "SCHEDULED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "5380153758692064", "is_first_party": false, "enables_in_app_ads": false, "is_concept": false, "platform": "ANDROID_6DOF"}, "iarc_cert": {"id": "595088973200516"}, "iarc_email": null, "msrp": null, "translations": {"nodes": [{"images": {"nodes": [{"file_name": "GuardiansFrontline_Meta_CoverSquare_1440x1440.png", "id": "27969697519345556", "image_type": "APP_IMG_COVER_SQUARE", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/39031348_27969697526012222_3504532415855693825_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=JlAXeS7w0bAQ7kNvwFXSM1v&_nc_oc=AdmrOdc_W2oqTgcP8o44gn-QcCGgiOvFltLQZqJhIV_DrR8-FZxsL3acZvWtXP0Ohrc&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_Aff1rudf2xzb2nHca5MmW-A16tBL509m1tCAN6C9wiGpuw&oe=68ECF9AF"}, {"file_name": "GuardiansFrontline_Meta_Logo(transparent)_9000x1440.png", "id": "3417499418524331", "image_type": "APP_IMG_LOGO_TRANSPARENT", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/38974593_3417499421857664_6123855900765801281_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=cn34PBb3srgQ7kNvwGyL1ln&_nc_oc=AdlskeyWjQGwvzd7RVd_uUvaxXtLgbR2Xnls9cSBClNWjc91H9Z0hvBJmZs_k1t8Q1w&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfdWWFsxGZGMjDcrjVHS25Ie7-qDxbRCT2tRNEt8FKlSjw&oe=68ECD71B"}, {"file_name": "GuardiansFrontline_Meta_CoverLandscape_2560x1440.png", "id": "2516591438538141", "image_type": "APP_IMG_COVER_LANDSCAPE", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/389