_nc_ohc=62veBKfarVIQ7kNvwGRkcLV&_nc_oc=AdnGSJoVrEMq0QFxZ6OAjU0HX4B6FXPqq3iJ0mC0APGPr_Q-2OQ1y94wqgq-eJq3ISI&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_Afsu4OUpD8TNuvDPrcLb2xc6ZvHd6JeHgtUnfaYotAt0IA&oe=6984A118"}, {"file_name": "d64f2767ff324b92bcb96f59ed6e56bf.png", "id": "24141795705522612", "uri": "https://scontent-ord5-2.oculuscdn.com/v/t64.5771-25/491830888_24141795715522611_765570189279721868_n.png?_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Ik2wV-MN-JsQ7kNvwEwT5ln&_nc_oc=Adnv-CKR68Br1friIySgFUtXZBvhtF4f5AYBINJT5E4in1jiN5FIy_fQ8LvdyEOrxP8&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AfuvdRyif6CDcUZt6sBdE2ptLuVRUqqt2PUFReDDW_zo0w&oe=69848095"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Shotcut_00_01_54_167.png", "id": "698233276598128", "uri": "https://scontent-ord5-2.oculuscdn.com/v/t64.5771-25/491822897_698233279931461_6330445805536336726_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=2CjdwxPWXBwQ7kNvwF4elFM&_nc_oc=AdlUPIa85qCtSwVmrxRYDYI9J6v_WQbaBu62GYVd0grMrAqORDK6AZomr4MSZ56Xmtc&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AfupWjFJel9eEO1eyS8I0uLmxG0YYfTPetDNhrqUWy4THw&oe=69849EFD"}, "display_name": "Databender", "keywords": ["art", "creative", "image", "editor", "glitch"], "long_description": "Create striking glitch art in virtual reality!\nWith Databender, you can apply digital effects in entirely new ways, unlocking fresh dimensions of creativity. Load and save any standard 2D image (no special formats needed), and start creating with the most psychotropic VR image editor available.\n\nIn Databender, every edit happens in 3D VR space, not through flat menus on a 2D screen. This unlocks powerful new interactions most editors can't offer:\n- Want to load another image? Don\u2019t mess with layers, instead just spawn another copy into cyberspace and move it around freely.\n\n- Want to stretch an image from a specific point? Grab a 3D cursor and drag it right where you want the effect to start.\n\n- Want to isolate or invert a color? Skip the flat color tabs and sliders and use an interactive 3D color wheel instead.\n\nDatabender works directly with the images saved on your Quest headset with no requirement to plug into a computer. You can even sync images from your headset to your phone using the Horizon mobile app and the built in Quest Gallery app.", "short_description": "Edit 2D images in 3D to create a unique, raw, glitch art style! Every edit merges pixel data using 3D interactions, allowing for a completely new creative process. Load and save images directly from your Quest, no PC required."}]}, "developer_name": null, "developer_privacy_policy_url": "https://odachivr.art/privacy-policy.md", "developer_terms_of_service_url": null, "publisher_name": "OdachiVR", "support_website_url": null, "website_url": "https://odachivr.art/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1819021644882647"}, {"id": "393222478507326"}, {"id": "1055187466124150"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "32189183700680670"}}]}, "scheduled_offers": {"count": 4, "nodes": [{"status": "PUBLISHED", "id": "1851434695536344"}, {"status": "PUBLISHED", "id": "2406209139796740"}, {"status": "PUBLISHED", "id": "1153127300200533"}, {"status": "PUBLISHED", "id": "2983174548511370"}]}, "baseline_offer": {"id": "2983174548511370"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1769626800, "status": "PUBLISHED", "id": "1851434695536344"}]},