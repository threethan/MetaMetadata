des. "}]}, "developer_name": null, "developer_privacy_policy_url": "https://beatshapers.com/holotanks_privacy.txt", "developer_terms_of_service_url": null, "publisher_name": "Fortell Games Inc", "support_website_url": "https://www.fortell.games/", "website_url": "https://www.fortell.games/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER", "CO_OP"], "id": "7619404678188533"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sjc6-1.oculuscdn.com/v/t64.5771-25/57569457_1005703390526206_4811816033278719630_n.png?stp=dst-png_s360x360&_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=gE8O_i69QgIQ7kNvwEoL8BF&_nc_oc=AdlCsTY-GtPvvRXDQmK34eWgow6v3kMiGRRKHDhv5Nm75W4rDZE8uxCgYG88UtWzrBY&_nc_zt=3&_nc_ht=scontent-sjc6-1.oculuscdn.com&oh=00_AfURgp3iPOBgZF4FinUDkmlHlMwCtagIdPMEIDbG33HO5w&oe=68B59053"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Holotanks (BETA)", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Action", "id": "459781504968480"}], "release_date": 1707240836, "lastRevision": {"nodes": [{"id": "6602381423224202", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "6602381406557537", "is_first_party": false, "enables_in_app_ads": false, "is_conce