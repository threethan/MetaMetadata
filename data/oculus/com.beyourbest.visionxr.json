": null, "video_trailer_thumbnail": {"file_name": "byb_landscape_2560x1440.png", "id": "1648517615520174", "uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/75210889_1648517618853507_2007437631331052425_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=O9YCWLlzj0sQ7kNvwEAqTrN&_nc_oc=Adl_ZMUaytSjYU8AUkA2kSHS75ajnlEIpUKbjFruFR7rk4kquhHrk5u50v5s_h3nV4k&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_AfalwVOlZ8yYtZ6W-O4JADKxFeFaxjRbDwV-ooiY6Tx4gg&oe=68DA64ED"}, "display_name": "Be Your Best", "keywords": [], "long_description": "To gain an edge over your competition in football, it's essential to put in high-quality extra repetitions. Training that lets you replicate situations you meet on the field is key to maximizing your time and potential.\n\nWith Be Your Best, you can train perception & decision-making via real football match simulations, which is the same principle used for training airplane pilots, military personnel, and other high-performance professionals who need to react quickly and accurately to new information.", "short_description": "Build for professional footballers, Be Your Best provides a soccer training system based on real-world situations specially designed to improve the cognitive performance of players on and off the pitch. "}]}, "developer_name": "Be Your Best", "developer_privacy_policy_url": "https://www.beyourbest.com/privacy", "developer_terms_of_service_url": "https://www.beyourbest.com/terms", "publisher_name": "Be Your Best", "support_website_url": "https://www.beyourbest.com/contact", "website_url": "https://www.beyourbest.com/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "2776542286000603"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["EDUCATIONAL", "SIMULATION", "SPORTS"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics":