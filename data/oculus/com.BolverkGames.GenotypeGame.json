{"data": {"node": {"__typename": "Application", "id": "6694982853879859", "organization": {"id": "1076023469116844", "viewer_permissions": [], "application_groupings": {"nodes": [{"viewer_has_update_access": false, "full_applications": {"nodes": [{"id": "7038470162875978"}, {"id": "6694982853879859"}]}, "id": "268044155967894"}, {"viewer_has_update_access": false, "full_applications": {"nodes": [{"id": "5961180047289039"}]}, "id": "576076090073839"}, {"viewer_has_update_access": false, "full_applications": {"nodes": [{"id": "2851341088231760"}]}, "id": "447100129263181"}, {"viewer_has_update_access": false, "full_applications": {"nodes": [{"id": "911170512333286"}]}, "id": "517575311767219"}]}}, "firstRevision": {"nodes": [{"application": {"id": "6694982853879859", "release_date": 1697128200, "is_concept": false, "platform": "ANDROID_6DOF", "scheduled_offers": {"count": 12}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "7897652336946232"}, "id": "979379636457661"}]}}, "binary": null, "live_binary": null, "created_date": 1735821412, "id": "9226249154086537", "is_initial_release": false, "name_with_fallback": "v19", "pdp_metadata_with_fallback": {"version": 13, "application": {"is_concept": false, "latest_released_pdp_metadata": {"id": "9226250470753072"}, "release_status": "RELEASED", "id": "6694982853879859"}, "id": "9226250470753072", "release_status": "RELEASED", "status": "APPROVED"}, "release_date": null, "administration_status": "APPROVED", "is_first": false, "release_status": "RELEASED", "release_type": "SCHEDULED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "6694982853879859", "is_first_party": false, "enables_in_app_ads": false, "is_concept": false, "platform": "ANDROID_6DOF"}, "iarc_cert": {"id": "1466491804133659"}, "iarc_email": null, "msrp": null, "translations": {"nodes": [{"images": {"nodes": [{"file_name": "SquareExport.png", "id": "238690210484