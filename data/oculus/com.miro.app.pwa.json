w, and feel the connection of working side by side \u2014 even in remote, distributed, and hybrid work environments."}]}, "developer_name": "Miro", "developer_privacy_policy_url": "https://miro.com/legal/privacy-policy/", "developer_terms_of_service_url": "https://miro.com/legal/terms-of-service/", "publisher_name": "Miro", "support_website_url": "https://help.miro.com/hc/en-us", "website_url": "https://www.miro.com/", "category": "APPS", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": ["PRODUCTIVITY", "UTILITY", "ART_CREATIVITY"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "472216757677394"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "963994957560628"}]}, "baseline_offer": null, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1640109660, "status": "PUBLISHED", "id": "963994957560628"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "5018892544801159", "version": "1.0"}, "id": "604316590844695", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "5017282864962127", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "5018892544801159"}, "release_status": "SCHEDULED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "604316590844695"}]}