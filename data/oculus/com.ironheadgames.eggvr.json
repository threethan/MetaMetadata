hc=dy34hxjNDGEQ7kNvwErKTmG&_nc_oc=AdnFQPRAxB9n2hIrRYwIiEjdDTJgDpAVD2UQqqFCusMGd9ttGczbdfWcy-ZB08Ndoqk&_nc_zt=3&_nc_ht=scontent-ord5-3.oculuscdn.com&oh=00_AfXQWXVsA-3GDflrKASK5Cavg4vKny_xMXWwjNqRWNbx2Q&oe=68A864D1"}, {"file_name": "2.png", "id": "739706671712874", "uri": "https://scontent-ord5-3.oculuscdn.com/v/t64.5771-25/499305085_739706675046207_2011885201003731337_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=nTOG7AYfPG4Q7kNvwHK03iu&_nc_oc=AdkVm8-8giy1ySmgXe9oJ7k_LkhoYVj3KKz3H9rWt9MlLUqX4O5gVm50O_9HoGjKtBg&_nc_zt=3&_nc_ht=scontent-ord5-3.oculuscdn.com&oh=00_AfXq-zx6qqexlrknOzX4XvRyB3yexCEb-oa01pO_fEHWIA&oe=68A845F5"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Cover Landscape.png", "id": "1323707589326782", "uri": "https://scontent-ord5-2.oculuscdn.com/v/t64.5771-25/491823485_1323707592660115_5185212186300928661_n.png?_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=2sgmAtGaw7MQ7kNvwHGe3Dt&_nc_oc=AdkSKFKx2CY0KPEeSwtUcmIPaffZztW-oUtfcy9-zNZzRs0dxB4pgdDNOGcqDtCtxCs&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AfXM6S7oce0akP_iJkumuHvP_zfUvgEe1sbPmpBpYqimRA&oe=68A84271"}, "display_name": "Egg VR", "keywords": ["gorilla", "fangame", "monkey", "eggvr", "egg"], "long_description": "Reject Reality and Become an Egg!\n\nRun, climb, and jump in VR using a unique locomotion method inspired by Gorilla Tag. This method only requires the movement of your hands and arms. No buttons, no sticks, no teleportation. Push off surfaces to jump and squeeze them with both hands to climb.\n\nCurrently, there is one game mode called Smash mode where one egg has to reach other eggs, but soon, there will also be shooter mode, hunt mode, infection mode, casual mode, and horror mode.\n\nUsing the Smash mode, you can play different mini-games such as:\n\n- Hide and Seek\n- Sharks and Minnows\n- Ghost in the Graveyard\n- Monkey on the Shelf\n\nRun from the other eggs or outmaneuver the survivors to catch them. Parkour up kitchen objects and cliff cupboards to evade and chase down eggs. The movement is easy to learn but hard to master.\n\nCurrently, there are 3 maps to explore: Kitchen, Horror, and Speedrun!\n\nVisit the Egg Store in the kitchen, where you can buy, play with, and wear to express your true nature.\n\nBecome an Egg!", "short_description": "Reject Reality and Become an Egg!"}]}, "developer_name": "FutureOfVR", "developer_privacy_policy_url": "https://futureofvr.games/privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "FutureOfVR", "support_website_url": "https://futureofvr.games/egg-vr/", "website_url": "https://futureofvr.games/egg-vr/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "en_US"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER", "CO_OP"], "id": "24400048992923500"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ord5-3.oculuscdn.com/v/t64.5771-25/499303386_24626112703663149_7204214379478324713_n.png?stp=dst-png_s360x360&_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=IAiLXjZgUvsQ7kNvwEH3nUi&_nc_oc=Adm8Prmmgg92w6kdNtXvfmL9j8JQwHlWTsm4zeHHWBB2N8Dc-Rqc2nTWw87rDeyogiU&_nc_zt=3&_nc_ht=scontent-ord5-3.oculuscdn.com&oh=00_AfXUv0OoOR-go7MZsTRPhCaZfmgHavIn37g-9kN1dBOAAA&oe=68A86B2A"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Egg VR", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Action