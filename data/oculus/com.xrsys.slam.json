aZ&_nc_oc=AdgXail5r2fE2LjJHaEZOFlJCakW2YHQP-la-MMqtsLZLc0b2sKKfZythVREBxLWCTg&_nc_zt=3&_nc_ht=scontent-sin6-2.oculuscdn.com&oh=00_AYFoj6BgGZL-KxdxK1P7MTyqdnuJsd85x-wPGV30AIlTCg&oe=67DD65E8"}, {"file_name": "Screenshot 2023-08-28 at 9.53 1.png", "id": "701670631500809", "uri": "https://scontent-sin6-2.oculuscdn.com/v/t64.5771-25/38974456_701670634834142_5121723908228449659_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=YSfMIeNhLlQQ7kNvgGyTzIQ&_nc_oc=AdjpJWutkllKrGTnCrDWBSiTdKztaIuWryt-9E-stTlL1Hni5djUucCA7cQdnQJBGsw&_nc_zt=3&_nc_ht=scontent-sin6-2.oculuscdn.com&oh=00_AYEU8IbGyhsViG1WYNaKU36ufqlvCfEE_CDFhy4WGr6fUw&oe=67DD3A88"}, {"file_name": "Screenshot 2023-08-28 at 9.56 1 (1).png", "id": "294280946636804", "uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/39001660_294280949970137_6965906294267218481_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=SBIi7Ug7NNAQ7kNvgE_u-CL&_nc_oc=Adgz3wPTEKh1rIlXpYY6OkbXd89OYroejkSKzjHnA7PwVDukdwB3WTk2y01Uq8LX27k&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AYFZ59LRkyXNWeDYvB7C9eVughg_c2uHY9rWc03v9O6mlQ&oe=67DD5753"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Group 144.png", "id": "2072152056540067", "uri": "https://scontent-sin11-2.oculuscdn.com/v/t64.5771-25/75295893_2072152059873400_2455308825358559417_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=r3WqbDaZ0aQQ7kNvgHqkiWw&_nc_oc=Adh_pWB0Ux91UTc3olcAJ75Z7yRe-hrpPqvb-rzwkvzZ5DbcAGgBY3ZyUO0s3G6EjfM&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_AYEwdbRoI5Md4sA4ZhRiIfrLkOr55Nn4zOjtn8wRqAxYAA&oe=67DD6B58"}, "display_name": "Slam Tennis VR", "keywords": ["tennis", "sports", "racket", "rhythm", "arcade"], "long_description": "Ready to take your tennis game to the next level\u2014and beyond reality? Dive into **Slam**, the ultimate multiplayer tennis experience exclusively on Meta Quest! Remember that classic motion-controlled tennis game everyone was obsessed with? Imagine that, but fully immersive in VR! **Slam** is rhythm-based and seriously action-packed. You'll smash on-court targets to charge up your energy, then unleash epic power shots to obliterate your opponents.\n\nFeel the beat as you dominate the court with jaw-dropping moves and combos synced perfectly to the rhythm. Challenge your friends or face off against players worldwide in this revolutionary tennis game.\n\nSo grab your headset, step onto the virtual court, and get ready to **slam** like never before!", "short_description": "Ready to level up your tennis game? Dive into Slam, the ultimate multiplayer tennis experience exclusive to Meta Quest! Remember that classic motion-based tennis game everyone loved? This is like that\u2014but in VR. It's rhythm-based and seriously action-packed! Hit on-court targets to charge your energy, then unleash epic power shots to smash your opponents. Feel the beat, dominate the court, and challenge your friends in this game-changing tennis game. Grab your headset and get ready to slam!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.xrsys.net/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "XRSYS", "support_website_url": null, "website_url": "https://www.xrsys.net/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["MUSIC", "SPORTS"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER"], "id": "9882266585121870"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://