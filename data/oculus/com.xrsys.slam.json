lK&_nc_oc=AdkJjg2tALLvbwDxmUEerayMK7u06vcLFPT2LA3hCponhmnk9BmXb_6eIDQGk2wnUFQ&_nc_zt=3&_nc_ht=scontent-sin6-2.oculuscdn.com&oh=00_AfFxtxrEnsRr4-gqXIW3rkCRW_0r55ZENVDlpi6HsHhPCg&oe=67F91568"}, {"file_name": "Screenshot 2023-08-28 at 9.53 1.png", "id": "701670631500809", "uri": "https://scontent-sin6-2.oculuscdn.com/v/t64.5771-25/38974456_701670634834142_5121723908228449659_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=-A9u5KkDWl0Q7kNvwF-Q56Z&_nc_oc=AdmpW22mG1_ZSZya32eaMXgJvIimVCIYWn1Y123JfVQcMAkrQyTANWs0s2Eb_hX1EhY&_nc_zt=3&_nc_ht=scontent-sin6-2.oculuscdn.com&oh=00_AfEWz0aOUyAvk60OofHbOx5uZcrVqnyZVG6hI_R_VG_1Rw&oe=67F8EA08"}, {"file_name": "Screenshot 2023-08-28 at 9.56 1 (1).png", "id": "294280946636804", "uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/39001660_294280949970137_6965906294267218481_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=hfKu5Onwu7MQ7kNvwHtuUvn&_nc_oc=AdmBlnHVLfJ41q89I4ft7ljoIWnR9rrUfKMI0Xr81hxowmL0PleJ58DZIDQcioDlUHU&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfFttkKLlyLpltX7YvD_IluafIehFXppH0ezjz5T3rdteg&oe=67F906D3"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Group 144.png", "id": "2072152056540067", "uri": "https://scontent-sin11-2.oculuscdn.com/v/t64.5771-25/75295893_2072152059873400_2455308825358559417_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=O_FlohhMpiEQ7kNvwFOfiST&_nc_oc=AdmWoHvd7XNx1-RLr_nnH81SDi-LUZmLwfCxEYpE8Igi8PQzAz6SbnKfWGXW8dDZonQ&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_AfHm1rrGCIsXswXtCBioW_Q5H3qp_CpNaHd4eV2VDKDhyg&oe=67F91AD8"}, "display_name": "Slam Tennis VR", "keywords": ["tennis", "sports", "racket", "rhythm", "arcade"], "long_description": "Ready to take your tennis game to the next level\u2014and beyond reality? Dive into **Slam**, the ultimate multiplayer tennis experience exclusively on Meta Quest! Remember that classic motion-controlled tennis game everyone was obsessed with? Imagine that, but fully immersive in VR! **Slam** is rhythm-based and seriously action-packed. You'll smash on-court targets to charge up your energy, then unleash epic power shots to obliterate your opponents.\n\nFeel the beat as you dominate the court with jaw-dropping moves and combos synced perfectly to the rhythm. Challenge your friends or face off against players worldwide in this revolutionary tennis game.\n\nSo grab your headset, step onto the virtual court, and get ready to **slam** like never before!", "short_description": "Ready to level up your tennis game? Dive into Slam, the ultimate multiplayer tennis experience exclusive to Meta Quest! Remember that classic motion-based tennis game everyone loved? This is like that\u2014but in VR. It's rhythm-based and seriously action-packed! Hit on-court targets to charge your energy, then unleash epic power shots to smash your opponents. Feel the beat, dominate the court, and challenge your friends in this game-changing tennis game. Grab your headset and get ready to slam!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.xrsys.net/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "XRSYS", "support_website_url": null, "website_url": "https://www.xrsys.net/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["MUSIC", "SPORTS"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER"], "id": "9882266585121870"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://