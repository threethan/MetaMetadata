=scontent-lga3-2.oculuscdn.com&oh=00_AfcA1IZIh5bsg3zByPKAxdskIBZnVGNFygmyJ0wcq0Y-Hg&oe=6900AC4F"}, {"file_name": "SCREEN4.png", "id": "2076396659440356", "uri": "https://scontent-lga3-2.oculuscdn.com/v/t64.5771-25/473392019_2076396662773689_8266087291122776285_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=3uaDyopiWSkQ7kNvwG8Uync&_nc_oc=Adl6LuevhF7q6uPL1e_nhf1ObxQR1y2Y7NwYP95kmdE83FdLtSImDYUfTYJtWRzasBo&_nc_zt=3&_nc_ht=scontent-lga3-2.oculuscdn.com&oh=00_AfeCiBSOuCPxEjL2HHNKABSIByFa9GXF_FXDjMqzJnOvgw&oe=6900A583"}, {"file_name": "SCREEN5.png", "id": "1351573939362261", "uri": "https://scontent-lga3-1.oculuscdn.com/v/t64.5771-25/473393325_1351573942695594_8627772183416663494_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=gZBGaR-4sKQQ7kNvwFnP05E&_nc_oc=AdkxKBwy6gR_fuAreqph6U5PHIMCJ_9WxN2HkPb-s0bN9WObh6iLX7BWGGWyy86Zxz4&_nc_zt=3&_nc_ht=scontent-lga3-1.oculuscdn.com&oh=00_AfdwhaQlas-2_-gbyJZIJbNsE6UOKjD38CX6Jkd5sLFaQQ&oe=69009B36"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "LANDSCAPE.png", "id": "9644139012286835", "uri": "https://scontent-lga3-2.oculuscdn.com/v/t64.5771-25/473391883_9644139018953501_5648143411478632363_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=r-E3O3U0pLwQ7kNvwFAUQTL&_nc_oc=AdnmVsPQd0XsB1UCgafCrZWlQ5wGYwvTI96Eto0bOa4DxS-7KhbrY0UPVVDdV2s8nfU&_nc_zt=3&_nc_ht=scontent-lga3-2.oculuscdn.com&oh=00_Afc7XHV1R6ATrkHIzfpwJ2ZK9sZH8sJTP1eEBQnFXAQvEQ&oe=6900A6BD"}, "display_name": "War Broke in Heaven", "keywords": ["action", "dragon", "fitness", "fight", "wrath"], "long_description": "Based on the events of the beginning and the end, this VR experience may cause motion sickness, user discretion is advised.\n\nWar Broke in Heaven is a VR experience that puts you in the air while you fight enemies and a dragon to stop the ultimate rebellion. Use controlled moving and tactics to destroy the enemy and not get engulfed by their attacks. You will have a variety of weapons at your disposal each with a unique attribute to destroy your enemy. \n\nUse your head to look at the directions you want to fly to and slash, or fire spirit bombs at the enemy. War broke in heaven has beautiful graphics that will keep you in awe and that will leave you having fun and wanting to come back to play more. ", "short_description": "An epic battle and the fall of a rebellion, this VR experience will keep you engaged and entertained "}]}, "developer_name": "Traindz LLC", "developer_privacy_policy_url": "http://biims.net/warbrokeinheaven", "developer_terms_of_service_url": null, "publisher_name": "Traindz LLC", "support_website_url": null, "website_url": "http://biims.net/warbrokeinheaven", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9444449118955217"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-lga3-1.oculuscdn.com/v/t64.5771-25/473391934_617973464283098_4288500641272340701_n.png?stp=dst-png_s360x360&_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=_fm7dhIZC1YQ7kNvwEHnbVP&_nc_oc=AdnRW4Rgemb3X9q7ETO7V9O33PDyuZ1qvcY2g8jTx2KC9sn1Tdeeqh2YnDWNYuE4CA0&_nc_zt=3&_nc_ht=scontent-lga3-1.oculuscdn.com&oh=00_AfcZTZ_H87is9ZnhfUxM9SRHrre_D6drI7WL7h5VlLEEKA&oe=6900A14C"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "War Broke in Heaven", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "conte