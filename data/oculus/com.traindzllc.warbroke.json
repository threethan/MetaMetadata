tained "}]}, "developer_name": "Traindz LLC", "developer_privacy_policy_url": "http://biims.net/warbrokeinheaven", "developer_terms_of_service_url": null, "publisher_name": "Traindz LLC", "support_website_url": null, "website_url": "http://biims.net/warbrokeinheaven", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9444449118955217"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/473391934_617973464283098_4288500641272340701_n.png?stp=dst-png_s360x360&_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=vWafeN1NkzMQ7kNvwF_Rc0h&_nc_oc=AdkcjGdJKchsoaBPehL6lRPQiLYrzvmiafEUTVA8TXB3xN_5IELlhhnRFcsfoQ7lgAI&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AftPd6tleHPWsRMfFTUMEV2p13GME_IM8HVw6VWbBxANTw&oe=69908D0C"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "War Broke in Heaven", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Action", "id": "459781504968480"}], "release_date": 1683647096, "lastRevision": {"nodes": [{"id": "6343012632432230", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "6343012625765564", "is_first_party": false, "enables_in_app_ads": false, "is_concept": false, "p