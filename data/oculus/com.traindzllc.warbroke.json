 experience will keep you engaged and entertained "}]}, "developer_name": "Traindz LLC", "developer_privacy_policy_url": "https://www.traindz.com/warbrokeinheaven", "developer_terms_of_service_url": null, "publisher_name": "Traindz LLC", "support_website_url": null, "website_url": "https://www.traindz.com/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "1196559731056726"}}]}, "scheduled_offers": {"count": 0, "nodes": []}, "baseline_offer": null, "is_approved": true, "firstOffer": {"nodes": []}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "6375949352471891", "version": "1.0"}, "id": "601929808668041", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "6343012625765564", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "6375949352471891"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "601929808668041"}]}, "change_log": null, "created_date": 1684188675, "id": "6375949352471891", "version": "1.0", "version_code": 3, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": null, "