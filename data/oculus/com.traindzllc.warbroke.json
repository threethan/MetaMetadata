=scontent-sjc6-1.oculuscdn.com&oh=00_AfXtbI9MLcuQv9ic1HSbRr8El3Pl3FahOLs1AZzM0r5cRw&oe=68B586CF"}, {"file_name": "SCREEN4.png", "id": "2076396659440356", "uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/473392019_2076396662773689_8266087291122776285_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=ta6eghZR0-0Q7kNvwEx8Ss_&_nc_oc=AdnsJPzEJSa1PGHF0lNZWIHoGrhMOdk-Ojf77lIToF43V2sHo23mbaFI2FW37mUYa90&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfV8nIYSB2vLfNRht-7cOC5BKB0AuL6XgKVxLXLTqwbJnQ&oe=68B58003"}, {"file_name": "SCREEN5.png", "id": "1351573939362261", "uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/473393325_1351573942695594_8627772183416663494_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=3m5P2pM5Pn4Q7kNvwH2wbKI&_nc_oc=Adng_SMgsoWP-PGF6rKXUEzVEIvvr7L6IPx1k8ConUiX46RAPsf6lXbecfURbCeU0IE&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfV7uGNjs7CS6lWuWpmGBNH7836rJz0YAcHVuy6IBDsv0w&oe=68B575B6"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "LANDSCAPE.png", "id": "9644139012286835", "uri": "https://scontent-sjc6-1.oculuscdn.com/v/t64.5771-25/473391883_9644139018953501_5648143411478632363_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=iOyjZ3UnM3oQ7kNvwGZoi6-&_nc_oc=AdlU_gj_w7C3znCKWulyj0pFHVraxHTsG0a7cNQEzQkvfVuGxqUwuMmMr5tIR4RlMKI&_nc_zt=3&_nc_ht=scontent-sjc6-1.oculuscdn.com&oh=00_AfVNW13FjE92vhAYENVFYJ5tvAgFG16unCc53vq8EVRy7Q&oe=68B5813D"}, "display_name": "War Broke in Heaven", "keywords": ["action", "dragon", "fitness", "fight", "wrath"], "long_description": "Based on the events of the beginning and the end, this VR experience may cause motion sickness, user discretion is advised.\n\nWar Broke in Heaven is a VR experience that puts you in the air while you fight enemies and a dragon to stop the ultimate rebellion. Use controlled moving and tactics to destroy the enemy and not get engulfed by their attacks. You will have a variety of weapons at your disposal each with a unique attribute to destroy your enemy. \n\nUse your head to look at the directions you want to fly to and slash, or fire spirit bombs at the enemy. War broke in heaven has beautiful graphics that will keep you in awe and that will leave you having fun and wanting to come back to play more. ", "short_description": "An epic battle and the fall of a rebellion, this VR experience will keep you engaged and entertained "}]}, "developer_name": "Traindz LLC", "developer_privacy_policy_url": "http://biims.net/warbrokeinheaven", "developer_terms_of_service_url": null, "publisher_name": "Traindz LLC", "support_website_url": null, "website_url": "http://biims.net/warbrokeinheaven", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9444449118955217"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/473391934_617973464283098_4288500641272340701_n.png?stp=dst-png_s360x360&_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=5vgLxzCp4g8Q7kNvwEeOfgl&_nc_oc=Adk6XACfeoXUWjPMbL-r02CfhDALSwqK2p798osmrXLcKsAXc5huPURj4bOeXy3si-8&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfVGQKSc6qmtVzeiXaEBR4Ayw7ABM5wHV87fYg2aSIGSZA&oe=68B57BCC"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "War Broke in Heaven", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "conte