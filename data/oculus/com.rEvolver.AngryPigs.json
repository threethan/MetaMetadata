rops. Watch out cause they bite too. "}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.revolver.si/gaming/angry-pigs/privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "Revolver", "support_website_url": null, "website_url": "https://revolver.si/gaming", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ACTION", "ADVENTURE", "SHOOTER"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "5144208692309800"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "383147556844940"}]}, "baseline_offer": {"id": "383147556844940"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1634056560, "status": "PUBLISHED", "id": "383147556844940"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "4968947646508219", "version": "1.51"}, "id": "623737108607669", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "4234711956598462", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "4968947646508219"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "623737108607669"}]}, "change_log": "EASTER UPDATE! - Fig