name": "Breakout VR", "keywords": [], "long_description": "Play the famous classic arcade Brekout Out now in Virtual Reality. \n\nThe VR version of an alltime classic brings you hours of fun and explosive gameplay in an unique atmosphere.\n\nStory:\nYou are inside the space ship and can only move your ship left and right with your triggers.\nAim and destroy all the bricks to get to the next level.\n\nThe game got inspired by Alleyway's cover image.\n\n- Play from inside the ship in VR\n- Mess with the world and find out who is the best VR brick breaker in the world\n- Starting with 15 Levels in Early Access and more are planned and coming", "short_description": "Play the brick breaker arcade game now in Virtual Reality."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.nerdvision.net/privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "Nerdvision Games", "support_website_url": null, "website_url": "https://www.nerdvision.net/breakout-vr/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1592192974313489"}, {"id": "3744040075834813"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ARCADE", "CASUAL"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "476555377414062"}, "review_messages": ["", ""]}]}, "cover_square_image": {"uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/38982701_274483044018384_7239360513491713496_n.png?stp=dst-png_s360x360&_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=03-UGjlCmKUQ7kNvwHZb0UN&_nc_oc=Ad