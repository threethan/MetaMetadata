hc=tvBrrgT33zUQ7kNvwFhB0C7&_nc_oc=Adlg8m48BEBAVDZX_Gpj1vYXc3c2LffvYmqON_jnG_B9xkqiclVf4jHhi0HLdgzVBBQ&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_Aflb2-NsEJP7vNgNoH6J6pJlKcSt-QOHs0w5JSwUkj6M3g&oe=693955B4"}, "display_name": "Proteus", "keywords": ["Science", "Biology", "Laboratory", "Medicine", "Vaccine"], "long_description": "Proteus is a learning & engagement app that introduces students to the world of advanced cell & gene therapies.\n\nDraw the link between life-saving medicines and careers in advanced therapies development & manufacturing.\nLook inside a production room from a real manufacturing facility.\nBring classroom learning to life by exploring a cell suspension and the inside of a human cell, contextualising information from biology class.\n\nUse Proteus to generate excitement around cell & gene therapy jobs at careers and recruitment events. Use in classrooms as an immersive and engaging learning experience.\n \nFourPlus Immersive create VR for the life science sector, combining expertise in advanced therapies & immersive technology to create impactful tools for training, marketing & recruitment.", "short_description": "Introducing the world of advanced cell and gene therapies and what it means to work in medicines manufacturing. For engagement, outreach, recruitment and education. "}]}, "developer_name": null, "developer_privacy_policy_url": "https://fourplus.co.uk/proteus/privacypolicy", "developer_terms_of_service_url": null, "publisher_name": "FourPlus Immersive", "support_website_url": null, "website_url": "https://fourplus.co.uk/", "category": "APPS", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1043107547362485"}, {"id": "434961926215971"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["EDUCATIONAL"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graph