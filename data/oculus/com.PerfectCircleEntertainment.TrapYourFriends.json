e where anything can happen!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.trapyourfriends.com/privacypolicy", "developer_terms_of_service_url": null, "publisher_name": "Perfect Circle Entertainment", "support_website_url": null, "website_url": "https://www.trapyourfriends.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "1700921480735423"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["HAND_TRACKING", "OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER", "SINGLE_USER", "CO_OP"], "id": "8610716568992095"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "949408447211771"}]}, "baseline_offer": {"id": "949408447211771"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1732039200, "status": "PUBLISHED", "id": "949408447211771"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "24665234669780362", "version": "5.0.8"}, "id": "1305699657260502", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "8610716555658763", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "24665234669780362"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1305699657260502"}]}, "change_log": null, "crea