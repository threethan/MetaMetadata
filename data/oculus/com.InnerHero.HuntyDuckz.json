enshots": [{"file_name": "HDSS3.png", "id": "265634459405639", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/38974504_265634462738972_3826937040244828182_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=XOJecwkmhK8Q7kNvgGMeUxH&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AYDlJ8b1lA_2RYT95AJcryLHmBRXhybOHQU6z4ZcaPfi6A&oe=67910909"}, {"file_name": "SSDeer1.png", "id": "587710920494842", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/75287554_587710927161508_8723681794411469417_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=yU3xMhkNRscQ7kNvgHb3hXd&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AYAANL72v3kyQtGt9spUQ5FdAo_iOvuIg3hk-g7rA8IZBw&oe=6790D77D"}, {"file_name": "HDSS4.png", "id": "294622629635381", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/86610704_294622632968714_2759426918840616148_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=UHT7YKLSms4Q7kNvgHZNo4n&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYBIo7mpbduuKhG4ipfmQGuSyLmgUsDZPjEfSEDGAVBZsg&oe=6790FAEB"}, {"file_name": "HDSS1.png", "id": "288763350305896", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/43997716_288763353639229_8986142747210473159_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=ypZynhSY67IQ7kNvgFZv4ec&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AYBJdWvaaIXW15FVV9lD83t55cv-_m4UdtCRWeFRNnDVZw&oe=6790FC02"}, {"file_name": "HDSS2.png", "id": "973363880652366", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/38982678_973363883985699_4062922903346010643_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=gfkO_nwxKtgQ7kNvgGFBkRB&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYCtt1cQFQJDbzLW6LQzaYTKiYGqkohJLgIDOy5BGZygfw&oe=6790E4AC"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "HV_V3_Banner_2560x1440_png.png", "id": "543921755297531", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/39001717_543921758630864_8555301344170265825_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=rnAHBstf6RMQ7kNvgF3yJ0E&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AYCV1TlZ0jSi9hRWidKJr69WhyUmsEjrNoMzVmOj8Ji9Jw&oe=6790E5E8"}, "display_name": "HuntyVerse", "keywords": ["arcade", "shooter", "hunting", "animals", "guns"], "long_description": "Step into the enchanting world of HuntyVerse, a delightful virtual reality hunting experience. With its playful charm and captivating gameplay, HuntyVerse offers a lighthearted alternative to traditional hunting games\u2014don't worry no animals were harmed!\n\nChoose between two unique worlds: Hunty Duckz, a free-to-play adventure, and Deer Park, available for just $2.99. Both feature vibrant, animated landscapes filled with adorable targets and exciting challenges. Whether you\u2019re a casual gamer or a competitive player, HuntyVerse guarantees endless entertainment with its accessible and addictive gameplay.\n\nTest your skills across three engaging modes designed to challenge your reflexes, precision, and strategy. Compete with friends or climb global leaderboards to prove yourself against players worldwide. Each round brings fresh opportunities for fun and friendly competition in a fully immersive VR setting.\n\nWhat sets HuntyVerse apart is its commitment to accessibility and comfort. Play from your chosen space\u2014no need for bulky gear, outdoor setups, or weather concerns. Designed for newcomers and seasoned VR players alike, it\u2019s an experience everyone can enjoy.\n\nWhether you\u2019re perfecting your aim in Hunty Duckz or exploring the whimsical wilderness of Deer Park, HuntyVerse combines charming visuals, intuitive controls, and action-packed content for a memorable arcade-style shooter. Ready to dive in? Start your adventure today!", "short_description": "Experience the cutest hunting adventure in VR with HuntyVerse! \n\nPlay in two vibrant worlds\u2014Hunty Duckz (Free) and Deer Park ($2.99)\u2014featuring three addictive game modes. Compete with friends or top the global leaderboards in this casual arcade shooter. \n\nNo animals harmed, no gear required\u2014just pure fun from the comfort of your VR setup. Dive in and let the hunt begin!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://innerherostudios.com/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Inner Hero LLC", "support_website_url": null, "website_url": "http://innerherostudios.com/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1592192974313489"}, {"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ARCADE", "SHOOTER", "SIMULATION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8876204045800666"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/39003286_8965122430244594_6846837812817669511_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=VipT65_ZYm0Q7kNvgEinGjN&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYBlYMN1SHoNZYRAfRJ3vVlwBG6qG8L3tpw6C6npeD_45A&oe=6790FC1B"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "HuntyVerse", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Arcade", "id": "1592192974313489"}, {"topic_name": "Action", "id": "459781504968480"}], "release_date": 1689310800, "lastRevision": {"nodes": [{"id": "425186015966474", "release_type": "SCHEDULED", "administration_status": "APP