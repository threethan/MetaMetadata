.com/privacy.html", "developer_terms_of_service_url": null, "publisher_name": "Nerd Ninjas", "support_website_url": null, "website_url": "https://www.nerdninjas.com/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "644354873106712"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER", "CO_OP"], "id": "1006395617474681"}}]}, "scheduled_offers": {"count": 3, "nodes": [{"status": "PUBLISHED", "id": "1339277974038676"}, {"status": "PUBLISHED", "id": "666934715711297"}, {"status": "PUBLISHED", "id": "973998388170389"}]}, "baseline_offer": {"id": "973998388170389"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1752512400, "status": "PUBLISHED", "id": "1339277974038676"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "8436618536463044", "version": "1.0.11"}, "id": "610827167572701", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "5316449838479945", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "8436618536463044"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": []}, "change_log": null, "created_date": 1750648718, "id": "8786810008110560", "version": "1.0.11", "version_code": 8995, "release_status": "RELEASED", 