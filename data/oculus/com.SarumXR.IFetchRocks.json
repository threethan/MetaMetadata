roid Hauler simulator designed from the ground up for Room Scale Virtual Reality. Completely customize, maintain and repair the controls and systems of your ship using 100s of interconnecting components that fully simulate power and data distribution. "}]}, "developer_name": null, "developer_privacy_policy_url": "https://sarumxr.com/i_fetch_rocks_privacy.txt", "developer_terms_of_service_url": null, "publisher_name": "SarumXR", "support_website_url": null, "website_url": "https://sarumxr.com/", "category": "GAMES", "comfort_rating": null, "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}, {"id": "1043107547362485"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["SIMULATION", "SPACE_UNIVERSE", "FLYING"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER", "CO_OP"], "id": "7488302667945177"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "2209065186118586"}, {"status": "PUBLISHED", "id": "927665445726151"}]}, "baseline_offer": {"id": "927665445726151"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1726765200, "status": "PUBLISHED", "id": "2209065186118586"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "24463570813325097", "version": "1.2.2.7"}, "id": "1364350543986050", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "46876