oc=AdkZleu1ULALNiUfLWVLDUi5Dr0cmTdZnykKcBqN0OTSxJZSUPaGgbG-J4o14R60YrM&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_Afd8-iD2hL-1x_JMQ5Uxp1NqedmJ0A-0hEoVnC_K0esOdA&oe=68F399E8"}, {"file_name": "RoundScreen.png", "id": "221669187563733", "uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/38974883_221669197563732_673204419216604099_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=EhajVJHTGr4Q7kNvwHWwbVB&_nc_oc=Admszwa2Qd7tT5tLYiCnbdFqhBZjOnmUfvinNMqoPa3Jin-BlrVlDYDBBDuv44Mhvh4&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_AfcG9LWXffUW4L_OYLscH6i0NL9JPjfGWS3_HW1nPRG0dA&oe=68F3932B"}, {"file_name": "PoolScreen.png", "id": "243997814869068", "uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/38982675_243997818202401_35384399934413085_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=V8-FJZDf4XUQ7kNvwFKlNR5&_nc_oc=AdkAElZ6vm9bmPZn9JiyQ7J9j1IngZ5PYWW1mpyx37h50x-slW4xhFCci1lXUigHYnI&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_AfdQX2p3KSvpQhfEhTbmM12f53SFxCYItlTbuNlglATTYA&oe=68F37B08"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "GenieGymCoverLandscape.png", "id": "328896759640837", "uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/75203622_328896762974170_9218873658952822499_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=23EVyA2y8RoQ7kNvwHkv0h_&_nc_oc=AdlAPJI9YRAfzMbgyVbv9xGzE78NAF6bMEMVpTy2EU3vMeGwCQNcz9uSTnwBu7Fdcj0&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_AffQ0ODOd6To_cu30STJNimuSYzLiZnksWfcUxdrPh7sTA&oe=68F38782"}, "display_name": "Genie Gym", "keywords": [], "long_description": "Are you looking for something more than a virtual copy of a traditional game? Want to play something playfully unique? Genie Gym is a reimagining of popular sports games for the virtual world. 3D versions of pool, bocce, and cornhole offer a casual experience, while 3D versions of pong, tennis, handball, and spikeball offer something more fast paced. There is a game for everyone!\n\nTrack your wins on the leaderboard and improve your skill to become the best. Customize your Genie to match your style. Three themes are available to choose from: solar, stone, and metal. \n\nFree updates are coming for all players of Genie Gym that will bring a variety of arenas to choose from and play in, additional styles of game tables, matches, rule sets, and multiplayer variety.", "short_description": "A playfully unique game with traditional origins. Enjoy seven game modes that turn the familiar into something new. Play in Single Player or Multi Player (1 vs. 1) modes. Sports such as tennis, ping pong, bocce, pool, handball, cornhole, and spikeball inspired these unique VR centric games."}]}, "developer_name": null, "developer_privacy_policy_url": "https://twowolvesstudio.com/wp-content/uploads/2023/09/Genie-Gym-Privacy-Policy.pdf", "developer_terms_of_service_url": "https://twowolvesstudio.com/wp-content/uploads/2023/09/TOS.pdf", "publisher_name": "Two Wolves Studio", "support_website_url": null, "website_url": "https://twowolvesstudio.com/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}, {"id": "331529188132278"}, {"id": "1592192974313489"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER"], "id": "27114431424836845"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/39036631_3668543670083977_3134814342871428072_n.png?stp=dst-png_s360x360&_nc_cat=101&ccb=1-7&_nc_sid=6e7a