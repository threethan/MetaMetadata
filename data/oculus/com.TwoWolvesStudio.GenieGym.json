oc=AdnqEoRBSLhgDr1pBzS4Xw7VNd29NOTvUgCiwLo5tLR4tJAymwHK8wDNckJ1iSGYSnk&_nc_zt=3&_nc_ht=scontent-lhr6-2.oculuscdn.com&oh=00_AfQqw-6Yh1jPY82otIp0ZFXnsfbiT4rT1gd2ZIZ0FqWajA&oe=688A21E8"}, {"file_name": "RoundScreen.png", "id": "221669187563733", "uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/38974883_221669197563732_673204419216604099_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=S8Wlt_lmjhAQ7kNvwHmtQqr&_nc_oc=Adkaes_7fVhW1OBbYzslQA0jODphgVH6jMnh-xVRJbh0DKiTt4fvghgsjZWRP4roSUE&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AfS8O-J_i84-hVLdS4pUGpsFynTlMe_NM1clW6o5S4wk9Q&oe=688A1B2B"}, {"file_name": "PoolScreen.png", "id": "243997814869068", "uri": "https://scontent-lhr6-2.oculuscdn.com/v/t64.5771-25/38982675_243997818202401_35384399934413085_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=2hV6vGhHBDQQ7kNvwH9tknz&_nc_oc=Adlkn4KsoojFoYKNAV15HL8oYR7kKCP80RmqHRaWIBKurHJ7rgii0BCO0GB64KyBRNM&_nc_zt=3&_nc_ht=scontent-lhr6-2.oculuscdn.com&oh=00_AfTPP9WKXC0frnJPWYNQvjX5soBLpUnM1Jjbs2-6W6DWVw&oe=688A0308"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "GenieGymCoverLandscape.png", "id": "328896759640837", "uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/75203622_328896762974170_9218873658952822499_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=3T4myNDq-B4Q7kNvwGqDaM7&_nc_oc=AdmrfymS4hSDlH7MJqTcgWT1aNIJGwO1GXr7cU_VxHBySMmpixoG4rg2FRpm5BG_Nxs&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AfSFbKZr7IpAvIQdlkkTJsLOEWVdJFDd0vaf5rNzq8GvTg&oe=688A0F82"}, "display_name": "Genie Gym", "keywords": [], "long_description": "Are you looking for something more than a virtual copy of a traditional game? Want to play something playfully unique? Genie Gym is a reimagining of popular sports games for the virtual world. 3D versions of pool, bocce, and cornhole offer a casual experience, while 3D versions of pong, tennis, handball, and spikeball offer something more fast paced. There is a game for everyone!\n\nTrack your wins on the leaderboard and improve your skill to become the best. Customize your Genie to match your style. Three themes are available to choose from: solar, stone, and metal. \n\nFree updates are coming for all players of Genie Gym that will bring a variety of arenas to choose from and play in, additional styles of game tables, matches, rule sets, and multiplayer variety.", "short_description": "A playfully unique game with traditional origins. Enjoy seven game modes that turn the familiar into something new. Play in Single Player or Multi Player (1 vs. 1) modes. Sports such as tennis, ping pong, bocce, pool, handball, cornhole, and spikeball inspired these unique VR centric games."}]}, "developer_name": null, "developer_privacy_policy_url": "https://twowolvesstudio.com/wp-content/uploads/2023/09/Genie-Gym-Privacy-Policy.pdf", "developer_terms_of_service_url": "https://twowolvesstudio.com/wp-content/uploads/2023/09/TOS.pdf", "publisher_name": "Two Wolves Studio", "support_website_url": null, "website_url": "https://twowolvesstudio.com/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}, {"id": "331529188132278"}, {"id": "1592192974313489"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER"], "id": "27114431424836845"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-lhr8-2.oculuscdn.com/v/t64.5771-25/39036631_3668543670083977_3134814342871428072_n.png?stp=dst-png_s360x360&_nc_cat=101&ccb=1-7&_nc_sid=6e7a