ts": [{"file_name": "Image Sequence_060_0000.png", "id": "1951250988995644", "uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/499623179_1951250992328977_4077182840788099607_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=PvfdXnuL3m4Q7kNvwHqUAH2&_nc_oc=AdllhlHZzgKdnSnQdCid2oWrgJ26DKbMz-2F2nHUyPVODrP8CqVfvULGaFfcV1WI8Ao&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_Afk5GYsPVIQGASTPRTavKsNbq_p4x5wbChIKH7Piat0Tag&oe=695120E9"}, {"file_name": "ActionShot.png", "id": "1547131896455573", "uri": "https://scontent-atl3-2.oculuscdn.com/v/t64.5771-25/499619098_1547131899788906_756131324390421151_n.png?_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=LdHbezTP3isQ7kNvwE2DT-_&_nc_oc=Adl6lSY1s-PudwdWJFe2SywTP-ekBrWmGizD_GCdHEEz06K4q1VuGUmbbPBLd3yDAV4&_nc_zt=3&_nc_ht=scontent-atl3-2.oculuscdn.com&oh=00_AfmhV-hu4M6cVdQRON2PocHO00csgz6TxQz5Q4ppf3DjfQ&oe=69511137"}, {"file_name": "Image Sequence_089_0000.png", "id": "805063061857555", "uri": "https://scontent-atl3-1.oculuscdn.com/v/t64.5771-25/499619808_805063065190888_1968071561341593887_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=vOyRAmIu7YQQ7kNvwE-u-qm&_nc_oc=AdmzT4KuEhf5k_ngK2SzuyIyiOJojfPVG6TtCVmHj0_YevI7e1DEqKS-rOi7-YGeqhY&_nc_zt=3&_nc_ht=scontent-atl3-1.oculuscdn.com&oh=00_AfnDjHUifGiq0pJTPTnH3MBBkmKpzM_QFgBseELC1rQOxg&oe=69511205"}, {"file_name": "Image Sequence_032_0000.png", "id": "956237166638765", "uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/507827918_956237169972098_5262338259199021762_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=kZFiKc4IHQAQ7kNvwFcdwGg&_nc_oc=AdlVUa8WVRNS-ykyI3UQol4Z9p7-22Rm0O5-ZVZgsjk6WY8Stgk4ztul_CLnqKlVBsc&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_AfnvPe-pTEjjiDiIbCauVLE-ua5_Q877u9ScsxFyL_ZgLg&oe=69510A51"}, {"file_name": "Gymnasium.png", "id": "1499719844505054", "uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/499307466_1499719847838387_2905519117021823644_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=i3hMl4gx0LYQ7kNvwFEGebN&_nc_oc=Adlm-vkZrCTRb35ScpfU6HPclLZhB8Wdm_gmXpm6SzSIjVBJjT8oZyodZbwRWcipqYI&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_AfkUPYaIn4Rh0xh96lAqzkZzEZesCVyxvoZEW9wAiAeAZw&oe=69511182"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Gloves up_Landscape Game Cover.png", "id": "1104717774910712", "uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/499619829_1104717781577378_8494564826830694206_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=IyE08aPgYI8Q7kNvwHBFxtO&_nc_oc=AdkrkoN9Sp77f-ZVhl9QeQ7-Tp5P0bVbxTaj80sHJSJ9hN1oVwv6gaORwNv6zKpI9uo&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_Afk8_xg6lapblz7BgWKrii_2AIZPZPSHr8hUmmvhaxr5CA&oe=69512385"}, "display_name": "Gloves Up", "keywords": ["Fighting", "Simulation", "SinglePlayer", "Fitness", "Training"], "long_description": "{1440165610621462}\nJoin Discord:  https://discord.gg/ZERYSbXXV6  \n\n**Gloves Up** is a physics-based boxing game featuring the long-awaited single-player career mode fans have been craving.\n\n**Define your legacy:** Rise, peak, and fade across a lifelong career shaped by performance, aging, and relentless training. Play as either male or female!\n\n**Feel the fight:** Every interaction runs on physics, from head-snapping punches to satisfying knockdowns. Even the menus are physics-based.\n\n**Train smart:** Build muscle memory with slowed-down AI in diverse training drills. Master slips, rolls, blocks, combos at your pace! \n\n**Dynamic opponents:** Each fighter is distinct in appearances and styles\u2014ranging from outside fighters and patient counter punchers to aggressive swarmers and punishing sluggers. All procedurally generated ensuring a fresh supply of new faces during your career. **No two fights play out the same**\n\n**Hype up the crowd** with ring introductions using your profile name. Scale from intimate amateur bouts to glorious pro fights in front of 1,000+ dynamic spectators, feel the adrenaline rush when the crowd gets loud!\n\n**Customize your career:** Dial in your fight style\u2014from quick knockouts to full-length tactical battles. Set round count, duration, break times and more\n\n**Early Access Notice:** Core systems are live and evolving \u2014your feedback will shape the future of the fight. Let\u2019s build this legacy together. ", "short_description": "**Gloves Up** is a physics-based boxing game featuring the long-awaited single-player career mode fans have been craving. There's no limit to the amount of Ai fighters you'll come across during your career."}]}, "developer_name": null, "developer_privacy_policy_url": "https://glovesupgame.github.io/GlovesUp/privacy-policy.html", "developer_terms_of_service_url": null, "publisher_name": "Space Junk Games LLC", "support_website_url": null, "website_url": "https://glovesupgame.github.io/GlovesUp/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}, {"id": "459781504968480"}, {"id": "331529188132278"}, {"id": "2710427532541308"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "24905512202379697"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-atl3-1.oculuscdn.com/v/t64.5771-25/499618444_3798484617109750_3716370912452312485_n.png?stp=dst-png_s360x360&_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=7JZTWH1lhtsQ7kNvwEQeIbM&_nc_oc=AdnFCJdu2Gytkw2qscqwTfVPlrZoURYKVz5rs5EpKEpqPFCZP4j-PvPow0w4fRZFIzQ&_nc_zt=3&_nc_ht=scontent-atl3-1.oculuscdn.com&oh=00_Afn98SqskHrNyFqVSOkmyXIKR0Re2YLvqCQlM_gnQjWS7