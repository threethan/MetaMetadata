b=1-7&_nc_sid=6e7a0a&_nc_ohc=-Mf5whUf3j0Q7kNvwGQ1UxS&_nc_oc=AdkUOIyVoA0z9Av7CtUDfsirBPfz6aULscuewfX-dLGW40-MDiJ-xBadeT-89JL3IWQ&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfF5V3HOjXx1k8I8aWGF2bwO6PlatMSPpzvtboyjwhaAbQ&oe=67F8F8D6"}, {"file_name": "04.png", "id": "3089141321405611", "uri": "https://scontent-sin2-1.oculuscdn.com/v/t64.5771-25/39035386_3089141324738944_4856569391505471458_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=GhCq9IajI5MQ7kNvwEoKBH6&_nc_oc=AdmPhChFKNVQWIYoDm2UcJD6-NTnR4SLrwD6hucL8msGs7Bf3fWENq9P5p2IA-TE7Hw&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_AfF4IMLDbdSMz7qCm5eoVK6MFhfH9hgjnUEIjXkkahtzuQ&oe=67F8EAA1"}, {"file_name": "05.png", "id": "1433526053764657", "uri": "https://scontent-sin2-2.oculuscdn.com/v/t64.5771-25/39031610_1433526057097990_3602149113657224182_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=qRFbabAyPwsQ7kNvwGihQpF&_nc_oc=Adk39pRF4DMaK1coJa8f4iWspSbxZgqN52zyQHbTBR8jnkaI7zYxawTWQzQVFAyzWOo&_nc_zt=3&_nc_ht=scontent-sin2-2.oculuscdn.com&oh=00_AfHxOt54clqlXcmLoOV9OuzD1JjBJG7ycLNlk_OKeg-HjQ&oe=67F907B7"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "cover.png", "id": "680947166669746", "uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/38974798_680947173336412_962573008308668399_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=jUBekFe_aekQ7kNvwFFxCwy&_nc_oc=Adn-3QlxLPNLxmyvxlGhxjFZg_fU_zUM--DDSrk9Miyucn3DUGmrhUGAOFtWJV_YFww&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfFKZNJIUHeaR1tXksT3STDstZta6K7KVoVPn7rymyN4Sw&oe=67F8EADB"}, "display_name": "History of A Painting - \"The light in the Shadow\"", "keywords": [], "long_description": "(Hi)story of a Painting is an animated 6dof VR series aimed at engaging young audiences with art by creating intimate, gripping and relatable experiences focused on the human stories behind some of the world\u2019s greatest art and artists.\n\nNarrated by contemporary creative influencers, each episode uncovers themes from the artist\u2019s context that resonate with contemporary life. \n\nThis episode, \u201cThe Light in the Shadow\u201d, explores the extraordinary story of one of today\u2019s most celebrated Baroque artists: Artemisia Gentileschi. We follow her extraordinary journey as she navigates a time and world where women have virtually no rights and are essentially men\u2019s property, and becomes one of Europe\u2019s most celebrated and in-demand painters of her time. \n\nArtemisia painted herself into posterity, she was a feminist at her core and this story is about how Artemisia rose up against the odds and impressed the whole of Europe with her talent within her own lifetime, and continues to capture our imaginat", "short_description": "A journey into the art that changed the world"}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.historyofapainting.com/privacypolicy", "developer_terms_of_service_url": null, "publisher_name": "HOAP", "support_website_url": null, "website_url": "https://www.historyofapainting.com/", "category": "ENTERTAINMENT", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "726368587961803"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["DOCUMENTARY_HISTORY", "NARRATIVE", "ART_CREATIVITY"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "fr", "hu", "es", "es_CL", "es_AR", "es_CO", "es_MX", "es_PE", "es_PR", "es_VE", "pt", "el", "tr", "it"], "supported_input_devices": ["GAMEPAD"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "859263369178388"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-sin2-1.oculuscdn.com/v/t64.5771-25/38974538_1097467127713530_5562179493833340005_n.png?stp