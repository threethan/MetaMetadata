b=1-7&_nc_sid=6e7a0a&_nc_ohc=2jTh-pWeQCEQ7kNvwGRkdn3&_nc_oc=AdkW_M93O3aMhFzYMIgc2iVenXNm4Z2aQe5DxOT6NCiCy4gkLrTGnTYICREO-TxcRPQ&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfQXZ1zjYbMW_dxogl34nF0CSxQ61TlS0Jx_a8OhOQARRw&oe=686E4E56"}, {"file_name": "04.png", "id": "3089141321405611", "uri": "https://scontent-sin2-1.oculuscdn.com/v/t64.5771-25/39035386_3089141324738944_4856569391505471458_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=belyr1SeGZoQ7kNvwGqAVDI&_nc_oc=AdlSuPa-O1jb2lA9ix1oF-vOV5Umk1OfbxkmwlKcwe6vH09ISmeXnDWJdkTx7ekaOU0&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_AfSwWUpZ_dDEpjMEEorHxlynNM81YSLitBBtu-FbLxPXFA&oe=686E4021"}, {"file_name": "05.png", "id": "1433526053764657", "uri": "https://scontent-sin2-2.oculuscdn.com/v/t64.5771-25/39031610_1433526057097990_3602149113657224182_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=szKHVb72XQwQ7kNvwGIPxVV&_nc_oc=AdkZYUbaGMAaB3eDM_pTxj4POJCGlqm2uMIChRSB7EbF9BMAqyvbTabh7NEKl8dtfrA&_nc_zt=3&_nc_ht=scontent-sin2-2.oculuscdn.com&oh=00_AfRAjNrxwBK2Gh1RMB4czk6_mCv1X4hs_Gf5TCSTDzWerw&oe=686E5D37"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "cover.png", "id": "680947166669746", "uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/38974798_680947173336412_962573008308668399_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=JXO160q86EQQ7kNvwHVLaoC&_nc_oc=AdkxDtWj_lnGnQjBLrvWJ0hxqQEKUfH3UwunnCzLvchcpSTDAKRyC0k-odgRp12zaeg&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfTlhHaQ7ln7gXq3SMdlDHzoGo5ybwyTpGPqutjGXQc_aQ&oe=686E405B"}, "display_name": "History of A Painting - \"The light in the Shadow\"", "keywords": [], "long_description": "(Hi)story of a Painting is an animated 6dof VR series aimed at engaging young audiences with art by creating intimate, gripping and relatable experiences focused on the human stories behind some of the world\u2019s greatest art and artists.\n\nNarrated by contemporary creative influencers, each episode uncovers themes from the artist\u2019s context that resonate with contemporary life. \n\nThis episode, \u201cThe Light in the Shadow\u201d, explores the extraordinary story of one of today\u2019s most celebrated Baroque artists: Artemisia Gentileschi. We follow her extraordinary journey as she navigates a time and world where women have virtually no rights and are essentially men\u2019s property, and becomes one of Europe\u2019s most celebrated and in-demand painters of her time. \n\nArtemisia painted herself into posterity, she was a feminist at her core and this story is about how Artemisia rose up against the odds and impressed the whole of Europe with her talent within her own lifetime, and continues to capture our imaginat", "short_description": "A journey into the art that changed the world"}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.historyofapainting.com/privacypolicy", "developer_terms_of_service_url": null, "publisher_name": "HOAP", "support_website_url": null, "website_url": "https://www.historyofapainting.com/", "category": "ENTERTAINMENT", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "726368587961803"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["DOCUMENTARY_HISTORY", "NARRATIVE", "ART_CREATIVITY"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "fr", "hu", "es", "es_CL", "es_AR", "es_CO", "es_MX", "es_PE", "es_PR", "es_VE", "pt", "el", "tr", "it"], "supported_input_devices": ["GAMEPAD"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "859263369178388"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sin2-1.oculuscdn.com/v/t64.5771-25/38974538_1097467127713530_5562179493833340005_n.png?stp=d