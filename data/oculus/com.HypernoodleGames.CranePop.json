t=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfS95c5rwQ6jGYXieTHnAvZSRXfGOFRscw8up4Mox_2m9g&oe=6894B66B"}, {"file_name": "SideView_1.1.3.png", "id": "1486738105216656", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/38982522_1486738108549989_2800012866689650826_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=QGf_aNyQOQ4Q7kNvwHUMaWH&_nc_oc=AdkR6afa-90VnvKlZATD41S1LctaKEkEGvO4yjXQps5IoxP5crw5SYZ1TxKwk7enck8&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfSF4mLtVVpwv9KV7YfGxxqy8cEikNM3k3Ziygf3nPEP5Q&oe=6894A03C"}, {"file_name": "VeryFullBox_1.1.3.png", "id": "7019028088185721", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/75411995_7019028091519054_2996925865049541052_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=f4seuVv5J3IQ7kNvwGuYIsp&_nc_oc=AdnsvcHM9e0O3iIwXDIuC_1g310U0UzewPcWLUDkNRiCCDxqwo_4oUc-6W5Hz0Rb8K4&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfR92e5I-NXDH6-IC7q40G2XVyWJKMb0M-2zJ1H8UpP1nw&oe=6894A39D"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "TrailerThumbnail_1.1.1.png", "id": "1069758007599612", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/75269589_1069758010932945_7109376256242061612_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=92jwMoacANwQ7kNvwFLAStX&_nc_oc=Adn-tZ9PI6u6Ku19ESZ2kX1yJDEyJa9EnCkt4FeS4uTcGXL8xYGv8N2PCVbNZfng2Bs&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfSXs76lyj6foidCYgK_5IXPBMU9RacUTWVInaxiQfOhjQ&oe=6894B07A"}, "display_name": "Crane Pop", "keywords": [], "long_description": "Drop 'em in, once two of a kind touch they'll pop into a bigger one. Keep dropping 'em until they all fall out! The bigger they get, the higher your score!\nUse a virtual joystick to intuitively move a claw-machine's claw around, dropping plush birds into the box below to combine them with matching birds. The bigger the plushies, the more points you'll earn from combining them. \nAccessibility options are available to use your controller thumbsticks instead of the machine's joystick, as well as to change grip button behavior. \nLeaderboard support means you can compare your score to your friends as well as the rest of the world, and your own daily scoreboard is reset each day that you play.", "short_description": "Fill a claw machine with bigger and bigger plush birds, combine pairs into massive new plushies and go for the high score! But once anything falls out of the machine, it's game over."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.hypernoodlegames.com/crane-pop-privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Hypernoodle Games", "support_website_url": "https://www.hypernoodlegames.com/feedback", "website_url": "https://www.hypernoodlegames.com/", "category": null, "comfort_rating": null, "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1592192974313489"}, {"id": "377978390272490"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8082048191843338"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/39031377_334952739357630_3705917076246488455_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=R1VvDyVTwUoQ7kNvwFL_zBQ&_nc_oc=AdmUmAQqAzDAwpZVTjiusszrGEuxspjsCWp3eMdqRom6z5QaXjAYWVaBlRsEge8hR6c&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfRN7iLheP1OPV6w7XnHYOXaGtZtwBYZi7Ea0YfSVBJMxA&oe=68949E48"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Crane Pop", "p