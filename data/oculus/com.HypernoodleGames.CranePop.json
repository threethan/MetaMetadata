t=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfGokbE9U6JB2DIt0z9nxjZDqRqKPMaydm4_BO4-p08Z3w&oe=67FCE1AB"}, {"file_name": "SideView_1.1.3.png", "id": "1486738105216656", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/38982522_1486738108549989_2800012866689650826_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=YAKwJl6qP3EQ7kNvwESWF4d&_nc_oc=AdlzsYCh8e5InlI4_axnLZo4p018eqsVAf0YVkIb1Ud9O_AX5CnIsM0ZxefCz7l4vMQ&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfEFFBYDe9ah4_aAD8gDnqc6Y3sXh6dqgMC9wfei8r0Epw&oe=67FD03BC"}, {"file_name": "VeryFullBox_1.1.3.png", "id": "7019028088185721", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/75411995_7019028091519054_2996925865049541052_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=A167L4W-yLIQ7kNvwHPU1EJ&_nc_oc=AdlAriYuMAdHudDh8gDbXMkR-Z8FyLX9zh8z3u5zxjh_ZH9jjLyaM2ZcSbRLkTtCV-s&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfHYZB0CprZdVWsik7-vo9odRhLNe57313hK0Mk8gmDbuQ&oe=67FD071D"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "TrailerThumbnail_1.1.1.png", "id": "1069758007599612", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/75269589_1069758010932945_7109376256242061612_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=l3V23DCi5FEQ7kNvwGrR-Xe&_nc_oc=AdlR27uGObz6gYXklnPBEWLxo5TuLc1KKM_AfWw91vBOfPGKwtwDTNEzS-5eW398WSc&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfEjW4K_GdMAW2-Pox5FoGpkBu6-9GewQ8d4wSjEfMm-Hw&oe=67FCDBBA"}, "display_name": "Crane Pop", "keywords": [], "long_description": "Drop 'em in, once two of a kind touch they'll pop into a bigger one. Keep dropping 'em until they all fall out! The bigger they get, the higher your score!\nUse a virtual joystick to intuitively move a claw-machine's claw around, dropping plush birds into the box below to combine them with matching birds. The bigger the plushies, the more points you'll earn from combining them. \nAccessibility options are available to use your controller thumbsticks instead of the machine's joystick, as well as to change grip button behavior. \nLeaderboard support means you can compare your score to your friends as well as the rest of the world, and your own daily scoreboard is reset each day that you play.", "short_description": "Fill a claw machine with bigger and bigger plush birds, combine pairs into massive new plushies and go for the high score! But once anything falls out of the machine, it's game over."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.hypernoodlegames.com/crane-pop-privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Hypernoodle Games", "support_website_url": "https://www.hypernoodlegames.com/feedback", "website_url": "https://www.hypernoodlegames.com/", "category": null, "comfort_rating": null, "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1592192974313489"}, {"id": "377978390272490"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8082048191843338"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/39031377_334952739357630_3705917076246488455_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=saLz87v0IKQQ7kNvwGznFqd&_nc_oc=AdnPupt32W1UyCIU6lhqRSH53wu4JF_1PzMRcP1cv-uAlWx5bkKBjoSjrEtdFfQCMtE&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfFvVHdRUmi2yvzkQ2idnSd8xUI64CmW5xuAKr35QUfOJw&oe=67FD01C8"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Crane Pop", 