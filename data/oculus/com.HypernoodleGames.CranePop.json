t=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfH5Qj8MbLImxJ3ABJK46BEq9EHG27z8tFJ8J8QqfKRIXQ&oe=68076DAB"}, {"file_name": "SideView_1.1.3.png", "id": "1486738105216656", "uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/38982522_1486738108549989_2800012866689650826_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=T_0vDAJmP-MQ7kNvwEEMX2l&_nc_oc=AdkXtvca1q4VljccPYB4UWzCU1iN9arUVVZCnTzAmH0UCDbYd2xjW9hrh3oC6icNqec&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfHW3EQxkGtGQpYqq2nBd3QdHWKmNOtpAFtYP-lBWO8NDw&oe=68078FBC"}, {"file_name": "VeryFullBox_1.1.3.png", "id": "7019028088185721", "uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/75411995_7019028091519054_2996925865049541052_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=7YBjRr59ZCUQ7kNvwHOPz4b&_nc_oc=Adn-CQC--77Of9BgUaIvJ6Gw98XUUvDZu0h9MHrF_taTS6JeKe_qdXR9uCcg5MVtzCQ&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfF4hpI0yZRvlXfxu2S38Ym5MupkvlqGeuN3SzBqInFNSg&oe=6807931D"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "TrailerThumbnail_1.1.1.png", "id": "1069758007599612", "uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/75269589_1069758010932945_7109376256242061612_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=L--E68KZiPcQ7kNvwHOe8XT&_nc_oc=AdmJJOn4_qUo49r-RVOu5MpuLiBRmGkSnaUztCAJfmC1bFtY5FXaC8NwbjjXG8uAMuQ&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfHMv5ZpQg-qq-khmiacSndk9iwl6zF7Y4i63Y5fB2XB5g&oe=680767BA"}, "display_name": "Crane Pop", "keywords": [], "long_description": "Drop 'em in, once two of a kind touch they'll pop into a bigger one. Keep dropping 'em until they all fall out! The bigger they get, the higher your score!\nUse a virtual joystick to intuitively move a claw-machine's claw around, dropping plush birds into the box below to combine them with matching birds. The bigger the plushies, the more points you'll earn from combining them. \nAccessibility options are available to use your controller thumbsticks instead of the machine's joystick, as well as to change grip button behavior. \nLeaderboard support means you can compare your score to your friends as well as the rest of the world, and your own daily scoreboard is reset each day that you play.", "short_description": "Fill a claw machine with bigger and bigger plush birds, combine pairs into massive new plushies and go for the high score! But once anything falls out of the machine, it's game over."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.hypernoodlegames.com/crane-pop-privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Hypernoodle Games", "support_website_url": "https://www.hypernoodlegames.com/feedback", "website_url": "https://www.hypernoodlegames.com/", "category": null, "comfort_rating": null, "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1592192974313489"}, {"id": "377978390272490"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8082048191843338"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/39031377_334952739357630_3705917076246488455_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=g2iEJebcY4sQ7kNvwH4EZd_&_nc_oc=AdmwMA_Bz1WgO6lMIQyd94AnaeILjuIXqyZFS9m9j-rv0aAe2eeFBYDEw2Ot4uzlH7o&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfE4Pavsn0ZL4ZbJ_Z9JOlHFp5Ed67zdc4plCsSvn1arrg&oe=68078DC8"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Crane Pop", 