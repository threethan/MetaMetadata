_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=YLRAbjiboEsQ7kNvwHVvSOE&_nc_oc=AdmSBXDiAsR8ehze3lelRcfONoXfPdBg4FdYiIsLwvovsoqPUudzxHHI1CfPqfMTDOA&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AfiU-OfMsY-BJgzWrDnUzzHIloIWi8r5BcTH7jbDqPaUfA&oe=69108116"}, {"file_name": "1.png", "id": "9543126012369889", "uri": "https://scontent.oculuscdn.com/v/t64.5771-25/38974654_9543126022369888_1314598922850701453_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=kSqdLZhRveUQ7kNvwHGmsoX&_nc_oc=Adl6WrYL8rE4Qe_ii05SVZUkLoeRpI_itDYgNCGlJRJnQYCDo6IDbAgya7N5OvG75qw&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AfgvdTn1Grw_rbJ1JyTaOeMv57J-1xtsQk1tMGm1YkuKCw&oe=69107071"}, {"file_name": "5.png", "id": "628012680295114", "uri": "https://scontent.oculuscdn.com/v/t64.5771-25/499618069_628012683628447_6313840828862506420_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=HYsWOe5y-agQ7kNvwElm_61&_nc_oc=AdlQDzu2-08doj3xgfkGbqgz_e43wSxPknALhB90M8WaZziyUFN9UzduPgvU1TXhGVE&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AfgXWdHjgpWUL_bmaTsRQzw7xcLc5ZsdzIZg3BfPt3yOmw&oe=69109C3C"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "4.png", "id": "857764556924160", "uri": "https://scontent.oculuscdn.com/v/t64.5771-25/499617847_857764560257493_6518929680756477415_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=LvCHOAG4KTkQ7kNvwHNWfWj&_nc_oc=Admm1Tir7e2BynGvxv22XH3TWXGoRJYNXkX1sUTftLbPzo5tA0JptdLcbYP9t3xaq1I&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_Afihb9fb2ENtXKxPrY9qyIga8L4ycD2Rn71azhva5znUOA&oe=69109209"}, "display_name": "VR Zombie Escape", "keywords": [], "long_description": "The VR zombie escape is a VRFPS game that focuses on PVP network online biochemical battle mode, an improvement from the original CS's popular biochemical mode. The game features two modes: zombie escape and biochemical infection. In these modes, online players must play as either humans or zombies. Within a limited time frame, humans must either annihilate all zombies or save themselves, while zombies aim to infect all humans through an all-out attack.\n1. Biochemical infection: Within a limited timeframe, humans must eliminate all zombies using firearms or evade them in order to prolong survival, while the zombies actively seek out humans to infect them.\n2. Zombie escape: Within a restricted time frame, humans are required to fend off zombies with firearms for self-preservation and successfully reach the designated endpoint in order to achieve successful escape. The immediate infection of all human players by zombies results in victory.", "short_description": "The first VR Biochemical frenzy online battle in history, Multiplayer shooter in VR with zombies escape, officially opened \u2014\u2014Official version"}]}, "developer_name": null, "developer_privacy_policy_url": "https://vrdanceyinsi.webnode.page/", "developer_terms_of_service_url": null, "publisher_name": "Holoren", "support_website_url": null, "website_url": "https://renrenren.webnode.page/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "644354873106712"}, {"id": "3744040075834813"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER", "CO_OP"], "id": "24377929568551319"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent.oculuscdn.com/v/t64.5771-25/39035376_2381365365549941_658729145781930262_n.png?stp=dst-png_s360x360&_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=9YTSSschePIQ7kNvwHGSZ0K&_nc_oc=AdmCnk4G5zs_phDXjN-HaYltpLLClu9vqvsfTlm7X8MQdZiveULxQ42jnHRCZKEaYmg