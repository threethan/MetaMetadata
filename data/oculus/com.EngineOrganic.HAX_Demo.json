y_name": "HAX Demo", "keywords": [], "long_description": "Battle Sentient Intelligent robots with digital weaponry in this fast-paced, early-access, VR shooter. With intuitive VR controls, HAX aims to stay true to the fun and excitement offered by traditional shooters, while harnessing the power and immersion of virtual reality. While this demo is single player only, when complete, HAX will be a multiplayer game.\n\nHAX is currently in development by Engine Organic Ltd. The Multiplayer version of the game is currently in pre-alpha testing and will be released later this year (2024). For more insight into HAX please visit EngineOrganic.com/HAX\n\nDiscord: https://discord.gg/XjsSVQKRwt", "short_description": "HAX blurs the lines between digital and physical warfare. Battle robots with digital weaponry in the single player demo of this fast-paced, early-access, VR shooter."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.engineorganic.com/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Engine Organic", "support_website_url": null, "website_url": "https://www.engineorganic.com/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ACTION", "SHOOTER"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "25407859112138659"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ord5-2.oculuscdn.com/v/t64.5771-25/38974855_917708498777323_4673452182580199923_n.pn