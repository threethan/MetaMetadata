ull, "display_name": "Fruit Catcher VR", "keywords": [], "long_description": "Step into a picturesque village and put your reflexes to the test in Fruit Catcher VR, a fast-paced and engaging virtual reality game. As fruits fall from above, your goal is to catch as many as possible before they hit the ground. Using intuitive VR controls, you\u2019ll need quick hands and sharp focus to grab the right fruits while avoiding obstacles and distractions. As the game progresses, the speed increases, making each round more challenging and rewarding. The peaceful village setting, combined with the excitement of high-speed gameplay, creates a perfect balance of relaxation and challenge. Whether you're aiming for a new high score or simply enjoying the immersive environment, Fruit Catcher VR offers a fun and addictive experience for players of all ages.", "short_description": "Fruit Catcher VR is a fun and fast-paced game set in a charming village, where players must catch falling fruits before they hit the ground. Test your reflexes and coordination as the pace increases and the challenge grows."}]}, "developer_name": null, "developer_privacy_policy_url": "https://lucido.life/policy/catcher/", "developer_terms_of_service_url": null, "publisher_name": "Lucido Inc", "support_website_url": null, "website_url": "https://lucido.life/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1592192974313489"}, {"id": "2776542286000603"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING"], "supported_tracking_modes": [], "user_