deo_trailer": null, "video_trailer_thumbnail": {"file_name": "Cover_Landscape_2560x1440px.png", "id": "1262478551230077", "uri": "https://scontent-lga3-1.oculuscdn.com/v/t64.5771-25/39031318_1262478554563410_8871946481246640259_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=39gh_ysE52QQ7kNvwFiL0sF&_nc_oc=Adk7UBC63WdkjXSIhcKdp3tCjmtp0DMN1Pjmb03da50YQb4Sr4HmcyG8kWGaGardX1o&_nc_zt=3&_nc_ht=scontent-lga3-1.oculuscdn.com&oh=00_Afd3sObG2skk0qxNFow-tQ_x8_B_KJOvudJ1RdrAciqMhA&oe=69073180"}, "display_name": "Take Care VR", "keywords": [], "long_description": "Face the darkest horror in this stunning new survival adventure!\n\nWHERE IS SHE?\nYou find yourself in this dark room over and over again. Next to you a mysterious woman. \nWhat is she trying to tell you? \nWhy are you there? \nFollow her path and take the challenge of your life. \n\"Take Care...\"\n\nTHEY ARE HERE!\nFight your way through horrible zombie hordes with different weapons. \nUse your old flashlight to illuminate the dark corridors and solve the mysteries of your mind. \nCollect important items in your backpack to advance and face your toughest hurdles. \nBut try not to make any noise, they might hear you...\n\nHIGHLIGHTS\n* An immersive VR horror story that demands everything from you.\n* Fight cruel zombies and collect items to succeed. \n* Discover the dark places of a hopeless world.\n* A 90 FPS performance mode can be activated anytime.\n* Use two weapons at once for enhanced combat.\n* Train your skills at the tryout.\n* Spatial 3D audio provides a realistic experience.\n* Accessibility options for custom player or inventory height. (whole game can be played seated)", "short_description": "Face the darkest horror in this stunning new survival adventure!\n\nFollow the paths of the mysterious woman and face the challenge of your life. Fight your way through creepy zombie hordes or solve puzzles to succeed.\nUse your old flashlight to illuminate the dark corridors and collect important items in your backpack.\nBut try not to make any noise, they might hear you...\n\nExperience an immersive VR horror story, cruel zombies, two weapon combat, scary graphics and stunning 3D audio."}]}, "developer_name": "Tom Foerster", "developer_privacy_policy_url": "http://forstech.de/privacy.html", "developer_terms_of_service_url": null, "publisher_name": "Tom Foerster", "support_website_url": null, "website_url": "http://forstech.de/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "644354873106712"}, {"id": "2691890164460465"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ACTION", "ADVENTURE", "HORROR"], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "821452698940807"}}]}, "scheduled_offers": {"count": 5, "nodes": [{"status": "PUBLISHED", "id": "2100369290493726"}, {"status": "PUBLISHED", "id": "1042454750669489"}, {"status": "PUBLISHED", "id": "438348918587340"}, {"status": "PUBLISHED", "id": "1221719392118296"}, {"status": "PUBLISHED", "id": "508619114694426"}]}, "baseline_offer": {"id": "2100369290493726"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1758573780, "status": "PUBLISHED", "id": "2100369290493726"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "5639866516113317", "version": "1.1.2"}, "id": "1264872494253168", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBin