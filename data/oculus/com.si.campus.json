iler_thumbnail": {"file_name": "Untitled (2560 x 1440 px) (5).png", "id": "912341420285618", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/38974812_912341423618951_5224547764061425385_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=0dzNWp3kv5wQ7kNvwH6OcsQ&_nc_oc=AdmpN7gEJ2WF9HnB9B9D1-6-WQiWqlMj9ZARbRjI3vBtNJw37hE5D5x7HpJqs1NH0Eg&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_Afs6MoPoJn5RNMXXm7CcwsDobl2pMepHaCwvKVIhwwgSSw&oe=6987311C"}, "display_name": "Campus360", "keywords": [], "long_description": "The world of academia is vast and diverse, dotted with institutions rich in history and innovation. With Campus360, embark on a virtual journey to explore universities from every corner of the globe. From the prestigious corridors of the Ivy League to community colleges and beyond, our VR app brings campuses of all shapes and sizes to your fingertips. Experience everything from the Midwest to the Middle East, loaded in moments, accessible everywhere.\n\nCampus360 is more than just a virtual tour; it's a 360 immersive experience that connects you with the essence of each university. With intuitive controls and menu options, you can effortlessly navigate through different campuses, engaging with interactive and dynamic story content every step of the way.\n\nEducation knows no bounds, and with Campus360, neither does your exploration. Dive into the world of universities like never before, discovering places and stories that shape the leaders of tomorrow. Every university has a story to tell, and with Campus360, you're not just a visitor but a part of that story. Join us in redefining the way the world explores universities.", "short_description": "Campus360 is a virtual campus tour platform allowing prospective students and families to tour campuses from anywhere. Immerse yourself and find your new academic home from our library of virtual tours of universities around the world. From household names in the Ivy League to regional universities, community colleges, and schools in emerging study destinations. Experience a 360-degree view of campuses all over the world with intuitive navigation and interactive story content."}]}, "developer_name": null, "developer_privacy_policy_url": "https://campus360.org/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Campus360", "support_website_url": null, "website_url": "https://campus360.org/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "726368587961803"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH", "HAND_TRACKING"], "supported_player_modes": ["STANDING", "SITTING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "7019992798096385"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1065381431183797"}]}, "baseline_offer": {"id": "1065381431183797"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1708451880, "status": "PUBLISHED", "id": "1065381431183797"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "7048675485228116", "version": "0.2"}, "id": "3598338517106030", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "6955915631170769", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "7048675485228116"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_nam