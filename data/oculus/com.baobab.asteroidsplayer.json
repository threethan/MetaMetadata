n": {"__typename": "Application", "id": "1309677705722446", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "2186826184674256"}, "release_status": "RELEASED"}, "id": "2186826184674256", "version": "3.91", "version_code": 91, "release_status": "RELEASED", "status": "APPROVED"}, "live_binary": null, "created_date": 1636139373, "id": "1650263811979306", "is_initial_release": false, "name_with_fallback": "v22", "pdp_metadata_with_fallback": {"version": 7, "application": {"is_concept": false, "latest_released_pdp_metadata": {"id": "369786917811976"}, "release_status": "RELEASED", "id": "1309677705722446"}, "id": "369786917811976", "release_status": "RELEASED", "status": "APPROVED"}, "release_date": null, "administration_status": "APPROVED", "is_first": false, "release_status": "RELEASED", "release_type": "SCHEDULED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "1309677705722446", "is_first_party": false, "enables_in_app_ads": false, "is_concept": false, "platform": "ANDROID"}, "iarc_cert": {"id": "1416786595021253"}, "iarc_email": null, "msrp": null, "translations": {"nodes": [{"images": {"nodes": []}, "id": "3292107721016115", "locale": "ko_KR", "screenshots": [], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "ASTEROIDS! \u2018\ud480 \ubc84\uc804 \uacf5\uac1c\u2019", "keywords": [], "long_description": "Madagascar\uc758 \uac10\ub3c5\uc774 Emmy Award \uc218\uc0c1\uc791\uc778 INVASION!\uc758 \ud6c4\uc18d \uc5d0\ud53c\uc18c\ub4dc\ub85c ASTEROIDS!\ub97c \uc120\ubcf4\uc785\ub2c8\ub2e4.\n\uc0c8\ub85c\uc6b4 \uc6b0\uc8fc \ubaa8\ud5d8 \uc560\ub2c8\uba54\uc774\uc158\uc5d0 \ube60\uc838\ub4e4\uc5b4 \uc6b0\uc8fc \uacf5\uac04\uc5d0 \ub3c4\uc0ac\ub9ac\uace0 \uc788\ub294 \ub09c\uad00\ub4e4\uc744 \uacbd\ud5d8\ud574 \ubcf4\uc138\uc694. \ubcc0\ub355\uc2a4\ub7ec\uc6b4\n\uc678\uacc4\uc778 Mac\uacfc Cheez(Elizabeth Banks), \uadf8\ub9ac\uace0 \uc774\ub4e4\uc758 \uc870\uc218\uc778 Peas(Ingrid Nilsen)\uacfc \ud568\uaed8\ud558\ub294 \uc5ec\uc815\uc744 \ud1b5\ud574\n\ubb34\ub9ac\uc758 \uc911\uc694\ud55c \uc77c\uc6d0\uc774 \ubcf4\uc138\uc694.\n\ubab0\uc785\uac10 \ub118\uce58\ub294 11\ubd84\uc9dc\ub9ac \uc601\uc0c1\uc5d0\uc11c \uc5ec\ub7ec\ubd84\uc740 \uc791\uc5c5 \ub85c\ubd07\uc774 \ub429\ub2c8\ub2e4. \uc774\ub97c \uac08\uba74\uc11c \ub2ec\ub824\ub4dc\ub294 \uc2a4\ud398\uc774\uc2a4 \ubc84\uadf8\uc640\n\uc704\ud5d8\ud558\uac8c \ub0a0\uc544\uc624\ub294 \uc18c\ud589\uc131\uc744 \ub9c8\uc8fc\ud574\uc57c \ud558\ub294 \uc6b0\uc8fc \uc784\ubb34\ub97c \uc218\ud589\ud560 \ub54c, \ub3d9\ub8cc\ub4e4\uc744 \uad6c\ud558\ub294 \uac83\uc740 \ubc14\ub85c \uc5ec\ub7ec\ubd84\uc758\n\uc190\uc5d0 \ub2ec\ub824 \uc788\uc2b5\ub2c8\ub2e4. \ub3d9\ub8cc\ub4e4\uc744 \uad6c\ud558\ub294 \uacfc\uc815\uc744 \ud1b5\ud574 \uc5ec\ub7ec\ubd84\uc740 \uc6b0\uc8fc\uc120\uc758 \uc874\uacbd\ubc1b\ub294 \uc77c\uc6d0\uc774 \ub418\uace0, \uc0b6\uc5d0\uc11c\n\uc911\uc694\ud55c \uac83\uc774 \ubb34\uc5c7\uc778\uc9c0 \uae68\ub2eb\uac8c \ub429\ub2c8\ub2e4.\nASTEROIDS!\ub294 Sundance\uc758 \uacf5\uc2dd \ucd08\uccad\uc791\uc774\uba70, Unity Vision Award \ucd5c\uace0 \uac00\uc0c1\ud604\uc2e4 \ubd80\ubb38\uc744 \uc218\uc0c1\ud588\uc2b5\ub2c8\ub2e4.\n\n~\ubcf4\ub108\uc2a4 \uc6b0\uc8fc\uc120 \ud0d0\ud5d8 \ubaa8\ub4dc~\nMAC\uacfc CHEEZ\uc758 \ud658\uc0c1\uc801\uc778 \uc6b0\uc8fc\uc120\uc744 \uc0b4\ud3b4\ubcf4\uace0 \ub2e4\ub978 \uc0ac\ub78c\ubcf4\ub2e4 \uba3c\uc800 \ube44\uacf5\uac1c \ub3c5\uc810 \ucee8\uc149 \uc544\ud2b8\uc640 \ub2e4\ucc44\ub85c\uc6b4 \ubc30\uacbd\n\uc2a4\ud1a0\ub9ac\ub97c \ud655\uc778\ud574 \ubcf4\uc138\uc694. \ub9c8\uc74c\ub300\ub85c \ub458\ub7ec\ubcf4\uace0 \uc790\uc2e0\ub9cc\uc758 \ud2b9\uc218 \ud2b8\ub79c\uc2a4\ud3f0\ub354\ub97c \uc0ac\uc6a9\ud558\uc5ec \uc678\uacc4\uc758 \ube44\ubc00\uc744\n\ucc3e\uc544\ubcf4\uc138\uc694. \uadf8\ub798\ub3c4 \uc608\uc758\ub294 \uac16\ucdb0\uc57c \ud558\uaca0\uc8e0? \uce68\uc2e4\uc744 \ub4e4\uc5ec\ub2e4\ubcf4\uc9c0\ub294 \ub9c8\uc138\uc694!", "short_description": "Madagascar\uc758 \uac10\ub3c5\uc774 Emmy Award \uc218\uc0c1\uc791\uc778 INVASION!\uc758 \ud6c4\uc18d \uc5d0\ud53c\uc18c\ub4dc\ub85c ASTEROIDS!\ub97c \uc120\ubcf4\uc785\ub2c8\ub2e4.\n\uc0c8\ub85c\uc6b4 \uc6b0\uc8fc \ubaa8\ud5d8 \uc560\ub2c8\uba54\uc774\uc158\uc5d0 \ube60\uc838\ub4e4\uc5b4 \uc6b0\uc8fc \uacf5\uac04\uc5d0 \ub3c4\uc0ac\ub9ac\uace0 \uc788\ub294 \ub09c\uad00\ub4e4\uc744 \uacbd\ud5d8\ud574 \ubcf4\uc138\uc694. \ubcc0\ub355\uc2a4\ub7ec\uc6b4\n\uc678\uacc4\uc778 Mac\uacfc Cheez(Elizabeth Banks), \uadf8\ub9ac\uace0 \uc774\ub4e4\uc758 \uc870\uc218\uc778 Peas(Ingrid Nilsen)\uacfc \ud568\uaed8\ud558\ub294 \uc5ec\uc815\uc744 \ud1b5\ud574\n\ubb34\ub9ac\uc758 \uc911\uc694\ud55c \uc77c\uc6d0\uc774 \ubcf4\uc138\uc694."}, {"images": {"nodes": []}, "id": "1320578925045867", "locale": "ja_JP", "screenshots": [], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "ASTEROIDS! \u201c\u30d5\u30eb\u30ea\u30ea\u30fc\u30b9\u201d", "keywords": [], "long_description": "\u6620\u753b\u300c\u30de\u30c0\u30ac\u30b9\u30ab\u30eb\u300d\u306e\u30c7\u30a3\u30ec\u30af\u30bf\u30fc\u304c\u624b\u304c\u3051\u305f\u300cASTEROIDS!\u300d\u306f\u3001\u30a8\u30df\u30fc\u8cde\u3092\u53d7\u8cde\u3057\u305f\n\u300cINVASION!\u300d\u306e\u7d9a\u7de8\u3067\u3059\u3002\n\n\u3053\u306e\u65b0\u4f5c\u30a2\u30cb\u30e1\u30fc\u30b7\u30e7\u30f3\u3067\u5b87\u5b99\u63a2\u691c\u306b\u51fa\u304b\u3051\u3001\u304a\u304b\u3057\u306a\u30a8\u30a4\u30ea\u30a2\u30f3\u306eMac\u3001Cheez\uff08Elizabeth Banks\uff09\u3001\n\u305d\u3057\u3066\u5f7c\u3089\u306e\u89aa\u53cbPeas\uff08Ingrid Nilsen\uff09\u3068\u4e00\u7dd2\u306b\u5b87\u5b99\u8239\u5185\u5916\u3067\u8d77\u3053\u308b\u3055\u307e\u3056\u307e\u306a\u554f\u984c\u3092\u89e3\u6c7a\u3057\u307e\u3057\u3087\u3046\u3002\n\n\u3053\u306e11\u5206\u9593\u306e\u30a4\u30f3\u30bf\u30e9\u30af\u30c6\u30a3\u30d6\u6620\u753b\u3067\u306f\u3001\u30e6\u30fc\u30b6\u30fc\u306f\u30bf\u30b9\u30af\u3092\u9042\u884c\u3059\u308b\u30ed\u30dc\u30c3\u30c8\u3068\u306a\u308a\u4e57\u7d44\u5