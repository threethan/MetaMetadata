ht=scontent-ams4-1.oculuscdn.com&oh=00_AYAMN50BlgAlJv7XpN5otGQgXr1Tx7wJ_obqd6Uk-8fT9A&oe=67B4635F"}, "display_name": "CenarioVR", "keywords": ["scenario", "cenario", "elearning", "education", "tour"], "long_description": "CenarioVR is a powerful and easy-to-use virtual reality training tool that enables you to drive engagement, retention, and performance.\nIts intuitive design allows users to generate interactive, immersive scenarios using 360\u00b0 images and videos. These scenarios can be viewed using the CenarioVR App for Oculus.\n\nYou can create your own VR experiences with CenarioVR by signing up at https://CenarioVR.com.", "short_description": "The CenarioVR App for Oculus allows you to experience interactive 360\u00b0 VR scenarios, including ones that you create!\n\nCenarioVR enables you design, share, view, and track immersive learning experiences using 360\u00b0 images and videos."}]}, "developer_name": "eLearning Brothers", "developer_privacy_policy_url": "https://www.trivantis.com/privacy-policy/", "developer_terms_of_service_url": "https://trivantis.com/saas-agreement/", "publisher_name": "eLearning Brothers", "support_website_url": null, "website_url": "https://cenariovr.com/", "category": "APPS", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": "NOT_REQUIRED", "genres": ["EDUCATIONAL", "EXPERIENCE_360_NON_GAME", "TRAVEL"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["GEAR_VR_CONTROLLER"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "654119819073487"}, "review_messages": ["", "Thank you for submitting an update to your application. Your metadata requires some changes before it can go live on the Oculus store.