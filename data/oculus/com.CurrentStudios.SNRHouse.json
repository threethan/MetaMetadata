7&_nc_sid=6e7a0a&_nc_ohc=ZpLLVJ5LddoQ7kNvwGG9CWt&_nc_oc=AdlkEFlqHxkPD0NBi_gP0ueeAPToHSgKWznIa3UjxZUa2SjCE-QOormhYc182KWL0A0&_nc_zt=3&_nc_ht=scontent-lhr8-1.oculuscdn.com&oh=00_AfVkinRkOh9eCQjNVHLNCoc2kyKWjIIHstanKc-LUIiBWg&oe=689B4BC2"}, {"file_name": "Oculus-Screenshots-3.png", "id": "1624494821746935", "uri": "https://scontent-lhr8-1.oculuscdn.com/v/t64.5771-25/39031435_1624494825080268_1696240370691242191_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=4-_X0bVdwZQQ7kNvwHLhteA&_nc_oc=Adnln25DHJW-v-IQtnbCobV5Jd1iIfq5u9QTSU0hswMoS0xlnvknYOaz4jhHRViOR0Y&_nc_zt=3&_nc_ht=scontent-lhr8-1.oculuscdn.com&oh=00_AfVS47VD3z-j8-P1YmSMLdG7TJyJDnwzxGl3rirInL93Ag&oe=689B2E07"}, {"file_name": "Oculus-Screenshots-4.png", "id": "466032423164525", "uri": "https://scontent-lhr6-2.oculuscdn.com/v/t64.5771-25/38982488_466032426497858_4370629542069459434_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=bDBfUtWNOx0Q7kNvwFQulPe&_nc_oc=AdmmNSzUYV3dk547JKpdl_c2esuYwPLqyhbIFqbZPat-e08yA9orQcJQDqbJJGaszTk&_nc_zt=3&_nc_ht=scontent-lhr6-2.oculuscdn.com&oh=00_AfV1p0rZUC53U_yX0NAzPpFHBdfGdvn5pZT_HqmtnCWq3w&oe=689B4B25"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "SNR House", "keywords": [], "long_description": "SNR House transforms math learning into an immersive VR adventure! Step into a virtual house to explore signal-to-noise ratio (SNR) measurements and uncover how distance and obstacles impact Wi-Fi signal strength. Students engage with core math concepts\u2014ratios, proportions, and fractions\u2014through interactive, hands-on lessons that connect theory with real-world applications.\nSNR House features three structured lessons, each paired with quizzes to reinforce learning. As students progress, they can put their skills to the test in an exciting escape room level, combining education with a fun, game-like challenge. SNR House makes math meaningful, interactive, and fun for students of all levels.", "short_description": "Dive into SNR House, an interactive VR app where students explore math concepts like ratios, proportions, and fractions through Wi-Fi signal measurements. Learn and test your skills with engaging lessons and quizzes, then take on the challenge of an escape room level!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://currentstudios.ca/privacy?pid=true", "developer_terms_of_service_url": null, "publisher_name": "Current Studios", "support_website_url": null, "website_url": "https://currentstudios.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1043107547362485"}, {"id": "377978390272490"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "24605732605732281"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-lhr8-1.oculuscdn.com/v/t64.5771-25/75254872_1934106747069577_4393937004111974463_n.png?stp=dst-png_s360x360&_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=dZ62tUDOZTwQ7kNvwG6sL2R&_nc_oc=Admzy4R8lC3QszAZP95r9KEUfMer0nnEfbHbAXuqSzoHphnQ67oJuZUDIoMR_7xW8Mw&_nc_zt=3&_nc_ht=scontent-lhr8-1.oculuscdn.com&oh=00_AfUxeDMMMmUnDIF0_k6068J9AAANMlTM0CDIo9mYak1XHw&oe=689B3580"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "SNR House", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Learning", "id": "1043107547362485"}, {"topic_name": "Puzzle", "id": "377978390272490"}], "release_date": 1731697920, 