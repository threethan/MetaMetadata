ose planets."}]}, "developer_name": "Magnified Studios", "developer_privacy_policy_url": "https://sites.google.com/view/spacersvr/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Magnified Studios", "support_website_url": null, "website_url": "https://sites.google.com/view/spacersvr", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2691890164460465"}, {"id": "1023770785722696"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER"], "id": "8468699213179627"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "979721087522363"}]}, "baseline_offer": {"id": "979721087522363"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1735090080, "status": "PUBLISHED", "id": "979721087522363"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "24173788698910759", "version": "1.1.1"}, "id": "764774402457309", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "8468699203179628", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "24173788698910759"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "764774402457309"}]}, "change_log": null, "created_date": 1756049087, 