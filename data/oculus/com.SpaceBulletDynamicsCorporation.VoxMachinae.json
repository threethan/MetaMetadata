lien landscapes. "}]}, "developer_name": null, "developer_privacy_policy_url": "http://voxmachinae.com/privacy.shtml", "developer_terms_of_service_url": null, "publisher_name": "Space Bullet Dynamics Corporation", "support_website_url": null, "website_url": "http://voxmachinae.com/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2691890164460465"}, {"id": "459781504968480"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ACTION", "SIMULATION", "NARRATIVE"], "has_in_app_ads": false, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER", "CO_OP"], "id": "7613645705383435"}, "review_messages": ["approving this update. "]}]}, "cover_square_image": {"uri": "https://scontent.oculuscdn.com/v/t64.5771-25/39031436_979212320330797_423954783105650227_n.png?stp=dst-png_s360x360&_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=dwam8WEE0OQQ7kNvgFbBLec&_nc_ht=scontent.oculuscdn.com&oh=00_AYAJHx0HeP1NWamdZ2Vx_u8e_XXPSLtAbsMxq8k9T3ljaQ&oe=66CA3646"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Vox Machinae", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Adventure", "id": "2691890164460465"}, {"topic_name": "Action", "id": "459781504968480"}, {"topic_name": "Simulation", "id": "331529188132278"}], "release_date": 1646330383, "lastRevision": {"nodes": [{"id": "1951186588309228", "release_type": "MANAGED", "administration_status": "APPROVED", "pdp_metadata": {"app