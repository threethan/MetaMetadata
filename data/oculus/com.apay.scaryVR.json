 hunt."}]}, "developer_name": null, "developer_privacy_policy_url": "http://www.atmegame.com/privacy-policy", "developer_terms_of_service_url": "http://www.atmegame.com/terms-condition", "publisher_name": "Apay (Atmegame)", "support_website_url": null, "website_url": "http://www.atmegame.com/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": null, "genres": ["HORROR", "ADVENTURE", "ACTION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["TOUCHPAD"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "290573869637116"}}]}, "scheduled_offers": {"count": 3, "nodes": [{"status": "PUBLISHED", "id": "846026702266109"}, {"status": "PUBLISHED", "id": "1715381118521251"}, {"status": "PUBLISHED", "id": "549010735978330"}]}, "baseline_offer": {"id": "1715381118521251"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1522220400, "status": "PUBLISHED", "id": "846026702266109"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "1778414452287947", "version": "1.3.1"}, "id": "787293968110852", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "1286670651462332", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "1778414452287947"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "787293968110852"}]}, "change_log": null, "created_date": 1543487682, "id": "1778414452287947", "vers