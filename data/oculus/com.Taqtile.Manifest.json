video_trailer": null, "video_trailer_thumbnail": {"file_name": "Taqtile Thumb.png", "id": "7263609417020684", "uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/38974455_7263609423687350_8599335878915417165_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=pPqOD8X9T9sQ7kNvwFf1c2z&_nc_oc=Adm0yogN4hok4z5MrE6g_53nf2fgD1oq6ZFu8MRGAcs_I8ZtFIH7F_teujlPmHl2Px4&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_Afu9cel8NIMuwrTQsfe5OJRTma71lmyNEc1A-sRn0Ea8yw&oe=69832F9A"}, "display_name": "Manifest for Quest", "keywords": ["taqtile", "manifest", "remote", "assist", "work"], "long_description": "Easily capture and harness the knowledge of your experts and most experienced employees\n- Easy and fast start to building augmented reality work instructions with minimal configuration \n- No coding, development, CAD, or special skills needed \n- On-the-job and in-situ authoring and AR content creation on many different devices \n- Support for static and animated 3D models and objects, digital twins, and augmented reality.\nApply expert knowledge to help workers complete jobs and training more consistently and accurately\n- Easy to follow step-by-step instructions augmented with augmented reality technology, 3D objects, videos, photos, digital documents, and real-time IoT sensor data  \n- Worker generated video, audio and text notes for evidence of job completion and accuracy \n- Shared work instructions with job assignments across multiple workers \n- Complex branched, nonlinear workflow guidance\n- Complete fault reporting system to easily flag, resolve, and archive operational and procedural faults for improving auditing and inspections\nRemotely connect, collaborate, and assist\n- Integrated connectivity and collaboration capabilities to enable video chats and file sharing \n- \u201cSee-what-I-see\u201d video streaming and sharing \n- Remote direction and guidance with augmented reality and eye tracking \n- Designed for low bandwidth but works great on the latest 5G networks\nManage, coordinate, and track job performance\n- Scheduling and assigning of jobs across teams and individuals, regardless of location.", "short_description": "Empower the doers:\n\nManifest\u00ae gives frontline workers instant access to step-by-step guidance from your most experienced technicians and trainers anywhere, anytime. Make everyone an expert by putting knowledge right at their fingertips. Device agnostic and scalable, Manifest is an AR work instruction tool that helps train and upskill your workforce, wherever they may be."}]}, "developer_name": "Taqtile, Inc.", "developer_privacy_policy_url": "https://taqtile.com/privacy", "developer_terms_of_service_url": null, "publisher_name": "Taqtile, Inc.", "support_website_url": null, "website_url": "https://taqtile.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1063584910766706"}], "external_subscription_type": "REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH", "HAND_TRACKING"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "24589463890700707"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "1066714432227130"}, {"status": "PUBLISHED", "id": "1559430951538864"}]}, "baseline_offer": {"id": "1066714432227130"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1748553780, "status": "PUBLISHED", "id": "1066714432227130"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "31412035468443481", "version": "4.2.1"}, "id": "395147163016507", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "24589463874034042", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "31412035468443481"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "395147163016507"}]}, "change_log": null, "created_date": 1758832962, "id": "31412035468443481", "version": "4.2.1", "version_code": 20, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": null, "asset_files": {"count": 0}, "debug_symbols": {"count": 0}, "__isAppBinaryWithFileAsset": "AndroidBinary"}, {"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "24589463874034042", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "31412035468443481"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "395147163016507"}]}, "change_log": null, "created_date": 1753907784, "id": "30546586924988344", "version": "4.2.1", "version_code": 19, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": null, "asset_files": {"count": 0}, "debug_symbols": {"count": 0}, "__isAppBinaryWithFileAsset": "AndroidBinary"}, {"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "24589463874034042", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "31412035468443481"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "395147163016507"}]}, "change_log": "New in this Release:\n\nExternal Camera for recording, remote assist and QR code scanning is here!!! You can now use Manifest to record procedures, document inspections, and to call other users all while using the Meta Quest 3 and 3s headsets.\n\nChecklist and Step Marker Design Updates:\n*Redesigned 3D step marker with improved clarity and readability, highlighting evidence requirements and consolidating commonly used features into an expandable options menu\n*Updated drag functionality for repositioning step markers and avoid view obstructions\n*Refreshed 2D checklist design to align with updated step marker and display enhanced step information\n*Increased functionality of checklist steps, enabling users to complete steps, view notes/evidence, reassign, initiate Connect calls, and report faults directly from the Job window\n\nConnect Enhancements:\n*When filtering by an Org, only users with that Org as their primary/top-level are shown.\n*Ability to multi-select Orgs when filtering\n\nAdditional Updates and Enhancements:\n*Improved accuracy of 3d-authored Alignment Actions\n*Various bug fixes for improved user experience", "created_date": 1748552661, "id": "29684701161176929", "version": "4.2.0", "version_code": 17, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": null, "asset_files": {"count": 0}, "debug_symbols": {"count": 0}, "__isAppBinaryWithFileAsset": "AndroidBinary"}, {"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "24589463874034042", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "31412035468443481"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": []}, "change_log": null, "created_date": 1747425752, "id": "29507954305518283", "version": "4.1.226", "version_code": 15, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": null, "asset_files": {"count": 0}, "debug_symbols": {"count": 0}, "__isAppBinaryWithFile