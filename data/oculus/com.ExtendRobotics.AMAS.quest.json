_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=BfD6KeRPuJAQ7kNvwFDmVyu&_nc_oc=AdmZ7c8tx3pOMZI3GJyZCNYIDlq8WB-553YPFR1OHHFO-wt4kiLq5aIn-TaqXJzzrxE&_nc_zt=3&_nc_ht=scontent-ord5-3.oculuscdn.com&oh=00_AfpTlwnbSLp7zFKFS4r5hPh2vpcPgyU4fuEbDqM1nhZZvA&oe=6960EE84"}, {"file_name": "Screenshot (123).png", "id": "441798080794085", "uri": "https://scontent-ord5-2.oculuscdn.com/v/t64.5771-25/39031313_441798084127418_6466023488837283457_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=-T7kd0Xm65YQ7kNvwFBInXJ&_nc_oc=AdlPJNt0n0mxvhVkcd3HtwaZ0Yy0vQhQO-Od0H6H1lUk16b_obWXf9DMqT9u_n9uqHw&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AfpPRTFrnxMr2pPV8XOa0dQ3ojiAGLZbTv2VKf_ZFfZyNw&oe=6960F61C"}, {"file_name": "Screenshot (125).png", "id": "1077642882808314", "uri": "https://scontent-ord5-2.oculuscdn.com/v/t64.5771-25/38982535_1077642886141647_3996924414895117640_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=n9iq8qQv5qcQ7kNvwGZmnM1&_nc_oc=Adk85t8XhCbpQ6L8wnBQ-aHZEY9CgdOy1wUznrGd0l9m4Lw-2hGLpdg-jxXNo5FtwAA&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AfreUBJP70M7pvMZi1X-uUI7dZlBO0lLmJlRw5SlxT4Vjw&oe=6960E768"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Landscape.png", "id": "1789612074582758", "uri": "https://scontent-ord5-2.oculuscdn.com/v/t64.5771-25/39031316_1789612077916091_4846501484232258193_n.png?_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=-pzQj_UGdk4Q7kNvwGAnjDx&_nc_oc=Adks9XJJg-Vri0D3ygTrWOwzO8n3AMqikXOFq4X6sjbTD1FYYbU05JMRauSCrjBQwq8&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AfpH9U-NNnS1ph9Je61wRO-ztQsFbeDRQmvAV8ksrgyavA&oe=69610CB2"}, "display_name": "AMAS AppLab", "keywords": [], "long_description": "Imagine if you could use your own hands to control a robotic arm on the other side of the world. Imagine with the right robotic arm if you could lift up an object larger than a car, or smaller than the tiniest grain of sand, without being physically present.\n\nWelcome to AMAS, the Advanced Mechanics Assistance System. AMAS lets you physically control robotics arms remotely over the internet, using intuitive gestures in an immersive 3D environment.\n\nThe application begins with a tutorial where you learn to control a simulated robotic arm; and record and replay your movements of the robot. When you\u2019ve mastered the simulation, you can then graduate to controlling real robots anywhere in the world.\n\nIf you have a robotic arm that you\u2019d like to connect with, please get in touch with Extend Robotics at (http://www.extendrobotics.com/). (please note that you will need to buy some extra hardware and software to enable tele-control). Extend Robotics are always on hand to offer expert advice.", "short_description": "This is not a game.\n \nApplication for physical robot control using VR with 3D camera input.\n \nExperience above in simulation without hardware components.\n \nFor hardware integration contact Extend Robotics."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.extendrobotics.com/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Extend Robotics", "support_website_url": null, "website_url": "http://www.extendrobotics.com/", "category": "APPS", "comfort_rating": null, "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "514087707868786"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["EDUCATIONAL", "PRODUCTIVITY", "UTILITY"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "7355842087854683"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ord5-3.oculuscdn.com/v/t64.5771-25/38974538_28880162649150