er": null, "video_trailer_thumbnail": {"file_name": "VideoPreview.png", "id": "1558640238437455", "uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/499617850_1558640241770788_373292387739959835_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=YTI9BLI_poIQ7kNvwF2NzGO&_nc_oc=AdnqHJJIJOCZnHAA8O3dZ2gzmiK-p8AhdC_ictnVZz8nBS25VCPiHc_GtHl6jAJL0bM&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_AfrhvTDgyu5Ye5Ep1ncp4nF3QaxWmbQQwVsin4O8ciBugw&oe=69679976"}, "display_name": "AR Planes", "keywords": [], "long_description": "Fly 17 legendary U.S. warplanes around your room in mixed-reality or experience them at actual size!  Fire guns, missiles, and bombs to destroy targets.\n\nAll models are finely-detailed and scaled to 1:48.  They have proportionally-accurate flight characteristics that are carefully tuned for living-room sized gameplay (air-density is thick like water, gravity adjusted, etc.)\n\nPlanes  included:\n\u2022 B-17G Flying Fortress\n\u2022 P-38J Lightning\n\u2022 P-51D Mustang\n\u2022 F4U-1C Corsair\n\u2022 F-86E Sabre\n\u2022 F-4C Phantom II\n\u2022 SR-71A Blackbird\n\u2022 F-14A Tomcat\n\u2022 F-15E Strike Eagle\n\u2022 A-10C Thunderbolt II\n\u2022 F-16C Fighting Falcon\n\u2022 F/A-18F Super Hornet\n\u2022 F-117A Nighthawk\n\u2022 YF-23\n\u2022 F-22 Raptor\n\u2022 F-35A Lightning II\n\u2022 B-21 Raider\n\nDesigned for a single controller:\n\u2022 Button A/X: next plane (long-press for menu)\n\u2022 Button B/Y: toggle weapons-mode (long-press to toggle scale)\n\u2022 Trigger: forward thrust\n\u2022 Thumbstick button: afterburner (if available)\n\u2022 Thumbstick: (model-scale) plane pitch/yaw, (full-scale) move player & snap-turn\n\u2022 Grip: grab plane\n\u2022 You can move/rotate up to twice as fast with a second controller.\n\nDestroyable targets are contained within your physical room if you scan it in Quest Environment setup, otherwise a generic space is used.", "short_description": "Fly 17 famous U.S. warplanes around your room in mixed-reality or explore them at actual size!  Fire guns, missiles, and bombs to destroy targets!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://vroom.space/arplanes/privacy.html", "developer_terms_of_service_url": null, "publisher_name": "VROOM", "support_website_url": null, "website_url": "https://vroom.space/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "459781504968480"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE", "STANDING", "SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "24393325243665274"}}]}, "scheduled_offers": {"count": 5, "nodes": [{"status": "PUBLISHED", "id": "2310162386136030"}, {"status": "PUBLISHED", "id": "1606504597178602"}, {"status": "PUBLISHED", "id": "2063528141151748"}, {"status": "PUBLISHED", "id": "2550252985342833"}, {"status": "PUBLISHED", "id": "1114445443577615"}]}, "baseline_offer": {"id": "2310162386136030"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1767932940, "status": "PUBLISHED", "id": "2310162386136030"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "24735750666089395", "version": "1.04"}, "id": "1741407059854648", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "24347870634877402", "is_concept": f