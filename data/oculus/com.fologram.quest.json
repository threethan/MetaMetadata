boards. \n\nFologram sessions can be shared between multiple headsets to create powerful immersive presentations and experiences. "}]}, "developer_name": null, "developer_privacy_policy_url": "https://fologram.com/privacy#3", "developer_terms_of_service_url": "https://fologram.com/terms", "publisher_name": "Fologram", "support_website_url": null, "website_url": "https://fologram.com/", "category": null, "comfort_rating": null, "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1819021644882647"}, {"id": "1063584910766706"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH", "HAND_TRACKING"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "7839894052757396"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "441571705518798"}]}, "baseline_offer": {"id": "441571705518798"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1722220897, "status": "PUBLISHED", "id": "441571705518798"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "8559281400818654", "version": "2024.3.27"}, "id": "747833139485182", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "3728318473914995", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "8559281400818654"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": 