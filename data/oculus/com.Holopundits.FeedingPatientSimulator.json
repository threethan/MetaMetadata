, {"file_name": "Feeding_Patient_SS_04.png", "id": "496623763435297", "uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/57570298_496623766768630_2581877682244505766_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=CMTecKHxABEQ7kNvwF86WQM&_nc_oc=AdnI5_-qLRdovrL_OERa8-SayBnxeW6VclzdHb_uDfRflMIIz3ZW2WGhCDgRaTCcQQQ&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfFS3B02s59xeIe85XR36G_JxO6Scg4dlIbrHAjtsVzALg&oe=6817523D"}, {"file_name": "Feeding_Patient_SS_05.png", "id": "600572125740865", "uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/38974685_600572129074198_695572474938852958_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=lVnw_wvTx9IQ7kNvwGkrfdm&_nc_oc=AdlvmJxrBKlo9fWfciyua9DUa3BQOEwkUi0l8kCjfX87PhZ9GTNpcFQI3NF8PMPqNVw&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfGb_gr20NVb8f9EEEGunoyQrU_odhEd5b_GdBBKuKexNQ&oe=68175409"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Feeding_Patient_SS_03.png", "id": "1208675347489828", "uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/38974455_1208675350823161_1656817107211530419_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=wclsFl45UsUQ7kNvwHJL7Ob&_nc_oc=AdlWA2WaVTCZA5e7vMt0p9-kfNEjpJB2Owtqw9z0HoGMIlMjAnKOf2gOmnec3MhMffA&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfFtJgCfJtd3oUyg51P8eAYpkBjlWkLsDBDO9EmImCnrdA&oe=68176318"}, "display_name": "Feeding Patient Simulator", "keywords": ["CNA", "nurse", "nursing", "feeding", "healthcare"], "long_description": "The Feeding Patient tutorial immerses Certified Nursing Assistant (CNA) trainees in a realistic virtual environment to practice and master the skills required for properly feeding patients. This application guides users through each step of the process, including preparing food trays, positioning patients for safe eating, and monitoring patients for potential difficulties.  \n\nWith multi-language support (English, Spanish, French, and Arabic), this simulator ensures inclusivity and accessibility for a global audience.  \n\nThe experience is enhanced by an AI companion, acting as a virtual mentor, to answer questions to reinforce learning.\n \n# Key Features: \n- Multi-Language Support: Tailored instructions and narrations in four major languages to cater to diverse learners. \n- AI Companion: An interactive assistant that responds to user queries, and provides clarifications to meet learning needs.(NOTE: Generative AI is still experimental)\n\n# Learning Objectives:  \n- Remembering the protocols for patient feeding \n- Understanding the importance of nutrition and safe feeding practices \n- Applying the correct techniques for assisting patients with eating \n- Analyzing the patient's ability to swallow and eat safely \n- Creating a supportive and respectful environment to enhance the patient's mealtime experience \n\nThis comprehensive approach ensures CNAs are well-prepared to assist patients with feeding efficiently and compassionately in real-world clinical settings. ", "short_description": "Learn how to provide proper feeding assistance through interactive, hands-on practice with AI guidance and multilingual support for better learning."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.holopundits.com/privacy-policy.html", "developer_terms_of_service_url": null, "publisher_name": "Holo Pundits", "support_website_url": null, "website_url": "https://www.holopundits.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "514087707868786"}, {"id": "478089301798119"}, {"id": "1043107547362485"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "ar", "fr", "es_MX"], "support