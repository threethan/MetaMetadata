, but don't fall or touch the lava!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://slindev.com/privacy", "developer_terms_of_service_url": "https://slindev.com/terms", "publisher_name": "SlinDev", "support_website_url": null, "website_url": "https://slindev.com/grab", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "2691890164460465"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["SOCIAL", "EXPLORATION", "ADVENTURE"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER", "CO_OP"], "id": "630351748179695"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "473475945549280"}, {"status": "PUBLISHED", "id": "222566259952082"}]}, "baseline_offer": {"id": "473475945549280"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1723136400, "status": "PUBLISHED", "id": "473475945549280"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "9651955071569277", "version": "0.60.4"}, "id": "1974803902687197", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "4104088956355944", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "9651955071569277"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "19748039