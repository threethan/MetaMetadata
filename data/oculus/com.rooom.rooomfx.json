oc=AdnxW6GS7sjrp2_nABPW0M1cD20azgB32HMr98Xgur4KKJ7FvrCkpb4ruvVwblgYqmw&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_AfJJVqUESK9_aE6wolrOsIeGCFw6CFdEfW0SxWZ3qcxqhA&oe=682093F7"}, {"file_name": "rooomFX_Banner_Screenshots_2560x1440.png", "id": "170735808405685", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/38982695_170735811739018_4414633431748974438_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=ZFkyFt30OC4Q7kNvwFccV6w&_nc_oc=AdnKazzP4fCF-_DpPBeRwvaZAln8Tj1iWBAkl2q24UfxjQT6w3WNMU1yhk_4mALWWzw&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfJRYj-02No4ZbopN_iM4fydDR0CDfh0OuPm9sn6lxc5jw&oe=6820A969"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "rooomFX_Banner_Screenshots_2560x1440.png", "id": "869347497020732", "uri": "https://scontent-sin2-1.oculuscdn.com/v/t64.5771-25/38974448_869347503687398_7666091570050688722_n.png?_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=W6GxlMhpNRoQ7kNvwFXRK3K&_nc_oc=AdmKrNGLspZyPrddV66Mf-nOpoZ6Bb-SsNxnZSbVxk06XxE2_jEgn5F_kazjuWYCRUs&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_AfLB1Pv3lvXQsr6KvKxsfn64BbsmOAdVpkz1UC8znFiLTg&oe=68208CBA"}, "display_name": "rooom", "keywords": [], "long_description": "The award-winning VR platform lets you create engaging environments for virtual showcases. \nUsing rooom, the experience of discovering VR spaces will become even more engaging! Create unique virtual venues for digital live performances, interactive exhibitions, smart learning projects and vivid product presentations. Use our best practice templates or design your own digital experience world \u2013 there are no limits!\nWith rooom you are not restricted by a web browser anymore. Supported by the rooom database and API, you can enjoy your virtual spaces in high-resolution VR quality.\nImmerse yourself into virtual brand worlds, walk through vibrant exhibition halls and marvel at high-resolution artworks and 3D models. Interact with the environment, discover integrated content, and meet other guests using your avatar.\nrooom makes it even possible to present your virtual showcases offline \u2013 an internet connection is not necessary for the virtual experience.", "short_description": "Build and explore digital experience worlds in Virtual Reality with rooom!\nrooom releases you from the limitations of a browser and lets you enjoy high-performing VR spaces. Discover virtual learning environments, interactive showrooms, digital events, real estate showcases and much more. \nFeatures:\n\u2022\tWalk-through environments\n\u2022\tLive reflections\n\u2022\tHigh-quality graphics\n\u2022\tInteractions\n\u2022\tAvatars"}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.iubenda.com/privacy-policy/69710204", "developer_terms_of_service_url": "https://www.rooom.com/sites/termsOfUse", "publisher_name": "rooom AG", "support_website_url": "https://rooom.com/contact", "website_url": "https://rooom.com/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "2687655338230691"}, {"id": "1063584910766706"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": ["EXPERIENCE_360_NON_GAME"], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "de"], "supported_input_devices": ["OCULUS_TOUCH", "GAMEPAD", "HAND_TRACKING"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "1112585766168084"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "946810982625284"}]}, "baseline_offer": {"id": "946810982625284"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1643929200, "status": "PUBLISHED", "id": "946810982625284"}]