_ohc=Jigcm1i1JGIQ7kNvwHB3SO1&_nc_oc=AdmmOiW3xFkKqHIk2Q-uLWttoljCNyflfT-Uw_5TFahQnwozUzdaqCV4m9vzaskTzlk&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_AfhEMi9gmdamDKpXwTak0w87D5wO1Q1mlsx3pPhYYkGAKA&oe=690DD4DB"}, {"file_name": "Master-Builder-ScreenShot_Flaying.png", "id": "321901866117163", "uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/39031453_321901869450496_5543971142046234662_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=mbjLAOjsw18Q7kNvwHJxWVo&_nc_oc=AdngvjHTq79czkD-AdZpBE1yHCWVUg2MG-gcVnXu64U1vi4HuqSrG3Xk6ySRfXJmmuo&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_Afg_0eLrslRcqgz6tLzGH2wl2wn3bacfwcS7HzIS4mUCLw&oe=690DD5F4"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Master-Builder-Asset-LandscapeBig.png", "id": "2783007095332848", "uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/75392880_2783007098666181_7547733992213214953_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=pwfvfTHt6DwQ7kNvwFEN98s&_nc_oc=AdnnxvFifk2HNiFNMFaiJwJDkqfFQ4cC_1qDkjGW9VqYoy8xB7exur1NY_SOq37uxbE&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_AfiNErtPkdT6z6yCv40eZwEyWqbDVxV7Kst_jnB43OnNgw&oe=690DD7A4"}, "display_name": "MasterBuilder", "keywords": [], "long_description": "To become MasterBuilder you need to acquire all the master artifacts that you find in the world. \n\nUnlike many similar VR games available for a standalone device MasterBuilder offers an organic world with a free-form building system and its physicalities. It is built to be played on standalone Oculus Quest devices ( no link needed ), standing or seated.\n\nThe game is available as early access on Quest devices, and we plan to keep adding more features in the coming time. The pre-release game is fully playable with one single open-world island.\n\n1) Building and crafting: Freeform and physical building system allows you to use resources and special tools to build whatever your imagination wants to. \n\n2) Smashing: This allows players to fully enjoy the physics of smashing in one of a kind experience for a standalone VR devices. \n\n3) Adventuring: Customize your avatar, explore the island, find all the artifacts, climb, fly, teleport, and more to ultimately become the MasterBuilder.\n", "short_description": "MasterBuilder is an open-world sandbox build 'n' smash experience for all ages.\n\nBuild anything your imagination can produce and reach new heights. It allows creative thinking, exploration, adventure, and good exercise since it utilizes most of VRs physical capabilities. \n"}]}, "developer_name": null, "developer_privacy_policy_url": "https://masterbuilder.mailchimpsites.com/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "MediaUp", "support_website_url": null, "website_url": "https://masterbuilder.mailchimpsites.com/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1990104051447093"}, {"id": "1725996564472128"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["SIMULATION", "ADVENTURE", "ART_CREATIVITY"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "343048671047180"}}]}, "scheduled_offers": {"count": 22, "nodes": [{"status": "PUBLISHED", "id": "1773851786788642"}, {"status": "PUBLISHED", "id": "412775605179671"}, {"status": "PUBLISHED", "id": "1470964007637778"}, {"status": "PUBLISHED", "id": "738125273901258"}, {"status": "PUBLISHED", "id": "2440626982759675"}, {"status": "PUBLISHED", "id": "3257895777869326"}, {"status": "PUBLISHED", "id": "537457961258765"}, {"status": "PUBLISHED", "