ople !!"}]}, "developer_name": "VR RPG INC.", "developer_privacy_policy_url": "https://www.vrrpginc.com/en/privacy_policy", "developer_terms_of_service_url": null, "publisher_name": "VR RPG INC.", "support_website_url": null, "website_url": "https://www.theknightofqueen.com/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "821934236779161"}, {"id": "2691890164460465"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ADVENTURE", "EXPLORATION", "RPG"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "ja"], "supported_input_devices": ["GAMEPAD"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "1005805633612875"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "849385162572073"}]}, "baseline_offer": {"id": "849385162572073"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1616348400, "status": "PUBLISHED", "id": "849385162572073"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "5748388848520475", "version": "1.2.6"}, "id": "458886025270326", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "5734211583271535", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "5748388848520475"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "458886025270326"}]}, "change_log": null, "created_date": 1612781062, "id": "5748388848520475", "version": "1.