: {"application": {"default_locale": "en_US", "id": "5353996901307344", "is_first_party": false, "enables_in_app_ads": false, "is_concept": false, "platform": "ANDROID_6DOF"}, "iarc_cert": {"id": "218211790137451"}, "iarc_email": null, "msrp": null, "translations": {"nodes": [{"images": {"nodes": [{"file_name": "ChessClub_MixedReality_Landscape [2560x1440].png", "id": "9447336635351332", "image_type": "APP_IMG_COVER_LANDSCAPE", "uri": "https://scontent-sin2-1.oculuscdn.com/v/t64.5771-25/473391924_9447336638684665_1318965815215001784_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=eZ56jC_4Y28Q7kNvwFxnk26&_nc_oc=AdkEHDD_hgtX95NA7Xv5-2COOr-LX6CUiAMf5Y7SL7pBnPocGBSy0D5HwS5Dnx-Zkxg&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_AfVTIEyUi_KeETKXn-5KFgqxZzAqJh6OiOEvR2Oj9XLmnw&oe=68B2F792"}, {"file_name": "01_Logo.png", "id": "5663868400396148", "image_type": "APP_IMG_IMMERSIVE_LAYER_LOGO", "uri": "https://scontent-sin2-2.oculuscdn.com/v/t64.5771-25/39001732_5663868403729481_7821155838146347579_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Ko8QGna5hzgQ7kNvwFpmR3y&_nc_oc=AdnHdrQmjkTf5A3hW5jAoJMjcmTx-KXTe8-coDRp1EYOSHw1pBHra6L4mcIb793L_4Q&_nc_zt=3&_nc_ht=scontent-sin2-2.oculuscdn.com&oh=00_AfUGhLrDshmuZ8wSzyZ4n0S6_g886vD4dD21LJAyye0BQw&oe=68B2D7AD"}, {"file_name": "ChessClub_logo_5500x1440 (1).png", "id": "1872292280180105", "image_type": "APP_IMG_LOGO_TRANSPARENT", "uri": "https://scontent-sin6-2.oculuscdn.com/v/t64.5771-25/473393095_1872292283513438_6185252380382329789_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Id8HFW-vX4YQ7kNvwFqt7o3&_nc_oc=AdmHrpJkQaV41muwFYeMBU8DU6M2b1GU2hr2Fq9WJxSKIj9pP4zjGa9LedKNtxcvo60&_nc_zt=3&_nc_ht=scontent-sin6-2.oculuscdn.com&oh=00_AfWC_fJJdkjWqVzHQJdn9uqYuC5qiVeDfk7tEln3Jqs8ow&oe=68B2F32B"}, {"file_name": "ChessClub_MixedReality_Square [1440x1440].png", "id": "1356382542159281", "image_type": "APP_IMG_COVER_SQUARE", "uri": "https://scontent-sin2-3.oculuscdn.com/v/t64.5771-25/473391959_1356382545492614_8102471634527266667_n.png?_nc