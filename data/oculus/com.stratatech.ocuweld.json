eo_trailer": null, "video_trailer_thumbnail": {"file_name": "TrailerCover Image_2560x1440_2.png", "id": "502160987954598", "uri": "https://scontent-sjc6-1.oculuscdn.com/v/t64.5771-25/38974738_502160991287931_8356783430210980710_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=9QnJxDJqzb8Q7kNvwFo_mmG&_nc_oc=AdnnJQ9gvjBL9WGyiGoJIHtukFlFUtYdlq9XAaocx3s2kvaDFoHWuXb0Ca9Izo46DNk&_nc_zt=3&_nc_ht=scontent-sjc6-1.oculuscdn.com&oh=00_AfpKX6bvHMnU-FkuJAqG7kHbUAO4oMBCyEGgEmhD9StEvw&oe=696397DC"}, "display_name": "OcuWeld", "keywords": [], "long_description": "OcuWeld Provides Training in VR on:\n\u2022\t19 different modules covering TIG, MIG, Stick, Flex-Core and more\n\u2022\tA specific welding process  (e.g. The tools and method used)\n\u2022\tA particular weld position (e.g. A 45-degree pipe weld)\n\u2022\tAnd corresponding type of material (e.g. Stainless steel)\n\nOcuWeld provides enhanced academic benefits for students in three primary ways: \n\u2022\tAccess:  OcuWeld is fully self-contained within a Quest device. Students can access OcuWeld any place, any time, with or without internet connection, if they have their Oculus device with them.\n\u2022\tAutonomy: while OcuWeld will be fully integrated with our curriculum, it provides students the autonomy to drill down into process or positions where they feel the need for additional practice or have a stronger personal interest.\n\u2022\tAmplification: OcuWeld amplifies the student experience by providing supplemental learning outside of the classroom environment, reinforcing the skills and knowledge these students need.", "short_description": "OcuWeld is a new and innovative Virtual Reality (VR) welding simulator designed by expert welding instructors for welders. OcuWeld provides welding students the opportunity to practice hands-on classroom training in a virtual environment, making concerns about safety, access, and cost of materials no longer relevant. OcuWeld fully immerses students in a virtual environment that mimics classroom lab training and provides access to lessons that complement their online and onsite curriculum."}]}, "developer_name": null, "developer_privacy_policy_url": "https://stratatech.com/wp-content/uploads/STEG-POL-70006-3-Global-Privacy-Policy-1.pdf", "developer_terms_of_service_url": null, "publisher_name": "Stratatech", "support_website_url": null, "website_url": "https://stratatech.com/ocuweld", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "514087707868786"}], "external_subscription_type": "REQUIRED", "genres": ["UTILITY"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "1170872266981413"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "503336226125091"}, {"status": "PUBLISHED", "id": "752666329512934"}]}, "baseline_offer": {"id": "503336226125091"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1736947680, "status": "PUBLISHED", "id": "503336226125091"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "32439340605680669", "version": "OcuWeld v1.5.5"}, "id": "971615530116612", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "6904853139556100", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "32439340605680669"}, "release_status": "RELEASED"}, "binary_release_chann