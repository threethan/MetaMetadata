nail": null, "display_name": "Ferris Wheel", "keywords": ["ferris", "wheel", "giantwheel", "park", "joyland"], "long_description": "Step into the immersive world of VR with Ferris Wheel! Take control of your virtual Ferris Wheel ride by setting the speed and duration to your liking. Whether you prefer a slow and serene ride or a fast-paced thrill, the choice is yours. Perfectly crafted for VR, this experience offers stunning visuals, realistic physics, and the joy of soaring high above a picturesque virtual landscape. Ideal for relaxation or fun, Ferris Wheel is your ticket to unforgettable moments in the VR realm. Download now and elevate your VR experience!\n\nNote: This app is not suitable for children under 10 years old. ", "short_description": "Experience the joy of a virtual Ferris Wheel ride! Set your speed, choose your time, and enjoy a thrilling VR adventure.\n\nNote: This app is not suitable for children under 10 years old. "}]}, "developer_name": null, "developer_privacy_policy_url": "https://zaras-organization-6.gitbook.io/ferris-wheel-privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "Little Zara Gaming Co", "support_website_url": null, "website_url": "https://zaras-organization-6.gitbook.io/ferriswheel-game/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}, {"id": "2691890164460465"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8923978697690932"}}]}, "scheduled_offer