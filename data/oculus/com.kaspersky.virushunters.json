0442\u0438 \u0441\u0430\u043c\u043e\u0433\u043e \u0443\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432\u0430!\n\u0415\u0434\u0438\u043d\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u0432\u044b\u0445\u043e\u0434 \u0441\u043f\u0430\u0441\u0442\u0438 \u0441\u043c\u0430\u0440\u0442\u0444\u043e\u043d \u2013 \u043f\u0440\u043e\u043d\u0438\u043a\u043d\u0443\u0442\u044c \u0432 \u043d\u0430\u0441\u0442\u043e\u044f\u0449\u0438\u0439 \u043b\u0430\u0431\u0438\u0440\u0438\u043d\u0442 \u043c\u0438\u043a\u0440\u043e\u0441\u0445\u0435\u043c \u0442\u0432\u043e\u0435\u0433\u043e \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0430 \u0438 \u0441\u0440\u0430\u0437\u0438\u0442\u044c\u0441\u044f \u0441 \u0432\u0438\u0440\u0443\u0441\u0430\u043c\u0438! \u041e\u0449\u0443\u0442\u0438 \u0441\u0435\u0431\u044f \u0441\u043d\u0430\u0439\u043f\u0435\u0440\u043e\u043c, \u043c\u043e\u043d\u0441\u0442\u0440\u044b \u043c\u043e\u0433\u0443\u0442 \u043f\u0440\u044f\u0442\u0430\u0442\u044c\u0441\u044f \u0432 \u043b\u044e\u0431\u043e\u043c \u0443\u0433\u043e\u043b\u043a\u0435 \u0442\u0432\u043e\u0435\u0433\u043e \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0430, \u043d\u0430\u0439\u0434\u0438 \u0438\u0445 \u0432\u0441\u0435\u0445 \u0438 \u0441\u043a\u043e\u0440\u0435\u0435 \u0441\u0442\u0440\u0435\u043b\u044f\u0439!\n\u041e\u0445\u043e\u0442\u043d\u0438\u043a\u0438 \u043d\u0430 \u0432\u0438\u0440\u0443\u0441\u044b \u2013 \u044d\u0442\u043e \u0448\u0443\u0442\u0435\u0440 \u0432 \u0432\u0438\u0440\u0442\u0443\u0430\u043b\u044c\u043d\u043e\u0439 \u0440\u0435\u0430\u043b\u044c\u043d\u043e\u0441\u0442\u0438. \u041f\u043e\u0447\u0443\u0432\u0441\u0442\u0432\u0443\u0439\u0442\u0435 \u0441\u0435\u0431\u044f \u0447\u0430\u0441\u0442\u044c\u044e \u0434\u0440\u0443\u0433\u043e\u0439 \u0432\u0441\u0435\u043b\u0435\u043d\u043d\u043e\u0439!\n1.    \u0427\u0442\u043e\u0431\u044b \u043e\u0442\u043a\u0440\u044b\u0442\u044c \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0435, \u0432\u0441\u0442\u0430\u0432\u044c