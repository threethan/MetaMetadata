_nc_oc=AdlnLSHBVPjGgjV1Fj2K2Aatfp9GeKlmQUnD1tzeNnWlyDgIJ6ZejSXpXbNoRbBcEcg&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfoXWD6v4zI3-GY0ysdd3rB0DS3dj5qZXT52Eb7JJmcL3Q&oe=6964E326"}, {"file_name": "image (5).png", "id": "115885694791570", "uri": "https://scontent-sin11-2.oculuscdn.com/v/t64.5771-25/39035357_115885698124903_7417904670662466120_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=iBQiYlGSkNIQ7kNvwE8eXCn&_nc_oc=Adl9L9k1yYTCdZpQwRQrTuXsdxhyT7ytjt5wNZFtIkCZH9alDjS-34HGerugFd5SKQA&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_Afr7AGnprWq4aiZ0SdmdTwcTbC4vSFWRDbr3qmrLYQHwLg&oe=6964ECCD"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "DownhillBikeLandscape.png", "id": "947940119680763", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/38974727_947940126347429_8874924691088065496_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=WSRqev63fJwQ7kNvwGDz-qd&_nc_oc=AdkCnO5jjJT88Y_KO50zOT2-D0JaTGbzP6IsGyhP0ISN0y44TaPau7tRmrRg4pdZtxo&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfqtnoQn0qDSRq2Ht0cikyPpo1Fsz2nD6PabDG3bD5F3gw&oe=6964EC38"}, "display_name": "Downhill Bike", "keywords": [], "long_description": "Demo Version\n\nExperience the thrill of downhill mountain biking like never before in Downhill Bike, the latest VR game from Nomad Monkey. Get ready to ride through scenic landscapes, tackle challenging terrain, and feel the rush of adrenaline as you race against the clock to reach the finish line. With intuitive controls and immersive graphics, Downhill Bike lets you fully immerse yourself in the world of mountain biking. Take on steep descents, sharp turns, and jumps as you navigate through each level. With multiple levels to unlock and explore, there's always a new challenge waiting for you. Whether you're a seasoned biker or new to the sport, Downhill Bike offers an exciting and realistic experience that will keep you coming back for more. So what are you waiting for? Grab your headset, hop on your virtual bike, and start your ride today!\n\nPlease comment and give us feedback so we can improve the game and implement new worlds for the full version release.", "short_description": "Mountain bike simulation game inside beautiful landscapes and city parkours."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.joygamemobile.com/privacy-policy-vr/", "developer_terms_of_service_url": "https://www.joygamemobile.com/tos/", "publisher_name": "Joygame Oyun ve Teknoloji", "support_website_url": null, "website_url": "https://www.joygame.com/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "258329783237538"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "552171693736935"}]}, "baseline_offer": {"id": "552171693736935"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1683306976, "status": "PUBLISHED", "id": "552171693736935"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "25548129314832783", "version": "4.2"}, "id": "9179338252137152", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "9076937272378581", "is_concept": false, "latest_released_binary": {"__typename": 