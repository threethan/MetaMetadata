ut a framework for leting you experince awesome Trading Card Games such as Grand Archive.\n\nFor your convenience, images can be easily integrated using REST APIs."}]}, "developer_name": null, "developer_privacy_policy_url": "http://www.unicorn-production.com/tcgxr/privacy.html", "developer_terms_of_service_url": null, "publisher_name": "Andreas Mank", "support_website_url": null, "website_url": "http://www.unicorn-production.com/tcgxr/", "category": null, "comfort_rating": null, "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1676485315806614"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER"], "id": "7233178500114095"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "976672750721154"}]}, "baseline_offer": {"id": "976672750721154"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1717620180, "status": "PUBLISHED", "id": "976672750721154"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "9444962458935677", "version": "1.2.45"}, "id": "1754663498390250", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "7233178493447429", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "9444962458935677"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"chan