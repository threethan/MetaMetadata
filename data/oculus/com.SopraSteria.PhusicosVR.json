_nc_zt=3&_nc_ht=scontent-ord5-3.oculuscdn.com&oh=00_Afe14WCXe8ozzJvzjr8x3ItTvYK3u9SzipXMkYqHnT6I2Q&oe=68FA1FD5"}, {"file_name": "PDP_screenshot 3.png", "id": "2890978447814413", "uri": "https://scontent-ord5-2.oculuscdn.com/v/t64.5771-25/75422370_2890978451147746_5354799878650684127_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=bvA3cUmk86MQ7kNvwE0cbEW&_nc_oc=Adkd0n6B1Xw_rIDoFA37946PMlN14mw_autnRALcTv5JSkzxw1VngTL8Uhs67jADYuA&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AfcvBh9og3P57GzIXD_Bji6DpHkhymkpWpNUGm4NYLclWg&oe=68FA22C1"}, {"file_name": "PDP_screenshot 4.png", "id": "925061788301183", "uri": "https://scontent-ord5-2.oculuscdn.com/v/t64.5771-25/75392879_925061794967849_6153618265932835287_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=nIobLK352c4Q7kNvwHPaP2T&_nc_oc=AdmIQnMpv9PyxoJZ6dOqY8DKigEqnZLfxfDX7l8u5KiDVSe41yCEhtowQJpDerXUnIQ&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AffTbHOu8nc-Agg9JgrMFQNzIbbb-yxSyBS76rI25DDdCA&oe=68FA1DED"}, {"file_name": "PDP_screenshot 5.png", "id": "242831764147620", "uri": "https://scontent-ord5-1.oculuscdn.com/v/t64.5771-25/38982658_242831767480953_7709159676039824957_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=wYyP5VsJEgcQ7kNvwGMPM-6&_nc_oc=AdmfB3-1sbSAIO5GBTlzMHjRdR-6HeTVd2RbQSOEKxJbQ94gmNv3m3ILfPZheTf15lE&_nc_zt=3&_nc_ht=scontent-ord5-1.oculuscdn.com&oh=00_AfcmsRtDbDcyKZISaWtM6t8ZR2YxnnGFUy6zz0UV9T2eXQ&oe=68FA0D2E"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Phusicos VR", "keywords": [], "long_description": "PHUSICOS-VR takes you to sites around Europe where various nature-based solutions can be used to mitigate natural hazards, including floods, agricultural run-off, rock falls and avalanches. Nature-based solutions are sustainable, green solutions which increase safety while also promoting biodiversity and provide other co-benefits compared to more traditional civil engineering measures. This is becoming increasingly important in a climate adaptation perspective.\n\nThis game currently shows a diverse range of nature-based solutions in Norway, Italy, France and Spain, and is designed so that additional examples can be added in the future. Our ambition is to educate and inspire our users about nature-based solutions, and to empower politicians, planning authorities and other decision-makers to help make these sustainable solutions a preferred choice for managing natural hazards.", "short_description": "Learn how nature-based solutions are used to mitigate natural hazards at sites around Europe."}]}, "developer_name": "Phusicos", "developer_privacy_policy_url": "https://docs.google.com/document/d/1cDBNKAGQz1AfBMiCEC485hCdsDTJMr4tu4HAP7xKFfY/edit?usp=sharing", "developer_terms_of_service_url": null, "publisher_name": "Phusicos", "support_website_url": null, "website_url": "http://phusicos.eu/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["EDUCATIONAL", "EXPLORATION", "SIMULATION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "996439500958466"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ord5-3.oculuscdn.com/v/t64.5771-25/57568684_149048917223826_7898211479413664086_n.png?stp=dst-png_s360x360&_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=JgBzqtiWOg0Q7kNvwGjNKWk&_nc_oc=AdmlqqfaNwtJ0YGNw7tdggKqAWCV9pGYVLWc1gwJUAyyF0Rdn0qqG79xHcadl3w-kQk&_nc_zt=3&_nc_ht=scontent-ord5-3.oculuscdn.com&oh=00_Afdq81ofYfoF2PmF-uKsBCzYmsvdE8Nh3XkNgFp47CEpMQ&oe=68FA252