c_ohc=7BEHjaWxwVUQ7kNvwEr4GAe&_nc_oc=AdnsMBFNCphrD07GmoNzq2RVFWJaPgLfESudMz4bHjzgWvh3EC4Mktj9gZYgNohsmTM&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfNDj5ZrrNM9OU9Rge189hMKb6KkM8z2Yt2cnR9Hs_20EA&oe=685D2AAE"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Phusicos VR", "keywords": [], "long_description": "PHUSICOS-VR takes you to sites around Europe where various nature-based solutions can be used to mitigate natural hazards, including floods, agricultural run-off, rock falls and avalanches. Nature-based solutions are sustainable, green solutions which increase safety while also promoting biodiversity and provide other co-benefits compared to more traditional civil engineering measures. This is becoming increasingly important in a climate adaptation perspective.\n\nThis game currently shows a diverse range of nature-based solutions in Norway, Italy, France and Spain, and is designed so that additional examples can be added in the future. Our ambition is to educate and inspire our users about nature-based solutions, and to empower politicians, planning authorities and other decision-makers to help make these sustainable solutions a preferred choice for managing natural hazards.", "short_description": "Learn how nature-based solutions are used to mitigate natural hazards at sites around Europe."}]}, "developer_name": "Phusicos", "developer_privacy_policy_url": "https://docs.google.com/document/d/1cDBNKAGQz1AfBMiCEC485hCdsDTJMr4tu4HAP7xKFfY/edit?usp=sharing", "developer_terms_of_service_url": null, "publisher_name": "Phusicos", "support_website_url": null, "website_url": "http://phusicos.eu/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["EDUCATIONAL", "EXPLORATION", "SIMULATION"], "has_in_app_ads": null, "internet_connection": "