_nc_zt=3&_nc_ht=scontent-ord5-3.oculuscdn.com&oh=00_AfYiIFAaFkTEYgGRg3ffFejzUBkB5UEBsaeYuQWWjAoY-g&oe=68C41255"}, {"file_name": "PDP_screenshot 3.png", "id": "2890978447814413", "uri": "https://scontent-ord5-2.oculuscdn.com/v/t64.5771-25/75422370_2890978451147746_5354799878650684127_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=sXKvvYMok-0Q7kNvwFPZIdx&_nc_oc=AdmPd22COJ6HY_Lkjpa7oX41OTZbK0dxJ4nM3djtoYicj_v_0EUI9cZ39BuEK4eBMtI&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_Afa226QVlDq7WrwYzEikFyAzf156ZUNZekSnFu1Iui4Tjw&oe=68C41541"}, {"file_name": "PDP_screenshot 4.png", "id": "925061788301183", "uri": "https://scontent-ord5-2.oculuscdn.com/v/t64.5771-25/75392879_925061794967849_6153618265932835287_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=h4GxK7LeUgkQ7kNvwG3NW7y&_nc_oc=Adn7l98yMrHtsp5nNJOfmkADwhJMBS9OW_Y--eMMmkP3lO3LZouRw6E--EOq7Kh27lQ&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AfYKQVNGJ6k7g6rDb2ZUJWzHOa_naZTfddGkaEm-PEdEMw&oe=68C4106D"}, {"file_name": "PDP_screenshot 5.png", "id": "242831764147620", "uri": "https://scontent-ord5-1.oculuscdn.com/v/t64.5771-25/38982658_242831767480953_7709159676039824957_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=VAF-auScWc4Q7kNvwF7J5ER&_nc_oc=AdmWlGfrsTm7J8-Y6ijb91p6JvCUex9VKU04cTcLeTr7Zlp5-gD8ZT2ZX1CSQy60BUA&_nc_zt=3&_nc_ht=scontent-ord5-1.oculuscdn.com&oh=00_AfasjwTrAUUtNBxE02vntUBfpy0Nc-aTkNQL0eNbhqrv6A&oe=68C3FFAE"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Phusicos VR", "keywords": [], "long_description": "PHUSICOS-VR takes you to sites around Europe where various nature-based solutions can be used to mitigate natural hazards, including floods, agricultural run-off, rock falls and avalanches. Nature-based solutions are sustainable, green solutions which increase safety while also promoting biodiversity and provide other co-benefits compared to more traditional civil engineering measures. This is becoming increasingly important in a climate adaptation perspective.\n\nThis game currently shows a diverse range of nature-based solutions in Norway, Italy, France and Spain, and is designed so that additional examples can be added in the future. Our ambition is to educate and inspire our users about nature-based solutions, and to empower politicians, planning authorities and other decision-makers to help make these sustainable solutions a preferred choice for managing natural hazards.", "short_description": "Learn how nature-based solutions are used to mitigate natural hazards at sites around Europe."}]}, "developer_name": "Phusicos", "developer_privacy_policy_url": "https://docs.google.com/document/d/1cDBNKAGQz1AfBMiCEC485hCdsDTJMr4tu4HAP7xKFfY/edit?usp=sharing", "developer_terms_of_service_url": null, "publisher_name": "Phusicos", "support_website_url": null, "website_url": "http://phusicos.eu/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["EDUCATIONAL", "EXPLORATION", "SIMULATION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "996439500958466"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ord5-3.oculuscdn.com/v/t64.5771-25/57568684_149048917223826_7898211479413664086_n.png?stp=dst-png_s360x360&_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=-1orXEbtzjwQ7kNvwE_uBDq&_nc_oc=AdmQFTcv6ZlB_H_6ZO8FjLNZoCihD7D6Fh4pNU31esrsiX4g8D7-1xaVK4S0KiMO-4U&_nc_zt=3&_nc_ht=scontent-ord5-3.oculuscdn.com&oh=00_AfYRNboOebCMxm34xbfQBhIQDCjPMBeS-dpIjnFmYipP0Q&oe=68C417A