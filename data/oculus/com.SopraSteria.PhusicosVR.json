_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfdWGoLNoMUYyj0WbnGlBxV0Dv_XKt1cr-R4N7WLYH3i9Q&oe=68FF65D5"}, {"file_name": "PDP_screenshot 3.png", "id": "2890978447814413", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/75422370_2890978451147746_5354799878650684127_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=LK3b6nLUVtwQ7kNvwE5ss2w&_nc_oc=Adk9jUnP9rplUQl8ctlLm9qjV9ONl0mhMab3xauy2M9Z5hwmChpVOkUJYm7wpGi03dg&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AffZ8LC3DICf9zo-xwZW0jpXPagJ5X1292abDqCXMeSQBw&oe=68FF68C1"}, {"file_name": "PDP_screenshot 4.png", "id": "925061788301183", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/75392879_925061794967849_6153618265932835287_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=JDM7LmeqfG0Q7kNvwHK7MuU&_nc_oc=AdkzHThJXlXP24VABEi7SIxqNHV0Z3pzDLnWaBN8Ag4HOY_9SeoGgcNlvl-HGNXEXi8&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfchA1uaKtf3YUcZ3iCYhRCh1Oy5y7PlnLDoyXiBq__WlQ&oe=68FF63ED"}, {"file_name": "PDP_screenshot 5.png", "id": "242831764147620", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/38982658_242831767480953_7709159676039824957_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=mGjJ1ewLgOAQ7kNvwHl6oeK&_nc_oc=AdnCC8yORIkVRutEme8cazoyTi9HJQmaQ00TdRopW9lBSmQbXQlK8BIbADP6ZZq2eFg&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_Affwtvq3vSZcREICCuwD8ed5NA97PTYiYGgHvLI0eAgeIQ&oe=68FF532E"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Phusicos VR", "keywords": [], "long_description": "PHUSICOS-VR takes you to sites around Europe where various nature-based solutions can be used to mitigate natural hazards, including floods, agricultural run-off, rock falls and avalanches. Nature-based solutions are sustainable, green solutions which increase safety while also promoting biodiversity and provide other co-benefits compared to more traditional civil engineering measures. This is becoming increasingly important in a climate adaptation perspective.\n\nThis game currently shows a diverse range of nature-based solutions in Norway, Italy, France and Spain, and is designed so that additional examples can be added in the future. Our ambition is to educate and inspire our users about nature-based solutions, and to empower politicians, planning authorities and other decision-makers to help make these sustainable solutions a preferred choice for managing natural hazards.", "short_description": "Learn how nature-based solutions are used to mitigate natural hazards at sites around Europe."}]}, "developer_name": "Phusicos", "developer_privacy_policy_url": "https://docs.google.com/document/d/1cDBNKAGQz1AfBMiCEC485hCdsDTJMr4tu4HAP7xKFfY/edit?usp=sharing", "developer_terms_of_service_url": null, "publisher_name": "Phusicos", "support_website_url": null, "website_url": "http://phusicos.eu/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["EDUCATIONAL", "EXPLORATION", "SIMULATION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "996439500958466"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/57568684_149048917223826_7898211479413664086_n.png?stp=dst-png_s360x360&_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=4ACdxDkPtj4Q7kNvwHyIx5D&_nc_oc=AdmfGbB1gp-Kia8IoU66MKPCE28Ww-vxat7_xGZkJ7vpLkQpsccNC2KSzCZiZ48dla8&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AffW3UAphC055zr70FLdfEGIYfHjivyQspWrhMvySSy-5A&oe=68FF6B2