_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYF7pkalr92vzsvIcpWGQ6VFlGhbvVCiwniI3bEdk4hGOQ&oe=67D02CD5"}, {"file_name": "PDP_screenshot 3.png", "id": "2890978447814413", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/75422370_2890978451147746_5354799878650684127_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=atd46cxxqLQQ7kNvgHlLuR_&_nc_oc=Adj7pamDke9XzbwFkb3iE6gKyarOxZyRHi2joU5UUJwTW7ly1M2_v9TL40aQIe0rDSE&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AYE7Zz4xR6SvedP1awuvup6e_C5f6NGUtg-r12k_-b1hnQ&oe=67D02FC1"}, {"file_name": "PDP_screenshot 4.png", "id": "925061788301183", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/75392879_925061794967849_6153618265932835287_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=MX-GmmJQaZIQ7kNvgEvJfdA&_nc_oc=AdgkZ1V2aJ5c_RicXNL2I32r0wf_z5b6eVt-KG3X1bNwZdqlQYT1GB4Y4EV9BokSYpQ&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYFKdac4GSigDGhcW98Z0Hvr_CQHXoTk07XtSludE2Rq9Q&oe=67D02AED"}, {"file_name": "PDP_screenshot 5.png", "id": "242831764147620", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/38982658_242831767480953_7709159676039824957_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=p8xYpzFSqd0Q7kNvgGNbFiB&_nc_oc=AdhXHsdYVDsdBn0wAihj9-XR042qdwxhgApADWhO6LkQ0xYA9sgCzN0o15EiaWq3gBw&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYHS1H41lHdq5rdlkCnkYLa7FJn_hIf3PHFHjkjm5cKt_Q&oe=67D01A2E"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Phusicos VR", "keywords": [], "long_description": "PHUSICOS-VR takes you to sites around Europe where various nature-based solutions can be used to mitigate natural hazards, including floods, agricultural run-off, rock falls and avalanches. Nature-based solutions are sustainable, green solutions which increase safety while also promoting biodiversity and provide other co-benefits compared to more traditional civil engineering measures. This is becoming increasingly important in a climate adaptation perspective.\n\nThis game currently shows a diverse range of nature-based solutions in Norway, Italy, France and Spain, and is designed so that additional examples can be added in the future. Our ambition is to educate and inspire our users about nature-based solutions, and to empower politicians, planning authorities and other decision-makers to help make these sustainable solutions a preferred choice for managing natural hazards.", "short_description": "Learn how nature-based solutions are used to mitigate natural hazards at sites around Europe."}]}, "developer_name": "Phusicos", "developer_privacy_policy_url": "https://docs.google.com/document/d/1cDBNKAGQz1AfBMiCEC485hCdsDTJMr4tu4HAP7xKFfY/edit?usp=sharing", "developer_terms_of_service_url": null, "publisher_name": "Phusicos", "support_website_url": null, "website_url": "http://phusicos.eu/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["EDUCATIONAL", "EXPLORATION", "SIMULATION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "996439500958466"}, "review_messages": ["", "", "", ""]}]}, "cover_square_image": {"uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/57568684_149048917223826_7898211479413664086_n.png?stp=dst-png_s360x360&_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=2Qajx312V5gQ7kNvgGHxaND&_nc_oc=Adgnsaqh7MQTq_lkqtqr9JsvnQe_PbrtTN6jcu4_4N_ilYqVWlk_flBo71p9f-A1ggo&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYHWGOMQfLb4JW6oAMvhbB9ohqOqIhyVByWNSrr-9W8