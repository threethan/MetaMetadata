_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfZ_3Sl-BoEXogemYut7imqyCEQ1CjJUX_FORxsLWmlD4w&oe=68D14155"}, {"file_name": "PDP_screenshot 3.png", "id": "2890978447814413", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/75422370_2890978451147746_5354799878650684127_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=A5sErnFm3NsQ7kNvwH0Achh&_nc_oc=AdlnR5d6g-MnthluCVeKlbTkUg3tK4vEA0J3y54Sx-heeHc_wpYOx3r8fR67-CqrfOc&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfavFt9KWqRIPVdbvXos6kR4Np4gnIm4ec0F-X3DzbPlsQ&oe=68D14441"}, {"file_name": "PDP_screenshot 4.png", "id": "925061788301183", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/75392879_925061794967849_6153618265932835287_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=7T1xK_kEziAQ7kNvwFO3EsD&_nc_oc=AdkmUtdy2knJzBj8YrgRhe83vTpGHEYKlz_E0fKIT15u_2EcnjHpp0Ni7uC3jIIuWZs&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_Afbi2AKP5yCS8FMR6rbihQVn1VioxGlGJQA17g7dlHPn8w&oe=68D13F6D"}, {"file_name": "PDP_screenshot 5.png", "id": "242831764147620", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/38982658_242831767480953_7709159676039824957_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=XbkC-vJHkTIQ7kNvwEfQAy8&_nc_oc=Adn_fXhiFTATKxUBkQb-Z_cM8ShxNrhd21TbIqQXHgMzYwI3-wvKw2FcxTUpLGsmkhQ&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_Afa-hSvAHG9RR53VMX43FQlT1CpfT98hEgwT5NWgtrgGGA&oe=68D12EAE"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Phusicos VR", "keywords": [], "long_description": "PHUSICOS-VR takes you to sites around Europe where various nature-based solutions can be used to mitigate natural hazards, including floods, agricultural run-off, rock falls and avalanches. Nature-based solutions are sustainable, green solutions which increase safety while also promoting biodiversity and provide other co-benefits compared to more traditional civil engineering measures. This is becoming increasingly important in a climate adaptation perspective.\n\nThis game currently shows a diverse range of nature-based solutions in Norway, Italy, France and Spain, and is designed so that additional examples can be added in the future. Our ambition is to educate and inspire our users about nature-based solutions, and to empower politicians, planning authorities and other decision-makers to help make these sustainable solutions a preferred choice for managing natural hazards.", "short_description": "Learn how nature-based solutions are used to mitigate natural hazards at sites around Europe."}]}, "developer_name": "Phusicos", "developer_privacy_policy_url": "https://docs.google.com/document/d/1cDBNKAGQz1AfBMiCEC485hCdsDTJMr4tu4HAP7xKFfY/edit?usp=sharing", "developer_terms_of_service_url": null, "publisher_name": "Phusicos", "support_website_url": null, "website_url": "http://phusicos.eu/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["EDUCATIONAL", "EXPLORATION", "SIMULATION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "996439500958466"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/57568684_149048917223826_7898211479413664086_n.png?stp=dst-png_s360x360&_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=XCu97i_7L0MQ7kNvwGU2Ren&_nc_oc=AdkPYGQQ53Vo5Kv2Fe_u296O75wS8lQpO1XiLr822_J2Z4J15t469HX99N1Ti07ttJA&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfZEV0zk5AwVBbn9ygwRzm0Y-V-ztnqlH3IpNE9BaJF1mg&oe=68D146A