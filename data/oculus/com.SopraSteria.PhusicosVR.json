_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfnfKzSou--r56pNouaE7v-6rfJF0mWvNoY8VHZ83RUOjg&oe=695668D5"}, {"file_name": "PDP_screenshot 3.png", "id": "2890978447814413", "uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/75422370_2890978451147746_5354799878650684127_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=aNUYIyNBfqgQ7kNvwFQG2mC&_nc_oc=AdmSuNuFdgOv2Kb-PoraP3AKHwyWtbpbbMGGJlAGrnrylE9j1R87uwORmFD0q_StaT8&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfkVGE0rPxA7ZDMfyRa6k34xSs29vm0CoqwYYBNR5gT9gA&oe=69566BC1"}, {"file_name": "PDP_screenshot 4.png", "id": "925061788301183", "uri": "https://scontent-sjc6-1.oculuscdn.com/v/t64.5771-25/75392879_925061794967849_6153618265932835287_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=EEfjlz23o3kQ7kNvwG1xS42&_nc_oc=AdkwoTrd7cASHq8Bpe61UsC4TZTfwUNMW9WGZsVzuuTTbQvtBhUp1NkhhbZVfWtvJAQ&_nc_zt=3&_nc_ht=scontent-sjc6-1.oculuscdn.com&oh=00_AfnpPuIq2OvBpfcCixZqlAjOQvOZlPqC19a6RYQplSQx7A&oe=695666ED"}, {"file_name": "PDP_screenshot 5.png", "id": "242831764147620", "uri": "https://scontent-sjc6-1.oculuscdn.com/v/t64.5771-25/38982658_242831767480953_7709159676039824957_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=4dsGOWa_q1EQ7kNvwG5dKiA&_nc_oc=AdmOB0MIncc11jx2ydFz971IuVz-eb58IRzvtrmTLDdKhb0aMP05q8pyGK_dIPq6IWo&_nc_zt=3&_nc_ht=scontent-sjc6-1.oculuscdn.com&oh=00_AflpbKS_ETSPQI5DHir-hhA6Wc5jqTx8MwjXh8qRD-sfdA&oe=6956562E"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Phusicos VR", "keywords": [], "long_description": "PHUSICOS-VR takes you to sites around Europe where various nature-based solutions can be used to mitigate natural hazards, including floods, agricultural run-off, rock falls and avalanches. Nature-based solutions are sustainable, green solutions which increase safety while also promoting biodiversity and provide other co-benefits compared to more traditional civil engineering measures. This is becoming increasingly important in a climate adaptation perspective.\n\nThis game currently shows a diverse range of nature-based solutions in Norway, Italy, France and Spain, and is designed so that additional examples can be added in the future. Our ambition is to educate and inspire our users about nature-based solutions, and to empower politicians, planning authorities and other decision-makers to help make these sustainable solutions a preferred choice for managing natural hazards.", "short_description": "Learn how nature-based solutions are used to mitigate natural hazards at sites around Europe."}]}, "developer_name": "Phusicos", "developer_privacy_policy_url": "https://docs.google.com/document/d/1cDBNKAGQz1AfBMiCEC485hCdsDTJMr4tu4HAP7xKFfY/edit?usp=sharing", "developer_terms_of_service_url": null, "publisher_name": "Phusicos", "support_website_url": null, "website_url": "http://phusicos.eu/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["EDUCATIONAL", "EXPLORATION", "SIMULATION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "996439500958466"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/57568684_149048917223826_7898211479413664086_n.png?stp=dst-png_s360x360&_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=BJCWKx7oL1AQ7kNvwHQLQYX&_nc_oc=AdkumEMHaVQrZ4jVoWa7LQfveArULnkAcx8BRhq9pnlfmKw1iOQWinTKOotBqP4aPOA&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfkVR2wSFcMjWDUYAt3H7wYNi2vEbX_SsgT8vEoj7c0SQQ&oe=69566E2