iAocWo6g&oe=69871A19"}, {"file_name": "013.BPPV_Screenshot_E.png", "id": "1740274659974648", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/499619805_1740274663307981_5923496579526209083_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=CIfWbtweW_cQ7kNvwEMPtBd&_nc_oc=AdkQnEOq5BZ4k-1kG1d4zEJHp-p_AkJEGFqVdZeQkRzxvzCFj0xJT8M6dqc8Pmd9a3U&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfujkbLD-3oiOWEUDSaOHuNnts4P-WU7A5pI3KN_Iyd3QA&oe=69872C61"}, {"file_name": "012.BPPV_Screenshot_D.png", "id": "1597651274566161", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/499618264_1597651277899494_42633863191051194_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=bIXZKBjjVNkQ7kNvwG8ttjQ&_nc_oc=AdkoYJu0YV6QiOMJv7Us2sbFHZnxZtE-19UIVJF_IxyCvYgkd7MnEcoKhOd1Xix8Yn8&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfvodY_FBashy2nyV2M7GgVTRv7JNunXRrVRiF6NDzfG6g&oe=6987448E"}, {"file_name": "010.BPPV_Screenshot_B.png", "id": "840442151810750", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/499620856_840442155144083_7266284938802113276_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=DLH3xRMdZJIQ7kNvwGXvLe0&_nc_oc=AdnBBX5K7ZPT7H4l3buCoEdnJ6AI8tK0H0JFk2lJXZiwNyX_2U4A7TqjxDbWaf3JohE&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfuUNQ5kOCTgxzKlaMpZHQkJeEyeK1h6fhpD2lVGG_3-cg&oe=698742E8"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "VR-BPPV", "keywords": [], "long_description": "VR-BPPV\ub294 VR \uae30\ubc18\uc758 \uc774\uc11d\uc99d(BPPV: Benign Paroxysmal Positional Vertigo) \uc9c4\ub2e8 \ubc0f \uce58\ub8cc \ud6c8\ub828 \uc2dc\ubbac\ub808\uc774\ud130\ub85c, \ud559\uc0dd\uacfc \uc758\ub8cc\uc778\uc774 \uc2e4\uc81c \ud658\uc790\uc5d0\uac8c \uc5b4\uc9c0\ub7fc\uc744 \uc720\ubc1c\ud558\uc9c0 \uc54a\uace0\ub3c4 \ub2e4\uc591\ud55c \uc774\uc11d\uc99d \ubcc0\ud615\uc5d0 \ub300\ud55c \uac80\uc0ac \ubc0f \uc774\uc11d \uce58\ud658\uc220\uc744 \uccb4\uacc4\uc801\uc73c\ub85c \ud559\uc2b5\ud560 \uc218 \uc788\ub294 \ud658\uacbd\uc744 \uc81c\uacf5\ud569\ub2c8\ub2e4.\n\uc0ac\uc6a9\uc790\ub294 Dix-Hallpike \uac80\uc0ac, Roll \uac80\uc0ac \ub4f1 \uc8fc\uc694 \uc774\ud559\uc801 \uac80\uc0ac\ub97c \uc2dc\ubbac\ub808\uc774\uc158\ud558\uba70, \ud658\uc790\uc758 \uba38\ub9ac \uc6c0\uc9c1\uc784\uacfc \uc790\uc138 \ubcc0\ud654\uc5d0 \ub530\ub77c \uc2e4\uc2dc\uac04\uc73c\ub85c \uc0dd\uc131\ub418\ub294 \uc548\uc9c4(nystagmus) \ubc18\uc751\uc744 \uad00\ucc30\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\n \ub610\ud55c Epley maneuver, Barbecue rotation maneuver \ub4f1 \ub2e4\uc591\ud55c \uc774\uc11d \uce58\ud658\uc220\uc744 \ub2e8\uacc4\ubcc4\ub85c \uc218\ud589\ud558\uba70 \uac01 \ubcc0\ud615(\ud6c4\ubc18\uace0\ub9ac\uad00, \uc218\ud3c9\ubc18\uace0\ub9ac\uad00, \uc804\ubc18\uace0\ub9ac\uad00)\uc5d0 \ub530\ub978 \uc801\uc808\ud55c \uce58\ub8cc \ud504\ub85c\ud1a0\ucf5c\uc744 \uc775\ud790 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\n\ube0c\uc774\uc54c\uc560\ub4dc\uc758 \uc758\ub8cc \uad50\uc721 \uc2dc\ubbac\ub808\uc774\uc158 \uc54c\uace0\ub9ac\uc998\uc740 \uc0ac\uc6a9\uc790\uac00 \uc124\uc815\ud55c \uba38\ub9ac \ud68c\uc804 \uac01\ub3c4, \uc911\ub825 \ubc29\ud5a5 \ubcc0\ud654, \uc774\uc11d\uc758 \uc704\uce58 \ubc0f \uc774\ub3d9 \uacbd\ub85c \ub4f1\uc744 \uae30\ubc18\uc73c\ub85c \uac01 \uc2dc\uc810\uc5d0\uc11c \ub098\ud0c0\ub098\ub294 \uc548\uc9c4\uc758 \ubc29\ud5a5 \ubc0f \uac15\ub3c4\ub97c \ub2a5\ub3d9\uc801\uc73c\ub85c \uc0dd\uc131\ud558\uc5ec \uc2e4\uc81c \ud658\uc790\uc640 \uc720\uc0ac\ud55c \uc784\uc0c1 \ubc18\uc751\uc744 \uc81c\uacf5\ud569\ub2c8\ub2e4.", "short_description": "VR-BPPV\ub294 VR \uae30\ubc18\uc758 \uc774\uc11d\uc99d(BPPV: Benign Paroxysmal Positional Vertigo) \uc9c4\ub2e8 \ubc0f \uce58\ub8cc \ud6c8\ub828 \uc2dc\ubbac\ub808\uc774\ud130\ub85c, \ud559\uc0dd\uacfc \uc758\ub8cc\uc778\uc774 \uc2e4\uc81c \ud658\uc790\uc5d0\uac8c \uc5b4\uc9c0\ub7fc\uc744 \uc720\ubc1c\ud558\uc9c0 \uc54a\uace0\ub3c4 \ub2e4\uc591\ud55c \uc774\uc11d\uc99d \ubcc0\ud615\uc5d0 \ub300\ud55c \uac80\uc0ac \ubc0f \uc774\uc11d \uce58\ud658\u