GEEvYT8g&oe=69677619"}, {"file_name": "013.BPPV_Screenshot_E.png", "id": "1740274659974648", "uri": "https://scontent-atl3-2.oculuscdn.com/v/t64.5771-25/499619805_1740274663307981_5923496579526209083_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=VxHDlBjbuJEQ7kNvwGdP6to&_nc_oc=Adk7y_X5pKV3VjOLnj5HoBbxzVHFpR34L1yR0hmCBXhoYNKZcbgSkvvfVKOWm-gSpZQ&_nc_zt=3&_nc_ht=scontent-atl3-2.oculuscdn.com&oh=00_AfrhWMqLpRN9pPEKkxwwoXQY0i23Z3i6eUuNY40V_uqKQg&oe=69678861"}, {"file_name": "012.BPPV_Screenshot_D.png", "id": "1597651274566161", "uri": "https://scontent-atl3-1.oculuscdn.com/v/t64.5771-25/499618264_1597651277899494_42633863191051194_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Qgmn_y8aXIQQ7kNvwF2bUTI&_nc_oc=AdndDJUG0AqbIYuvDSLkvXrJOgECJzZZ9wCaOiLgZZltJr1TJnlRBmJyZnwo5ELqO0g&_nc_zt=3&_nc_ht=scontent-atl3-1.oculuscdn.com&oh=00_Afq3aVOWCX3iFd6Thny1YxnvIHxBHI7KbVajoMeP6UO98Q&oe=6967A08E"}, {"file_name": "010.BPPV_Screenshot_B.png", "id": "840442151810750", "uri": "https://scontent-atl3-2.oculuscdn.com/v/t64.5771-25/499620856_840442155144083_7266284938802113276_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=9mYCb7sPJZ0Q7kNvwFaO44_&_nc_oc=AdkLiVO9kXdgLTfgmlwT127YYOhy7EtRuI3RMWu29EktrwBx33z9BD9Gofgon6HaGTs&_nc_zt=3&_nc_ht=scontent-atl3-2.oculuscdn.com&oh=00_AfqnNay02T_HUU2RIEAZM0JCJWxr3I4qwlPua7LdBAQ8BQ&oe=69679EE8"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "VR-BPPV", "keywords": [], "long_description": "VR-BPPV\ub294 VR \uae30\ubc18\uc758 \uc774\uc11d\uc99d(BPPV: Benign Paroxysmal Positional Vertigo) \uc9c4\ub2e8 \ubc0f \uce58\ub8cc \ud6c8\ub828 \uc2dc\ubbac\ub808\uc774\ud130\ub85c, \ud559\uc0dd\uacfc \uc758\ub8cc\uc778\uc774 \uc2e4\uc81c \ud658\uc790\uc5d0\uac8c \uc5b4\uc9c0\ub7fc\uc744 \uc720\ubc1c\ud558\uc9c0 \uc54a\uace0\ub3c4 \ub2e4\uc591\ud55c \uc774\uc11d\uc99d \ubcc0\ud615\uc5d0 \ub300\ud55c \uac80\uc0ac \ubc0f \uc774\uc11d \uce58\ud658\uc220\uc744 \uccb4\uacc4\uc801\uc73c\ub85c \ud559\uc2b5\ud560 \uc218 \uc788\ub294 \ud658\uacbd\uc744 \uc81c\uacf5\ud569\ub2c8\ub2e4.\n\uc0ac\uc6a9\uc790\ub294 Dix-Hallpike \uac80\uc0ac, Roll \uac80\uc0ac \ub4f1 \uc8fc\uc694 \uc774\ud559\uc801 \uac80\uc0ac\ub97c \uc2dc\ubbac\ub808\uc774\uc158\ud558\uba70, \ud658\uc790\uc758 \uba38\ub9ac \uc6c0\uc9c1\uc784\uacfc \uc790\uc138 \ubcc0\ud654\uc5d0 \ub530\ub77c \uc2e4\uc2dc\uac04\uc73c\ub85c \uc0dd\uc131\ub418\ub294 \uc548\uc9c4(nystagmus) \ubc18\uc751\uc744 \uad00\ucc30\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\n \ub610\ud55c Epley maneuver, Barbecue rotation maneuver \ub4f1 \ub2e4\uc591\ud55c \uc774\uc11d \uce58\ud658\uc220\uc744 \ub2e8\uacc4\ubcc4\ub85c \uc218\ud589\ud558\uba70 \uac01 \ubcc0\ud615(\ud6c4\ubc18\uace0\ub9ac\uad00, \uc218\ud3c9\ubc18\uace0\ub9ac\uad00, \uc804\ubc18\uace0\ub9ac\uad00)\uc5d0 \ub530\ub978 \uc801\uc808\ud55c \uce58\ub8cc \ud504\ub85c\ud1a0\ucf5c\uc744 \uc775\ud790 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\n\ube0c\uc774\uc54c\uc560\ub4dc\uc758 \uc758\ub8cc \uad50\uc721 \uc2dc\ubbac\ub808\uc774\uc158 \uc54c\uace0\ub9ac\uc998\uc740 \uc0ac\uc6a9\uc790\uac00 \uc124\uc815\ud55c \uba38\ub9ac \ud68c\uc804 \uac01\ub3c4, \uc911\ub825 \ubc29\ud5a5 \ubcc0\ud654, \uc774\uc11d\uc758 \uc704\uce58 \ubc0f \uc774\ub3d9 \uacbd\ub85c \ub4f1\uc744 \uae30\ubc18\uc73c\ub85c \uac01 \uc2dc\uc810\uc5d0\uc11c \ub098\ud0c0\ub098\ub294 \uc548\uc9c4\uc758 \ubc29\ud5a5 \ubc0f \uac15\ub3c4\ub97c \ub2a5\ub3d9\uc801\uc73c\ub85c \uc0dd\uc131\ud558\uc5ec \uc2e4\uc81c \ud658\uc790\uc640 \uc720\uc0ac\ud55c \uc784\uc0c1 \ubc18\uc751\uc744 \uc81c\uacf5\ud569\ub2c8\ub2e4.", "short_description": "VR-BPPV\ub294 VR \uae30\ubc18\uc758 \uc774\uc11d\uc99d(BPPV: Benign Paroxysmal Positional Vertigo) \uc9c4\ub2e8 \ubc0f \uce58\ub8cc \ud6c8\ub828 \uc2dc\ubbac\ub808\uc774\ud130\ub85c, \ud559\uc0dd\uacfc \uc758\ub8cc\uc778\uc774 \uc2e4\uc81c \ud658\uc790\uc5d0\uac8c \uc5b4\uc9c0\ub7fc\uc744 \uc720\ubc1c\ud558\uc9c0 \uc54a\uace0\ub3c4 \ub2e4\uc591\ud55c \uc774\uc11d\uc99d \ubcc0\ud615\uc5d0 \ub300\ud55c \uac80\uc0ac \ubc0f \uc774\uc11d \uce58\ud658\u