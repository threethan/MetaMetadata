ohc=LlwvhFI7xHsQ7kNvwHqRUDk&_nc_oc=AdmhfO4wuXJbh-4aew20woN5P_NH7s181JhMKx0Yt6xLaKelM9j7LF06JGTyyhpDxZM&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfZqW-aBSH0LaX9NPc9eAGcdbZgFXKvOBlBy-8NicT2rtA&oe=68C55717"}, {"file_name": "UB_5.png", "id": "1248579159976670", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/491819300_1248579163310003_8317599004920694681_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=t0hmHZRXveUQ7kNvwFO-9ps&_nc_oc=AdkQ8WvF4kzlZ_KHu3c5Zw-grUdjpddVZeyCnjwaImobTudOCiBkOCyc9GfqeEdqYfM&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfaEsn3vSqU1FjqMmTXqRr8QVj8JCSIvvmdT6DZgQqgg1w&oe=68C54FB5"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "alpha052.png", "id": "1045706654440262", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/499298291_1045706657773595_6910800927080218870_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=I7AhBKSdCicQ7kNvwGRHsgF&_nc_oc=AdkffdN0NVqebdvfS49Zp3l2s8Rm5gSRlsRc3MM7u75rN1BT7MG4sJshVtf7hLsSnC8&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfaXcQylydpMmGWrAJA9xThHhBwig1qMlx898Wx7aA_kFQ&oe=68C579F3"}, "display_name": "UltraBoxing - VR Boxing", "keywords": ["boxing", "fight", "box", "fitness", "combat"], "long_description": "EARLY ACCESS GAME\n\nFight against realistic boxing AI\n\n\u201cRealistic and strategic.\u201d \u2013 rbdinsdale\n\u201cA VR boxing game with smart and challenging AI, unlike any other.\u201d \u2013 alexdt113\n\u201cThis boxing game feels more realistic than others and has huge potential.\u201d \u2013 ashchan\n\u201cIt\u2019s pretty fun already at this stage of development and will only get better.\u201d \u2013 gabkicks\n\nExperience VR boxing like never before!\n\nFight ultra-realistic AI opponents with advanced dodging mechanics and lifelike animations. Choose between VR and mixed reality modes.\n\nJoin Early Access, lock in the early adopter price, and help shape the future of VR boxing!\n\nJoin our Discord community: https://ultraboxingvr.com/discord\n\nNote: If you\u2019re looking for a game like Creed with thumbstick movement and in-game stamina, UltraBoxing may not be for you. UltraBoxing focuses on realism.\n\nUltraBoxing offers a high-intensity workout \u2014 combining authentic boxing with fitness fun.", "short_description": "EARLY ACCESS GAME\n\nExperience VR boxing like never before! \n\nFight ultra-realistic AI opponents with advanced dodging mechanics and lifelike animations. \n\nJoin Early Access, lock in the early adopters\u2019 price, and support the developers in shaping the future of VR boxing!"}]}, "developer_name": "UltraBoxing", "developer_privacy_policy_url": "https://ultraboxingvr.com/privacy", "developer_terms_of_service_url": null, "publisher_name": "UltraBoxing", "support_website_url": "https://ultraboxingvr.com/discord", "website_url": "https://ultraboxingvr.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}, {"id": "331529188132278"}, {"id": "2710427532541308"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "23983310664668836"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/499308490_1771856786754702_520147293607250669_n.png?stp=dst-png_s360x360&_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=iVt3P0QNgMoQ7kNvwGeBdon&_nc_oc=AdkFz6mPjH_hwYYCckywR85g50Urql2EHySTu4uWV8HPKp9tj3yXv_hhCESiLNVekRg&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfalRhUrK0GzHVKXLwhJTvV0Gegyx92ZNdoNz