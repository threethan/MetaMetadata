ohc=pWKwwwy74m4Q7kNvwF6Tns0&_nc_oc=Adkcne9rCyWopjXDIu-Poz-_5s6D1tE_Hqe-aUXAt1OUs1TRNFkL9H3OSfVCjAfArfw&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AfeWDPvlZoXsTimFzpDljXT213XtBb0W2C9x9oMe_R9UkA&oe=6901FC17"}, {"file_name": "UB_5.png", "id": "1248579159976670", "uri": "https://scontent-ord5-1.oculuscdn.com/v/t64.5771-25/491819300_1248579163310003_8317599004920694681_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=dGU0KGaxJGQQ7kNvwH-Sk2s&_nc_oc=AdnjTe72OxSL8t01__dKEK-K54VARJPWLrnnlTyvfblYCtcvW37ttR92hg1R9E81NE8&_nc_zt=3&_nc_ht=scontent-ord5-1.oculuscdn.com&oh=00_AfcZIb5y3-yIs_lbvBQ6VUK6S7O3ok2EBWGl4dYwRHamEQ&oe=6901F4B5"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "alpha052.png", "id": "1045706654440262", "uri": "https://scontent-ord5-1.oculuscdn.com/v/t64.5771-25/499298291_1045706657773595_6910800927080218870_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=f_M6UxdwqroQ7kNvwE8CwS4&_nc_oc=AdkfIY5KlqvHssulXWevSmDrId2FuCPG6nn39IHBxg5U4X2v_PP9L8-34Y6UJrH68pk&_nc_zt=3&_nc_ht=scontent-ord5-1.oculuscdn.com&oh=00_AfflTqNSPvCyEHVvtQODyMbMDgfJyl2Pfkd1N4dM3-kjFA&oe=6901E6B3"}, "display_name": "UltraBoxing - VR Boxing", "keywords": ["boxing", "fight", "box", "fitness", "combat"], "long_description": "EARLY ACCESS GAME\n\nFight against realistic boxing AI\n\n\u201cRealistic and strategic.\u201d \u2013 rbdinsdale\n\u201cA VR boxing game with smart and challenging AI, unlike any other.\u201d \u2013 alexdt113\n\u201cThis boxing game feels more realistic than others and has huge potential.\u201d \u2013 ashchan\n\u201cIt\u2019s pretty fun already at this stage of development and will only get better.\u201d \u2013 gabkicks\n\nExperience VR boxing like never before!\n\nFight ultra-realistic AI opponents with advanced dodging mechanics and lifelike animations. Choose between VR and mixed reality modes.\n\nJoin Early Access, lock in the early adopter price, and help shape the future of VR boxing!\n\nJoin our Discord community: https://ultraboxingvr.com/discord\n\nNote: If you\u2019re looking for a game like Creed with thumbstick movement and in-game stamina, UltraBoxing may not be for you. UltraBoxing focuses on realism.\n\nUltraBoxing offers a high-intensity workout \u2014 combining authentic boxing with fitness fun.", "short_description": "EARLY ACCESS GAME\n\nExperience VR boxing like never before! \n\nFight ultra-realistic AI opponents with advanced dodging mechanics and lifelike animations. \n\nJoin Early Access, lock in the early adopters\u2019 price, and support the developers in shaping the future of VR boxing!"}]}, "developer_name": "UltraBoxing", "developer_privacy_policy_url": "https://ultraboxingvr.com/privacy", "developer_terms_of_service_url": null, "publisher_name": "UltraBoxing", "support_website_url": "https://ultraboxingvr.com/discord", "website_url": "https://ultraboxingvr.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}, {"id": "331529188132278"}, {"id": "2710427532541308"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "23983310664668836"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ord5-3.oculuscdn.com/v/t64.5771-25/499308490_1771856786754702_520147293607250669_n.png?stp=dst-png_s360x360&_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=s6-Dz_gznzEQ7kNvwGHt5ry&_nc_oc=AdkRFRI3qv1IOUg3AczxD7nGtwfkCSpHFDyi_gHPG4GPM3CBpL5KMSDY7F5d5JoyO2s&_nc_zt=3&_nc_ht=scontent-ord5-3.oculuscdn.com&oh=00_Afcky3SvziIyAwHyadPj_2wJ8Dx0xS4QghwOT