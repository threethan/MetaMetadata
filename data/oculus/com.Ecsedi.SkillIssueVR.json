_ht=scontent-lhr8-1.oculuscdn.com&oh=00_AfSPlNk9WEKJJ07GnqILjJeu0N63kmLgMaIyib7JLGlr7A&oe=6880E29A"}, {"file_name": "SIVR_Screenshot_08.png", "id": "1124885855945366", "uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/473391896_1124885859278699_4860992258972160356_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=JDlkzNdn8G8Q7kNvwFXYSkL&_nc_oc=Adn8gghsRbYgVTMJt7FX2FDUG112RAhPhCVWDHH0tdxYa29wtwklDjMH7lwE6GsmZW8&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AfRnb_hzOXBtz3ycBiMOmDHb03E8ABjMtjMTZ3SPSSqYew&oe=6880D968"}, {"file_name": "SIVR_Screenshot_06.png", "id": "1650821652454644", "uri": "https://scontent-lhr8-1.oculuscdn.com/v/t64.5771-25/473393444_1650821655787977_1764233444709482436_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=s8TPp4X2c2kQ7kNvwE3vQ14&_nc_oc=Adk8gxxyhBMMe-kSYeO6G2JJUlNUpcuPT0uqc_AY51lQL-Y6b6NjqBGhysN3TWpxnLw&_nc_zt=3&_nc_ht=scontent-lhr8-1.oculuscdn.com&oh=00_AfQMgSTRvsuh8s7qUGLyDRifDCwhWgWkzbYpbIisCB48QQ&oe=6880BC06"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "SIVR_Icon_1440_Landscape.png", "id": "575793175357082", "uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/473392010_575793178690415_3493516764322829240_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=shyRxpVTlAMQ7kNvwEOPSd0&_nc_oc=AdkNZ1WD4hToNDo52Z3zvTmMAJcNOWb3luOuatUEMcPTrnzmfm9-Bm7XRjfJESJHnHM&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AfSqtZW5rRF_lfhpoJx7nzGpPpgVnyxgvZ1-oPcc4DGAZA&oe=6880C9EE"}, "display_name": "Skill Issue VR", "keywords": ["ball", "puzzle", "physics", "creative", "challenge"], "long_description": "In this game, it's not enough to just aim and throw - you need to think carefully about every move, as the ball's path, speed and interaction with surfaces is determined by realistic physics simulation. Your goal is simple: throw the ball so that it bounces off all objects and hits the target perfectly. But this challenge goes far beyond ordinary skill games, as each track is a unique puzzle that requires logic, strategy and precision.\n\nThe game includes 16 unique maps, a creative mode and you can also create 5 custom maps.\n\nYou are required to use all the objects on the shelves to solve each level.\nEach level contains 10 optional objects that you can use to make your course more exciting. The more complex your course is, the more points your solution will earn.\n\nAre you skilled enough?", "short_description": "Imagine entering a virtual world where physics and skill meet in a fun challenge! The aim of the game is simple, yet incredibly addictive: throw the ball so that it bounces over all the placed objects and finally hits the target. It's up to you to plan the route and execute the perfect throw. Are you skilled enough ?"}]}, "developer_name": "Ecsedi", "developer_privacy_policy_url": "https://sites.google.com/view/skillissuevr/private-policy", "developer_terms_of_service_url": null, "publisher_name": "Ecsedi", "support_website_url": null, "website_url": "https://sites.google.com/view/skillissuevr", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "377978390272490"}, {"id": "331529188132278"}, {"id": "1592192974313489"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8539639959475766"}}]}, "scheduled_offers": {"count": 4, "nodes": [{"status": "PUBLISHED", "id": "1072490774983459"}, {"status": "PUBLISHED", "id": "561414119847604"}, {"status": "PUBLISHED", "id": "578505025190143"}, {"status": "PUBLISHED", "id": "644