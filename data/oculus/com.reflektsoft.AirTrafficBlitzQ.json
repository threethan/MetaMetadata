ocations while avoiding disastrous collisions."}]}, "developer_name": "reflektsoft.com", "developer_privacy_policy_url": "https://reflektsoft.com/atbvr-privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "reflektsoft.com", "support_website_url": null, "website_url": "https://reflektsoft.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "331529188132278"}, {"id": "275629210202852"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8852728318114703"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent.oculuscdn.com/v/t64.5771-25/38982518_1607411583483205_8254580847343259387_n.png?stp=dst-png_s360x360&_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=s_FAtypH6pEQ7kNvgERi-Hk&_nc_oc=AdgYzupnp6c1yTpCm640f203SJPv0cvB5GWKleUOJQG7ATD3YZuneNtyJE2pS8WsfRQ&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AYG-NZ0WBLzhmvW-rB_0F_sNI9_vkPt1P1oDHW6i3GLb4A&oe=67D82073"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Air Traffic BLITZ", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Action", "id": "459781504968480"}, {"topic_name": "Simulation", "id": "331529188132278"}, {"topic_name": "Strategy", "id": "275629210202852"}], "release_date": 1734726720, "lastRevision": {"nodes": [{"id": "416126806555839", "release_type": "SCHEDULED", "administration_status": "APPRO