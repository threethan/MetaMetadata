_oc=AdkKoX1vz3KjbrnxoGm3aOHFJ1WXOtq3PB0OAV_-eiSWXr8c1kdxPUI0XCQgipL_99k&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AfZK8xNNfJT3hONExUuRU-lJaKGtI9I4Gt70D81G8iMXoQ&oe=68E0FC68"}, "display_name": "Freerunner Championships", "keywords": ["parkour", "freerunning", "running", "jumping"], "long_description": "**Freerunner Championships** is an early access parkour-inspired racing game where strategy and precision collide in an exhilarating race through challenging urban landscapes to achieve the fastest time!\n\nEvery move matters, with speed and accuracy being rewarded. Your mission is to find the quickest route to victory!\n\nCompete against international opponents - only one can top the global leaderboard!\n\nFreerunner Championship is a Maze Theory prototype. This project is under continuous development, and we greatly appreciate your feedback.\n\n# Here\u2019s what\u2019s included:\n\n- Fresh New Look \u2013 experience Freerunner like never before with a visual overhaul (still work-in-progress)!\n\n- Tutorial Hub \u2013 whether you're new to Freerunner or looking to hone your skills, the all-new tutorial is here for you.\n\n- In-Game Progression \u2013 start off as a Newbie and work your way up to King by earning CRED and leveling up your FAME.\n\n- Time Trials \u2013 compete against friends and rivals in fast-paced trials to secure top spot on the leaderboard.\n\n- Ribbon Recording \u2013 use ribbon recording to track your performance and see where you can save valuable time.\n\n- Graffiti Tagging \u2013 grab your spray cans from your sling bag and be the first to tag a wall, showcasing your designs to the world (currently using stencils)!\n", "short_description": "Freerunner Championships is an early access parkour-inspired racing game where strategy and precision collide in an exhilarating race through challenging urban landscapes to achieve the fastest time!\n\nEvery move matters, with speed and accuracy being rewarded. Your mission is to find the quickest route to victory!\n\nCompete against international opponents - only one can top the global leaderboard!\n\nFreerunner Championship is a Maze Theory prototype. This project is under continuous development."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.maze-theory.com/legal/#privacy", "developer_terms_of_service_url": null, "publisher_name": "Maze Theory Games Limited ", "support_website_url": null, "website_url": "https://www.maze-theory.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}, {"id": "673385183287495"}, {"id": "1990104051447093"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER", "SINGLE_USER"], "id": "9571995822845504"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "3863926540546018"}]}, "baseline_offer": {"id": "3863926540546018"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1739268000, "status": "PUBLISHED", "id": "3863926540546018"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "24032905059661340", "version": "0.1"}, "id": "450462721225607", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "9321043171274105", "is_concept": false, "latest_released_binary": {"__typename":