ler": null, "video_trailer_thumbnail": {"file_name": "2560x1440 Trailer Cover.png", "id": "473554745711200", "uri": "https://scontent-lhr8-2.oculuscdn.com/v/t64.5771-25/75230040_473554749044533_5871965943647574044_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=iNFTbefEX-oQ7kNvwFeRTJL&_nc_oc=AdnEVigueoXNMdnxz02aA4EpgGh5ePyz3gAw5MOQNHxwZZAJU13csRI1umI6dXfxQfw&_nc_zt=3&_nc_ht=scontent-lhr8-2.oculuscdn.com&oh=00_Afb5hFhnPxY-OWMuuEmn4Q8P2M-cLN7l4acV-uMnPPUV7A&oe=68BEB98E"}, "display_name": "Songbird", "keywords": [], "long_description": "Step into the spotlight and transform your living room into a vibrant stage with **Songbird**! Whether you're a seasoned performer or having fun with friends, this immersive singing experience delivers the thrill of karaoke directly to your home. From the musical minds behind Guitar Hero, immerse yourself in a fantasy world that is the first ever Karaoke game in VR. Songbird is the ultimate choice for music lovers and VR enthusiasts alike.\n\n- **Find Your Voice:** Immerse yourself in the enchanting world of Nestopia. Featuring a variety of stunning stages, each environment is designed to elevate your karaoke performances. Engage with this vibrant world filled with catchables, companions, rhythmic multipliers, dancing and interactive elements to accompany your quest.\n\n- **33 Master Tracks:** With 33 songs at launch, choose from a diverse selection  spanning multiple genres, curated to cater to every musical taste and mood. New song packs (DLC) will soon be available to add to your catalog.\n\n- **Easy Setup:** Simply put on your headset and start singing\u2014the integrated microphone means no additional equipment needed.\nInteractive Gameplay: Sing to bring the world of Nestopia to life as the song progresses and improve your vocal skills along the way as you strive for high scores and earn achievements.\n\n- **Multiple Difficulty Levels:** Challenge yourself with songs offered in three difficulty levels: Easy, Medium, and Virtuoso. Comprehensive phrase training and feedback helps you perfect your skills.", "short_description": "Step into the spotlight and transform your living room into a vibrant stage with **Songbird**! Whether you're a seasoned performer or having fun with friends, this immersive singing experience delivers the thrill of karaoke directly to your home. From the musical minds behind Guitar Hero, immerse yourself in a fantasy world that is the first ever Karaoke game in VR. Songbird is the ultimate choice for music lovers and VR enthusiasts alike."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.playsongbird.com/policy", "developer_terms_of_service_url": null, "publisher_name": "Always Blue Games", "support_website_url": null, "website_url": "https://www.playsongbird.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1672577396871977"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH", "HAND_TRACKING"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9074277109249269"}}]}, "scheduled_offers": {"count": 7, "nodes": [{"status": "PUBLISHED", "id": "574727345297620"}, {"status": "PUBLISHED", "id": "560329496938454"}, {"status": "PUBLISHED", "id": "1187504099682284"}, {"status": "PUBLISHED", "id": "941681004551778"}, {"status": "PUBLISHED", "id": "878081634515830"}, {"status": "PUBLISHED", "id": "440344762442532"}, {"status": "PENDING", "id": "922501615970166"}]}, "baseline_offer": null, "is_approved": true, "firstOffer": {"nodes": [{"start_ti