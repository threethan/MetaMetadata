onance variations, creating a visually appealing scene that can be changed for different interaction patterns each time the user presses the A button. "}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.adindavantklooster.com/privacypolicy.html", "developer_terms_of_service_url": null, "publisher_name": "Adinda van 't Klooster", "support_website_url": "https://www.adindavantklooster.com/contact.html", "website_url": "https://www.adindavantklooster.com/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1672577396871977"}, {"id": "1592192974313489"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "928105881625935"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "812148190574929"}]}, "baseline_offer": {"id": "812148190574929"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1690307151, "status": "PUBLISHED", "id": "812148190574929"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "7492219090880204", "version": "3.0"}, "id": "1008806180500588", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "5775768202525310", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "7492219090880204"}, "release_statu