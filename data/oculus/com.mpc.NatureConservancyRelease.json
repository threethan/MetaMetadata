ideo_trailer_thumbnail": {"file_name": "TNC_MPCVR_Keyart_Landscape.png", "id": "513388785451428", "uri": "https://scontent.oculuscdn.com/v/t64.5771-25/11162700_513388788784761_9083067391008571392_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=OkHlQhKzVzwQ7kNvwEfPJ1t&_nc_oc=Adl1svWioF9sn1rc9cYJHmsPg5eq_mh79wxWTLzXhsswCiMmRgBnSURzxHr4PciZP4E&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_Afheor35-FIs4MxzM036ikS9VfRXthIrPrWFjMji08BPpg&oe=6910882A"}, "display_name": "THIS IS OUR FUTURE, The Nature Conservancy", "keywords": ["Nature", "Tuna", "Fish", "Ocean", "Conservancy"], "long_description": "The numbers are shocking. Overexploitation of the world's fish stocks is growing at an alarming rate, killing vulnerable species like sharks and turtles and robbing the economies of small pacific island nations. But if we can see it, we can save it. The Nature Conservancy, a leading conservation organization, is partnering with the tech community to bring deep learning to the deep seas to transform a multi-billion-dollar tuna industry and ultimately create a world where nature and people thrive. By mounting cameras on fishing boats and advanced machine learning that can track what is getting caught, we can prevent overfishing and help threatened and endangered sea life recover without putting fishermen out of work. Step into an immersive virtual reality art experience as you journey through gallery spaces and engage with poetic sculptures that communicate information through your actions.", "short_description": "The numbers are shocking. Overfishing is growing at an alarming rate, killing vulnerable species and robbing small pacific island nation economies. But if we can see it, we can save it. The Nature Conservancy is working with tech innovators to bring the next frontier for AI: Mounting cameras on fishing boats and teaching computers to better monitor what is caught. Journey through gallery spaces in this art experience as you engage with poetic sculptures that tell the story through your actions."}]}, "developer_name": null, "developer_privacy_policy_url": "http://www.nature.org/about-us/governance/privacy-policy.xml?intc=nature.fnav.governance", "developer_terms_of_service_url": "http://www.nature.org/about-us/governance/terms-of-use/index.htm?intc=nature.fnav.governance", "publisher_name": "The Nature Conservancy", "support_website_url": null, "website_url": "http://www.nature.org/", "category": "ENTERTAINMENT", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": null, "genres": ["CASUAL", "EDUCATIONAL"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": [], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "1374610193054217"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1923900391227202"}]}, "baseline_offer": {"id": "1923900391227202"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1495645200, "status": "PUBLISHED", "id": "1923900391227202"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "1519769304731796", "version": "115"}, "id": "1827954667470445", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "1388274394547955", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "1519769304731796"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1827954667470445"}]}, "change_log": "Manifest updated to exclude Lollipop phones", "