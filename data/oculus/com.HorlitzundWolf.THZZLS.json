o_trailer": null, "video_trailer_thumbnail": {"file_name": "THZZLS_CoverImage_Landscape.png", "id": "170729699115612", "uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/75240102_170729702448945_1401170828143698694_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=RhXap5_a71wQ7kNvwHJHxCO&_nc_oc=AdlLMBqMUZCezgARNUe3W8tgtg1C7WaEjOKYflomXTw7IkGrtz2v_0wg7BJbFSyUIew&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AffAJSNEcmo4EzQINTR4HauyKltiKCmo3d3TfIKkLjk82Q&oe=69034153"}, "display_name": "THZZLS - Treasures Lost In Time", "keywords": [], "long_description": "Our first THZZLE is called \"Der Bazar\", the world's first VR toy theatre puzzle game. \n\nThe Bazar is a VR puzzle game where you have to place furniture and props in the right places in old-fashioned rooms according to an old picture you see. The game is set in 19th century Germany and each room has objects taken from the German lifestyle magazine \"Der Bazar\", published in Berlin.\n\nPlay endless room arrangements!\n\nNo need for controllers here. You have your hands!", "short_description": "Play with old machines containing extraordinary well crafted game objects from the cultural heritage of the world. \n"}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.howo.games/privacypolicy/", "developer_terms_of_service_url": null, "publisher_name": "Horlitz & Wolf", "support_website_url": null, "website_url": "https://www.howo.games/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "726368587961803"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["HAND_TRACKING"], "supported_player_modes": [], "supported_trackin