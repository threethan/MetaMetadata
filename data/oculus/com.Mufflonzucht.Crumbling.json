_oc=AdigchP3kQ7ARIM17ohJ2XpR5gyBmCKrNdPJr0g4XrKDwKmnVFBx0U8znjNCBAl9Yd8&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AYFCCZ6qnyZ3EqdQ1vkgMCV0KuNPZor_TodlPvZuzhsVsg&oe=67DE9CE8"}, {"file_name": "Screenshot_2.png", "id": "271532111232490", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/75262744_271532117899156_2051566399827067609_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=j4R2Dkpa3k4Q7kNvgFCPJ0c&_nc_oc=AdjrkIFsCNJi6VeiCf9tGx4WRfyz75XM-j9SvHOyRpSGNpH9NCq1dhD5PQKw13VZQqI&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AYHLT4JdBi5nWLweDH2PvxL476k0z1ZmRhby2wwHq5bimA&oe=67DE8825"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Crumbling", "keywords": [], "long_description": "Crumbling is a rogue-like action adventure. You fight plastic monsters that are the minions of B\u00f6se Corp., an evil corporation that kidnaps Crumblings, seals them in blister packages and sells them off as merchandize. As you progress through an onslaught of monsters from one meticulously crafted diorama to the next you grow ever stronger. Ultimately you face the end boss, the B\u00f6sewicht Golem.\n\nThe game is played in third person. You control your character directly with your touch controller, as if it was an action figure in your hand. You can even use the native hand-tracking of the Oculus Quest to use your actual hands instead.", "short_description": "Woosh! Bam! Pow! Remember playing with action figures? Put on your VR headset, grab your favorite Crumbling and fight your way through an onslaught of enemies and epic bosses. Rescue and collect all Crumblings. Their fate is in your hand. Quite literally."}]}, "developer_name": null, "developer_privacy_policy_url": "https://crumbling-game.com/privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "Mufflonzucht", "support_website_url": null, "website_url": "https://crumbling-game.com/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context