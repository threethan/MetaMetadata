e in a beautifully designed digital universe."}]}, "developer_name": "Esti Soft", "developer_privacy_policy_url": "https://drive.google.com/file/d/1nRyME7ipZKdK4JPdh6Aza7RPKWnEWmNI/view", "developer_terms_of_service_url": null, "publisher_name": "Esti Soft", "support_website_url": null, "website_url": "https://www.bilgiplatformu.net/post/neon-blocks", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1023770785722696"}, {"id": "275629210202852"}, {"id": "377978390272490"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH", "GAMEPAD"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9071223759632716"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-syd2-1.oculuscdn.com/v/t64.5771-25/38982613_1254706058983991_3129812552310839503_n.png?stp=dst-png_s360x360&_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=xg8D3A7XiNQQ7kNvgHc6RR3&_nc_oc=AdkNj-ABbJ6ib5SULypKQcDYtq2iZg5b20HkTU5P0Ux2zahsBIOVi7u5djBgaX4IFiE&_nc_zt=3&_nc_ht=scontent-syd2-1.oculuscdn.com&oh=00_AYGR7DqCYlr2pENXDOf7GuWnVULf9Yg0XxwcyAohYR5GBA&oe=67E93031"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Neon Blocks", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Hangout", "id": "1023770785722696"}, {"topic_name": "Strategy", "id": "275629210202852"}, {"topic_name": "Puzzle", "id": "377978390272490"}], "release_date": 1734910500, "lastRevision": {"nodes": [{"id"