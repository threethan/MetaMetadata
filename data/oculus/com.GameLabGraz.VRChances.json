d Cook in this Virtual Reality experience."}]}, "developer_name": null, "developer_privacy_policy_url": "https://gamelabgraz.com/projects/vrchances-privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "GameLab Graz", "support_website_url": null, "website_url": "https://gamelabgraz.com/", "category": null, "comfort_rating": null, "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "879473995923081"}, {"id": "394813081514456"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": false, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "de"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "200982339062694"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1932658067179168"}]}, "baseline_offer": {"id": "1932658067179168"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1722412629, "status": "PUBLISHED", "id": "1932658067179168"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "8101044789954756", "version": "0.4.4"}, "id": "1391405385039053", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "6417182281674357", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "8101044789954756"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1391405385039053"}]}, "change_log": null, "created_da