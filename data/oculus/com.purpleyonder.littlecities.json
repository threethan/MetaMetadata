ns": {"nodes": [{"id": "387422848047994"}]}, "id": "550104751851527"}, {"viewer_has_update_access": false, "full_applications": {"nodes": [{"id": "1268071846552963"}]}, "id": "1085354344839186"}, {"viewer_has_update_access": false, "full_applications": {"nodes": [{"id": "863891333657154"}]}, "id": "1605249656462620"}, {"viewer_has_update_access": false, "full_applications": {"nodes": [{"id": "860381734033768"}]}, "id": "263707433973387"}]}}, "firstRevision": {"nodes": [{"application": {"id": "4988470131168299", "release_date": 1652374789, "is_concept": false, "platform": "ANDROID_6DOF", "scheduled_offers": {"count": 39}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "7369299379752017"}, "id": "2973666562876369"}]}}, "binary": null, "live_binary": null, "created_date": 1764169175, "id": "26099608632961142", "is_initial_release": false, "name_with_fallback": "v33", "pdp_metadata_with_fallback": {"version": 13, "application": {"is_concept": false, "latest_released_pdp_metadata": {"id": "26107782365477102"}, "release_status": "RELEASED", "id": "4988470131168299"}, "id": "26107782365477102", "release_status": "RELEASED", "status": "APPROVED"}, "release_date": null, "administration_status": "APPROVED", "is_first": false, "release_status": "RELEASED", "release_type": "SCHEDULED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "4988470131168299", "is_first_party": false, "enables_in_app_ads": false, "is_concept": false, "platform": "ANDROID_6DOF"}, "iarc_cert": {"id": "553551294205371"}, "iarc_email": null, "msrp": null, "translations": {"nodes": [{"images": {"nodes": []}, "id": "24643226632019198", "locale": "ko_KR", "screenshots": [], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Little Cities", "keywords": [], "long_description": "\uc548\ub77d\ud55c \ub3c4\uc2dc \uac74\uc124 \uac8c\uc784\n\n\ud5e4\ub4dc\uc14b\uc744 \uc4f0\uace0 \ub9e4\ub825\uc801\uc774\uace0 \uc548\ub77d\ud55c VR \ub3c4\uc2dc \uac74\uc124 \uc2dc\ubbac\ub808\uc774\uc158 Little Cities\ub85c \ub4e4\uc5b4\uc624\uc138\uc694.\n\ub098\ub9cc\uc758 \uc791\uc740 \ub3c4\uc2dc\ub97c \ub9cc\ub4e4\uace0 \ub3c4\uc2dc\uac00 \uc0b4\uc544 \uc6c0\uc9c1\uc774\ub294 \ubaa8\uc2b5\uc744 \uc9c0\ucf1c\ubcf4\uc138\uc694. \uc8fc\ubbfc\ub4e4\uc774 \uc6c0\uc9c1\uc774\uace0 \uc12c\uc744 \ucc44\uc6b0\uba70 \ub9c8\uc744\uc5d0\uc11c \uac70\ub300\ud55c \ub3c4\uc2dc\ub85c \ucee4\uc9c0\ub294 \uac83\uc744 \uac10\uc0c1\ud558\uc138\uc694. \ubc30\uce58\ub97c \uc124\uacc4\ud558\uace0, \ud3b8\uc758 \uc2dc\uc124\uc744 \uc804\ub7b5\uc801\uc73c\ub85c \uc124\uce58\ud558\uace0, \uc2dc\ubbfc\ub4e4\uc774 \uc0b4 \uc644\ubcbd\ud55c \ub3c4\uc2dc\ub97c \ub9cc\ub4dc\uc138\uc694. \uc775\ud788\uae30 \uc27d\uace0 \ubc1c\uacac\ud560 \uac83\ub3c4 \ub9ce\uc740 Little Cities\ub85c \uc138\uc0c1\uc5d0\uc11c \uac00\uc7a5 \ud070 \ucc3d\uc791\ubb3c\uc744 \ub9cc\ub4e4\uc5b4\ubcf4\uc138\uc694!\n\n\u25cf \uace0\uae09\uc2a4\ub7ec\uc6b4 \uc74c\uc545\uacfc \ub9e4\ub825\uc801\uc778 \uc12c \ubc30\uacbd, \ub192\uc740 \uc811\uadfc\uc131\uc744 \uac16\ucd98 \ub290\uae0b\ud55c VR \uac8c\uc784\uc785\ub2c8\ub2e4.\n\u25cf \ub3c4\ub85c\ub97c \uae54\uace0, \uc9d1\uc744 \uc124\uce58\ud558\uace0, \uc8fc\ubbfc\ub4e4\uc774 \uc774\uc0ac\uc624\ub294 \uac83\uc744 \uc9c0\ucf1c\ubcf4\uc138\uc694. \ub3c4\uc2dc\uac00 \ud589\ubcf5\ud558\uac8c \ubc88\uc601\ud558\ub3c4\ub85d \ub3c4\uc640\uc8fc\uc138\uc694!\n\u25cf \uc549\uc544\uc11c \ub610\ub294 \uc11c\uc11c, \ubc29 \uaddc\ubaa8\ub85c \ud3b8\uc548\ud558\uac8c \ub3c4\uc2dc \uc8fc\ubcc0\uc744 \ub3cc\uba70 \ud48d\uacbd\uc744 \uac10\uc0c1\ud558\uc138\uc694. \ub0b4\uac00 \ub9cc\ub4e0 \uc138\uacc4\uac00 \uc0dd\uba85\uc744 \uc5bb\ub294 \ubaa8\uc2b5\uc744 \uac00\uae4c\uc774\uc11c \uc9c0\ucf1c\ubcf4\uc138\uc694.\n\u25cf \uc9c1\uad00\uc801\uc778 Quest Touch \ucee8\ud2b8\ub864\ub85c \ub3c4\uc2dc\ub97c \ub354 \uc7ac\ubbf8\uc788\uace0 \uc27d\uac8c \ub9cc\ub4e4 \uc218 \uc788\uc2b5\ub2c8\ub2e4. ", 