est. Experience the thrill of navigating through vibrant cosmic environments, bouncing your way through intricate challenges. Utilize fast reflexes, creative strategies, and precise movements to master each level. With stunning visuals, realistic physics, and immersive gameplay, Super Bounce delivers endless excitement. Challenge your skills, achieve high scores, and become the ultimate bouncing ch"}]}, "developer_name": "Esti Soft", "developer_privacy_policy_url": "https://drive.google.com/file/d/1nRyME7ipZKdK4JPdh6Aza7RPKWnEWmNI/view", "developer_terms_of_service_url": null, "publisher_name": "Esti Soft", "support_website_url": null, "website_url": "https://www.bilgiplatformu.net/post/super-bounce", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1023770785722696"}, {"id": "3744040075834813"}, {"id": "528142316460358"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH", "GAMEPAD"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9168494889884860"}, "review_messages": ["", ""]}]}, "cover_square_image": {"uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/39036618_540828752270672_308301282435247488_n.png?stp=dst-png_s360x360&_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=b5n8E3y6J7AQ7kNvgGxuBTq&_nc_oc=Adm4Yg36vBtGFkRv7XV3iRLtftfL--ggrYzQmYrGg3l0guqWler9RqwNUDW1YHcao3g&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYHZefd1ULoaRmLZVrg6wDl1Oaz0GquHb1k46i4xw7JKeQ&oe=67E15E84"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name