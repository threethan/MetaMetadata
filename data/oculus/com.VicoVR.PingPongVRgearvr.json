nsor!"}]}, "developer_name": null, "developer_privacy_policy_url": "http://vicovr.com/", "developer_terms_of_service_url": "http://vicovr.com/", "publisher_name": "3DiVi Company", "support_website_url": null, "website_url": "http://vicovr.com/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": null, "genres": ["ARCADE", "CASUAL", "SPORTS", "SIMULATION", "SOCIAL"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["TOUCHPAD"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER", "SINGLE_USER"], "id": "515432619911801"}}]}, "scheduled_offers": {"count": 0, "nodes": []}, "baseline_offer": null, "is_approved": true, "firstOffer": {"nodes": []}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "2485108374841653", "version": "1.4.17"}, "id": "1643317315977528", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "1459163684102799", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "2485108374841653"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1643317315977528"}]}, "change_log": "Improved physics\nImproved Hero mode", "created_date": 1560168861, "id": "2485108374841653", "version": "1.4.17", "version_code": 1491372, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": null, "asset_files": {"count": 0}, "debug_symbols": {"count": 0}, "__isAppBinaryWi