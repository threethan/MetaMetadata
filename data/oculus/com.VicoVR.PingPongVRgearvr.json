ull, "developer_privacy_policy_url": "http://vicovr.com/", "developer_terms_of_service_url": "http://vicovr.com/", "publisher_name": "3DiVi Company", "support_website_url": null, "website_url": "http://vicovr.com/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": null, "genres": ["ARCADE", "CASUAL", "SPORTS", "SIMULATION", "SOCIAL"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["TOUCHPAD"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER", "SINGLE_USER"], "id": "515432619911801"}}]}, "scheduled_offers": {"count": 0, "nodes": []}, "baseline_offer": null, "is_approved": true, "firstOffer": {"nodes": []}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "2485108374841653", "version": "1.4.17"}, "id": "1643317315977528", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "1459163684102799", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "2485108374841653"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1643317315977528"}]}, "change_log": "Improved physics\nImproved Hero mode", "created_date": 1560168861, "id": "2485108374841653", "version": "1.4.17", "version_code": 1491372, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": null, "asset_files": {"count": 0}, "debug_symbols": {"count": 0}, "__isAppBinaryWithFileAsset": "AndroidBinary"}, {"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "1459163684102799", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "2485108374841653"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1643317315977528"}]}, "change_log": "Updated multiplayer menu", "created_date": 1555670557, "id": "2396685933683898", "version": "1.4.16", "version_code": 1491370, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": null, "asset_files": {"count": 0}, "debug_symbols": {"count": 0}, "__isAppBinaryWithFileAsset": "AndroidBinary"}, {"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "1459163684102799", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "2485108374841653"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1643317315977528"}]}, "change_log": "Having heard your feedback, in this update we have radically redesigned the management. \nMode \"FULL MOVEMENT\" is suitable for fans of RacketFury, this mode provides full control over the racket, but can cause dizziness.\nThe \"DYNAMIC RACKET\" mode is very intuitive and simple, but does not cause dizziness. This mode allows you to feel like a real tennis player.\n\nIn the next updates we will continue to improve the management of the racket, add spins and improve the opponent.", "created_date": 1550578020, "id": "2308963822456110", "version": "1.4.16", "version_code": 1491350, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": null, "asset_files": {"count": 0}, "debug_symbols": {"count": 0}, "__isAppBinaryWithFileAsset": "AndroidBinary"}, {"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "1459163684102799", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "2485108374841653"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": []}, "change_log": null, "created_date": 1550214325, "id": "2301851883167304", "version": "1.4.16", "version_code": 1491348, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": null, "asset_files": {"count": 0}, "debug_symbols": {"count": 0}, "__isAppBinaryWithFileAsset": "AndroidBinary"}, {"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "1459163684102799", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "2485108374841653"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1643317315977528"}]}, "change_log": "In this update, we have improved the physics of the ball, added sets with the system of rules of table tennis, including the first serve on his half of the table. \nWe also fixed a problem with the network code and control of the racket. \n\nIn the next updates we will continue to improve the physics in the game so that you can get the best experience from playing table tennis.", "created_date": 1548157065, "id": "2265519143467245", "version": "1.4.15", "version_code": 1491345, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": null, "asset_files": {"count": 0}, "debug_symbols": {"count": 0}, "__isAppBinaryWithFileAsset": "AndroidBinary"}]}}, "viewer": {"user": null}}}