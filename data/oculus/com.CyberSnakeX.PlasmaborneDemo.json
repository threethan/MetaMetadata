": [{"file_name": "2 SC - World -2.png", "id": "2316946772060862", "uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/491819356_2316946775394195_2054183498549688998_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=iOnK6QdbKj0Q7kNvwFmIe5P&_nc_oc=AdmuwJj04qtPQaSkMtN6q39MLYjYwxnolf48mWbIJkoz0RR-pjaaJrzutcgtjQ1-jfI&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_Afq5rplhsW_Naj67sL-vGZqD-PYQsb7o0fuB0amAGD0kQA&oe=6974C63A"}, {"file_name": "3 SC- upgrade.png", "id": "870905428848160", "uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/491821682_870905432181493_8930183678159322711_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=T94Ss-dA_2EQ7kNvwF-v3aW&_nc_oc=Adkyr4b3Eo5KuYvpWe9QZnFG17APlx73Sr8lAIjjfr-YnP7TGXVF2MHPAQlRz_NNA5c&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_AfqfonWYcwKcmfu98iWt0vHsHEMGP0a9NbwFNOJxGy1GZA&oe=6974B0D1"}, {"file_name": "1 SC - World.png", "id": "3396821733804461", "uri": "https://scontent-atl3-2.oculuscdn.com/v/t64.5771-25/491821115_3396821737137794_340421123404825388_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=P3wlYvja56cQ7kNvwFD4Gt4&_nc_oc=AdkjV68eOVChdEFRmECUYeOj--7ign79CpHsm0GMxXQEMd04gkNuFhXSET7I7GtGfvY&_nc_zt=3&_nc_ht=scontent-atl3-2.oculuscdn.com&oh=00_AfqhpTjjLxfDa1MIRltQZgqPzSGgHeZ_aMAjvhUYzwj8jQ&oe=6974CA8F"}, {"file_name": "4 SC - Rougelike.png", "id": "820505160792330", "uri": "https://scontent-atl3-1.oculuscdn.com/v/t64.5771-25/491821702_820505164125663_7924103931815905257_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=PTM2KgtI9j0Q7kNvwH-YRj6&_nc_oc=Adl2MFq0H0XaMCUTCPDKYz8gr6zCry8A5qgf52Gt43tzrxmIIsf1Of8BppDPUT1oyO0&_nc_zt=3&_nc_ht=scontent-atl3-1.oculuscdn.com&oh=00_AfqKGlLUXqA2KhCzS2f3p6F-MvqFaLnI44JsVC0jX1A3vQ&oe=6974B647"}, {"file_name": "5 SC - Evolve.png", "id": "2249413632247626", "uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/491823802_2249413635580959_4401107030297838154_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=4BN8j85BSHEQ7kNvwGFCLAo&_nc_oc=Adk31iAV_jH-3CE2f2jF_k8SHA87x6Z5TSEA2MyUZPLt3QF7zvBqkCSTnzXxpfhgoww&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_Afrlw43Dn_X_DAjodZTDvCzC8l5T9gafxhGwm4QmjWRG5w&oe=6974C1CE"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Thumbnail.png", "id": "832008162928160", "uri": "https://scontent-atl3-2.oculuscdn.com/v/t64.5771-25/491827567_832008166261493_2325861839012386275_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=SHM0NfnPa9MQ7kNvwEjrnDe&_nc_oc=Adkr2Np-XjyTAJJ-gjBEVMPOhcCuHJl_3sjU9KL1sH63CIb-7bg-_LRUx3fSJeXTq1k&_nc_zt=3&_nc_ht=scontent-atl3-2.oculuscdn.com&oh=00_Afq6cxsWBkv2MJUFh1DeZyHn88AhdhbLNMlvCVVocozfrA&oe=6974B5FB"}, "display_name": "Plasmaborne Demo", "keywords": ["Shooter", "Rougelike", "Boom", "Hell"], "long_description": "# In this demo version, you have a limited amount of weapons and abilities.\n\n# Fast. Fluid. Ferocious\nDescend into the abyss and tear through handcrafted arenas where speed, precision, and raw power rule. Plasmaborne is a high-intensity single-player VR roguelike shooter built for players who crave momentum, mastery, and chaos.\n{1551667349501830}\n# Evolve with Every Death\nFall. Rise. Adapt. Collect upgrades, forge new builds, and return stronger than before. Each failure feeds progress, sharpening your instincts and unlocking new ways to fight.\n{1177653031011467}\n# Dual-Wield Destruction\nBuild your own loadout, each hand with a powerful weapon and layer on passives that reshape your playstyle. Build your kit in the lobby, then unleash it in the arena. Every run becomes a different kind of chaos.\n{1534745057571181}\nFusing tight gunplay, brutal melee, and searing flamethrower chaos into one relentless VR combat experience.\n{1113144070708568}\n# Handcrafted Arenas. Infinite Replay\nEach battleground pushes fresh tactics and reactions. Learn enemy behaviors, chain stylish dashes, and ride perfect momentum through escalating challenges.", "short_description": "Plasmaborne is a fast, high-intensity VR roguelike shooter built for players who thrive on speed and precision. Tear through handcrafted arenas with dual-wielded weapons and powerful passives like swords, flamethrowers, and character augments. Build your loadout in the lobby, teleport-dash through escalating enemies, and unlock upgrades that transform every run. Built for full-motion VR, Plasmaborne delivers heavy recoil, crisp gunfeel, and endlessly replayable combat."}]}, "developer_name": null, "developer_privacy_policy_url": "https://cybersnake-x.com/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Cybersnake-X", "support_website_url": "https://discord.gg/9TX8fvD62e", "website_url": "https://cybersnake-x.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "25330179063317678"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "25085534587784314"}]}, "baseline_offer": {"id": "25085534587784314"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1765994700, "status": "PUBLISHED", "id": "25085534587784314"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "25269504092718509", "version": "1.0.1"}, "id": "1581402739876414", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "_