t_description": "You have been assigned to Space Station 51.  Backed by an arsenal of new weapons, you must test them on your Robotic Dummy.  Use  guns, blades and anything you can get your hands on. Test the weapon- get rewarded.  Simulated stress relief is guaranteed!"}]}, "developer_name": "John Ray", "developer_privacy_policy_url": "https://johnray32875.wixsite.com/johnraygames/space-station-51-privacy", "developer_terms_of_service_url": null, "publisher_name": "John Ray Games", "support_website_url": null, "website_url": "https://johnray32875.wixsite.com/johnraygames", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "331529188132278"}, {"id": "3744040075834813"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["CASUAL", "ARCADE", "SHOOTER"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "7683819988388891"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/39035353_1219864495695404_4872198366473797799_n.png?stp=dst-png_s360x360&_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=zPStm9o76mIQ7kNvwGxggpJ&_nc_oc=AdlggdKQETyXURSX-HEdZFnKAnZonrJp4QvCefGCBOm2OBLCCnkGbQnb6tiLVkEbAfg&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AfYCBVBE5KRNk5dfkKLRpP_4g5ixEUit_Vovt5WvFy6wzA&oe=68C2C126"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Space Station 51", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, 