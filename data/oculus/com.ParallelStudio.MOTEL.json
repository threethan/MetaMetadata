ing presence that you need to keep away from by all means."}]}, "developer_name": null, "developer_privacy_policy_url": null, "developer_terms_of_service_url": null, "publisher_name": "Oculus Studio", "support_website_url": null, "website_url": "http://www.parallel-studio.com/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": null, "genres": ["HORROR", "NARRATIVE", "ADVENTURE", "EXPLORATION", "PUZZLE"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "fr", "zh_CN", "ko", "de", "es_MX"], "supported_input_devices": ["TOUCHPAD", "GAMEPAD"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "655403412250920"}}]}, "scheduled_offers": {"count": 44, "nodes": [{"status": "PUBLISHED", "id": "142594936361902"}, {"status": "PUBLISHED", "id": "639319273177064"}, {"status": "PUBLISHED", "id": "1390271581109019"}, {"status": "PUBLISHED", "id": "637834363592842"}, {"status": "PUBLISHED", "id": "984534198609848"}, {"status": "PUBLISHED", "id": "473163233224149"}, {"status": "PUBLISHED", "id": "378406829474733"}, {"status": "PUBLISHED", "id": "2141371562816617"}, {"status": "PUBLISHED", "id": "1150011318713409"}, {"status": "CANCELLED", "id": "1433389413403294"}, {"status": "PUBLISHED", "id": "1815858598643586"}, {"status": "PUBLISHED", "id": "558548877964392"}, {"status": "CANCELLED", "id": "695627900634605"}, {"status": "PUBLISHED", "id": "2200896633262562"}, {"status": "PUBLISHED", "id": "1029115180531602"}, {"status": "PUBLISHED", "id": "635225576656820"}, {"status": "PUBLISHED", "id": "308213236768212"}, {"status": "CANCELLED", "id": "358674957926485"}, {"status": "CANCELLED", "id": "2014868225414642"}, {"status": "PUBLISHED", "id": "8205282516351