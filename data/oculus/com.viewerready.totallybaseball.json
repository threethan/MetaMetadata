LY BASEBALL is the only VR Baseball game that lets you play the full sport of Baseball!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://viewerready.com/totally-privacy", "developer_terms_of_service_url": null, "publisher_name": "VIEWER READY", "support_website_url": null, "website_url": "https://viewerready.com/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["SPORTS", "SIMULATION", "SOCIAL"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "624207972026092"}}]}, "scheduled_offers": {"count": 62, "nodes": [{"status": "PUBLISHED", "id": "1269677408014352"}, {"status": "PUBLISHED", "id": "521212377451817"}, {"status": "PUBLISHED", "id": "630428216319213"}, {"status": "PUBLISHED", "id": "1854573528409602"}, {"status": "PUBLISHED", "id": "8631836613560727"}, {"status": "PUBLISHED", "id": "1036077981640797"}, {"status": "PUBLISHED", "id": "1220742932431604"}, {"status": "PENDING", "id": "296571469487556"}, {"status": "PENDING", "id": "961796471550181"}, {"status": "PENDING", "id": "183312424450627"}, {"status": "PENDING", "id": "1008448970533291"}, {"status": "PENDING", "id": "1123091258570533"}, {"status": "PENDING", "id": "902717217296883"}, {"status": "PUBLISHED", "id": "3130290743939237"}, {"status": "PUBLISHED", "id": "474769668085161"}, {"status": "PENDING", "id": "1285934379024084"}, {"status": "PENDING", "id": "220995670408363"}, {"status": "PENDING", "id": "1