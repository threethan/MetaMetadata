-class Old Master paintings. The museum includes pieces by Rembrandt, Aelbert Cuyp, Frans Hals and many more Old Masters from the Dutch Golden Age."}]}, "developer_name": "Moyosa Media BV", "developer_privacy_policy_url": "https://www.iubenda.com/privacy-policy/61928640", "developer_terms_of_service_url": "https://termsfeed.com/terms-conditions/051227a0ff69016187322614025d0229", "publisher_name": "Moyosa Media BV", "support_website_url": null, "website_url": "http://kremer.moyosamedia.com/", "category": "APPS", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": "NOT_REQUIRED", "genres": ["ART_CREATIVITY", "DOCUMENTARY_HISTORY", "EXPERIENCE_360_NON_GAME"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": [], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "1130125167752215"}}]}, "scheduled_offers": {"count": 15, "nodes": [{"status": "PUBLISHED", "id": "456129498313477"}, {"status": "PUBLISHED", "id": "742822112759748"}, {"status": "PUBLISHED", "id": "185241422392272"}, {"status": "PUBLISHED", "id": "1120145351519732"}, {"status": "PUBLISHED", "id": "695383574537107"}, {"status": "PUBLISHED", "id": "434398623889729"}, {"status": "PUBLISHED", "id": "732656540502174"}, {"status": "PUBLISHED", "id": "796887054007157"}, {"status": "PUBLISHED", "id": "1281085415414548"}, {"status": "PUBLISHED", "id": "442432563248398"}, {"status": "PUBLISHED", "id": "497234757807614"}, {"status": "PUBLISHED", "id": "422095268616970"}, {"status": "PUBLISHED", "id": "655238618254298"}, {"status": "PUBLISHED", "id": "1915338515227213"}, {"status": "PUBLISHED", "id": "2383383808427507"}]}, "baseline_offer": {"id": "1915338515227213"}, "is_approved": true, "first