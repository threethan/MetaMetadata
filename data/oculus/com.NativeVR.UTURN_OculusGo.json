_ht=scontent-ams4-1.oculuscdn.com&oh=00_Afe9u_vPMuLFrdNKbG4rDwwQmw4Etrri1DyUn0GY7-l3oQ&oe=68EB9556"}, {"file_name": "Screenshot_04.png", "id": "340591443594238", "uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/75240094_340591446927571_5963273121078579791_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=zEhCl36mD34Q7kNvwEyztj8&_nc_oc=AdlY38_ihw9_niwgJV2Aqv3b86RfrFrCXV1T2MlrEr3dx4Nig5niN1EjGGTEIaTtO9g&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_AfczVw5pxgpBO-9eYVr-d0REK0jJy_lf9KdknlZdFjY78Q&oe=68EB995E"}, {"file_name": "Screenshot_04B.png", "id": "3478983458792004", "uri": "https://scontent-ams4-1.oculuscdn.com/v/t64.5771-25/75395600_3478983462125337_720710518849186992_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=onQzHZ3U6sAQ7kNvwHHJ6rz&_nc_oc=AdlZOT2FB5MSL9XEwgLonBA-yEHFkmm9Su9Lh3ZnQFAFvZOGAuzoe75zwP2UtR67zTA&_nc_zt=3&_nc_ht=scontent-ams4-1.oculuscdn.com&oh=00_AffWehU7cpOq8IB2i-O3F31ttDdYV6S0XZnO1CxL3DLJtQ&oe=68EB8FAE"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Trailer_Cover.png", "id": "3212504765439624", "uri": "https://scontent-ams4-1.oculuscdn.com/v/t64.5771-25/39035422_3212504775439623_355045172815088181_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=jUk98aN2eO0Q7kNvwEHn__7&_nc_oc=Adk63glnID9uGwgowwsEntHmBjCPKxrdgkHYnX-odwlxCFbxtwAbAQtQSSte1wGXOJY&_nc_zt=3&_nc_ht=scontent-ams4-1.oculuscdn.com&oh=00_AfcpIg5ymFV2twBrvai9YtLcSsPhXaXJM3GZRzj5yIC3AQ&oe=68EBA770"}, "display_name": "UTURN", "keywords": ["360", "narrative", "comedy", "gender", "unconsciousbias"], "long_description": "A dog-themed dating site goes down due to a data center server interruption that sweeps the nation. It is up to a female developer and her male teammates to get their site up and running again before their CTO, who\u2019s juggling his own problems, has to make a pivotal pitch to VCs the next day.\n\nUTURN innovates by placing the viewers in the middle of two intertwined stories, told from a female or a male character perspective. Viewers can choose what perspective to watch at any time. Only the scene being looked at can be heard through a unique spatial sound design. Whatever they miss changes their perspective on the overall story.\n\nAwards & Festivals:\n* Special Jury Prize Jump into VR Fest 2017\n* Nominated Best Interactive Experience FIVARS 2017 & Best Cinematic Narrative Experience Raindance 2017\n* 2019: Cannes XR, Women Deliver\n* 2018: SATIS, Nuit Blanche, Colcoa, Seattle, Cleveland, Nantucket \n* 2017: VR Days, UN Women, San Francisco, Mill Valley, Tacoma, Oculus Connect, Samsung DC", "short_description": "What happens when a young female coder joins a male-dominated floundering startup that\u2019s deep in an identity crisis?\n\nWith a comedic twist, UTURN is an interactive live-action VR film where you get to experience both sides of the gender divide."}]}, "developer_name": "NativeVR", "developer_privacy_policy_url": "http://www.uturnvr.com/wp-content/uploads/2020/UTURN-PrivacyPolicy.pdf", "developer_terms_of_service_url": null, "publisher_name": "NativeVR", "support_website_url": "http://www.uturnvr.com/", "website_url": "http://www.uturnvr.com/", "category": "ENTERTAINMENT", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": "NOT_REQUIRED", "genres": ["EDUCATIONAL", "EXPERIENCE_360_NON_GAME", "NARRATIVE"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["GEAR_VR_CONTROLLER"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "340781548056650"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/75392234_459127621619330_7352765207449514032_n.png?stp=dst-png_s360x360&_nc_cat=110&ccb=1-7&_nc_sid=6e7a