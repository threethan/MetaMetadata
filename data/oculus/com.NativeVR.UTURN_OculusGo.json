_ht=scontent-atl3-2.oculuscdn.com&oh=00_AfJPQosq9HoDhLXlx31CE3n6_i4MDw5-ggT50HwT8I1o8A&oe=6831B1D6"}, {"file_name": "Screenshot_04.png", "id": "340591443594238", "uri": "https://scontent-atl3-1.oculuscdn.com/v/t64.5771-25/75240094_340591446927571_5963273121078579791_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=N9OgeXp8WUMQ7kNvwE2hN48&_nc_oc=AdnO1s8GZ75I0iGeCaWODFeS-Fe5qqcfkeqc_eKSkaMdsQzvHLRJ6h-uMEgUhdxyozQ&_nc_zt=3&_nc_ht=scontent-atl3-1.oculuscdn.com&oh=00_AfLdvGp-_i797W__tskvqZwh38TJQfgS7YnGriSTk2vcgw&oe=6831B5DE"}, {"file_name": "Screenshot_04B.png", "id": "3478983458792004", "uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/75395600_3478983462125337_720710518849186992_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=dWTs2WpUVe4Q7kNvwFFTiBw&_nc_oc=AdkNPa-4v--uKHOjZ_a2kR7rCnL3g1HSDULAjm8Mu3t5ZvUoW_TF-HEePz_4mjESO0U&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_AfKfvnVX7qQso4r0gdC5O7DlUj-AG-RpZ-MXgYmJzITnQQ&oe=6831AC2E"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Trailer_Cover.png", "id": "3212504765439624", "uri": "https://scontent-atl3-2.oculuscdn.com/v/t64.5771-25/39035422_3212504775439623_355045172815088181_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=CRr5fuGehzgQ7kNvwF0YbmT&_nc_oc=AdnqVk-9oZvqeBkqt0EvmrWG5bkJFbNJz2gAUehIAaWpdN5fkvLHcbacK_xx-C_yxMY&_nc_zt=3&_nc_ht=scontent-atl3-2.oculuscdn.com&oh=00_AfIYgKptwa1spTHQoQeNT4u522r6jm8wzAYcrS3QJtpWBg&oe=6831C3F0"}, "display_name": "UTURN", "keywords": ["360", "narrative", "comedy", "gender", "unconsciousbias"], "long_description": "A dog-themed dating site goes down due to a data center server interruption that sweeps the nation. It is up to a female developer and her male teammates to get their site up and running again before their CTO, who\u2019s juggling his own problems, has to make a pivotal pitch to VCs the next day.\n\nUTURN innovates by placing the viewers in the middle of two intertwined stories, told from a female or a male character perspective. Viewers can choose what perspective to watch at any time. Only the scene being looked at can be heard through a unique spatial sound design. Whatever they miss changes their perspective on the overall story.\n\nAwards & Festivals:\n* Special Jury Prize Jump into VR Fest 2017\n* Nominated Best Interactive Experience FIVARS 2017 & Best Cinematic Narrative Experience Raindance 2017\n* 2019: Cannes XR, Women Deliver\n* 2018: SATIS, Nuit Blanche, Colcoa, Seattle, Cleveland, Nantucket \n* 2017: VR Days, UN Women, San Francisco, Mill Valley, Tacoma, Oculus Connect, Samsung DC", "short_description": "What happens when a young female coder joins a male-dominated floundering startup that\u2019s deep in an identity crisis?\n\nWith a comedic twist, UTURN is an interactive live-action VR film where you get to experience both sides of the gender divide."}]}, "developer_name": "NativeVR", "developer_privacy_policy_url": "http://www.uturnvr.com/wp-content/uploads/2020/UTURN-PrivacyPolicy.pdf", "developer_terms_of_service_url": null, "publisher_name": "NativeVR", "support_website_url": "http://www.uturnvr.com/", "website_url": "http://www.uturnvr.com/", "category": "ENTERTAINMENT", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": "NOT_REQUIRED", "genres": ["EDUCATIONAL", "EXPERIENCE_360_NON_GAME", "NARRATIVE"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["GEAR_VR_CONTROLLER"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "340781548056650"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-atl3-1.oculuscdn.com/v/t64.5771-25/75392234_459127621619330_7352765207449514032_n.png?stp=dst-png_s360x360&_nc_cat=110&ccb=1-7&_nc_sid=6e