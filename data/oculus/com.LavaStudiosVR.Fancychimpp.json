_nc_ohc=y0pTXD78ia0Q7kNvwG_-iQW&_nc_oc=AdkjvczhRPO8NM5V7XaHqe8SyEY7rltLxxnvkJqQhqI_ZtlCAsCd6NjTlVQo0L6BJbM&_nc_zt=3&_nc_ht=scontent-fra5-1.oculuscdn.com&oh=00_AfWxTOaWEc-TFA-jgjHnPdgBYwluFAx2i1C2upjUZCF10g&oe=68A9A399"}, {"file_name": "Add a heading (3).png", "id": "1928397367987265", "uri": "https://scontent-fra5-1.oculuscdn.com/v/t64.5771-25/473392023_1928397371320598_9036436963537282605_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=i9AluSVeUtgQ7kNvwFD1Ips&_nc_oc=AdkjQlLZzcVR0QOqnNiTmB2oVyI34jzeKZo97jLHBy5S8h5Im_UXtOwfmWL4kBHsvSk&_nc_zt=3&_nc_ht=scontent-fra5-1.oculuscdn.com&oh=00_AfXr4e9ntnzxTMQvR5VgWdl3kezYGUd5PLfehB08GGNOMg&oe=68A9B8E6"}, {"file_name": "Untitled design (11).png", "id": "1085590143605990", "uri": "https://scontent-fra3-1.oculuscdn.com/v/t64.5771-25/473392036_1085590146939323_5670802818562527441_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=zXbaaJlDUucQ7kNvwH2Qhro&_nc_oc=Adnxw4EA_X2TdmpvNkUIxc3nT538hlnR0ydlEHD1oULWtOAC1uJRDIGYhcY0ApJZXnc&_nc_zt=3&_nc_ht=scontent-fra3-1.oculuscdn.com&oh=00_AfUbBcCW_i0NfucderrepYOqgnpFDvgZMB_AH5nteziIEg&oe=68A9978A"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Paradise Island VR", "keywords": ["Action", "Fun", "Hangout"], "long_description": "Swing, climb, and leap your way through a tropical VR playground!\n\nWelcome to Paradis Island, a vibrant virtual paradise where movement is everything. Inspired by the exhilarating locomotion of Gorilla Tag, this game throws you into a lush island world where your arms are your only way to move. No buttons. No joysticks. Just pure physicality.\n\nRun on all fours, scale towering cliffs, vault across canyons, and dive into crystal-clear lagoons\u2014all with the freedom of fluid, physics-based motion. Whether you're racing your friends, exploring hidden caves, or pulling off wild stunts on palm-lined beaches, Paradis Island is your ultimate test of agility and creativity.\n\nFeatures:\n\n\ud83c\udf34 Unique arm-based locomotion in an open tropical world\n\n\ud83d\udc12 Multiplayer free-roam and competitive minigames\n\n\ud83e\uddd7 Vertical gameplay\u2014climb trees, cliffs, and ancient ruins\n\n\ud83c\udfdd\ufe0f Discover secrets, challenges, and dynamic environments\n\n\ud83c\udfa8 Customizable avatars and unlockable cosmetics\n\nGet ready to sweat, laugh, and move like never before. Your island adventure begins now!", "short_description": "Paradis Island is a game that uses gorilla loco motion but breaks the goals of vr."}]}, "developer_name": null, "developer_privacy_policy_url": "https://sites.google.com/view/paradise-island-vr/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Lava Studios VR", "support_website_url": null, "website_url": "https://sites.google.com/view/paradise-island-vr/home?authuser=1", "category": null, "comfort_rating": "COMFORTABLE_FOR_FEW", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "2691890164460465"}, {"id": "1023770785722696"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER"], "id": "9542200289148964"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-fra5-2.oculuscdn.com/v/t64.5771-25/473393406_1562353161128536_4880478324504184545_n.png?stp=dst-png_s360x360&_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=9CWWCYD2ROsQ7kNvwHzwOvd&_nc_oc=AdnLEubMyjZmL6QzShLhGBRlcfRUH3XkpS_WpJAGuCrHoUmw6Oj-eDaDOQ9td15GT9E&_nc_zt=3&_nc_ht=scontent-fra5-2.oculuscdn.com&oh=00_AfWphRKWQiIf9nUcfjqC8dA3X4L1LhGzJYuEtyNOWrnvNA&oe=68A9C4C9"}, "is_concept":