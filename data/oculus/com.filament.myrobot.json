_ohc=KU7xdwuGvtYQ7kNvwHNHZYu&_nc_oc=Adl_nD7zRdPBuJAiaRW2DFp1Msc4q9GyQP5RCgb6K37J6GFebIEBGy0ogGnJIVpIisg&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_Afqji0N-REz23R0chTLDR_4vnflfxWCK5yS9GNyADhtK_w&oe=695FB8AB"}, "display_name": "My Robot", "keywords": ["MR", "learning", "sandbox", "creativity", "scifi"], "long_description": "Turn any room into a fully interactive robot workshop with My Robot! Build, customize, and control robots with failproof instructions designed for all ages. Experiment with real robot parts like pistons, gears, motors, and manipulators. Make music, tackle obstacle courses, and fight zombies using the premade robot kits, then graduate to designing your own custom creations in the open-ended Free Build mode.\n\n# Transform Your Space\n\nUse passthrough mode to bring your robot to life in your own home. Map furniture, walls, and floor space so your robot can detect obstacles and respond in real time. Switch up your environment at any moment to explore a thrilling obstacle course, a musical aquatic garden, or a high-stakes zombie invasion.\n\n# Failproof Building\n\nStep-by-step instructions and narrated tutorials guide every move. First-time builders can follow the clear on-screen prompts, while experienced makers can jump ahead and customize each component. Every robot kit is specialized to its environment, so you can focus on building and playing instead of troubleshooting.\n\n# Build Any Robot You Can Imagine\n\nAfter completing the starter kits and themed challenges, you can build anything you can imagine! Combine motors, pistons, claws, cosmetics, and other mechanisms to create a robot that dances, plays instruments, or does anything else your house might need! Save multiple designs and revisit them any time to experiment and expand.\n", "short_description": "Turn any room into a fully interactive robot workshop with My Robot! Build, customize, and control robots with failproof instructions designed for all ages. Experiment with real robot parts like pistons, gears, motors, and manipulators. Make music, tackle obstacle courses, and fight zombies using the premade robot kits, and when you\u2019ve mastered that, build your own custom robots in Free Build mode!\n"}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.filamentgames.com/privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "Filament Games", "support_website_url": "https://www.filamentgames.com/contact", "website_url": "https://www.filamentgames.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1990104051447093"}, {"id": "1043107547362485"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "es", "fr", "ja", "pt", "pl", "ru", "de", "ko", "zh_CN"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "25733952842884736"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "1600791544557326"}, {"status": "PUBLISHED", "id": "1213540784127430"}]}, "baseline_offer": {"id": "1600791544557326"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1760985120, "status": "PUBLISHED", "id": "1600791544557326"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "32904119362534679", "version": "1.0.0"}, "id": "456664333391976", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typen