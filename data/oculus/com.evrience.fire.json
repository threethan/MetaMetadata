p-DEXR69X68aHfAV6imVy2wOewEbLVzwe4&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfmJVSfoq6cIpYsYZCzI0s8ahRKu3jRjif52GtLwlCAU5w&oe=695A57C5"}, "display_name": "Evrience Fire", "keywords": ["Counseling", "Therapy", "Psychology", "Training", "Education"], "long_description": "Evrience Fire offers a revolutionary way to practice therapy and counseling skills through VR. Create fully customized client cases - from help request to age, gender, appearance, voice, and language. Interact naturally with AI-driven clients who respond authentically to your questions and support. Perfect for universities, healthcare institutions, and trainees, Evrience Fire provides a realistic, safe, and fully immersive environment to build confidence and sharpen counseling techniques.\nEvery conversation is truly dynamic.", "short_description": "Generate fully custom client cases, including the client\u2019s type of help request, age, gender, appearance, voice, and any language. Interact naturally, and build experience in a safe, immersive environment to train with your AI client. Evrience Fire brings therapy education to life."}]}, "developer_name": null, "developer_privacy_policy_url": "https://evrience.com/evrience-fire-privacy-policy", "developer_terms_of_service_url": "https://evrience.com/evrience-fire-terms-of-service/", "publisher_name": "Evrience", "support_website_url": null, "website_url": "https://evrience.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1043107547362485"}, {"id": "514087707868786"}], "external_subscription_type": "REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes"