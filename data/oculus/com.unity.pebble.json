nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_Afqh5TV2lI9wqQZa0tb7ezmO8ag7gU-QVwzWvlxKQ4Hc1A&oe=6960DB29"}, "display_name": "Pebble", "keywords": ["sandbox", "build", "survival", "island", "base"], "long_description": "Pebble is a mixed reality hand tracked sandbox survival game where you play as a tiny pebble with god like powers. In a world that responds to your touch, you can shape terrain, move objects, and manipulate your surroundings using only your hands. Though you are small, threats still exist as strange enemies roam the land, and survival means staying alert and using your abilities wisely. Explore, build, and reshape the world around you as a powerful force in a miniature ever changing landscape.", "short_description": "Pebble is a mixed reality, hand-tracked sandbox survival game where you play as the smallest thing with the greatest power. Live and survive as a humble pebble in a vast, mysterious world, but wield the abilities of a god. Shape the environment, control the elements, and forge your own miniature universe. In Pebble, creation and survival go hand-in-hand."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.doubtnot.org/privacypolicy", "developer_terms_of_service_url": null, "publisher_name": "Doubt Not", "support_website_url": null, "website_url": "https://www.doubtnot.org/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1990104051447093"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["HAND_TRACKING"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"