P&_nc_oc=Adk_AzHoKs2L8hEHFCpyHKnHFofCNqcPzJn9x4-oCAsnnRnlFjH65vc7Q3_o5hNcHxs&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfmlAOAK6gvUzMe1vEqUUaW4FHQcjpc5jevqObmAfsvczg&oe=69395807"}, {"file_name": "NPcbuild Screenshot 2021.02.25 - 15.01.47.82.png", "id": "4012435722207959", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/39031322_4012435732207958_3100689301828259796_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=QMwaq3rxeE4Q7kNvwFNjSRs&_nc_oc=Admfon--KpmUEFFKuRDA9yGCYe_FufYIi7WTx1235buWgNcVwNubCUlfwbIVK3cd0yk&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_Afl5tyHM2SE-fauxlvvFCCBPu66jFEIzVQBBRl_8BokFIw&oe=69397377"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "2560x1440.png", "id": "186704189490343", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/75462206_186704199490342_8127873361575264646_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=6NbJX_KYbZ4Q7kNvwERRa2H&_nc_oc=AdlqWBL9lspoR_Asr1ZBnpsBWLF1MDnHdPMPDSUWDUk9fu9lvaDrZ9Y7-234loR2Vtg&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfmW1tYyf7-U5NiciW49vRz3B2LGg0pNqD4LgJh5x4VDKA&oe=69396377"}, "display_name": "Can You Do It - VR Experience", "keywords": [], "long_description": "Can You Do It is the epic experience of Virtual Reality Technology.\n(Supports 90Hz/120Hz on Oculus Quest 2)\nExperience and play with hang gliding, city crossing, shooting, ladder climbing, zip-lining, Elevator failure, and many more coming soon in an update.\n\nCan you do it? If Yes then don't release the grab button during play else you might fall down.\n\nPlay in free mode and time challenge mode.\n\n+++ Features +++\n= Gun Hunt Mode\n= Hand Gliding\n= Dual Rope Crossing\n= Zip Lining\n= Elevator Failure\n= Ladder Climbing\n\nExperience city from the top of the sky by doing Hang Gliding, Feel real - Climb the ladder and cross the road with rope, shooting, like a real experience.\n\nExperience the power of virtual reality.\n\n(To enable 120Hz, open the universal menu, go to Settings > Experimental Features, and then enable 120 Hz, Software version 28 required)", "short_description": "Climbing, Flying, Shooting, Falling, Handling and more fun"}]}, "developer_name": "Thunder Game Development Studio", "developer_privacy_policy_url": "https://www.thundergamestudio.com/privacy_policy.html", "developer_terms_of_service_url": "https://www.thundergamestudio.com/privacy_policy.html", "publisher_name": "Thunder Game Development Studio", "support_website_url": null, "website_url": "https://www.thundergamestudio.com/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "3744040075834813"}, {"id": "2691890164460465"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ADVENTURE", "SHOOTER", "ACTION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "en_US", "en_AU", "en_CA", "en_ZA", "en_GB"], "supported_input_devices": ["GAMEPAD_VIA_TOUCH", "OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "1135453547233463"}}]}, "scheduled_offers": {"count": 6, "nodes": [{"status": "PUBLISHED", "id": "1350184553483665"}, {"status": "PUBLISHED", "id": "2586747508356122"}, {"status": "PUBLISHED", "id": "3345054775637021"}, {"status": "PUBLISHED", "id": "1667163753967808"}, {"status": "PUBLISHED", "id": "316764996665110"}, {"status": "PUBLISHED", "id": "192325942751069"}]}, "baseline_offer": {"id": "1667163753967808"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1765821600, "status": "PUBLISHED", "id": "1350184553483665"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_res