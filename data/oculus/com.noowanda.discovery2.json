s looking breathtaking."}]}, "developer_name": null, "developer_privacy_policy_url": "https://noowanda.com/privacypolicies/discovery2-privacypolicy.html", "developer_terms_of_service_url": null, "publisher_name": "noowanda", "support_website_url": null, "website_url": "https://www.noowanda.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1725996564472128"}, {"id": "1676485315806614"}, {"id": "1990104051447093"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH", "HAND_TRACKING"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8734464593277828"}}]}, "scheduled_offers": {"count": 8, "nodes": [{"status": "PUBLISHED", "id": "829791966720861"}, {"status": "PUBLISHED", "id": "25079321365082534"}, {"status": "PUBLISHED", "id": "1479999426380350"}, {"status": "PUBLISHED", "id": "1427229888501532"}, {"status": "PUBLISHED", "id": "1466481911181539"}, {"status": "PUBLISHED", "id": "1293250122472262"}, {"status": "PUBLISHED", "id": "1298475784824052"}, {"status": "PUBLISHED", "id": "1117253239741736"}]}, "baseline_offer": {"id": "1117253239741736"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1771005600, "status": "PUBLISHED", "id": "829791966720861"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "25090820503882311", "version": "0.2.6"}, "id": "1295276635229464", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "An