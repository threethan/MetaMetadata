_ohc=613f5VCDURMQ7kNvwGYDYet&_nc_oc=AdlOAtxzx9wl-dDbNzdVVKgDJ7fP9Giz7WM2Q24Z2ABTcTp9795sVuVHe3CVS1cm_mo&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfctmWrxjd_POBB9Dhfpe-z_wkZ9eVNC8vkXG2ciziYhVg&oe=68F24812"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "38982658_535757437737335_5271675859752528110_n.png", "id": "816059002445368", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/75210893_816059005778701_1392764768497568274_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=ozcg5sTl7a8Q7kNvwE31-8C&_nc_oc=Adn-CWGt13-7MqOoDpW2wbm4hXMv4ycO8YBjYW4b7pu1PrH_gBJwrz16L73za6deGvs&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_Afd8rKAJds5NEz7YHzYJ_oA7vC_1ZwZNgeIdAqp2A9_T7g&oe=68F23DD3"}, "display_name": "The Secret of Retropolis Demo", "keywords": [], "long_description": "The Secret of Retropolis is a VR narrative game of a new kind, a blend of classic point-and-click adventure with immersive cinematography. Embody the witty protagonist of a film noir plot. Meet unforgettable characters. Solve puzzles. Lose yourself in the retro-futuristic atmosphere.\n\nEXPERIENCE IN COMFORT.\nAs a seated experience with intuitive controls, first time VR users will find this game engaging and easy to play. Experienced VR users will find this game casual and relaxing.\n\nSTORY. ART. MUSIC.\nEnter a sprawling retro-futuristic cityscape with an original score, ensemble of voice actors and detailed hand drawn (in VR!) environments.\n\nGOOD \u2018OL ADVENTURE GAME\nBoot up that nostalgia for your favorite 90's point-and-click adventure games. Look around. Find items. Solve puzzles. Keep the past alive.\n\n\n* No robots were harmed in the making of this game.", "short_description": "The Secret of Retropolis is a VR narrative game of a new kind, a blend of classic point-and-click adventure with immersive cinematography. Embody the witty protagonist of a film noir plot. Meet unforgettable characters. Solve puzzles. Lose yourself in the retro-futuristic atmosphere."}]}, "developer_name": null, "developer_privacy_policy_url": "https://peanutbutton.net/privacy", "developer_terms_of_service_url": null, "publisher_name": "Peanut Button", "support_website_url": "http://peanutbutton.net/", "website_url": "http://peanutbutton.net/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": "NOT_REQUIRED", "genres": ["ADVENTURE", "PUZZLE", "NARRATIVE"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "462641452012006"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/39031587_556896002160062_6576043756821935546_n.png?stp=dst-png_s360x360&_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=1ZdlOtcTFC8Q7kNvwGuQTl0&_nc_oc=AdkNP8sZiTb0u64ySxaY0VIJMnQsBrBIkR4yhY6zgFM1PpMjET3ufx-Qcrllfg0Cju0&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AffMJKMQVkcaWVNezA-ktVMchsw1UJPYykBR1fOS02txdw&oe=68F22FA1"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "The Secret of Retropolis Demo", "platform": "ANDROID_6DOF", "context_category": null, "context_topics": [], "release_date": 1628609636, "lastRevision": {"nodes": [{"id": "4166716843411450", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "3902153103245162", "is_first_party": false, "enables_in_app_ads": false, "is_concept": false, "platform": "ANDROID_6DOF"}, "iarc_cert": {"id": "248741739793005"}, "iarc_email": null, "msrp": null, "translations": {"nodes": [{"images": {"nodes": [{"file_name": 