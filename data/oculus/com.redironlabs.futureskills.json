ideo_trailer": null, "video_trailer_thumbnail": {"file_name": "img1.png", "id": "2830690787070278", "uri": "https://scontent-ord5-3.oculuscdn.com/v/t64.5771-25/39001731_2830690790403611_3963931024632297416_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=-6A-_fQPRjUQ7kNvwHS8Ida&_nc_oc=AdmQtlRNUZwksuSP176tVMJPbfY5Jn7uroUHOToFuu8D5bAc8FGewon_WUbllk8bYSo&_nc_zt=3&_nc_ht=scontent-ord5-3.oculuscdn.com&oh=00_AffBRVRzIsWQWqQYHmYxrnyl4uT27NzkDi7e0dnVZ3XbHA&oe=68E799C6"}, "display_name": "Experience the Energy: Adapting Futures", "keywords": [], "long_description": "\"Experience the Energy: Take the Challenge\" is an immersive VR experience that offers players an interactive journey through various roles in the energy industry. Participants are introduced to different job opportunities within the sector, ranging from renewable energy technicians to traditional oil and gas engineers. Through engaging mini-games and simulations, players get hands-on experience performing tasks relevant to each role. Whether it's troubleshooting a wind turbine or managing a solar panel installation, the experience provides valuable insights into the diverse careers available in the energy field, while also highlighting the importance of sustainable practices.", "short_description": "\"Experience the Energy: Take the Challenge\" is a VR experience where players explore different jobs in the energy industry through interactive mini-games."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.redironlabs.com/privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "Red  Iron Labs", "support_website_url": null, "website_url": "https://careersinenergy.ca/programs/virtual-reality-experiences/take-the-challenge/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], 