_ht=scontent-iad3-1.oculuscdn.com&oh=00_Afou7cLRezIa0-juALhEaqTn5EDX9a69fDMU-jff3Pw8bA&oe=697B40CD"}, "display_name": "Attack of the Jersey Drones", "keywords": ["meme", "drones", "political", "funny", "freedom"], "long_description": "ATTACK OF THE JERSEY DRONES is a short and simple Virtual Reality game about FREEDOM and PRIVACY!\n\nAs a retired war veteran, all you wanted was to live the rest of your days out in peace and quiet. Unfortunately for you, a swarm of drones (WHO COULD THEY POSSIBLY BE?) won't leave you alone. So there's only one thing to do. DEFEND. YOUR. FREEDOM!\n\n- 15 to 30 minutes of pure patriotism!\n- Shoot down an aggressive hoard of drones invading your neighborhood\n- Unlock various classic American firearms (M1911, M16A1, UZI, M134 Minigun)\n- Simple, easy-to-understand gameplay\n- Roomscale experience. No need to worry about motion sickness!", "short_description": "As a retired war veteran, all you wanted was to live the rest of your days out in peace and quiet. Unfortunately for you, a swarm of drones (WHO COULD THEY POSSIBLY BE?) won't leave you alone. So there's only one thing to do. DEFEND. YOUR. FREEDOM!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://nekomatagames.com/privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "Nekomata Games LLC", "support_website_url": null, "website_url": "https://nekomatagames.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "3744040075834813"}, {"id": "1592192974313489"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "