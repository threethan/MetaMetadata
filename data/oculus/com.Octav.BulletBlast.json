06"}, {"file_name": "screenshot04.png", "id": "1637912793329104", "uri": "https://scontent-sea5-1.oculuscdn.com/v/t64.5771-25/38982497_1637912796662437_8775068616685465023_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=RhVNqSfABk0Q7kNvwFJKMIc&_nc_oc=Admjn96m_RH88jeHNkIi52iOjpokTDOJ4RrPfnbKb06vcri40jJd3iD-95j81ZaGYRE&_nc_zt=3&_nc_ht=scontent-sea5-1.oculuscdn.com&oh=00_AfSItGUIzGajdzJccjiiXamiaFi88Ru18PqRGeb7MOfwnQ&oe=687CEEF0"}, {"file_name": "38982714_874566830542957_2674849975252225794_n.png", "id": "751787513387347", "uri": "https://scontent-sea5-1.oculuscdn.com/v/t64.5771-25/39035321_751787516720680_7308791965007247415_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=65kj9RV2zJIQ7kNvwEpPK-Y&_nc_oc=Admtpyi2bWq83nFXELWW_zQFyQdw3tZ_ZJZ7Lhpv9NjF8ECbf4PvvaLs6v00F2jLou0&_nc_zt=3&_nc_ht=scontent-sea5-1.oculuscdn.com&oh=00_AfS9f5wiGu39epQgPenAGyHsWF9Q1_hKpKJxC_IYcExy8w&oe=687CEAC5"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "38982714_874566830542957_2674849975252225794_n.png", "id": "859208219578912", "uri": "https://scontent-sea1-1.oculuscdn.com/v/t64.5771-25/38974551_859208222912245_328377278643023193_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=dNaa9lKR-XQQ7kNvwGqi6XP&_nc_oc=AdlgvMjbEeKqqFNZ39lXTmOBLwNC1BpY11FtIab5iLAxFyjdc4RH7hKaoWpBHAggSFk&_nc_zt=3&_nc_ht=scontent-sea1-1.oculuscdn.com&oh=00_AfSULkPIg5vWzk-ufRXRHu-QEXJjMkRfMhXq6277bkw7wA&oe=687CBFF3"}, "display_name": "Bullet Blast", "keywords": [], "long_description": "Get ready for a colorful, high-speed challenge with Bullet Blast! Blast your way through a barrage of glass panels in this VR game experience.\nDodge and destroy:\nColor-matching mayhem: Grab your pistols and fire matching colors to shatter the glass obstacles hurtling towards you.\nTest your reflexes: React quickly and aim precisely to avoid getting knocked off the belt.\nHow far can you go? The further you get, the faster it gets, and the crazier the patterns become. Can you survive the chaos?\nBecome a Bullet Blast master:\nSharpen your skills: Improve your accuracy and reaction time with each thrilling run.\nClimb the leaderboards: Compete with friends and players worldwide for the highest score (Coming soon!)\nUnlock new challenges: Discover unique levels with different obstacles and patterns to keep you on your toes (New levels coming soon!)\n\nDownload Bullet Blast today and experience the ultimate test of speed and precision!\n\n", "short_description": "Bullet Blast is a vibrant VR action game where you shatter colorful glass obstacles with your pistols. Match colors, test your reflexes, and see how long you can survive the chaos!\n"}]}, "developer_name": "Octav", "developer_privacy_policy_url": "https://octav.studio/Privacy%20Policy%20for%20Bullet%20Blast.html", "developer_terms_of_service_url": null, "publisher_name": "Octav", "support_website_url": null, "website_url": "https://octav.studio/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8055167944587839"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sea1-1.oculuscdn.com/v/t64.5771-25/38974593_3616070102014767_3995042336607872066_n.png?stp=dst-png_s360x360&_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=rbSMDDoB428Q7kNvwG0aTOg&_nc_oc=AdlI1JRgA1JTv2vKjNrxc4CvPp2p9Bp6_9LttGoUOlHbMzebJJFNIcM4yHOpB_M9zoA&_nc_zt=3&_nc_ht=scontent-sea1-1.oculuscdn.com&oh=00_AfS