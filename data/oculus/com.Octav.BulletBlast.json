06"}, {"file_name": "screenshot04.png", "id": "1637912793329104", "uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/38982497_1637912796662437_8775068616685465023_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=4_bq7ITj4McQ7kNvwE45ANT&_nc_oc=Adl4NalTEHgg5peC_vjZCuCTlcNbfWS48-M36V_py6OLr3lb2wCIvzkcnGlPJcHns40&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_AfdK5pn01zdnloPCn4bNozXj_da5IWcnfhNcQ024n8suEw&oe=68EBACF0"}, {"file_name": "38982714_874566830542957_2674849975252225794_n.png", "id": "751787513387347", "uri": "https://scontent-ams4-1.oculuscdn.com/v/t64.5771-25/39035321_751787516720680_7308791965007247415_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=1hiMMFiBm70Q7kNvwGx4QNe&_nc_oc=AdkxxMJdMcav2vfh_hWcxiMbf0JyaVygznbqZXMJJP8UQmdub4fdFl7jYRnWqn1ph-k&_nc_zt=3&_nc_ht=scontent-ams4-1.oculuscdn.com&oh=00_Afcg3LSboVAgnAy4wwg_ZDD8Wg1B98pQtyX-PdoexllX_w&oe=68EBA8C5"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "38982714_874566830542957_2674849975252225794_n.png", "id": "859208219578912", "uri": "https://scontent-ams4-1.oculuscdn.com/v/t64.5771-25/38974551_859208222912245_328377278643023193_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=tp28ZkT7_zYQ7kNvwFPhl9I&_nc_oc=Adk0c6XoZSOrA6clr5FLWtOZ6JcWIePOMNBLLxZp_msFlKP1X3MMTKCs9vMbUU82VO8&_nc_zt=3&_nc_ht=scontent-ams4-1.oculuscdn.com&oh=00_AfeqEgPXdMHMhGX-R-zcx5szCu5E8e639jvoKd_LpdeYjQ&oe=68EB7DF3"}, "display_name": "Bullet Blast", "keywords": [], "long_description": "Get ready for a colorful, high-speed challenge with Bullet Blast! Blast your way through a barrage of glass panels in this VR game experience.\nDodge and destroy:\nColor-matching mayhem: Grab your pistols and fire matching colors to shatter the glass obstacles hurtling towards you.\nTest your reflexes: React quickly and aim precisely to avoid getting knocked off the belt.\nHow far can you go? The further you get, the faster it gets, and the crazier the patterns become. Can you survive the chaos?\nBecome a Bullet Blast master:\nSharpen your skills: Improve your accuracy and reaction time with each thrilling run.\nClimb the leaderboards: Compete with friends and players worldwide for the highest score (Coming soon!)\nUnlock new challenges: Discover unique levels with different obstacles and patterns to keep you on your toes (New levels coming soon!)\n\nDownload Bullet Blast today and experience the ultimate test of speed and precision!\n\n", "short_description": "Bullet Blast is a vibrant VR action game where you shatter colorful glass obstacles with your pistols. Match colors, test your reflexes, and see how long you can survive the chaos!\n"}]}, "developer_name": "Octav", "developer_privacy_policy_url": "https://octav.studio/Privacy%20Policy%20for%20Bullet%20Blast.html", "developer_terms_of_service_url": null, "publisher_name": "Octav", "support_website_url": null, "website_url": "https://octav.studio/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8055167944587839"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/38974593_3616070102014767_3995042336607872066_n.png?stp=dst-png_s360x360&_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=7jS-RdRw-rkQ7kNvwHM9APd&_nc_oc=AdnwM5UT69fJXTO8A2ErkSwTyPEGTlMCSqf4hdC4_0rE5urjgfWkvxMwLNEFxFG6D0k&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_Aff