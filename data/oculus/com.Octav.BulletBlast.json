06"}, {"file_name": "screenshot04.png", "id": "1637912793329104", "uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/38982497_1637912796662437_8775068616685465023_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=KJJSCV_yu4EQ7kNvwGY9CJI&_nc_oc=Adm5mryKEgu90AimRu6c2jwlGPGI0IJTYfAj4Y1Pd4ogTsQBu9Y303oIm9NjGdPyezI&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_Afp1uF0O5lsH5kY_doeAN5pyOFVH2MEjQ5iHUVSIokRb2g&oe=6974C8F0"}, {"file_name": "38982714_874566830542957_2674849975252225794_n.png", "id": "751787513387347", "uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/39035321_751787516720680_7308791965007247415_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=al2k36gllq8Q7kNvwHnb1k9&_nc_oc=AdlLwbi_IPv2d50vil110sPwKQlz2MYszilDylEgcS1BY60g1IiDss46GOURtrXK5MM&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_AfqayDStOa6T1q0p10IDzQxcXsasSQoPM6Tzlb83o2F3kw&oe=6974C4C5"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "38982714_874566830542957_2674849975252225794_n.png", "id": "859208219578912", "uri": "https://scontent-atl3-2.oculuscdn.com/v/t64.5771-25/38974551_859208222912245_328377278643023193_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=AMEpN9ZKbl4Q7kNvwElQ-yG&_nc_oc=AdmtoAewhqd06KcyKxC6lWa2FGZV_SaOl5sHfCZ12Ccq1NrzcrNSmosGweyORUoRx6Y&_nc_zt=3&_nc_ht=scontent-atl3-2.oculuscdn.com&oh=00_Afr4iAvw1D7R5-2QTOHOAtcjALsu30ysdV7ITZvreWnPgw&oe=6974D233"}, "display_name": "Bullet Blast", "keywords": [], "long_description": "Get ready for a colorful, high-speed challenge with Bullet Blast! Blast your way through a barrage of glass panels in this VR game experience.\nDodge and destroy:\nColor-matching mayhem: Grab your pistols and fire matching colors to shatter the glass obstacles hurtling towards you.\nTest your reflexes: React quickly and aim precisely to avoid getting knocked off the belt.\nHow far can you go? The further you get, the faster it gets, and the crazier the patterns become. Can you survive the chaos?\nBecome a Bullet Blast master:\nSharpen your skills: Improve your accuracy and reaction time with each thrilling run.\nClimb the leaderboards: Compete with friends and players worldwide for the highest score (Coming soon!)\nUnlock new challenges: Discover unique levels with different obstacles and patterns to keep you on your toes (New levels coming soon!)\n\nDownload Bullet Blast today and experience the ultimate test of speed and precision!\n\n", "short_description": "Bullet Blast is a vibrant VR action game where you shatter colorful glass obstacles with your pistols. Match colors, test your reflexes, and see how long you can survive the chaos!\n"}]}, "developer_name": "Octav", "developer_privacy_policy_url": "https://octav.studio/Privacy%20Policy%20for%20Bullet%20Blast.html", "developer_terms_of_service_url": null, "publisher_name": "Octav", "support_website_url": null, "website_url": "https://octav.studio/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8055167944587839"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/38974593_3616070102014767_3995042336607872066_n.png?stp=dst-png_s360x360&_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=HaIzvl68zAwQ7kNvwFY9n8B&_nc_oc=AdkFuxH7S8WvaOyDJFec-neNT7iWl7owfnM8VcxI-_I9O9yxc4-SfGQLYLk27rhAJJY&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_Afr