_trailer_thumbnail": {"file_name": "75422369_471993569038157_9043023931207755646_n.png", "id": "530984482998568", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/38982695_530984486331901_5808837765461904761_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=bNwGPviMJMMQ7kNvgHLfiRN&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AYAvp75g6XxZ1P17YCIHqumYjHzY31y1m-fUPnDYq5dgRQ&oe=67911B2E"}, "display_name": "Drakheir The First Hand Tracking ARPG", "keywords": [], "long_description": "New Features\n\u25c9 Enhanced for META QUEST 3: Elevate your gaming experience with improved visual performance, offering unparalleled visual details and immersion.\n\u25c9 New Gameplay Snow Fight - Upgrade your snowfight skills in Drakheir this winter! Additionally, explore the Endless Challenge mode to enjoy the magic and chanllenge your limits!\n\u25c9 Improved Game Contents: Experience a significant enhancement in your gameplay with smoother hand tracking and more thrilling battle experience with added physical effects!\n\nIn this exciting adventure, your hands become powerful weapons \u2013 forget those controllers, as you wield magic with your very fingertips! \n\nGame Highlights:\n- Hand-Tracked Magic: Unleash over 40 awe-inspiring spells using hand tracking.\n- Fantasy Cartoon Style: Immerse yourself in a magical world filled with delightful characters and captivating scenes.\n- Engage with In-Game Characters.\n- Three Diverse Realms: Traverse 20 enchanting game environments.\n- Thrilling Boss Battles: Take on three impressive bosses!\n- Exciting Mini-Games: Enjoy fun activities like Whack-a-Mole to elevate your adventure.\nGet ready to wield the power of magic in the most extraordinary way! Drakheir awaits your courage and skills. Join us on this epic journey and become a legendary hero!", "short_description": "Embark on an enchanting journey with Drakheir, the ultimate Magic Adventure Roguelite VR game. Immerse yourself in a world of hand tracking , where a myriad of magical skills and gameplay await. Don't miss the infinite survival mode, ready to test your mettle! Say goodbye to traditional controllers and step into a groundbreaking VR gaming realm like never before.."}]}, "developer_name": null, "developer_privacy_policy_url": "http://privacy.5151vr.com/privacy.html", "developer_terms_of_service_url": null, "publisher_name": "Shambala", "support_website_url": null, "website_url": "https://drakheir.shambala.cn/pre-registration/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "2691890164460465"}, {"id": "821934236779161"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["HAND_TRACKING"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8241846272530680"}}]}, "scheduled_offers": {"count": 5, "nodes": [{"status": "PUBLISHED", "id": "949138393810230"}, {"status": "PUBLISHED", "id": "1555998121697684"}, {"status": "CANCELLED", "id": "475331411735103"}, {"status": "PUBLISHED", "id": "1087034189493989"}, {"status": "PUBLISHED", "id": "581651967730138"}]}, "baseline_offer": {"id": "1555998121697684"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1737396000, "status": "PUBLISHED", "id": "949138393810230"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "8676375372411099", "version": "V1.1.0"}, "id": "1425169095538370", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary