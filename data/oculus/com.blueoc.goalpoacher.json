_description": "Become a pro football striker going up against challenging real-life defenders and goalkeepers to score header goals. "}]}, "developer_name": null, "developer_privacy_policy_url": "https://blueoc.tech/en/case-study/goal-poacher/policy", "developer_terms_of_service_url": null, "publisher_name": "BlueOC studio", "support_website_url": null, "website_url": "https://blueoc.tech/en/case-study/goal-poacher", "category": null, "comfort_rating": null, "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "7279132668874361"}}]}, "scheduled_offers": {"count": 21, "nodes": [{"status": "PUBLISHED", "id": "769290909416289"}, {"status": "PUBLISHED", "id": "1482369592902711"}, {"status": "PUBLISHED", "id": "628741129830736"}, {"status": "CANCELLED", "id": "24527831153489455"}, {"status": "CANCELLED", "id": "1946903046046232"}, {"status": "PUBLISHED", "id": "1009841654556097"}, {"status": "PUBLISHED", "id": "1201801230957806"}, {"status": "PUBLISHED", "id": "1090373256365249"}, {"status": "PUBLISHED", "id": "643037761533629"}, {"status": "PUBLISHED", "id": "1160797465708262"}, {"status": "CANCELLED", "id": "1288817962233684"}, {"status": "PUBLISHED", "id": "9107387325989410"}, {"status": "PUBLISHED", "id": "962032732503028"}, {"status": "PUBLISHED", "id": "578730664721535"}, {"status": "PUBLISHED", "id": "523410713401199"}, {"status": "PUBLISHED", "id": "1738205490252930"}, {"status": "C