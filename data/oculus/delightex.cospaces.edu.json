ld trips to provide students with original and engaging learning experiences that captivate and last."}]}, "developer_name": "Delightex GmbH", "developer_privacy_policy_url": "https://cospaces.io/edu/app-privacy-policy.html", "developer_terms_of_service_url": "https://cospaces.io/edu/terms-of-use.html", "publisher_name": "Delightex GmbH", "support_website_url": "https://cospaces.io/edu/contact.html", "website_url": "https://cospaces.io/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1043107547362485"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "ru", "de", "cs", "pt_BR", "es", "zh_CN", "lt", "bg", "tr", "fr", "nl", "ro", "da", "ko", "it"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "7784743034923733"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1051009939639697"}]}, "baseline_offer": {"id": "1051009939639697"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1710255600, "status": "PUBLISHED", "id": "1051009939639697"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "24079396545031790", "version": "2025.2"}, "id": "827403265483128", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "6880063282058384", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary"