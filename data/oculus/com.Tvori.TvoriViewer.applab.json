oh=00_Afcg4m4MMOh3FE4YiR4Oq-UIKtixvp-rtlLuk6tb4zMEyQ&oe=68F8C959"}, {"file_name": "05.png", "id": "324094699020262", "uri": "https://scontent-lhr8-1.oculuscdn.com/v/t64.5771-25/38982671_324094705686928_7023416651586249447_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=24oNVOYocFEQ7kNvwEkAgEY&_nc_oc=AdmCEt7iBLgYizom51MHpx3tMPi_ozLvdtenQ4mu0UWcLU9UvUJTJWbRWdx4UZEOLbY&_nc_zt=3&_nc_ht=scontent-lhr8-1.oculuscdn.com&oh=00_AfeMjnCQPayv8uUVlzTHvn67ZgeoHpfAIrUtTKr5OK8_kw&oe=68F8CE62"}, {"file_name": "04.png", "id": "230881942091749", "uri": "https://scontent-lhr8-1.oculuscdn.com/v/t64.5771-25/86615896_230881945425082_3303861453012130059_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=FEdU413HZMoQ7kNvwHmUvHd&_nc_oc=AdlIeWrrZN24g-4vHv-E0IzAb0dCrKy9te9S6Sug_hFcOePGhjVldxyuJ2tQPKAazDA&_nc_zt=3&_nc_ht=scontent-lhr8-1.oculuscdn.com&oh=00_AfcfV3PBnYqdhd8VXA4YNqNuo7PjavWy0XHIF5BQwJf56Q&oe=68F8DD81"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Viewer2550WHITETTT23.png", "id": "3159930287440672", "uri": "https://scontent-lhr8-1.oculuscdn.com/v/t64.5771-25/38982535_3159930290774005_8911193312761706255_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=9HxxwGsn4RwQ7kNvwHALh2k&_nc_oc=AdltYdTt6VH3ld4ZHF6nnYWASBv5b_JxC0vupISJKIK8HyioH15O1FxWACB-d8QggGk&_nc_zt=3&_nc_ht=scontent-lhr8-1.oculuscdn.com&oh=00_Afc8R2-inYwMxySgvB84oBsEJJIAXpleqmrAJSHMjsD7tA&oe=68F8ABF5"}, "display_name": "The Viewer", "keywords": [], "long_description": "With the Tvori Viewer, you can gather as a team in VR to review and discuss your creations, made in Tvori. As a host you can control the presentation: play animations, switch between different scenes, jump from one viewpoint to another, mute guests, transit host rights.\n\nThe aim is to quickly share, present, and discuss 3D ideas in 3D with as little friction as possible.\nThe Viewer works on Oculus Quest, PC VR headsets and in a 2D desktop mode.", "short_description": "Tvori Viewer allows to host collaborative design reviews in VR and make impressive 3D pitches of your 3D designs and animated prototypes to colleagues and clients.\n\nYou create in Tvori editor and then export your creation to the Viewer. You can invites team members or clients to join the review session. \n\nThe aim of the Viewer is to quickly share, present, discuss  and get feedback for your 3D ideas in 3D with as little friction as possible."}]}, "developer_name": null, "developer_privacy_policy_url": "https://tvori.co/privacy", "developer_terms_of_service_url": null, "publisher_name": "Tvori", "support_website_url": null, "website_url": "https://tvori.co/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1819021644882647"}, {"id": "1063584910766706"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": ["UTILITY"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER", "SINGLE_USER"], "id": "276206754613489"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1130017940743911"}]}, "baseline_offer": {"id": "1130017940743911"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1618202640, "status": "PUBLISHED", "id": "1130017940743911"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "4061846907182183", "version": "0.103.4"}, "id": "240701700928303", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__type