."}]}, "developer_name": null, "developer_privacy_policy_url": "https://tvori.co/privacy", "developer_terms_of_service_url": null, "publisher_name": "Tvori", "support_website_url": null, "website_url": "https://tvori.co/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1819021644882647"}, {"id": "1063584910766706"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": ["UTILITY"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER", "SINGLE_USER"], "id": "276206754613489"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1130017940743911"}]}, "baseline_offer": {"id": "1130017940743911"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1618202640, "status": "PUBLISHED", "id": "1130017940743911"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "4061846907182183", "version": "0.103.4"}, "id": "240701700928303", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "4061130713920469", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "4061846907182183"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "240701700928303"}]}, "change_log": null, "created_date": 1612795703, "id": "4061846907182183", "version": "0.103.4", "versi