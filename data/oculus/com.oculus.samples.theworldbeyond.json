deo_trailer": null, "video_trailer_thumbnail": {"file_name": "TrailerCover.png", "id": "614824873441818", "uri": "https://scontent-lhr6-2.oculuscdn.com/v/t64.5771-25/75505861_614824883441817_5563202159333149085_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=W6neE5_cAPoQ7kNvwEcdecu&_nc_oc=Adnn14uOtF1ADXlK2aRK2OVjTwMsz_mhGXzzjWTWoOusWQTnaxeypGRjbU9Y4gYAXpw&_nc_zt=3&_nc_ht=scontent-lhr6-2.oculuscdn.com&oh=00_AfmOT0-EiyqWmZ7_rphUdWnPFeoc0GOcxr48ARMZLus9gg&oe=694A758E"}, "display_name": "The World Beyond", "keywords": [], "long_description": "Discover an engrossing new world of mixed reality, where your play space merges with a virtual landscape. Interact with Oppy and The World Beyond using your hands and voice.  Use a magic flashlight to find energy orbs in your own room, hidden behind Passthrough. Grab them with your hands, tossing them into your space as Oppy navigates around your furniture and room. Use voice commands to ask Oppy to jump or come, or just say hi!  Feed her energy orbs so she can return home!\n\nThe World Beyond is a 5 minute tech demo of Presence Platform features. As such, a full game experience should not be expected; it's instead meant to inspire developers to build mixed reality experiences!\n\nIf you get a black screen while running The World Beyond, please update and restart your headset.", "short_description": "Discover a mixed reality world in this Presence Platform experience."}]}, "developer_name": "Oculus Samples", "developer_privacy_policy_url": "https://www.oculus.com/legal/privacy-policy/", "developer_terms_of_service_url": "https://www.oculus.com/legal/terms-of-service/", "publisher_name": "Oculus", "support_website_url": null, "website_url": "https://developer.oculus.com/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "726368587961803"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ADVENTURE"]