4__WPG2TpaMQ7kNvgE1u74l&_nc_oc=AdiB2MAMiFUtBkK1SaneViboQcGdbhmPVdLMWdqqypsecAmUZbz8nnRs7N2pOxKyERg&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYC2Tk3MGm3zKHuD5CeNx7aWorg7DiQpVoGco8jsen1eWg&oe=67C6E36F"}, {"file_name": "Screenshot_7.png", "id": "311764160408823", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/57566677_311764163742156_6354851173974657435_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Hf0TwaUpnGQQ7kNvgGX3ACU&_nc_oc=AdioDLA_nrmRRv-bZO3hZu9pYrMo-5lzh7AcGtDvqAysV2MTeLeV_aS7qjEFa-MRDYQ&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AYA_Jz9sBPCtpEaXWYc3159M1bna2f7A4-lSL234zUpDSw&oe=67C6EF7F"}, {"file_name": "Steam-Screenshot2.png", "id": "515575053121672", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/38982660_515575056455005_5034961616916098855_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=0wTETwyyFPAQ7kNvgFKYH1G&_nc_oc=Adi-mP9V5sbyEAPMrE3VfhBPZ0NwhgCedIWlcdJi3dyQgXjMUMchRLjKYgG1t5pEVr4&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AYA5zplsfSInN7PbksX26heC6C8YpFh1BxXtyVFTqZYBrg&oe=67C6FE66"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "TrailerCover (1).png", "id": "319098836717241", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/38974671_319098840050574_1723902986571985662_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=o3Etd_qWbcUQ7kNvgFXxonm&_nc_oc=AdgPQDmRhsMIQ-JkvULNCDni5lx6HFo_YjjWss7d-B5qS43VqRTztEY0Y86b6X4Bzz0&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYCbGYPaDhJTXdAK44AFyc408tXJGQjfmOSDKRwfiluJDA&oe=67C6D9EF"}, "display_name": "Potato Party: Hash It Out", "keywords": [], "long_description": "Welcome, friends, to POTATOPIA \u2014 where you can finally paint your mashterpiece.\n\nIn this quick-draw multiplayer game, it's YOU (the VR potato) against YOUR FRIENDS (iOS and Android potatoes), as they race to be the first to guess what you're drawing in three dimensions.\n\nUp to 8 players can join using the free mobile app on iOS and Android. Watch the VR potato draw on your device or on a stream, input custom prompts, make your guesses, and use special powers to foil your fellow future french fries!\n\nAs the VR potato, choose your medium \u2014 3D shapes, air-painting, or canvas drawing \u2014 to dazzle your fellow spuds with your artistic prowess.\n\nDraw your worst, guess your best \u2014 just don't eat the artist. Please.", "short_description": "Prepare to laugh when you and your friends gather (virtually or physically) for this quick-draw asymmetrical VR game! Be the artist in VR, or guess what's being drawn as one of 1-8 challengers with the free iOS/Android app. Just don't eat the artist."}]}, "developer_name": "Fanaticus XR", "developer_privacy_policy_url": "https://www.fanaticusxr.com/privacy-policy.html", "developer_terms_of_service_url": null, "publisher_name": "Fanaticus XR", "support_website_url": null, "website_url": "http://potatoparty.games/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1700921480735423"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["SOCIAL", "ART_CREATIVITY", "ACTION"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER"], "id": "4854591554633040"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/38982660_777975326212114_2622217636817396423_n.png?stp=dst-png_s360x360&_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=zf1RRpS3D4oQ7kNvgGZUD_h&_nc_oc=AdhfZX8CXH8KcQ8a762HgEUac02UTVwA3bTKP6Ny8CZa7N29gDY71vCnVsqOY-vUGlg&_nc_zt=3&_nc_ht=scontent-iad3-2.ocul