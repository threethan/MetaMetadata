fyB86_xgMCYQ7kNvwHw8wrA&_nc_oc=Adm-M9q2vySOUML9-Dfv5t63t2lp4TTu9MQF0KodZ8JKDniANQtS3tVySI5DadsjK5o&_nc_zt=3&_nc_ht=scontent-hkg4-1.oculuscdn.com&oh=00_AfQg1gv9bznWpkFOIt8ChR2CYwMtZPKpCUBCd9iYjlISLw&oe=6870F4EF"}, {"file_name": "Screenshot_7.png", "id": "311764160408823", "uri": "https://scontent-hkg1-1.oculuscdn.com/v/t64.5771-25/57566677_311764163742156_6354851173974657435_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=fAqj0chg018Q7kNvwFrOQ_r&_nc_oc=AdlzxMRVjd7MmYP9-Zmpum8MQQGP3e6rUgDs8x4dGjo_nxz7EOgjT5k-Z-7mNiN_tlw&_nc_zt=3&_nc_ht=scontent-hkg1-1.oculuscdn.com&oh=00_AfRrCTOvdzbeyrt8LewytrXIvsGpfuhhW_B2d54gFLZPtw&oe=687100FF"}, {"file_name": "Steam-Screenshot2.png", "id": "515575053121672", "uri": "https://scontent-hkg1-1.oculuscdn.com/v/t64.5771-25/38982660_515575056455005_5034961616916098855_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=cbX3NeMuNIIQ7kNvwF0-hWI&_nc_oc=AdmSyYEBk72EpxSLu4OmD_VgBptqAXwbk8NsLTcKnxQjnkSyP57fAMW7lu-VfZrz1bw&_nc_zt=3&_nc_ht=scontent-hkg1-1.oculuscdn.com&oh=00_AfRtFshcbOyPXuHhNH8RpL5Ps5fPbADuy0xujpDyuJ8YnQ&oe=68710FE6"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "TrailerCover (1).png", "id": "319098836717241", "uri": "https://scontent-hkg4-1.oculuscdn.com/v/t64.5771-25/38974671_319098840050574_1723902986571985662_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=lh7HElLUND0Q7kNvwEZYRS5&_nc_oc=AdlGtRqFZpl4rOdIXuVlrnVR4jjMFBmibx5mLiEExKKzYkBXsPSjVJQVL_8cb5YRpig&_nc_zt=3&_nc_ht=scontent-hkg4-1.oculuscdn.com&oh=00_AfQex4kyGMO_w36ENPHZvWpJ9g002-kVISWGWDkxJpUb7g&oe=6870EB6F"}, "display_name": "Potato Party: Hash It Out", "keywords": [], "long_description": "Welcome, friends, to POTATOPIA \u2014 where you can finally paint your mashterpiece.\n\nIn this quick-draw multiplayer game, it's YOU (the VR potato) against YOUR FRIENDS (iOS and Android potatoes), as they race to be the first to guess what you're drawing in three dimensions.\n\nUp to 8 players can join using the free mobile app on iOS and Android. Watch the VR potato draw on your device or on a stream, input custom prompts, make your guesses, and use special powers to foil your fellow future french fries!\n\nAs the VR potato, choose your medium \u2014 3D shapes, air-painting, or canvas drawing \u2014 to dazzle your fellow spuds with your artistic prowess.\n\nDraw your worst, guess your best \u2014 just don't eat the artist. Please.", "short_description": "Prepare to laugh when you and your friends gather (virtually or physically) for this quick-draw asymmetrical VR game! Be the artist in VR, or guess what's being drawn as one of 1-8 challengers with the free iOS/Android app. Just don't eat the artist."}]}, "developer_name": "Fanaticus XR", "developer_privacy_policy_url": "https://www.fanaticusxr.com/privacy-policy.html", "developer_terms_of_service_url": null, "publisher_name": "Fanaticus XR", "support_website_url": null, "website_url": "http://potatoparty.games/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1700921480735423"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["SOCIAL", "ART_CREATIVITY", "ACTION"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER"], "id": "4854591554633040"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-hkg1-1.oculuscdn.com/v/t64.5771-25/38982660_777975326212114_2622217636817396423_n.png?stp=dst-png_s360x360&_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=pp7uw3TYVKkQ7kNvwFndFEk&_nc_oc=AdlI6ioIg1jM3ons2OgLy3HuzxDhTYwq49WDPdDG3TSGdO4n1h02poO2WhTnBBTOmuM&_nc_zt=3&_nc_ht=scontent-hkg1-1.oculus