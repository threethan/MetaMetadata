content.oculuscdn.com&oh=00_Afg41g6zedLO8d_lViGnWUiNyJajjaVvXhrBIKqIyvqLFA&oe=69107BC0"}, {"file_name": "OculusArt_Screenshot04.png", "id": "972901181324074", "uri": "https://scontent.oculuscdn.com/v/t64.5771-25/473391967_972901184657407_1156148169822448151_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=0VkorupyKvIQ7kNvwE1WJGr&_nc_oc=AdnzOPeVowS80KMJntEbwk7hQhBaRT5moUEmR2ANoI3wloSoo3-Td_9lAS7ImjmajZY&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_Afi-jFpfHLuEQgi3ztJv-sO2hop-ZIVPVqRXT6X5rjsqTQ&oe=691099A6"}, {"file_name": "OculusArt_Screenshot05.png", "id": "689092010277027", "uri": "https://scontent.oculuscdn.com/v/t64.5771-25/473393315_689092013610360_2192795500144739507_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=mB_WtJi1NY8Q7kNvwFytayS&_nc_oc=AdnGaKD7KR5AvcI79ZeEmkjS5rJMmDtACIy0kYWT7ASOkonJzfeCSOS0gfLA2KiNmPE&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_Afjwd6A-OIgxoYVKHUhRJE5AURqQcm80eFDGndhD39J_yw&oe=69109F59"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "American Heart Association CPR VR", "keywords": [], "long_description": "Welcome to the ultimate immersive experience in lifesaving! Learn how to perform Hands-Only CPR, a skill that can truly make a difference in saving someone\u2019s life. You will be guided through the process of Hands-Only CPR, step-by-step, and how to use an automated external defibrillator, also known as an AED. These skills will help you be ready to act in case of an emergency. Are you ready to become a lifesaver!?\nTake part in Learn Mode where you can pick a song that will inspire you and keep you motivated during this life-saving technique. The beat of the music will guide your compressions. In Practice Mode you will be placed in three real-life scenarios but you will not be guided by the narrator. Put your skills to the test and let\u2019s see how well you\u2019ve mastered these lifesaving techniques. \nThis is not a CPR certification course. Visit www.heart.org to learn more.", "short_description": "Welcome to the ultimate immersive experience in lifesaving! Learn how to perform Hands-Only CPR, a skill that can truly make a difference in saving someone\u2019s life. You will be guided through the process of Hands-Only CPR, step-by-step, and how to use an automated external defibrillator, also known as an AED. These skills will help you be ready to act in case of an emergency. Are you ready to become a lifesaver!?\nThis is not a CPR certification course. Visit www.heart.org to learn more."}]}, "developer_name": null, "developer_privacy_policy_url": "https://groovejones.com/meta-app-aha-hands-cpr-app-privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "American Heart Association", "support_website_url": null, "website_url": "https://www.heart.org/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "514087707868786"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["HAND_TRACKING"], "supported_player_modes": ["ROOM_SCALE", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9508516642531388"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent.oculuscdn.com/v/t64.5771-25/491820248_1868308920678483_7253105869070691592_n.png?stp=dst-png_s360x360&_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=GM0bEJdRsRgQ7kNvwF9d9zW&_nc_oc=AdmpwFbwe8MBQdhogRDm6dN2EMPoc0g4EXwoNnjJDkku-RIzTTk-GvteEn3XLql5rFI&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_Afgz8nEzMqY5_CO6mQwseRtXm6hTsfXnEk2H41XkaGgQfg&oe=69109E23"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "American Heart Association C