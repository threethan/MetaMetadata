trailer": null, "video_trailer_thumbnail": {"file_name": "CoverLandscape_v12202024.png", "id": "1962982060856007", "uri": "https://scontent-atl3-2.oculuscdn.com/v/t64.5771-25/473391937_1962982064189340_6268151051896701859_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=vVR4a8Dd6s8Q7kNvwHyIjPH&_nc_oc=AdkLoMh_TCt6HNU8gz5u_3DCZxCgbfO3S3i11LX_5nC5Pe10H56ZgOl4eURATyaYGW4&_nc_zt=3&_nc_ht=scontent-atl3-2.oculuscdn.com&oh=00_AfEMHXtO7D9aYCi5pqwM08Mt3PVKM7OwtlFvQr9l2zeo2w&oe=681A0E9C"}, "display_name": "COCO 3D", "keywords": ["Design", "Collaboration", "Planning", "MR", "Measure"], "long_description": "COCO 3D is the ultimate MR toolkit for spatial thinking, planning, and communication. Sketch, measure, and design concepts directly in 3D space. Whether you\u2019re planning a remodel, mapping a project, or brainstorming ideas, COCO 3D helps you turn abstract thoughts into clear, tangible forms.\n\nKey Features\n\n- 3D Sketching & Design: Draw lines, shapes, and forms in 3D, visualizing ideas from every angle.\n\n- Spatial Reasoning & Planning: Plan remodels, layouts, and pathways before making physical changes.\n\n- Room Scanning: Scan your entire room and create accurate objects with a single click.  \n\n- Live Collaboration: Work with others to brainstorm and refine concepts in real time.\n\n- User-Created Sketches: Your sketches are stored locally on your device, giving you full control and privacy.\n\n- Intuitive VR Controls: Simple, hands-on controls make it easy for anyone to start creating.\n\nCheck out the support page for the FAQ and guides:\nhttps://coco3d.app/support\n\nAnd join the discord to chat with our devs!\nhttps://discord.com/invite/GeDDFJgtZn", "short_description": "COCO 3D is the ultimate MR toolkit for spatial thinking, planning, and communication. Sketch, measure, and design concepts directly in 3D space. Whether you\u2019re planning a remodel, mapping a project, or brainstorming ideas, COCO 3D helps you turn abstract thoughts into clear, tangible forms.\n"}]}, "developer_name": null, "developer_privacy_policy_url": "https://coco3d.app/privacy-policy", "developer_terms_of_service_url": "https://coco3d.app/terms-of-service", "publisher_name": "Chicken Tractor LLC", "support_website_url": "https://coco3d.app/support", "website_url": "https://coco3d.app/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1819021644882647"}, {"id": "1063584910766706"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["HAND_TRACKING", "OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "28855000717477426"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-atl3-2.oculuscdn.com/v/t64.5771-25/473393856_624976256803933_2933620096149304979_n.png?stp=dst-png_s360x360&_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=VQJ9HvFCTFwQ7kNvwE2Fmvw&_nc_oc=Adlg3oKlQU77aEfZzJzQgYFehyLK0yilTaAWAvrhL4CB3XjpyHyfzQcXomEJO24czxk&_nc_zt=3&_nc_ht=scontent-atl3-2.oculuscdn.com&oh=00_AfFF2U4ySIyuHaLcL04o81o1vjiZLW-le6svlh1B8DobNA&oe=681A1283"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "COCO 3D", "platform": "ANDROID_6DOF", "context_category": {"context_category": "APPS", "id": "362692211442873"}, "context_topics": [{"topic_name": "Creativity", "id": "1819021644882647"}, {"topic_name": "Productivity", "id": "1063584910766706"}], "release_date": 1735083720, "lastRevision": {"nodes": [{"id": "26078295815147944", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {"application": {"default_locale": "e