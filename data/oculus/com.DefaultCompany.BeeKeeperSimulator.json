!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.bendisgames.com/beekeeping_simulator_privacy_policy.html", "developer_terms_of_service_url": null, "publisher_name": "BENDIS Games", "support_website_url": "https://www.bendisgames.com/", "website_url": "https://www.bendisgames.com/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "961172191044986"}, {"id": "394813081514456"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": false, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "7341306535927685"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "769525187964755"}]}, "baseline_offer": {"id": "769525187964755"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1708643520, "status": "PUBLISHED", "id": "769525187964755"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "8242782075780122", "version": "1.2.8"}, "id": "1129670058219718", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "7341306525927686", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "8242782075780122"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1129670058219718"}]}, "change_log": "NEW UPDATE! - V1.2