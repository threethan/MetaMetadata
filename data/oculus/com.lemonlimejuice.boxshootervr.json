r simulator you shoot boxes and stars to collect points and extra time! How long can you last?"}]}, "developer_name": null, "developer_privacy_policy_url": "http://www.lemonlimejuice.com/box-shooter-privacy-policy/", "developer_terms_of_service_url": "http://www.lemonlimejuice.com/box-shooter-vr-terms-of-service/", "publisher_name": "Lemon Lime Juice", "support_website_url": null, "website_url": "http://www.lemonlimejuice.com/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": "NOT_REQUIRED", "genres": ["CASUAL", "ARCADE", "SHOOTER"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": [], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "464945311901239"}, "review_messages": ["", ""]}]}, "cover_square_image": {"uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/12680304_1040881602691460_8485016070036389888_n.png?stp=dst-png_s360x360&_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=_tejLmqkiDQQ7kNvgGUcc4x&_nc_oc=Adgm5Jjj7AlDhk53Ub-0fPHhmTmWxDVZmQz2W43Yopqgj48hgdnrV9a2jbLTfQ7VPWs&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AYFtMcfOhvVwi72fNfCmWD0fw_mcLTbcSqHcpDK4_Jdm0g&oe=67DD3E36"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Box Shooter VR", "platform": "ANDROID", "context_category": null, "context_topics": [], "release_date": 1481097600, "lastRevision": {"nodes": [{"id": "1783253238618030", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "1110867552334533", "is_first_party": false, "enables_in_app_ads": false, "is_concept": false, "platform": "ANDROID"}, "iarc_cert": null, "i