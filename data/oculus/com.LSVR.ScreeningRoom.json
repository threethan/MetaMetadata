railer": null, "video_trailer_thumbnail": {"file_name": "Title_2560x1440px.png", "id": "181058191005506", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/39036679_181058194338839_6518638220623320000_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=PMmVVTCv6wYQ7kNvwG6XEig&_nc_oc=Admi8EVRlh7EZ1L95OOE6SXNpFWjUTkToqje-FUKQhVqfYlxy-sT7smy1-cWC69qvIc&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfpPxBrSNDvJC33qDIbHMsuRPxMduLO4IpZmLNodllbZQg&oe=696B8AB2"}, "display_name": "Screening Room", "keywords": [], "long_description": "Introducing \"Screening Room\u201d - the ultimate Meta Quest application for seamless collaboration on immersive video projects! Speed up your creative process by stepping into a virtual screening room where you can review and comment on your team's work all within the comfort of your headset. Screening Room brings your video content to life in a whole new dimension, connecting to Frame.io to give you instant access to your files.\n\nBrowse your Frame.io teams, projects, and folders in mixed reality. Use the hand tracking navigation to quickly find what you need without fumbling for controllers. Pair a tracked keyboard and effortlessly type timecode stamped notes that get synced with Frame.io\u2019s web interface.  Download files for review later or stream them for instant access. With full support for flat, 360, 180, monoscopic and stereoscopic videos, its versatility lets you focus on the creative work, not the technology.\n\nSay goodbye to tedious downloading, sideloading, and taking your headset on and off repeatedly to make notes. Embrace the future of creative collaboration with Screening Room.\n\nCurrently requires an account with Frame.io.\n\nThis application is not affiliated with Frame.io or Adobe.", "short_description": "Introducing \"Screening Room\u201d - the ultimate Meta Quest application for seamless collaboration on immersive video projects! The virtual screening room allows for reviewing and commenting on team work within the headset, connecting to Frame.io for instant access to files. With hand tracking navigation and support for various video formats, Screening Room streamlines the creative process and eliminates tedious tasks."}]}, "developer_name": null, "developer_privacy_policy_url": "https://lightsailvr.notion.site/Privacy-Policy-bfc4747dbd2b42f18e2927b318c7476d", "developer_terms_of_service_url": null, "publisher_name": "Light Sail VR", "support_website_url": "https://www.lightsailvr.com/support", "website_url": "https://www.lightsailvr.com/tools.php", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "434961926215971"}, {"id": "1063584910766706"}], "external_subscription_type": "REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH", "HAND_TRACKING", "TRACKED_KEYBOARD"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "951865466251828"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "600400825453316"}]}, "baseline_offer": {"id": "600400825453316"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1683291600, "status": "PUBLISHED", "id": "600400825453316"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "7270569813027228", "version": "2.0a"}, "id": "733295545172319", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "6170814063002814", "is_c