_ohc=gxaW4HCj-bUQ7kNvwHVWGQh&_nc_oc=Adk-6U6JvoCv4hbZMlEmlw0pl9kH74yvKJ29nHxOgez0NYUa5lDe9fFvotwFIJ_9AZ8&_nc_zt=3&_nc_ht=scontent-sea1-1.oculuscdn.com&oh=00_AfmatY7KeSAf2-d4XuuGagpCSFXZHgBGfT5THqxSg0KX4Q&oe=6941557C"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "FEYDOM Designer", "keywords": [], "long_description": "FEYDOM Designer is a virtual reality app designed to revolutionize how you create and visualize your living space. Exclusively available for Meta Quest 2 and above, this immersive experience allows you to design your own modular sofa configurations from the comfort of your home. Whether you're an interior designer, home decor enthusiast, or just someone looking to furnish a room, FEYDOM Designer offers a dynamic, interactive platform to bring your ideas to life.\n\nExplore a vast range of modular sofa pieces and experiment with various styles, colors, and fabrics. With intuitive controls and a user-friendly interface, you can easily move and rotate the sofa modules, adjust layouts, and visualize your custom design in three different ambients. The app provides a realistic 3D experience, letting you walk around your designs and check out details from every angle.\n\nFEYDOM Designer makes it easy to turn your ideas into reality. Whether you're rearranging an existing set or starting from scratch, FEYDOM Designer gives you the freedom to create the perfect furniture solution for your space.\n\nKey Features:\n\n- Design custom modular sofas in an immersive VR environment\n- Explore various styles, colors, and fabrics to match your taste\n- Easy-to-use interface\n- Real-time 3D visualization of your creations\n\nStep into the future of interior design with FEYDOM Designer on Meta Quest!", "short_description": "FEYDOM Designer is an innovative app for Meta Quest that lets you design and visualize custom modular sofas in stunning VR. Mix and match pieces, explore fabrics, and create your perfect furniture setup in an immersive 3D environment. Ideal for home designers and enthusiasts alike!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://feydom.com.mk/vr-privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "FEYDOM", "support_website_url": null, "website_url": "https://feydom.com.mk/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1819021644882647"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "26675578458723604"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "544187377973115"}]}, "baseline_offer": {"id": "544187377973115"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1729608840, "status": "PUBLISHED", "id": "544187377973115"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "27751988487749257", "version": "3.4.4"}, "id": "1047262023098049", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "26675578428723607", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "27751988487749257"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": []}, "change_log": null, "created_date": 1734696455, "id": "27928819130066191", "version": "3.4.5", "version_code": 43, "release_s