nc_ohc=OwVUv9zjE_sQ7kNvwH18LKa&_nc_oc=AdnmT-zjyxjdqKj2v0BwjC0L4Yhf7uhHs0XqTMtEXNKvZTGmiYIcF3X63P-zAJSNhjs&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfJPpuVVTTLygagmoBF_ZMa0Jqp3CVcVwwhUZ0Qc7FnRFA&oe=6829BAB4"}, {"file_name": "Screenshots5.png", "id": "1856438794557963", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/86625129_1856438797891296_1499696175572873419_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=zjv8eP-h5wsQ7kNvwEwCaA8&_nc_oc=Adm2kIS2ZcrClaV5422YBtA_tACr0Y7yMcMOOzxlaY_AAT3zNq0447yyOpoHyaCftcw&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfLzTF91kLmFnKqpi7XLmenUm27zdUiktLoNHT6GOXDzNA&oe=6829CF0A"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Hoppin'", "keywords": [], "long_description": "Meet with Friends and Family in real Hoppin' Destinations, 360 video chatrooms, using your Oculus avatar. Follow your host or lead the way. When you like where you're at, send yourself some information about the Hoppin' place by email.\nActivities, culture, events, landmarks or touristic locations can benefit from this unique opportunity to welcome potential visitors onsite. Contact us through hoppin.world to participate in the media revolution.\nCreators, producers, studios or 360 capture enthusiasts earn commissions when referring their favorite destinations and posting their amazing experiences. Once approved and published, they automatically become official Hoppin' creators, and we'll send their way anyone who asks about immersive content production in their region.\nEnjoy!", "short_description": "Hoppin' is the Social VR platform offering telepresence in real destinations together! Meet in real locations as avatars in 360 videos. Upload your own content. Promote your location in public channels. Hangout with your friends in personal channels. Invite your customers in private channels. Previously on the Oculus Go, now in Beta on Apps Lab for Quest. Let us know what you like and what you need next! feedback@hoppin.world"}]}, "developer_name": null, "developer_privacy_policy_url": "https://hoppin.world/privacy.html", "developer_terms_of_service_url": null, "publisher_name": "Hoppin' World", "support_website_url": null, "website_url": "https://hoppin.world/", "category": "ENTERTAINMENT", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "726368587961803"}, {"id": "1228796255027646"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["EXPERIENCE_360_NON_GAME", "SOCIAL", "TRAVEL"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["es_VE", "es_AR", "es_PE", "en", "fr", "es", "fr_CA", "es_PR", "fr_BE", "es_CL", "es_MX", "es_CO"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER"], "id": "620614662573959"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "379810056743344"}]}, "baseline_offer": {"id": "379810056743344"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1621983060, "status": "PUBLISHED", "id": "379810056743344"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "29361142946833744", "version": "3.7.2"}, "id": "228885958890273", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "5105861412788569", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "29361142946833744"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [