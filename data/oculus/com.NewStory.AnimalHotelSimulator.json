ri": "https://scontent-ams4-1.oculuscdn.com/v/t64.5771-25/473392168_1797314227727431_157826986001053307_n.png?_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=wJs7EfW0X90Q7kNvwEUKPOj&_nc_oc=Adk0BSTAbqHscizmlzCWROzhlTZxvdMXuwV31qRiF_Cp-CINL5FgtBZTw9N4FVOrVtQ&_nc_zt=3&_nc_ht=scontent-ams4-1.oculuscdn.com&oh=00_AfKTrJjZIxybmCZBUScPAPKbmQ8mfAi4n1vKLb5rfgSAIQ&oe=6845873D"}, {"file_name": "screenshot3.png", "id": "1036771268273592", "uri": "https://scontent-ams4-1.oculuscdn.com/v/t64.5771-25/473393718_1036771271606925_2485325265061563247_n.png?_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=MyvfRAqqG6oQ7kNvwHoBaFC&_nc_oc=AdnvcIP1C5fO0GxPvj4ch29yMihjifNoboXJCf1AyqddR-hkb8F1MKKvlia13fgqXoI&_nc_zt=3&_nc_ht=scontent-ams4-1.oculuscdn.com&oh=00_AfITwyqpNKuBui5-CDLzVOeDKg8rBBhFidiB-61p4DSWGg&oe=68457764"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Landscape_2560x1440.png", "id": "652312050611976", "uri": "https://scontent-ams4-1.oculuscdn.com/v/t64.5771-25/473391946_652312053945309_4070145602436780626_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=bpgARWjEOigQ7kNvwFlnKO9&_nc_oc=AdmDquKEPKmk7Gk9Nb2LpX6Tb0AyWb-8IPdf6SjYKp7P3gPemYCRFVPTOWfBEcyggX0&_nc_zt=3&_nc_ht=scontent-ams4-1.oculuscdn.com&oh=00_AfLSx03VTKi6Nx0KvTFnJr4Mvltw2bOP0ITtFONPHQxptw&oe=684567BE"}, "display_name": "Animal Hotel Simulator VR", "keywords": ["animal", "hotel", "simulator", "action", "simulation"], "long_description": "Dive into a deep hotel management simulation! Craft your dream hotel from the ground up and optimize your services to attract guests from around the globe. Hire the best staff, master the art of hospitality, and climb the ranks to become the world's No.1 hotel owner!!\n\nI am hotel staff, and my job is to provide super service and keep this wild crew happy. Think you can handle the chaos? As a super hotel staff member, you'll provide Security and great customer service to all your animal guests.\n\nHere's what awaits you:\n- Super Security Challenges: Keep those mischievous animals from turning your hotel into a zoo!\n- Hilarious Animal Interactions: Get up close and personal with elephants, crocs, and hippos, each with their own crazy quirks.\n- Hotel Management Mayhem: Upgrade your facilities, hire staff, and build the ultimate animal paradise.\n- Immersive VR Experience: Feel like you're right in the middle of the action with stunning graphics and realistic animal behaviors.\n", "short_description": "Aim to be the world's best hotel owner! Manage a hotel bustling with animals and provide the highest level of service. Experience interactions with adorable animals and the depth of hotel management in this simulation game!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://newstory-inc.com/privacypolicy", "developer_terms_of_service_url": null, "publisher_name": "New Story Inc", "support_website_url": null, "website_url": "https://newstory-inc.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}, {"id": "2691890164460465"}, {"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "29601482622800245"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ams4-1.oculuscdn.com/v/t64.5771-25/473393652_28729421250005963_9206221269940275866_n.png?stp=dst-png_s360x360&_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Bjbt9unVb6oQ7kNvwFxRIA8&_nc_oc=AdlmhKZ1eVrUaQ31Cpwz8dBFWVwF7JOaJBxnZQnp02XL9OBDvn1iP9cY57dzZyUU_ks&_nc