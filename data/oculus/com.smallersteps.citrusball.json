 by tennis and pickleball."}]}, "developer_name": "Smaller Steps", "developer_privacy_policy_url": "https://citrusballgame.com/#privacy", "developer_terms_of_service_url": null, "publisher_name": "Smaller Steps", "support_website_url": null, "website_url": "https://citrusballgame.com/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "6971296739626223"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "592552653877036"}, {"status": "PUBLISHED", "id": "1520084615502676"}]}, "baseline_offer": {"id": "592552653877036"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1752549960, "status": "PUBLISHED", "id": "592552653877036"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "23926178030378162", "version": "1.1"}, "id": "938364484153392", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "6971296729626224", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "23926178030378162"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "938364484153392"}]}, "change_log": null, "created_date"