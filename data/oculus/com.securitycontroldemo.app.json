video_trailer": null, "video_trailer_thumbnail": {"file_name": "VideoThum.png", "id": "465105502695692", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/39035321_465105506029025_1294952427660291068_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=YQ-0kI1B8V8Q7kNvgHMVOxy&_nc_oc=Adjz6kl1Zv23GlykqsRiYRajIIhhbl31RLb53cFOpnj8PIKb3vT_Wfd7IZ3foqKP1_U&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AYB2HsfRnhXpbw9B0gmMHurp0JrDXtTSC_OGPbY5BV_JaQ&oe=67AB21BB"}, "display_name": "Security Control Demo", "keywords": [], "long_description": "Security Control is a 1-4 player co-op game where you unleash the chaos of your first day at your new job as you and your crew wake up, realizing you've fallen asleep. Team up to outwit the security guard, or assume the role of the guard and capture the intruders in this action-packed, job-quitting adventure.\n\nGAMEPLAY\n\nPlay With Friends\nEmbark on your escape mission solo, with a squad of friends, or join an online lobby to outwit the vigilant security team. Utilize every resource available to make your way out of the corporate maze before you're caught. Can you and your team uncover the secrets of the office and make your grand escape, or will you assume the role of the relentless security guard to thwart their every move?\n\nTRAVERSE EVER-CHANGING WORKPLACES\nPlay in one of ten diverse workplace settings, each uniquely designed to capture the essence of different professional environments. With procedurally generated layouts, no two missions are alike. Discover and navigate through various workplace hazards, security measures, and secrets, ensuring each escape endeavor provides a fresh and exhilarating challenge.\n\nFREE YOUR FRIENDS\nWhen you are caught it isn't over yet! Work with your friends to save yourself from jail and continue to complete the mission.", "short_description": "Security Control: A 1-4 player co-op game where you navigate the chaos of your first day on the job, either as employees oversleeping or as the security guard capturing them in an action-packed adventure."}]}, "developer_name": null, "developer_privacy_policy_url": "https://securitycontrolgame.com/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Greavette Games", "support_website_url": null, "website_url": "https://securitycontrolgame.com/", "category": null, "comfort_rating": null, "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER", "SINGLE_USER", "CO_OP"], "id": "7923473937686755"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1376207559746993"}]}, "baseline_offer": {"id": "1376207559746993"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1713821400, "status": "PUBLISHED", "id": "1376207559746993"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "9631490286885103", "version": "0.2.9.8"}, "id": "1131314921392739", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "7920312491336233", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "9631490286885103"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1131314921392739"}]}, "change_log": null, "created_date": 17383038