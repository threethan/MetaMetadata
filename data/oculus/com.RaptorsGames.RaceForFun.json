r the best lap time. Each car has its own characteristics. Unique trails have different levels of difficulty."}]}, "developer_name": "Kaplunov Yurii", "developer_privacy_policy_url": "https://raptorsreality.com/privacypolicy.html", "developer_terms_of_service_url": null, "publisher_name": "Raptor`s Games", "support_website_url": null, "website_url": "https://raptorsreality.com/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "673385183287495"}, {"id": "331529188132278"}, {"id": "1592192974313489"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ARCADE", "RACING", "SPORTS"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9884675641582617"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-sin2-2.oculuscdn.com/v/t64.5771-25/473393516_1009273264043408_5764651662021615606_n.png?stp=dst-png_s360x360&_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=bEn5BgBUCKIQ7kNvwGFBZxB&_nc_oc=AdkZ9cJUQrrbNcr-5wByALK4uFlI5G79JW8jgeVmCbDFI9BDy6FLcpCkCdN5qCDy1A8&_nc_zt=3&_nc_ht=scontent-sin2-2.oculuscdn.com&oh=00_AfFXSK-0QY2X4Pd3-2Or5_yDhBvGxVNGrfD3r0xMQIeg5g&oe=6818C569"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Race For Fun", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Racing", "id": "673385183287495"}, {"topic_name": "Simulation", "id": "331529188132278"}, {"topic_name": "Arcade", "id": "1592192974313489"}], "release_date": 1625784484, "lastR