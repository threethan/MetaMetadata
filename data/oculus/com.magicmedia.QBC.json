scription": "Elev8 Academy VR Demo offers a unique training experience with motion capture from professional Mike Giovando, allowing players to mirror and learn quarterback motions with real-time feedback. The demo also features realistic football physics, providing an authentic throwing experience. This free demo previews the full build's capabilities."}]}, "developer_name": null, "developer_privacy_policy_url": "http://elev8academyvr.com/privacy", "developer_terms_of_service_url": null, "publisher_name": "Vrtcl", "support_website_url": null, "website_url": "http://elev8academyvr.com/", "category": null, "comfort_rating": null, "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "2776542286000603"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "6160971017360780"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PENDING", "id": "1170894288447190"}, {"status": "PUBLISHED", "id": "1099971667972876"}]}, "baseline_offer": {"id": "1099971667972876"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 0, "status": "PENDING", "id": "1170894288447190"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "24011785335186074", "version": "3"}, "id": "414280224488539", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "6010572735733943", "is_