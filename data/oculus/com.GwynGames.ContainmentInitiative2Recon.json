pons and equipment."}]}, "developer_name": null, "developer_privacy_policy_url": "https://gwyngames.net/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Gwyn Games LLC", "support_website_url": null, "website_url": "https://gwyngames.net/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "459781504968480"}, {"id": "644354873106712"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "24082500961388786"}}]}, "scheduled_offers": {"count": 6, "nodes": [{"status": "PUBLISHED", "id": "854939950548880"}, {"status": "PUBLISHED", "id": "796310743005450"}, {"status": "PUBLISHED", "id": "1585854865723116"}, {"status": "PUBLISHED", "id": "1069768805334067"}, {"status": "PUBLISHED", "id": "1669267427040915"}, {"status": "PUBLISHED", "id": "618692934463319"}]}, "baseline_offer": {"id": "618692934463319"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1768939200, "status": "PUBLISHED", "id": "854939950548880"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "25077402321898640", "version": "1.3.3"}, "id": "1845974482843913", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "10024621434269975", "is_concept": false, "latest_released_b