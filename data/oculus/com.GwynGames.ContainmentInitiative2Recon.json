c_ohc=K1YuxEPaX8EQ7kNvwF51Bxq&_nc_oc=AdkVar9EA7fbJP5ZeJ556GTPqp3n136iP9R9hSzMCCDuYWVNWdNp1TQhUOhO85y8r7M&_nc_zt=3&_nc_ht=scontent-atl3-2.oculuscdn.com&oh=00_AfgTnAq31Tp7MBeh2IH70huiiFm-vn_yo0VhV8sGy5iRBg&oe=69282CDF"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "trailerss.png", "id": "1415229259669041", "uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/491822045_1415229263002374_1316771994605676758_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=dwiBCLAdIrkQ7kNvwFkVqvo&_nc_oc=Adl-TlPdtLVthJkjP4XXUAYN2zVDnLRRfkW6EtJbW73YglZPuFxGpBqdRxXo_deJtRY&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_AfhWt2c5uNXqsfXoCW79k4dpM3D27ckYj6s8i1Fy_CPBSQ&oe=69284C3F"}, "display_name": "Containment Initiative 2: Recon", "keywords": [], "long_description": "Containment Initiative 2: Recon is a singleplayer version of Containment Initiative 2 designed for standalone headsets.\n\n# Story:\nKay is your communications specialist and will provide mission details and valuable information while you try to find a means of escape.\n\n# Realistic Reloading:\nEach weapon handles and reloads just like it's real world counterpart. Mastering reloading is essential to surviving the horde!\n\n# Progression:\nUnlock guns by successfully obtaining supply drops or by finding hidden weapon caches scattered around Sector 4.\n\n# Dismemberment/Blood:\nHeadshots trigger a slow motion effect for maximum carnage!\n\n# Mission Types:\n- Supply drop missions require the player to put their money on the line for a chance to unlock better equipment.\n- Scanner Defense missions push the action to maximum by giving the player plenty of ammo and putting them on the defensive.\n- Take a slower pace and visit different areas for a chance to collect crafting materials and ammo\n", "short_description": "Explore, scavenge, and fight as you attempt to escape from Sector 4. Survive the horde by finding supplies to upgrade your gear, collecting supply drops and finding hidden caches of weapons and equipment."}]}, "developer_name": null, "developer_privacy_policy_url": "https://gwyngames.net/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Gwyn Games LLC", "support_website_url": null, "website_url": "https://gwyngames.net/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "459781504968480"}, {"id": "644354873106712"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "24082500961388786"}}]}, "scheduled_offers": {"count": 5, "nodes": [{"status": "PUBLISHED", "id": "796310743005450"}, {"status": "PUBLISHED", "id": "1585854865723116"}, {"status": "PUBLISHED", "id": "1069768805334067"}, {"status": "PUBLISHED", "id": "1669267427040915"}, {"status": "PUBLISHED", "id": "618692934463319"}]}, "baseline_offer": {"id": "618692934463319"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1765821600, "status": "PUBLISHED", "id": "796310743005450"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "25077402321898640", "version": "1.3.3"}, "id": "1845974482843913", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "10024621434269975", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "250