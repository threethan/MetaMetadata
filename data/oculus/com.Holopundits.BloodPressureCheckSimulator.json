": null, "video_trailer_thumbnail": {"file_name": "Blood Pressure Check Simulator_Screenshot_03.png", "id": "933691004936744", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/39031437_933691008270077_921704919857808857_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=1SN2zM-0vsAQ7kNvgEA61g5&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AYBK4ntv7zKkrPOXe9lqiks1e0yl-Se4CrsWn5CtsjIWjg&oe=67A9E2A6"}, "display_name": "Blood Pressure Check Simulator", "keywords": ["cna", "nurse", "nursing", "BP", "Healthcare"], "long_description": "The Blood Pressure Check Simulator app immerses participants in a realistic virtual environment where they can practice and refine their skills in measuring and interpreting blood pressure readings. This application guides users through the proper selection and use of a blood pressure cuff, the correct procedure for taking a reading, and the interpretation of the results.\n\nWith multi-language support (English, Spanish, French, and Arabic), this simulator ensures inclusivity and accessibility for a global audience.  \n\nThe experience is enhanced by an AI companion, acting as a virtual mentor, to answer questions to reinforce learning.\n \n# Key Features\n- Multi-Language Support: Tailored instructions and narrations in four major languages to cater to diverse learners. \n- AI Companion: An interactive assistant that responds to user queries, and provides clarifications to meet learning needs.(NOTE: Generative AI is still experimental)\n\n\n# Learning Objectives:\n- Remembering the steps of the procedure\n- Understanding the significance of systolic and diastolic values\n- Applying the correct technique in a virtual scenario\n- Analyzing the accuracy of readings\n- Evaluating the results against normal ranges \n- Creating a plan for patient follow-up based on the readings obtained\n\nThis comprehensive simulation ensures that certified nursing assistant's or anyone interested in the healthcare field are well-prepared to perform blood pressure measurements accurately and confidently in real-world clinical settings.", "short_description": "Designed to train CNAs in accurately measuring and interpreting patient blood pressure using an interactive, hands-on virtual scenario with AI guidance and multilingual support for better learning."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.holopundits.com/privacy-policy.html", "developer_terms_of_service_url": null, "publisher_name": "Holo Pundits", "support_website_url": null, "website_url": "https://www.holopundits.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "514087707868786"}, {"id": "478089301798119"}, {"id": "1043107547362485"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "ar", "fr", "es_MX"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "27776850205295810"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/38974788_544350578364689_3596253973155763826_n.png?stp=dst-png_s360x360&_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=S7e2myLk4ycQ7kNvgH1VV2u&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AYBiO3nlF1rnfvU4S666i1OCgXKq7g_eB1wsM3O3zwHBcA&oe=67A9E793"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Blood Pressure Check Simulator", "platform": "ANDROID_6DOF", "context_category": {"context_category": "APPS", "id": "362692211442873"}, "context_topics": [{"topic_name": "Hands-On Training", "id": "514087707868786"}, {