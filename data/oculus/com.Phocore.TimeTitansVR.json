 or dodge counter attacks. "}]}, "developer_name": "Phocore Inc.", "developer_privacy_policy_url": "https://www.phocoregames.com/privacy-policy", "developer_terms_of_service_url": "https://www.phocoregames.com/privacy-policy", "publisher_name": "Phocore Inc.", "support_website_url": null, "website_url": "https://www.phocoregames.com/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "3744040075834813"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ACTION", "SHOOTER", "STRATEGY"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "1085785358587291"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "1683776405906839"}, {"status": "PUBLISHED", "id": "1176641729600043"}]}, "baseline_offer": {"id": "1683776405906839"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1736601540, "status": "PUBLISHED", "id": "1683776405906839"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "6397749587008140", "version": "1.1"}, "id": "855481695627351", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "5458248330958275", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "6397749587008140"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name":