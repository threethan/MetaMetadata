r fear, and make it out alive.\n\n"}]}, "developer_name": null, "developer_privacy_policy_url": "https://sites.google.com/view/scaredyrat/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "LPorsomething", "support_website_url": null, "website_url": "https://sites.google.com/view/scaredyrat/home", "category": null, "comfort_rating": "COMFORTABLE_FOR_FEW", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "377978390272490"}, {"id": "331529188132278"}, {"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER"], "id": "8995807467117653"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "889272296627665"}]}, "baseline_offer": {"id": "889272296627665"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1733693820, "status": "PUBLISHED", "id": "889272296627665"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "9527844730580588", "version": "2.1"}, "id": "1524310668280038", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "8957228857642181", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "9527844730580588"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1524310668280038"}]}, "change_log": null,