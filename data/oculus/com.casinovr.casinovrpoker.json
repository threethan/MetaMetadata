h=00_AfbfABmG9FI1kjtChjoVhyJ2kQes2qkfm6Mo6yVfIbcqIQ&oe=68C17BA0"}, {"file_name": "migrated_image.png", "id": "2778828985504138", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/38974710_369274084006757_4844399866354335744_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=CI1iv3dyStEQ7kNvwHm-Hg0&_nc_oc=AdkZZvrbp2t9lBgA4kmzoQ2wsAatmeMSNxf0sSMc4TWf7upTWc8lecXWEyii84djd9o&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfZOXS8J6zcnMH6beu7hZ6bwD3WB7VdtYcZxUltn4Gvahg&oe=68C16DFC"}, {"file_name": "migrated_image.png", "id": "205414920805768", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/39031580_518243338950436_4337549547392729088_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=gADSBQ-0-D4Q7kNvwEo48Af&_nc_oc=AdmMQQUQ_YlIYKgLq6m8FHN0CMJqTjyDpyTXeYSRYW8961Jw67fQcFcmpJ-xuimB7RU&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfZyj_siTfEZKvHpIed-jQP5im1927wQJs-7kL76ViZtHw&oe=68C15005"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "/tmp/8OqhYX", "id": "886213865076064", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/38974459_886213868409397_7277970959523381248_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=QN8PjdY0g_cQ7kNvwHPvqb_&_nc_oc=Adl21YvOEZ-tJKvoPshYHGBbS52_GHL0lXo_sppcXKYE7vyuLDdrrR7R5w8s4UITJxA&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_Afah_Xrurp_AZ1BLKvlg0qyxnsYPqwj1AI73yg-DwectzQ&oe=68C17A2F"}, "display_name": "Poker VR", "keywords": ["poker", "casino", "casual", "social", "free"], "long_description": "The most popular social app on Oculus Go, now reimagined for Oculus Quest! Play poker like a pro, Poker VR is the only app to feature both multi table tournaments and cash games.  \n\nMain features: \n- Most expressive avatars built for reading tells with growing customisations  \n- Built for the quest, environments and avatars look sharp and clear \n- Play poker just like in real life, knock to check, throw your cards over the line to fold etc. \n- Feel like you are in a casino and not just at one table\n- Lose hours making chip stacks and doing chip tricks. Poker VR features physics-based gameplay at the table.  \n- Multi table tournaments with automated table balancing and live leaderboard. Tournaments support 100s of players at the same time. \n- Scope all casinos before joining a table. View where your friends are playing, their chip stack, and more.\n- Smart friends\u2019 system to invite your friends or join them directly at the table. \n- \u2026 and much much more!", "short_description": "The most popular social app on Oculus Go, now reimagined for Oculus Quest! Play poker like a pro, Poker VR is the only app to feature multi table tournaments and cash games. \n\nPlaying poker with friends was never more fun and social. Stimulated gaze tracking, facial expressions and full hand presence makes reading tells even more fun. Want to enjoy poker as a sport? Join 100s of other players in daily tournaments to hone your skill and prove to your friends that you are the best!"}]}, "developer_name": "Mega Particle, Inc.", "developer_privacy_policy_url": "https://www.pokervr.co/privacy-policy", "developer_terms_of_service_url": "https://www.pokervr.co/termsofservice", "publisher_name": "Mega Particle, Inc.", "support_website_url": "https://www.pokervr.co/", "website_url": "https://www.pokervr.co/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1676485315806614"}, {"id": "1023770785722696"}, {"id": "275629210202852"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["CASUAL", "SOCIAL", "SPORTS"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "sup