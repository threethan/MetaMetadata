_sid=6e7a0a&_nc_aid=0&_nc_ohc=hr9QMD0QQacQ7kNvwGvwkUI&_nc_oc=AdkHBHGDeDZooybQ39o1kgGYO1mvkoPnyNZuKobz_-jICrXnzmFhlZriF-FjQEFCIKE&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfJvNOjSy-aBFxFkHjE_8joHvFnaj_llNYBWh0Lmzwo_QA&oe=6820843B"}, {"file_name": "NotAlone_hard_0515.mp4.png", "id": "1834560810346274", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/75485576_1834560817012940_5193466785641071115_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=CDy51_yfqckQ7kNvwF2CybU&_nc_oc=Adnl-oIbiFVz-1FpVPxoWJzQePq8fN3Eg76r8hsMOdhONs81RfQg2JtDSLNLHNY0eNg&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfIdNlHbQL8RwjH5HGfGnsDTgv_kDZFmWI6oBIJPUmVs5Q&oe=68207F3A"}, {"file_name": "LunarSync_MRC.png", "id": "542493068760919", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/38982477_542493072094252_5868106385987132485_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=AxwZ6Ya1ADwQ7kNvwFq6jn_&_nc_oc=AdmYyYBYfKiGtwprX7HhU7hm-b9oMbUbjfFPAq2to05QgzCa87xyh87LUbYrNjvPdZ4&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfI0oBrKaRmEgVLaRkJ5J4GLu5RXP8CU72ZsEUJDf2ovHA&oe=6820788B"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "video_cover.png", "id": "820996726026654", "uri": "https://scontent-sin11-2.oculuscdn.com/v/t64.5771-25/86610562_820996729359987_4412348469651450537_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Xd2G2gn9n7kQ7kNvwFycMMK&_nc_oc=AdkvEmmv0TZgsxhm0C-hVb3jL1N7pc3NEzlokYrSZ3uy8qifot4jZBzzxxf3riqEUCY&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_AfJE94GVThhyPZ2VL2AbFOTVKuJZp6HU1xWEefRPutaP7A&oe=682076D9"}, "display_name": "Rhythm Pop!", "keywords": [], "long_description": "Rhythm Pop! is an addictive Music Rhythm VR Game using your hands to beat waves of bubbles with Rock, Paper, Scissors hand gestures!   \n\nHere's the twist:\nIn our \"Classic RPS Mode\", you would beat the bubbles by making the OPPOSITE gestures.  That's right, Rock beats Scissors, Scissors beat Paper, and Paper beats Rock!\n\nFor those looking for a more conventional rhythm game experience, in \"Copycat\" mode, copy the SAME hand gestures as the bubbles to pop them.  \n\nIt may look simple, but it's hard to master!  How fast can you move your hands?!  This is a hand-tracking only experience.\n\nThe game currently includes 14 fantastic songs with 5 difficulty levels (Baby, Easy, Normal, Hard, Crazy).  The song genre ranges from pop, rock, classical, to EDM.", "short_description": "An addictive Music Rhythm VR Game with a twist: use your hands to beat waves of bubbles with Rock, Paper, Scissors hand gestures!  This is a hand-tracking only experience."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.hypercreative.games/privacy-rhythmpop", "developer_terms_of_service_url": null, "publisher_name": "hyperCreative Games", "support_website_url": null, "website_url": "https://linktr.ee/rhythmpop", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1672577396871977"}, {"id": "1592192974313489"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["HAND_TRACKING"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9004269699701704"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/75476595_2250722861941990_1613619129447981166_n.png?stp=dst-png_s360x360&_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=ZuqpdqRYb3MQ7kNvwEBGTAT&_nc_oc=AdmymW6iw0w7qfBk9X6ofLN9m6CV-dS6CBD-K8m9eqR4r2ATSds_d8VZqv0_CdvUWBc&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfJ6MEpwYxhmSW55AbHr0AT