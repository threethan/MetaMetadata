_sid=6e7a0a&_nc_aid=0&_nc_ohc=UmWWSwQLmWkQ7kNvwGiX3--&_nc_oc=Adm4u9jUYxE1v1IX9keDE2szFWppwnJuie8M_axgwXq9ejbK1TtPsN9midTsXfXiq6s&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfG8nDC8RIjoL30jUzrImzVRqYul5IIHLZrWMsDkD_HAGw&oe=67F8F73B"}, {"file_name": "NotAlone_hard_0515.mp4.png", "id": "1834560810346274", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/75485576_1834560817012940_5193466785641071115_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=570cWBaCQAEQ7kNvwHF6Xcr&_nc_oc=AdnqA0i_szXB1GTz_J-teQ1oAZ1YU_uTUEAI4kIr_yoaHuBslPJzJBIEB7A62IkcMmQ&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfGVOR1LPCYhbLsngoqqSGbSWb5604Y_sCvZOysPPloQKg&oe=67F8F23A"}, {"file_name": "LunarSync_MRC.png", "id": "542493068760919", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/38982477_542493072094252_5868106385987132485_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=LktDV0HxEYAQ7kNvwG0NizP&_nc_oc=Adlp3OBOtD7DN1ZVT9Hj_52Z74TCZyjjlVwkHDWPqnoeb83RbdscKaRXfxEoQA5Bo-Q&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfENE16AgoyTk4k1SDrRb7UEKY0amO5cDpyiT825fVW8Fg&oe=67F8EB8B"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "video_cover.png", "id": "820996726026654", "uri": "https://scontent-sin11-2.oculuscdn.com/v/t64.5771-25/86610562_820996729359987_4412348469651450537_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=3du19MnHEfgQ7kNvwFz015C&_nc_oc=Adl63N75DBXj_JbdioeV977bSQHHZi60o9y5h60Fkf-TRqPuvCxjqD3D9wDBFk0ITW4&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_AfE1_IlTYYx8DAcVsEg3X_Z8eYWI84IhDRu4tdEVxoOO0Q&oe=67F8E9D9"}, "display_name": "Rhythm Pop!", "keywords": [], "long_description": "Rhythm Pop! is an addictive Music Rhythm VR Game using your hands to beat waves of bubbles with Rock, Paper, Scissors hand gestures!   \n\nHere's the twist:\nIn our \"Classic RPS Mode\", you would beat the bubbles by making the OPPOSITE gestures.  That's right, Rock beats Scissors, Scissors beat Paper, and Paper beats Rock!\n\nFor those looking for a more conventional rhythm game experience, in \"Copycat\" mode, copy the SAME hand gestures as the bubbles to pop them.  \n\nIt may look simple, but it's hard to master!  How fast can you move your hands?!  This is a hand-tracking only experience.\n\nThe game currently includes 14 fantastic songs with 5 difficulty levels (Baby, Easy, Normal, Hard, Crazy).  The song genre ranges from pop, rock, classical, to EDM.", "short_description": "An addictive Music Rhythm VR Game with a twist: use your hands to beat waves of bubbles with Rock, Paper, Scissors hand gestures!  This is a hand-tracking only experience."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.hypercreative.games/privacy-rhythmpop", "developer_terms_of_service_url": null, "publisher_name": "hyperCreative Games", "support_website_url": null, "website_url": "https://linktr.ee/rhythmpop", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1672577396871977"}, {"id": "1592192974313489"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["HAND_TRACKING"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9004269699701704"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/75476595_2250722861941990_1613619129447981166_n.png?stp=dst-png_s360x360&_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=xSthI91Lp-QQ7kNvwFsBRa_&_nc_oc=AdnA18jVyX6oH96FWUmrejJWVHDOLDG_6R7Bexd7Jaqkwb-kDWrpNlN29cTn5w_GnBc&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfETJTqE3mQ8r1ek4ptIT4S