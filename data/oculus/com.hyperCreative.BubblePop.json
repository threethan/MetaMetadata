ideo_trailer_thumbnail": {"file_name": "echo_2560.png", "id": "379905024899686", "uri": "https://scontent.oculuscdn.com/v/t64.5771-25/39001715_379905028233019_3723091916722888668_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=dzoMdwFmSJEQ7kNvwEI_Kc8&_nc_oc=Adk65DEYpV7AVTspm9MVjLqDA4fz3Cf8sEHmB5cHIN2ZrOYep1Ode9ISixOwrftQ85U&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AfMXXY77L1wiiVGn2qAVEYWWfhlFN4QvABVi-VuNTVNwoA&oe=686130C1"}, "display_name": "BubblePop! Rock Paper Scissors", "keywords": [], "long_description": "BubblePop! Rock Paper Scissors is an addictive Music Rhythm VR Game with a twist:  Use your hands to beat waves of bubbles with Rock, Paper, Scissors hand gestures!   \n\nInstead of making the same gesture as the bubbles, you need to make the gesture that beats them.  You've known it since your childhood: Rock beats Scissors, Paper beats Rock, and Scissors beat Paper!  How fast can you move your hands?! \n\n This is a hand-tracking only experience.\n\nThe game includes 1 fantastic song that is free-to-play, with 5 difficulty levels (Baby, Easy, Normal, Hard, Crazy).  You can preview the 13 additional music tracks which are available as in-app-purchase.", "short_description": "An addictive Music Rhythm VR Game with a twist:  Use your hands to beat waves of bubbles with Rock, Paper, Scissors hand gestures!  This is a hand-tracking only experience."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.hypercreative.games/privacy", "developer_terms_of_service_url": null, "publisher_name": "hyperCreative Games", "support_website_url": null, "website_url": "http://hyperCreative.games/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1672577396871977"}, {"id": "1592192974313489"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_a