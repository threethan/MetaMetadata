ler": null, "video_trailer_thumbnail": {"file_name": "2560x1440.png", "id": "2530296700641628", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/491818849_2530296703974961_2959736677113405401_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=st3Ox_JmoRoQ7kNvwEtt_Iu&_nc_oc=Adm56A6Rr3Vy-wvD2nq5FPzZbRR9Gquga_Cqa0Zng0tMtF8q4gD1M4l7h2H3EMdamdY&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfmiuW8K5exERNcrK2e6R_GUpihd2KkP2SHJgdHCqPJ1Og&oe=69356897"}, "display_name": "Days of Heroes: Stalingrad (Demo)", "keywords": ["Shooter", "War", "Action", "ww2", "FPS"], "long_description": "Experience the Siege of Stalingrad Like Never Before\nImmerse yourself in the most intense chapter of World War II in this cinematic VR action adventure. Every shot, every explosion, and the roar of artillery draw you deeper into the chaos of urban combat.\n\n25 Adrenaline\u2011Fueled Missions\nFrom frontal assaults on fortified lines, through daring sabotage operations, to relentless trench battles\u2014each mission delivers unique challenges and pure adrenaline in this VR shooter.\n\nFull VR Immersion\n\u2022 Traverse ruined factories, rubble\u2011strewn streets, and muddy trenches\n\u2022 Take cover under a rain of bullets and feel the thunder of artillery around you\n\u2022 Operate heavy machine guns, sniper rifles, grenades, and explosive charges\n\u2022 Command a tank in a crushing armored assault and unleash the power of steel beasts\n\nTwo Perspectives\nLive the conflict as both a Soviet soldier and a German combatant\u2014each side fighting for victory in the ultimate World War II showdown.\n\nPrepare for a visceral VR odyssey that will test your courage, skill, and resolve in an action\u2011packed, historical setting.\n\n\ud83c\udfaf Play Days of Heroes: Stalingrad today and carve your name into history!", "short_description": "Step into the shattered streets of Stalingrad in this epic single\u2011player VR shooter!!! Face brutal urban assaults, daring sabotage runs, fierce trench warfare, and heart\u2011stopping sniper duels in World War II action. Feel a rain of bullets and the thunder of artillery as every street becomes a battlefield. Command a tank in a crushing armored assault! 25 cinematic missions, two perspectives\u2014both striving for victory. Do you have the courage to change history? Play Days of Heroes: Stalingrad now!!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://dohvr.com/privacy_policy/stalingrad/", "developer_terms_of_service_url": null, "publisher_name": "Zen Apps Studio", "support_website_url": null, "website_url": "https://dohvr.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "459781504968480"}, {"id": "2691890164460465"}], "external_subscription_type": "REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["GAMEPAD"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "28319695394312052"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1761557651133597"}]}, "baseline_offer": {"id": "1761557651133597"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1752260400, "status": "PUBLISHED", "id": "1761557651133597"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "32569010982713784", "version": "1.0.6"}, "id": "624622313360876", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_appl