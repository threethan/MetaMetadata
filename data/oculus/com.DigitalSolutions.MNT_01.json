video_trailer_thumbnail": {"file_name": "landscape.png", "id": "689845179995120", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/38974855_689845183328453_919185214266887576_n.png?_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=q_w2O9ES4KMQ7kNvgFol28i&_nc_oc=AdhHjKmHVL2jQilIhEz0s-2bdOIqn30x_kPiH9w-ubpkBEBo5p2mhTentNYEOUO3Ok4&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYDh-2uGYXgbOzklwItvNAa5sDDRsHRratz4FfhZUeSXUw&oe=67CAC770"}, "display_name": "Secret History of Mongolia", "keywords": [], "long_description": " As users navigate through the app, they are transported to different sites, each meticulously rendered to provide a realistic sense of presence in those places. Accompanying these visual experiences are detailed explanations and narratives, shedding light on the historical importance of each location, its role in the \"Secret History of Mongolia\", and insights into the Mongolian culture and heritage. This app is a unique blend of history, technology, and storytelling, offering users an engaging and educational journey through Mongolia's rich past.", "short_description": "The VR app focused on the places mentioned in the \"Secret History of Mongolia\" is an immersive educational experience. It features stunning 360-degree images of various historical and culturally significant locations associated with the Mongolian Empire's history."}]}, "developer_name": null, "developer_privacy_policy_url": "https://digitalsolutions.mn/policy.html", "developer_terms_of_service_url": null, "publisher_name": "DigitalSolutionsVR", "support_website_url": null, "website_url": "https://digitalsolutions.mn/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1043107547362485"}, {"id": "726368587961803"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOV