": null, "video_trailer_thumbnail": {"file_name": "cover_wimpsody.png", "id": "4561781057237876", "uri": "https://scontent-hkg1-2.oculuscdn.com/v/t64.5771-25/75240098_4561781060571209_5102327165194826822_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=KBIgqjYEe3YQ7kNvwEfVNbu&_nc_oc=Adkt45mv8-lvyuJB5GKGM5WQCDln91YBU68mOGyQb_PW7NM8T-3EUUatLevZe1GQvRU&_nc_zt=3&_nc_ht=scontent-hkg1-2.oculuscdn.com&oh=00_Afiw0GZfEN4_5t2pGQ5IUhn0K08E92vxBDjbtdPNL6BSig&oe=692AEE73"}, "display_name": "Wimpsody", "keywords": [], "long_description": "With Wimpsody, you'll just have to seat back, relax and watch trippy animation that will match your music. \nWith the demo section, you'll discover the psychedelic Baron Samedi and the space ship orchestra's tunes, all locally, with no need for an internet connection or extra headphones, just your headset.\nWith the Spotify section, you can listen to your music on Spotify while watching Wimpsody's animations, it requires a Spotify Account, and will be much more capable with a premium account. You'll also need to listen to you Spotify music an an other device than your headset. Headphones are warmly recommended to be fully immersed.\nEnjoy :)", "short_description": "Wimpsody allows you to do visual trips with music in VR"}]}, "developer_name": null, "developer_privacy_policy_url": "https://wimpsody.com/privacy", "developer_terms_of_service_url": null, "publisher_name": "Antoine Germanique", "support_website_url": null, "website_url": "https://wimpsody.com/", "category": "ENTERTAINMENT", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "434961926215971"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": ["ART_CREATIVITY", "MUSIC", "RELAXATION_MEDITATION"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor":