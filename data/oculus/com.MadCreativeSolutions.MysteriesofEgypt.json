 that can lead to your freedom."}]}, "developer_name": null, "developer_privacy_policy_url": "https://madcreativestudios.com/?page_id=238", "developer_terms_of_service_url": "https://madcreativestudios.com/?page_id=246", "publisher_name": "Mad Creative Studios", "support_website_url": "https://www.madcreativestudios.com/contacts/", "website_url": "https://www.madcreativestudios.com/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "377978390272490"}, {"id": "2691890164460465"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "it"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "1928130380906101"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1433931960738096"}]}, "baseline_offer": {"id": "1433931960738096"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1692637410, "status": "PUBLISHED", "id": "1433931960738096"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "9340279086027935", "version": "1.1.0"}, "id": "112292658630137", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "6371436539578886", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "9340279086027935"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id"