Have you ever wanted to play a game with a fun, community, game modes, and story line? Well then this is the game for you.\n\nIn this game what happens is, the more people who join the game the better the game gets. So if you love fun events, or updates then join the Banana Company 2 community!\n"}]}, "developer_name": null, "developer_privacy_policy_url": "https://sites.google.com/view/bannacompanyoffical?usp=sharing", "developer_terms_of_service_url": null, "publisher_name": "Zane2evil", "support_website_url": null, "website_url": "https://66d7a5532d916.site123.me/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1023770785722696"}, {"id": "2691890164460465"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH", "GAMEPAD"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER"], "id": "23922271450692374"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent.oculuscdn.com/v/t64.5771-25/473393941_1132605785238849_5573771144057102146_n.png?stp=dst-png_s360x360&_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=TkQi4qOsZngQ7kNvwEurB8h&_nc_oc=AdlKw3JHFNTKgWPJ7cZ3fzFVf7qki_NlG3Yna7K0XF7rX9F5uAR4UHxtbo0TiUajDp0&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AfMFZPenWN566fouASVmGNhfS-uUcIzabQoBBtmrZH6a4A&oe=68568A69"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Banana Company 2", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Hangout