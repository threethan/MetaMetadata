_thumbnail": {"file_name": "VideoThumb_2560x1440.png", "id": "698638202067330", "uri": "https://scontent-sjc6-1.oculuscdn.com/v/t64.5771-25/38982719_698638205400663_289794054610158311_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=0WsUQl7lwOAQ7kNvwFxkoUk&_nc_oc=AdleE90H5BvA9ydee0SRkBLaDVfA1CrrOPMTSK5u4-nzhKoT8VSt0NDSxWHzqOE06Rc&_nc_zt=3&_nc_ht=scontent-sjc6-1.oculuscdn.com&oh=00_AfWKOhzqNaKef2uwxyAeDMKg1AZ2jMvuV9Z8LtkOLbwuHA&oe=68B6D4DD"}, "display_name": "SHARKS", "keywords": [], "long_description": "Eat your friends in 'SHARKS' a cross-platform (PC and VR) multiplayer hunting game. Hunt with groups of up to 10 people online. Eat whatever you can to survive and avoid the zombie sharks: last player standing wins. It's like Gorilla Tag with sharks.\n\nThe VR controls are super responsive (it feels like a human-shark mind meld): simply look where you want the shark to swim. Pull the trigger to move forward, press the grip button to chomp/eat things.", "short_description": "Eat your friends in 'SHARKS' a cross-platform (PC and VR) multiplayer hunting game. Hunt with groups of up to 10 people online. Eat whatever you can to survive and avoid the zombie sharks: last player standing wins. It's like Gorilla Tag with sharks."}]}, "developer_name": null, "developer_privacy_policy_url": "https://ezone.com/privacy", "developer_terms_of_service_url": null, "publisher_name": "Ezone PTY LTD", "support_website_url": null, "website_url": "https://ezone.com/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_