our bridging hand on a surface for shot making."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.privacypolicies.com/live/7f300f7a-199f-4d0f-84fe-cc5f25d19420", "developer_terms_of_service_url": null, "publisher_name": "Ric Haines", "support_website_url": null, "website_url": "https://killvrpool.wordpress.com/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["SPORTS", "SOCIAL"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER", "SINGLE_USER"], "id": "333889428613370"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "386198232685670"}]}, "baseline_offer": {"id": "386198232685670"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1621960620, "status": "PUBLISHED", "id": "386198232685670"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "6466990056737695", "version": "0.11.2"}, "id": "2839746609641065", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "3225521214217945", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "6466990056737695"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "2839746609641065"}]}, "change_log"