description": "Levitate designs spaces to balance inner and outer experiences.  These realms are created to illuminate self reflection and well-being."}]}, "developer_name": "Levitate, Inc.", "developer_privacy_policy_url": "https://www.levitate.world/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Levitate, Inc.", "support_website_url": null, "website_url": "https://levitate.world/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "810139357995560"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["RELAXATION_MEDITATION"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["HAND_TRACKING", "OCULUS_TOUCH"], "supported_player_modes": ["SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "25965804709675818"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/491818916_3296026050573370_7372791593817732539_n.png?stp=dst-png_s360x360&_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=GRMvZKM623YQ7kNvwHHuJ3c&_nc_oc=Admx955_xFEPmG1JQCVzPRAQ80w44C2XAsnT4xbXZmEkC0ff-f2QBg73OW7I518XqHQ&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_AfguXzyizmO0Jaf9S5NQpkbH6hrKKBjygpqQ5OJrBEVmlw&oe=69130FBF"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Levitate", "platform": "ANDROID_6DOF", "context_category": {"context_category": "APPS", "id": "362692211442873"}, "context_topics": [{"topic_name": "Fitness & Wellness", "id": "810139357995560"}], "release_date": 1651374000, "lastRevision": {"nodes": [{"id": "110397838312820", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {