_nc_oc=Adgws1R4PzVg6ZkVrgYEU4J1yzTEEWo8bwNVaiR-PeCnXMOK6TKOvgQ6HHexlX8ynFo&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AYAYZVhLpz-3fh0lz2G7jj7kVev0_Np9OVCXdFoGrNck3Q&oe=67C5952E"}, {"file_name": "Screen4_VRflowA320_Demo_2560x1440.png", "id": "220952026560670", "uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/38974834_220952033227336_2720568371537345287_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=6_rsXmVu_rAQ7kNvgGxO8yM&_nc_oc=AdjvfZELzKn3LNUoliE3nsq9XXInKL0SFe-HV0m62aJPTNrTDTUmFWoDXvnccfD1zco&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AYAOZySiHnV8-8mbPEBGZowwoSi5IzzlpfwgOoau39qBCw&oe=67C59D49"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "VRflow Oculus Store trailer_thumb.png", "id": "1170281000112103", "uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/38974503_1170281006778769_5305616749076049116_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=cfNgJhVULjgQ7kNvgHZQnpw&_nc_oc=Adj4Ecgl4Tei36dbUV7PM038zdr5tvk6KKvmZZor9hq4-WY_R2b5RzwbZF0NhaKwLGk&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AYDuNJ056ttYvvWkGsJB4Jc0QwbnU529nkdaLwwCcJeyLA&oe=67C5813B"}, "display_name": "VRflow A320", "keywords": [], "long_description": "- This is the individual/student version of VRflow -\n\nVRflow A320 is the interactive cockpit procedure trainer for pilots. Train procedures in training scenarios that incorporate detailed aircraft behavior including sounds, instruments and cockpit layout.\n\nVRflow incorporates numerous features for efficient learning and aircraft familiarization, ideal for type ratings. Train with or without visual help, track your errors, see your training time, train with an automatic crewmember and much more.\n\n17 normal procedures and 10 memory items are included.\n\nVRflow also contains Explore Mode, where you can explore the cockpit and learn about systems. Explore Mode can be used as a VR cockpit poster with Poster Mode.\n\nThe individual/student version does not include multi-crew functionality.", "short_description": "- This is the individual/student version of VRflow -\n\nVRflow A320 is the interactive cockpit procedure trainer for pilots. The app incorporates advanced VR learning technologies, making it an extremely efficient learning and training tool for pilots doing their initial or recurring type rating training.\n\nThe individual/student version does not include multi-crew functionality.\n\nAll purchases of VRflow will be upgraded as we add more procedures, fix bugs and make VRflow better."}]}, "developer_name": null, "developer_privacy_policy_url": "https://vrpilot.aero/privacy", "developer_terms_of_service_url": "https://vrpilot.aero/terms-of-service/", "publisher_name": "VRpilot", "support_website_url": null, "website_url": "https://vrpilot.aero/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "514087707868786"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["FLYING", "EDUCATIONAL", "UTILITY"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "631170615232170"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/38982602_5825180217556197_1070491614610685998_n.png?stp=dst-png_s360x360&_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=O6vPvnvdEf8Q7kNvgEQ0vd6&_nc_oc=AdjFk05VhPJTk34gFB4NP3TeHAz3FI4LDhGV25tMuxitaT82Uhf54yRpE-isdsoaD8E&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AYBUFz0pXTPvrL5iRbGsYQMBImgSFdbZpBy4Yhwxfql0oQ&oe=67C59D08"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_