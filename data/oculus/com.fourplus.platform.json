kthroughs and simulations provides exposure to a real-life facility, without the risks associated with in-person training.  "}]}, "developer_name": null, "developer_privacy_policy_url": "https://fourplus.co.uk/accelerate-privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "FourPlus Immersive", "support_website_url": null, "website_url": "https://fourplus.co.uk/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1043107547362485"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9188422801221850"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "1330903001396213"}, {"status": "PUBLISHED", "id": "1462122827810414"}]}, "baseline_offer": {"id": "1330903001396213"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1739368080, "status": "PUBLISHED", "id": "1330903001396213"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "25839422932361908", "version": "0.1.4"}, "id": "2034292766996142", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "9188422791221851", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "25839422932361908"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_na