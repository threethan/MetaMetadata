e learning."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.raum.app/privacy-policy/#app", "developer_terms_of_service_url": null, "publisher_name": "RAUM", "support_website_url": null, "website_url": "https://www.raum.app/", "category": "APPS", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1063584910766706"}, {"id": "1819021644882647"}, {"id": "2687655338230691"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": ["PRODUCTIVITY"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE", "STANDING", "SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["CO_OP", "MULTI_USER"], "id": "7438464409611972"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/39035463_829480138902154_7639516930637919260_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=WAAE98t2GoMQ7kNvgFT6Qq2&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYD40SeXfd6K7arzm8__FB1tY522Fsm-ceuZkplu-xzWDg&oe=67A09443"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "RAUM", "platform": "ANDROID_6DOF", "context_category": {"context_category": "APPS", "id": "362692211442873"}, "context_topics": [{"topic_name": "Productivity", "id": "1063584910766706"}, {"topic_name": "Creativity", "id": "1819021644882647"}, {"topic_name": "Events", "id": "2687655338230691"}], "release_date": 1628201199, "lastRevision": {"nodes": [{"id": "959054994927452", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "30734