ideo_trailer_thumbnail": {"file_name": "BH-Demo-CoverLandscape-2560x1440@3x.png", "id": "1166103434215700", "uri": "https://scontent-syd2-1.oculuscdn.com/v/t64.5771-25/39036670_1166103437549033_1399066160305085332_n.png?_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=QPKFeC3Y5KkQ7kNvwETcHY-&_nc_oc=Adl140fCq1OqLZtS1A13gFrLRYxVEhkwen3hmBCZNNsKayhgVdbJKjQ4qpgv7DivOzQ&_nc_zt=3&_nc_ht=scontent-syd2-1.oculuscdn.com&oh=00_AfQ8J-pkvPolpvGAMScz6BGQe2LycLX1eU9Eiy1zpz3mrw&oe=686FA8E5"}, "display_name": "Blacktop Hoops - Alpha", "keywords": [], "long_description": "Blacktop Hoops is the world\u2019s first competitive VR streetball game. Players can chain together flashy dribble tricks, shots, and high-flying dunks to finish off opponents the globe in epic games of 1-on-1 streetball. The game was designed with the most realistic basketball mechanics ever built for virtual reality. Play against dynamic AI with mocap animations from pro streetball players and dunkers. Ball out to an original 90s-inspired soundtrack with narration from legendary emcees Bobbito Garcia and WNBA legend Renee Montgomery. Unlock 30+ kicks from leading indie brands and shoe designers. Become the G.O.A.T.\n", "short_description": "Chain together flashy dribble tricks, shots, and high-flying dunks to finish off opponents in epic games of 1-on-1 streetball in Blacktop Hoops.\n"}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.blacktophoopsvr.com/privacy-policy", "developer_terms_of_service_url": "https://www.blacktophoopsvr.com/terms-of-use", "publisher_name": "Vinci Games", "support_website_url": "https://www.blacktophoopsvr.com/support", "website_url": "https://www.blacktophoopsvr.com/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}, {"id": "331529188132278"}, {"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres