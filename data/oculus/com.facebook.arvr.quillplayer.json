er_thumbnail": {"file_name": "/tmp/0v0bKO", "id": "431345634210355", "uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/75269160_431345637543688_191274663425343488_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=As17DDXMLq0Q7kNvwGll5Ba&_nc_oc=AdmOT90lhzAE7HhQFAMF0f-qPsZCVcXMKKMW5h40jBXHH8rvO4vtZtgMbHdjJTit8WA&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_AfgkLsnZ6vM2JuiflqguRjqdvvFsq6rDgRX083-XG1DD2w&oe=69147470"}, "display_name": "Quill Theater", "keywords": ["quill", "art", "media", "story", "narrative"], "long_description": "Step into wonderful, immersive VR stories, hand painted and animated in VR with Quill. These VR stories not only let your view a painting on a wall, but transport you into the painting itself. Experience expressive characters, fantastic creatures, and complete narratives in these VR animations. . \n\nRegularly updated, the Quill Theater lets you visit high quality immersive  animations from the comfort of Oculus TV, bringing you the best in immersive entertainment. \n\nThese immersive animations are fully 6DOF, some including multiple viewpoints to let your move positionally within the art, and the ability for you to both \u201cPause\u201d the story to take in every brush stroke, and \u201cStart\u201d to bring the story back to life! \n\nTo create your own VR stories, test your hand in the VR illustration and animation tool, Quill, available on the Rift platform. https://www.oculus.com/experiences/rift/1118609381580656/\n\nRequires the latest version of Oculus TV.", "short_description": "Step into wonderful VR stories, hand painted and animated in VR with Quill. These VR stories not only let your view a painting on a wall, but transport you into the painting itself. Experience expressive characters, fantastic creatures, and complete narratives in these VR animations. \n\nRegularly updated, the Quill Theater lets you visit high quality immersive  animations from the comfort of Oculus TV, bringing you the best in immersive entertainment. \n\nRequires the latest version of Oculus TV."}]}, "developer_name": "Facebook Technologies, LLC", "developer_privacy_policy_url": "https://www.oculus.com/legal/privacy-policy/", "developer_terms_of_service_url": "https://www.oculus.com/legal/terms-of-service/", "publisher_name": "Facebook Technologies, LLC", "support_website_url": "https://quill.fb.com/support/", "website_url": "https://quill.fb.com/", "category": "EARLY_ACCESS", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "434961926215971"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["NARRATIVE", "MEDIA_PLAYER", "ART_CREATIVITY"], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "1022951591901985"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "441100279911646"}]}, "baseline_offer": {"id": "441100279911646"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1576003570, "status": "PUBLISHED", "id": "441100279911646"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "9560411357338608", "version": "2.0.137 : vr_only"}, "id": "431812764355983", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "2515021945210953", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "9560411357338608"}, "release_s