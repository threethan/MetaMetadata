er software powering multiplayer Texas Hold\u2019em action."}]}, "developer_name": "Lucky VR Inc.", "developer_privacy_policy_url": "http://www.getluckyvr.com/privacy-policy/", "developer_terms_of_service_url": "https://www.pokerstars.com/poker/ps-vr/tos/", "publisher_name": "The Stars Group Inc.", "support_website_url": null, "website_url": "http://www.starsgroup.com/", "category": "EARLY_ACCESS", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1676485315806614"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["STRATEGY", "SOCIAL", "CASUAL"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER"], "id": "947216472618112"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "578617912645734"}, {"status": "PUBLISHED", "id": "1184051545573403"}]}, "baseline_offer": {"id": "578617912645734"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1558136220, "status": "PUBLISHED", "id": "578617912645734"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "23910195035232167", "version": "CP155hf1.3_Quest64_155-1"}, "id": "297349780982051", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "2370815932930055", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "23910195035232167"}, "release_status": "RELEASED"}, "binary_rele