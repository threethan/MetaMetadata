_trailer": null, "video_trailer_thumbnail": {"file_name": "2560x1440_Screenshot_4.png", "id": "845987028249729", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/499307451_845987031583062_7186945399041819738_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=F6OaWraNp10Q7kNvwHtstlg&_nc_oc=AdmkztnQuCF5gt7JlsHPPFMlBamOKdj16yv8pL_-w577ETM-1kWsAsa_zWkUHtRm-ek&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfpYDaW_e8Y4LqBIukL0i7kb2IITFrpEaqwLFAQwi8PLwg&oe=696B6E84"}, "display_name": "Dash Runner VR", "keywords": ["Fitness", "Runner", "Dash", "Exercise"], "long_description": "Dash Runner VR turns natural head movement into a fast-paced, low-impact VR fitness runner where motion drives every run.\n\nPlayers dash to avoid incoming obstacle blocks red and orange, and collect coins as rewards as speed and intensity increase. \n\nSmooth mechanics, responsive feedback, and short, repeatable runs make Dash Runner VR ideal for both quick workouts and extended play sessions just use your smart watch to monitor your workout. \n\nWhether you\u2019re chasing high scores or exploring a fun, accessible way to stay active in VR, every run challenges your reflexes, focus, and endurance.\n\nKey Features are \nHead-Driven Gameplay \u2013 Jump and dodge using subtle, natural head movements while facing forward\n\nDynamic Shield Tracking \u2013 A protective shield follows the headset position in real time\n\nController Haptics \u2013 Tactile feedback enhances impacts when players hit obstacles. \n\nLow-Impact Fitness Design \u2013 Encourages movement without requiring exaggerated physical effort\n\nPause Anytime \u2013 Press Y on the left controller to pause gameplay\n\nOrientation Re-Centering \u2013 Perform the \u221e (infinity) hand sign to realign orientation at any time and make sure you are position not too close behind the shield \n\nRoom-Scale Safety Awareness \u2013 Gameplay encourages players to remain centered within their boundary which is 90 in \u00d7 90 in \n\nShort, Repeatable Runs \u2013 Ideal for quick sessions or longer fitness-focused play\n", "short_description": "Dash Runner VR is a fast-paced virtual reality runner that gets you moving. Dash sideways, squat or jump to dodge obstacles, collect rewards, and push your limits in an immersive experience designed to make fitness feel like play."}]}, "developer_name": null, "developer_privacy_policy_url": "https://docs.google.com/document/d/1Zh9tl6yEtLCusq2FdHfURQO1WCMd15h2/edit?usp=sharing&ouid=106204558232171403572&rtpof=true&sd=true", "developer_terms_of_service_url": null, "publisher_name": "Tony Walker Studios", "support_website_url": null, "website_url": "https://tonywalkerstudios.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1592192974313489"}, {"id": "459781504968480"}, {"id": "2776542286000603"}, {"id": "673385183287495"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "25193060647042621"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "907009235189507"}]}, "baseline_offer": {"id": "907009235189507"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1767726360, "status": "PUBLISHED", "id": "907009235189507"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "25193123310369688", "version": "0.1.1"}, "id": "578737665334371", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nod