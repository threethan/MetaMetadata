c_oc=Adkh1SfmX2V-FKCiDN4aGxbPmqEELmbdpIte-_lymSJXDidCuTPBtaomQuezveZ6F_E&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AfPP1ST2dZqcHvUS1q5RLMyP6IN8WEqLN1eLKPGD6X924A&oe=68569F08"}, {"file_name": "cdf4d1fe80c81ed28ce5ca2951e198ba.png", "id": "668591601373714", "uri": "https://scontent.oculuscdn.com/v/t64.5771-25/38984448_668591604707047_2151042290361045954_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=ZzV1vv884soQ7kNvwHfOtAI&_nc_oc=AdnBSNFtAAKBhjijMihlf-pZUiM4ryvg1RX6Ht9MgWYoLb3FwBiIRLk_oe_bHSsN1d0&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AfPVtOMBhfPEj9K7fbEk6FeaFsawxNlpe8coQoHTQwahbw&oe=6856AB2F"}, {"file_name": "IMG_6792_2560x1440.png", "id": "590705416560911", "uri": "https://scontent.oculuscdn.com/v/t64.5771-25/57575360_590705419894244_1790132388911216295_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=ANaOWxDSMNkQ7kNvwEfRdp1&_nc_oc=Adk_oEvKpCJX0KTnETu8wYHZ8jlNI4dX8x56JtVtp6KVzyNYt82OcBniz2TDQK-be6c&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AfOJWKutWEq4REjwYHUeFHi7jsVFL0iHD5MIudTo2y-hCg&oe=6856AC85"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Lifeguard VR", "keywords": [], "long_description": "The American Red Cross offers lifeguard training courses year-round, working with local partners to provide science-based training that combines hands-on, classroom, and online instruction for lifeguards working in a pool and waterfront setting. A large component of lifeguarding involves cognitive skills such surveillance and visual scanning to identify distressed swimmers and drowning people. However, it is challenging to simulate the distractions and environmental factors necessary for this training in a classroom or online training environment. This virtual reality experience created by the ARC Innovation team aims to train future lifeguards in core cognitive skills such as drowning identification. ", "short_description": "Test and hone your lifeguard surveillance skills by identifying drowning people in American Red Cross's virtual reality lifeguard training experience."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.redcross.org/privacy-policy.html", "developer_terms_of_service_url": null, "publisher_name": "American Red Cross", "support_website_url": null, "website_url": "https://www.redcross.org/", "category": "APPS", "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "514087707868786"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["EDUCATIONAL"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8624889677592434"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent.oculuscdn.com/v/t64.5771-25/38974652_552883967394821_8315611085858578770_n.png?stp=dst-png_s360x360&_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=nwM9uARGKJwQ7kNvwFow8Dd&_nc_oc=Adm9O_330Kg9R-Pfo4oMf4H3Zx3uxI3DQBVmYKIGGtgFzm7P5xQc9f3wbnUUiI75Ym0&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AfPKVOKYqZpC497-0trcO6wPJjdMHgHLM8OcUipFro76UQ&oe=6856AE10"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Lifeguard VR", "platform": "ANDROID_6DOF", "context_category": {"context_category": "APPS", "id": "362692211442873"}, "context_topics": [{"topic_name": "Hands-On Training", "id": "514087707868786"}], "release_date": 1707228000, "lastRevision": {"nodes": [{"id": "103276885663006", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "4894014924013280", "is_first_party": false, "enables_in_app_ads": false, "is_concept": 