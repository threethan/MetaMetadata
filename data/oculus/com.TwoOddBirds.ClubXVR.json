_sid=6e7a0a&_nc_ohc=nRzomU1NZakQ7kNvwGSoF4y&_nc_oc=AdmrnLNz353JVzxUPB9EEJ-Bkn0awqex_U58oM_tK9zLrB9IJk6XPIyLqC0kURyv-S8&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfSQc9kZK0FptwbXsOHfm1b6mypVL_mReXvKWPIYWbMfWg&oe=687A4736"}, {"file_name": "2560.png", "id": "1632805540985931", "uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/75375771_1632805544319264_3920356593054675382_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=90b0TbZlw54Q7kNvwF1sfyS&_nc_oc=AdnIhskT-RISySbaILqgJX4mTeVZUeJ6YX3hVKBNrkAz_1fgRQ-P7JRNPAYIG62JpQs&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfRD--zpniY6ONohrqco7cuNrFB8RW2wVcTV74fMreFVFQ&oe=687A4AA5"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "2560_clubXvrprebeta.png", "id": "576757408485408", "uri": "https://scontent-sin6-1.oculuscdn.com/v/t64.5771-25/39036609_576757415152074_841334173352369828_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=vx1WvPUpc1wQ7kNvwFyQIFy&_nc_oc=AdnR_b6Pm3-iWgvCpH8bnkNBiGqR8iWefEyiZTXQaXLFgKTgIlQNmuKI7iLFS1WUWaw&_nc_zt=3&_nc_ht=scontent-sin6-1.oculuscdn.com&oh=00_AfTBexKqeUlRJnA8Zvzda5l1oWDezad8rbL0QHteoCQGdQ&oe=687A4AA5"}, "display_name": "ClubXVR", "keywords": [], "long_description": "Imagine a virtual reality space designed purely for connection and relaxation. \nWelcome to Club XVR, a unique social lounge built especially for VR, where socializing feels natural and effortless and most of all FUN. \nClub XVR is more than just an app; it's a place to unwind, meet new people, and share experiences in a comfortable, immersive setting.\n\nWith Cool clubs, Private hangouts and even personal apartments, you can\nconnect with people all across the world from all walks of life who share your interest in VR. Share your thoughts and ideas, build genuine connections, even share your favorite YouTube videos.\n\n    Be part of a growing community thats brand new to VR.\n ClubXVR is made to be a dynamic space that evolves with its users. As the community grows, you'll discover new connections and opportunities for shared experiences.", "short_description": "Explore a one-of-a-kind VR social lounge where members can connect in a relaxed, immersive environment. Designed to be a dynamic space, the app offers public areas where users can socialize and enjoy personalized content, like a private YouTube player and interactive social settings."}]}, "developer_name": "2 Odd Birds", "developer_privacy_policy_url": "https://clubxvr.wixsite.com/home/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "2 Odd Birds", "support_website_url": null, "website_url": "https://clubxvr.wixsite.com/home", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1023770785722696"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER", "SINGLE_USER"], "id": "8525172207583237"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "626337066488880"}]}, "baseline_offer": {"id": "626337066488880"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1734822540, "status": "PUBLISHED", "id": "626337066488880"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "9621889757911471", "version": "1"}, "id": "1107296917464490", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "A