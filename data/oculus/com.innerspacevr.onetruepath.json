_nc_ohc=EJ3zfO_346oQ7kNvwG_LX1x&_nc_oc=AdkJj5usd_jv0CQM3sMMlWOqCex1Gow0HhWCWMVSmdrAim1OWjZmjKD-fL4iNu_J6iM&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AfVxolJEJeHjjP3MJK34M-pzdzy8eGHpcnyvXaln82dLGw&oe=68A86202"}, "display_name": "One True Path", "keywords": ["onetruepath", "OneTruePath", "sword", "shooter", "puzzle"], "long_description": "YOU are drawn into the imaginary worlds of renowned adventure books author Jack Stevenson, on a fantastical adventure to uncover the dark secrets behind his last masterpiece\u2026\n\n# TRAVEL THROUGH FANTASTICAL WORLDS\n\nExplore the fictional worlds Stevenson created through his books, each featuring a different visual style and genre, from heroic-fantasy through science fiction to post-apocalyptic western.\n\n# ENGAGE IN A WIDE RANGE OF ACTION & PUZZLE GAMEPLAY\n\nExplore 3 worlds with different gameplays based on his famous Fiendish Fiends\u00a9 series: \n- In **Army of Evil**, use your sword skills to fight against the Army of Evil and its leader the Grey Knight with your magical blade\n- In **Butchers of Orion**, use your telekinetic and mind control powers to solve riddles and escape the ignominious butcher ship of Lady Oriana\n- In **Prisoner of the Cyberhounds**, stunt your enemies with your shock hand and blast them with your cyber shooter to step down the new sheriff Cassidy ruling on the wasteland penitentiary\n\n# EXPLORE THE MYSTERY OF THE AUTHOR\u2019S DEATH\nLearn about his personal rise and fall and start to unveil his ultimate project.\nDiscover clues, secrets and personal notes scattered across the worlds to decipher his life behind the books and the true meaning of your journey, tearing down the boundaries between each fictional world and reality.\n\nBut there is one more twist, mind the cracks in the worlds ! Use portals to cross between these worlds and their gameplays, in true mind bending multiverse fashion !\n", "short_description": "YOU are drawn into the imaginary worlds of renowned adventure books author Jack Stevenson, on a fantastical adventure to uncover the dark secrets behind his last masterpiece"}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.termsfeed.com/live/1e06831f-b759-4751-9ca4-99cbf88c206b", "developer_terms_of_service_url": null, "publisher_name": "InnerspaceVR", "support_website_url": null, "website_url": "https://www.innerspacevr.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2691890164460465"}, {"id": "377978390272490"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "7792161540886044"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "628185279577558"}, {"status": "PUBLISHED", "id": "2198340057193626"}]}, "baseline_offer": {"id": "628185279577558"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1740157200, "status": "PUBLISHED", "id": "628185279577558"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "8078214842280711", "version": "1.048"}, "id": "852505783514837", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "7658526007582932", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "8078214842280711"}, "release_status": "RELEASED"}