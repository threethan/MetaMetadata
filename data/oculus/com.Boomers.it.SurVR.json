 place in this world for yourself."}]}, "developer_name": "Boomers.IT", "developer_privacy_policy_url": "https://boomersitgames.com/survprivacypolicy.html", "developer_terms_of_service_url": null, "publisher_name": "BoomersIT", "support_website_url": null, "website_url": "https://boomersitgames.com/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1990104051447093"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ADVENTURE", "CASUAL", "EXPLORATION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "918308685548358"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "5147930111924001"}]}, "baseline_offer": {"id": "5147930111924001"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1643918700, "status": "PUBLISHED", "id": "5147930111924001"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "5312953385455884", "version": "0.2"}, "id": "435325708178320", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "4670951002989462", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "5312953385455884"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "435325708178320"}]}, "change_log": "Bug fixes\nNew feature - fireplace!", "c