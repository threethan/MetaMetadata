nkey. Cause chaos and have fun."}]}, "developer_name": "Bozo's Studio", "developer_privacy_policy_url": "https://docs.google.com/document/d/188XgsCdFHaJnoXAmNOMrpxFm7K6t3U-jKzyjhbxP2w8/", "developer_terms_of_service_url": null, "publisher_name": "MasterBozo", "support_website_url": "https://discord.gg/dNSWP4n5K2", "website_url": "https://sites.google.com/view/mighty-chimps", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "2691890164460465"}, {"id": "1023770785722696"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER"], "id": "24260570346895745"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ord5-2.oculuscdn.com/v/t64.5771-25/499314643_2438267466566333_7238902990150356615_n.png?stp=dst-png_s360x360&_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=L2mK_O2odo4Q7kNvwFJDJv5&_nc_oc=AdkfwYm6A-2tU30icJwgdcjc1sTHsPoZIcNzFls7psumfxUTaM2z7wDJloR0SHUYOXY&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AfaSjCuplpFjR90H9zz4gkvCa8wzVqaIY2PJB9v4K3XHYg&oe=68CD5F02"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Mighty Chimps", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Action", "id": "459781504968480"}, {"topic_name": "Adventure", "id": "2691890164460465"}, {"topic_name": "Hangout", "id": "1023770785722696"}], "release_date": 1705696501, "lastRevision": {"nodes": [{"i