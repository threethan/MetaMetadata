_description": "Bring your devices together"}]}, "developer_name": null, "developer_privacy_policy_url": "https://help.motorola.com/hc/apps/privacy/product/2024-02/en-us", "developer_terms_of_service_url": null, "publisher_name": "Motorola Mobility", "support_website_url": null, "website_url": "https://help.motorola.com/hc/apps/smartconnect/sc70/en-us/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1063584910766706"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "cs", "da", "nl", "en_US", "en_GB", "fi", "fr", "de", "el", "it", "ja", "ko", "nb", "pl", "pt_BR", "pt", "ro", "ru", "zh_CN", "zh_HK", "zh_TW", "es", "sv", "tr"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9518612734874741"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/473391978_1169072194761721_5656453708357512488_n.png?stp=dst-png_s360x360&_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=zUBZvxmbLTYQ7kNvwFXmBl6&_nc_oc=AdnC8hxriaoRDFFuxs7Zz3Ccg5XHKD1zcblMTFgj4pHRvzK00SiE3qgvWxqh4K-A2GY&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfFy1KgU_FdxQJs_cHoe1sMjV_GRLTzFk8a3WJO-RKmEiA&oe=6814A864"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Smart Connect", "platform": "ANDROID_6DOF", "context_category": {"context_category": "APPS", "id": "362692211442873"}, "context_topics": [{"topic_name": "Productivity", "id": "1063584910766706"}], "release_date": 1745254560, "lastRevision": {"nodes": [{"id"