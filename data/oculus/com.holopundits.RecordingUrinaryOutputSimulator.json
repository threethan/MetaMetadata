_ohc=Qh7B4ydTobMQ7kNvgHkbvbo&_nc_oc=AdjnB4A1QIUCGHyZKsaVoiYYVaH8Oh8l_IwgnQ33jrK0AUz0umCAORVZ0j4GgtQcygc&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AYGUOYbqNmYxotrz_c5kGYeex83W5NKs4CNkrQwFNVnIjQ&oe=67D7FAC9"}, {"file_name": "Recording_urinary_output_SS_05.png", "id": "1112731786543003", "uri": "https://scontent.oculuscdn.com/v/t64.5771-25/75462205_1112731789876336_7874361370153734845_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=ZoMpdnR2zvgQ7kNvgHrK2WV&_nc_oc=Adi_BH_Ls_fvgUqY7cK6GrJR7KcdYIvSTisJ_o5zdMcT6GXTXuF0iOirc_gePkcohEo&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AYGWpVobmxO8G2jcS_fdvcIdTgD7u6M0LokU-J3ogFPRgA&oe=67D800CF"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Recording_urinary_output_SS_02.png", "id": "595629112986504", "uri": "https://scontent.oculuscdn.com/v/t64.5771-25/38974582_595629116319837_3672331500502065539_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=QyvaS5IyEc0Q7kNvgGlFGWQ&_nc_oc=Adgx5fvzByjwMUtA09QfCAcO46NEPFVpSpxRNuNymAwTXBRLPX4rX8RMsMCcuZ5F0k0&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AYGdCxCzz9ow7mmK0D9VFVSrQzr8oGoRJnuWubT0Y1oY7Q&oe=67D81425"}, "display_name": "Recording Urinary Output Simulator", "keywords": ["CNA", "nurse", "nursing", "urinary", "Healthcare"], "long_description": "In this tutorial, learners will engage in a VR simulation to measure and record urinary output. The process includes preparation, patient communication, measurement, and documentation.\n\nWith multi-language support (English, Spanish, French, and Arabic), this simulator ensures inclusivity and accessibility for a global audience.  \n\nThe experience is enhanced by an AI companion, acting as a virtual mentor, to answer questions to reinforce learning.\n \n# Key Features: \n- Multi-Language Support: Tailored instructions and narrations in four major languages to cater to diverse learners. \n- AI Companion: An interactive assistant that responds to user queries, and provides clarifications to meet learning needs.(NOTE: Generative AI is still experimental)\n\n By the end of this tutorial, participants will be able to: \n- Remember the steps for measuring and recording urinary output. \n- Understand the importance of accurate measurements in patient care. \n- Apply the procedures in a clinical setting to ensure patient safety and data accuracy. \n- Analyze and identify potential issues or abnormalities in urinary output. \n- Evaluate their techniques and make necessary adjustments for improvement. \n- Create accurate and comprehensive records of urinary output for further clinical evaluation. \n\nThis comprehensive approach ensures CNAs are well-prepared to assist patients with feeding efficiently and compassionately in real-world clinical settings. ", "short_description": "Learn how to accurately measure and record urinary output to ensure precise patient care with AI guidance and multilingual support for better learning."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.holopundits.com/privacy-policy.html", "developer_terms_of_service_url": null, "publisher_name": "Holo Pundits", "support_website_url": null, "website_url": "https://www.holopundits.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "514087707868786"}, {"id": "478089301798119"}, {"id": "1043107547362485"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "ar", "fr", "es_MX"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "28764512229814211"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scon