It is a real helicopter flight. You never have seen Las Vegas from above like THIS! And you hang outside of the helicopter."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.vrtravelx.net/home/vegas", "developer_terms_of_service_url": null, "publisher_name": "VRtravelX", "support_website_url": null, "website_url": "https://www.vrtravelx.net/", "category": null, "comfort_rating": null, "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "726368587961803"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8087215344625845"}}]}, "scheduled_offers": {"count": 4, "nodes": [{"status": "PUBLISHED", "id": "1561181304554581"}, {"status": "PUBLISHED", "id": "949109597314222"}, {"status": "PUBLISHED", "id": "553654700792266"}, {"status": "PUBLISHED", "id": "1446903949295134"}]}, "baseline_offer": {"id": "1446903949295134"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1747674000, "status": "PUBLISHED", "id": "1561181304554581"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "10016340291713331", "version": "2.0"}, "id": "394397666534833", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "8087215334625846", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "10016340291713331"}, "release_status": "RELEA