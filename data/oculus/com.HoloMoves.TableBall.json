=00_AfNQucGraw3JO7fEwu0nMHrOoEHdBAf28WE5HsPYHMJvcg&oe=6861286E"}, {"file_name": "screenshot4.png", "id": "697657545817259", "uri": "https://scontent.oculuscdn.com/v/t64.5771-25/39031569_697657549150592_9084133421503538669_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=kxXidB_WP4sQ7kNvwHzI5M3&_nc_oc=Adkys57T_wPh8m88_bhcWEV65lT2rFhZG7sf777ASMunlrmfbo6KOx3FkyTICxXPK_U&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AfMAKoRDzwJJGRiFlLgil_jaMZ735YUUKMmFdyOCM5UeXw&oe=68611B2E"}, {"file_name": "screenshot5.png", "id": "894905578797895", "uri": "https://scontent.oculuscdn.com/v/t64.5771-25/75269157_894905582131228_448450545322347244_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Dln3zsjbWWUQ7kNvwG9u6nv&_nc_oc=AdnO3D0liFZys3MxqM7VXIoJX9Cp_OlYNR23Jav1jZnZlAhq1b07b_0z6g9R9lZ3x34&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AfOg5EnIJfrW2FmOP-WTdr9frAbx7F9nnR-Fl0J4yjUbiQ&oe=68611CCC"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "FirstFrame.png", "id": "347211281466264", "uri": "https://scontent.oculuscdn.com/v/t64.5771-25/38982708_347211284799597_2383655566166517173_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=feRx9RA3WIcQ7kNvwHPtqnY&_nc_oc=AdmNeXCd1mkmDqq38CR1XkWAK6kHfboKSdIlutwvh30MpbcujWpZPsIESNwqDwAEZbc&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AfNmjikcPw-s0Bz5ftsic60q6K-_AHyZypBq-E2ZJAd1VQ&oe=68613A06"}, "display_name": "TableBall", "keywords": [], "long_description": "At HoloMoves we make games and applications that help patients regain their mobility and quicken their transfer from the hospital to back home.\n\nTableBall is a physical therapy game specifically designed for arm-hand training during brain injury rehabilitation. With a quick setup, the patient can enjoy an understandable and easy to learn game, while unconsciously training. \n\nTableBall makes use of a complex set of parameters constantly tweaked based on the performance of the player. With these parameters the game is able to assess the capabilities of the player and change gameplay accordingly. Thus making sure each player is training on their own level.", "short_description": "At HoloMoves we make games and applications that help patients regain their mobility and quicken their transfer from the hospital to back home.\n\nTableBall is a physical therapy game specifically designed for arm-hand training during brain injury rehabilitation. With a quick setup, the patient can enjoy an understandable and easy to learn game, while unconsciously training. "}]}, "developer_name": null, "developer_privacy_policy_url": "https://holomoves.nl/en/privacy/", "developer_terms_of_service_url": null, "publisher_name": "HoloMoves B.V.", "support_website_url": null, "website_url": "https://www.holomoves.nl/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "810139357995560"}, {"id": "478089301798119"}], "external_subscription_type": "REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["HAND_TRACKING"], "supported_player_modes": ["SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "7132987440113407"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "711208841273825"}, {"status": "PUBLISHED", "id": "2384115061795509"}]}, "baseline_offer": {"id": "711208841273825"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1748856960, "status": "PUBLISHED", "id": "711208841273825"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "9960932560652200", "version": "1.3.2.4"}, "id": "427561402936702", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binarie