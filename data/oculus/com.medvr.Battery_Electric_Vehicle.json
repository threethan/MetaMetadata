railer_thumbnail": null, "display_name": "Certified EV Technician", "keywords": ["EV", "Battery", "Immerselearn", "Training"], "long_description": "Certified EV Technician is an advanced VR training app designed to provide hands-on experience in electric vehicle technology. This interactive simulation covers:\n\n-Types of EVs\n-Battery Technology\n-Electric Motors \n\nTrain in a realistic, risk-free virtual environment to develop critical skills in EV maintenance, troubleshooting, and technology. Whether you're an aspiring technician or a professional looking to enhance your expertise, Certified EV Technician equips you with the knowledge and practical experience needed for the growing EV industry. Start your training today!", "short_description": "Step into the future of automotive technology with Certified EV Technician! This immersive VR training app provides hands-on experience with EV types, battery technology, and electric motors. Learn essential skills in a realistic virtual environment to enhance safety, efficiency, and technical expertise. Perfect for aspiring EV technicians and professionals looking to upskill in the evolving electric vehicle industry."}]}, "developer_name": null, "developer_privacy_policy_url": "https://immerselearn.com/privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "Immerselearn", "support_website_url": null, "website_url": "https://immerselearn.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1043107547362485"}, {"id": "514087707868786"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_