MS Titanic. With a focus on realism and immersion, this multi-layered horror experience mixes puzzles and escape mechanics without compromise."}]}, "developer_name": "Globiss Interactive LLC", "developer_privacy_policy_url": "https://www.globissinteractive.com/titanicasbprivacypolicy", "developer_terms_of_service_url": null, "publisher_name": "BoomBox Games", "support_website_url": null, "website_url": "https://www.globissinteractive.com/coming-soon-01", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2691890164460465"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ADVENTURE", "ACTION", "HORROR"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "6991490680931796"}}]}, "scheduled_offers": {"count": 6, "nodes": [{"status": "PUBLISHED", "id": "550979750675876"}, {"status": "PUBLISHED", "id": "1113865896892089"}, {"status": "PUBLISHED", "id": "1141849484171271"}, {"status": "PUBLISHED", "id": "495364279986038"}, {"status": "PUBLISHED", "id": "1787073268484718"}, {"status": "PUBLISHED", "id": "1023737192029214"}]}, "baseline_offer": {"id": "1023737192029214"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1744995600, "status": "PUBLISHED", "id": "550979750675876"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "8546152058798976", "version": "0.2"}, "id": "733712628116786", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": 