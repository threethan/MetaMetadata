 closer."}]}, "developer_name": "MitchellGamz ", "developer_privacy_policy_url": "https://sites.google.com/view/thenightguard/home", "developer_terms_of_service_url": null, "publisher_name": "MitchellGamz", "support_website_url": null, "website_url": "https://sites.google.com/view/thenightguard/home", "category": null, "comfort_rating": "COMFORTABLE_FOR_FEW", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "644354873106712"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9701738513206581"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1673231796640835"}]}, "baseline_offer": {"id": "1673231796640835"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1747778400, "status": "PUBLISHED", "id": "1673231796640835"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "9711454852234947", "version": "0.46"}, "id": "2007496583405825", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "9568098686570565", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "9711454852234947"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "2007496583405825"}]}, "change_log": null, "created_date": 1747539703, "id": "97