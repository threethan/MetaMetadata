j6&_nc_oc=Adlrcbb6jTulB-FK6twsX9CPNzb7H-31vkLd6lWnEgZOgxwxy10z_95uOTc_1-432GY&_nc_zt=3&_nc_ht=scontent-ord5-1.oculuscdn.com&oh=00_Afr-bbkGnDc6r51me09yEruPQ1B-ORyGutciJBASyz9ACw&oe=697E0C5F"}, {"file_name": "Screenshoot_Applab_3.png", "id": "1328471114521780", "uri": "https://scontent-ord5-1.oculuscdn.com/v/t64.5771-25/38982671_1328471117855113_685468836251971060_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Sakva2TLQJwQ7kNvwEocOMt&_nc_oc=AdlqnEZqRnZPSO635I9ou14PNQaQ2Go_C6zvRXsL91Tp-Ro77VG78J_v1g3TMXCod_Q&_nc_zt=3&_nc_ht=scontent-ord5-1.oculuscdn.com&oh=00_AfpvphDPt3dcKiM1tpaU1VVvNuQbFSbQ5yF_Yf6o_1P6NA&oe=697DF1D2"}, {"file_name": "Screenshoot_Applab_2.png", "id": "1383184772300771", "uri": "https://scontent-ord5-1.oculuscdn.com/v/t64.5771-25/39031449_1383184775634104_2848305183515865790_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=nVreH5hUgPYQ7kNvwGex3KN&_nc_oc=AdkRfPvbKjtZBx_MKeaGDY2syyhW3vv1nyKnU7NX8tMK6ny-1g3DuozV_xsRUBt8YYE&_nc_zt=3&_nc_ht=scontent-ord5-1.oculuscdn.com&oh=00_Afo3Tx3tw-pmpmHyop9Xj3JAz92lUaolx4zfwYtkr0G7Pw&oe=697DFEA5"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "VIDEOCOVER.png", "id": "919237919541961", "uri": "https://scontent-ord5-2.oculuscdn.com/v/t64.5771-25/39001654_919237922875294_3330695461102676473_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=_mlmhPUJljEQ7kNvwE7_gBu&_nc_oc=Adnp21456YcLs3giB8z9VoOMQzFu6AcYSi74fybrgpEmCe-bJ4xSONbujhIVI0GXpGo&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_Afq5c-b-hmdBvc4an7dUiEmtRlrYyIklb-WBLkILKu8r4Q&oe=697E09E0"}, "display_name": "Carta Nostra", "keywords": [], "long_description": "-Travel to a beautiful island\nImmerse yourself in the sounds and colours of a small Sicilian town, a carefully crafted and hand-painted setting and characters\n\n- A story of revolt against Mafia oppression\nBecome a protagonist in an engaging story of struggle against the injustices of the underworld, help the animal citizens to redeem their rights and their territory.\n\n- Choose which cards to play!-\nDefeat the mafia by choosing the cards in your deck, you must be careful: not all cards bring positive results\n\n- Sensational visual effects\nEvery time you play your card, you activate its effect: energy bombs, storms, music and much more...", "short_description": "Carta Nostra is an educational virtual reality game, a compelling interactive narrative dealing with the theme of the Mafia. You will have a pack of cards to defeat the mafia, each card will have positive or negative consequences throughout the city. Make your choice and play the card!\n"}]}, "developer_name": null, "developer_privacy_policy_url": "https://lab.digitalmosaik.com/en/carta-nostra-privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "DigitalMosaik", "support_website_url": null, "website_url": "http://www.digitalmosaik.com/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2691890164460465"}, {"id": "1676485315806614"}, {"id": "1043107547362485"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["it"], "supported_input_devices": ["GAMEPAD"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "6181564018546685"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ord5-1.oculuscdn.com/v/t64.5771-25/38974692_1149684546009139_7602248288164393664_n.png?stp=dst-png_s360x360&_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=xqhGImUBLu8Q7kNvwHbdNYS&_nc_oc=AdmBcvO65cXXE0BRjYSVmmicstiooZLlPhyfKF_KRomFeVWObFArCVHUgrNZUdQdlxM&_nc_zt=3&_nc_ht=scontent-ord5-1.oculuscdn.com&oh=00_Afp7MV3DnNfNwXRCOpgmOFBUuVmYjZkSg6CIGRdSk408yw&oe=697DF430"}, "is_concept": fal