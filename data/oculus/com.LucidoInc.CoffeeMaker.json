splay_name": "Coffee Sim VR", "keywords": [], "long_description": "Step into the role of a barista in Coffee Sim VR, a dynamic and immersive coffee-making experience set in a beautiful beachside caf\u00e9. With customers arriving throughout the day, your challenge is to prepare and serve their favorite drinks as quickly and accurately as possible before time runs out. Using intuitive VR controls, craft everything from simple black coffee to elaborate espresso-based drinks, complete with milk frothing and flavor customizations. The game\u2019s realistic mechanics and time-based challenge create an exciting blend of skill, speed, and efficiency. As the demand increases, you\u2019ll need to multitask, manage ingredients, and keep up with the rush to satisfy your customers. Whether you're a coffee enthusiast or just looking for a fun and engaging VR challenge, Coffee Sim VR delivers a fast-paced and rewarding experience in a relaxing beachside setting.", "short_description": "Coffee Sim VR is a fast-paced virtual coffee shop experience set on a scenic beach. Serve a steady flow of customers by preparing their favorite drinks under time pressure while managing multiple orders"}]}, "developer_name": null, "developer_privacy_policy_url": "https://lucido.life/policy/coffeesim/", "developer_terms_of_service_url": null, "publisher_name": "Lucido Inc", "support_website_url": null, "website_url": "https://lucido.life/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}, {"id": "377978390272490"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_pla