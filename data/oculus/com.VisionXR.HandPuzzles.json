video_trailer_thumbnail": {"file_name": "HandPuzzlesCombined_2560x1440.png", "id": "1104024341747718", "uri": "https://scontent-lhr8-1.oculuscdn.com/v/t64.5771-25/473392082_1104024345081051_126250748999625745_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=EgoPe1DaFRwQ7kNvwGe41gl&_nc_oc=AdnkKo5h4vWGeJ-smSm9i21xN7SkXXddPNGJSpyK-XhwsQAtQILmedSq_3NCV0kbvr4&_nc_zt=3&_nc_ht=scontent-lhr8-1.oculuscdn.com&oh=00_AfQt0l8j24PkMACRTU2awePHhQS10Bo8eZEu2djjDWgokA&oe=68908B62"}, "display_name": "Hand Puzzles", "keywords": ["TrickPuzzles", "MixedRealityPuzzles", "HandPuzzles", "LogicGamesMR", "SlideBockGame"], "long_description": "Challenge Your Mind Like Never Before \u2014 In Mixed Reality!\nStep into the world of Hand Puzzle MR, a beautifully immersive collection of four timeless logic games, reimagined for your real-world space. Whether you\u2019re solving puzzles on your desk or in the middle of your living room, the handcrafted wooden aesthetics and intuitive hand-tracking bring these classics to life in an entirely new way.\n\n\ud83e\udde0 Sharpen your logic. Train your focus. Enjoy endless replayability.\n\n\ud83e\udde9 Included Games:\nSlide the Block\nShift wooden blocks on a dynamic grid to free the main piece. With increasing complexity across levels, you'll test your spatial awareness and strategic planning.\n\nTowers of Hanoi\nThis legendary logic puzzle comes alive in MR! Stack and move glowing discs across rods to recreate the tower. Easy to pick up\u2014tough to master.\n\nStick Puzzles\nRearrange glowing Sticks to fix equations or create visual patterns. Simple at first glance, yet deeply rewarding and endlessly challenging.\n\nMarble Solitaire\nA test of foresight and precision\u2014jump marbles over one another to clear the board. Beautifully animated and enhanced with realistic interaction.\n\n\ud83c\udfae Designed for Mixed Reality\n\nInteract naturally using your hands\u2014no controllers needed\n\nPlay puzzles on real surfaces like your table or floor\n", "short_description": "Solve Puzzles in Mixed Reality!\nChallenge your brain with four timeless games, reimagined for MR:\n\n\ud83d\udfea Slide the Block \u2013 Shift tiles to free the main block\n\ud83d\udd35 Towers of Hanoi \u2013 Stack and solve this legendary logic puzzle\n\ud83d\udfe8 Stick Puzzles \u2013 Rearrange matchsticks to correct the equation\n\ud83d\udfe3 Marble Solitaire \u2013 Jump and eliminate marbles with strategy\n\nStep into a handcrafted puzzle world that blends seamlessly with your real space!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.visionxr.co.in/handpuzzlesprivacypolicy", "developer_terms_of_service_url": null, "publisher_name": "VisionXR", "support_website_url": null, "website_url": "https://www.visionxr.co.in/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "377978390272490"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["HAND_TRACKING"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9461584370607148"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1262472538778726"}]}, "baseline_offer": {"id": "1262472538778726"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1747656480, "status": "PUBLISHED", "id": "1262472538778726"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "9956791151086465", "version": "1.3"}, "id": "534163419577195", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_bina