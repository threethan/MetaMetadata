hort_description": "Monke Horror is a terrifying VR game where you and your friends explore an abandoned facility, trying to survive relentless monsters. These creatures stalk your every move, using sight and sound to hunt you down. Stay quiet, hide, and work together to escape before it\u2019s too late. Every shadow hides danger\u2014will you make it out alive, or will the monsters get you first?"}]}, "developer_name": "Banan Games", "developer_privacy_policy_url": "https://enderdragon0928.wixsite.com/monkephobia/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Banan Games", "support_website_url": null, "website_url": "https://enderdragon0928.wixsite.com/monkephobia", "category": null, "comfort_rating": "COMFORTABLE_FOR_FEW", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "644354873106712"}, {"id": "2691890164460465"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER"], "id": "7777782315678808"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "645234118302041"}]}, "baseline_offer": {"id": "645234118302041"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1744235880, "status": "PUBLISHED", "id": "645234118302041"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "8061765070613863", "version": "1.0"}, "id": "1762835094664167", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__t