b=1-7&_nc_sid=6e7a0a&_nc_ohc=h957i5NgdMYQ7kNvwEtlcFI&_nc_oc=AdlUIMMsylJQsWSQ3lg9MkTSjJEZdTsgKNJuW3YeSZTE7MQjldIV5AkRczbvBcAliio&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfoAabEa1GgFtCgO--YttfDpxgRR1-uTysd259GlFDRT5g&oe=6964EAD4"}, {"file_name": "DrumsBrightCrop.png", "id": "906668313223333", "uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/38974538_906668316556666_7612337564929272449_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Vs9fSBPn5H4Q7kNvwFkbUgt&_nc_oc=Adks13X4mZyCrM9fYK9RhgkeoBIRyp25ickLeacWEsWNmghe2hsoOAZZxdPUCB35CJs&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfoZd1hyTK6u_291pZ2iJ758_9df1QXxlZfFsPvdIiWCRg&oe=6964D157"}, {"file_name": "vidwallcrop.png", "id": "188480243148567", "uri": "https://scontent-sin2-3.oculuscdn.com/v/t64.5771-25/38974503_188480246481900_1395795850249111562_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=XbuVih8_cX4Q7kNvwG_TRm8&_nc_oc=Adlw62-BceawZgGg2slykPmYR7qYq3ffgU0a67610BRG7SlodGh-SYHi5-QrUQx8VGI&_nc_zt=3&_nc_ht=scontent-sin2-3.oculuscdn.com&oh=00_Afpa7Wecq26k9LIF7RBi09IyTNkHW4sABWinef6QeCIGxA&oe=6964F91C"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "RPMSynth.png", "id": "529939358451396", "uri": "https://scontent-sin6-1.oculuscdn.com/v/t64.5771-25/86613168_529939361784729_5099460692979232560_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=vTy3aVYXmroQ7kNvwG26dw7&_nc_oc=AdnWtOowdDV0PoFEEMOtSHpOIrQoEMOTPpWyZMmthVshf2pJVrThTClzX0tNwSTe-Kg&_nc_zt=3&_nc_ht=scontent-sin6-1.oculuscdn.com&oh=00_AfpqBAb92VtbYt_BRcJCQqm8gVkVV6ATlt7WjEqxIGHYZQ&oe=6964FF02"}, "display_name": "Tranzient Lite", "keywords": [], "long_description": "Make beats in VR - This is not a rhythm game!\n\nImmerse yourself in live looping and music production in VR.\n\n- Synced looping with content library, auto time-stretched and pitch-shifted to fit the song.\n- Record vocals including mo-cap and replay of your avatar.\n- Use your custom Ready Player Me avatar.\n- Set of drum kits/one-shots.\n- Use Decks to create a seamless mix.\n- Full set of audio FX applied per clip or per scene (reverbs, chorus etc). \n- Loop record automation of audio FX parameters and synth parameters.\n- Assignable 3D control of FX and synth parameters.\n- Loop videos synced to the beat.\n- Step sequencer - trigger any item in the world and loop record instruments/drumkits. Edit steps, swing quantize.\n- Built in Analog Synth sim and Wavetable synth with >250 Sounds\n- Spatial Mixing - create a binaural mix based on clip positions.\n- In game camera and screen/mirror.", "short_description": "Make beats in VR. Full live looping music production and sequencing environment in VR including visuals. The Lite/Free version of Tranzient.\n\nThis is not a rhythm game.\n\nWho made the beats? You did!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.aliveintech.com/privacy-policy", "developer_terms_of_service_url": "https://www.aliveintech.com/eula", "publisher_name": "AliveIn Tech Ltd.", "support_website_url": "https://www.aliveintech.com/issues", "website_url": "https://aliveintech.com/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1819021644882647"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ART_CREATIVITY", "MUSIC", "PRODUCTIVITY"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "3097222440525123"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "1335814970899895"}, {"status": "PUBLISHED", "id": "3012857272266513"}]}, "baseline_offer": {"id": "13358