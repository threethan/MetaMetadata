_nc_ohc=6VAoHFrlxx8Q7kNvwF-DCsY&_nc_oc=AdnsbOuaALgYFaMaKrN9cDi-65JUDuSz3fmFkOVA7UyRjWBlO7iC6FxjwZOez_J0tvg&_nc_zt=3&_nc_ht=scontent-sjc6-1.oculuscdn.com&oh=00_Afk3RR0YIHhnfXMUuPEmbtftZS6OjUSmR2zfuibNiivIEQ&oe=695650E1"}, {"file_name": "Screenshot_1.png", "id": "641138525632261", "uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/491823341_641138528965594_1336938757637520992_n.png?_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=I9tEnXFiz_QQ7kNvwEP7a_Q&_nc_oc=AdkVx9HH7IevByYa4BVBQBlUoHY3NDGdWMqWI7_P9GdldXSg4vZyXYirVIFvkhyNXsE&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfljF4ndZs9m0GUcmeStkd_lvqa-rUXk98y00kD5QzEGNw&oe=6956641C"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "ChatGPT Image 8 Kas 2025 18_38_34.png", "id": "1410398484039692", "uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/491823812_1410398487373025_3799452646507340365_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Q0rGGie8nOEQ7kNvwGHMsLG&_nc_oc=AdlgM12kLnMJ6gH2FX2QVKVjfuMieYJLDe8EVKbWP397_uDcPbelIrzEyEmwoG3uM_o&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfnQnKze-FyBeiAC3zbPKR8EWnANnQgcx_TDbhXREDaSmQ&oe=69567F7C"}, "display_name": "Card Match VR", "keywords": ["memory", "brain", "puzzle", "casual", "vr"], "long_description": "Step into the world of Card Match VR, a relaxing yet challenging memory game built for virtual reality. Test your focus, memory, and attention to detail as you flip cards, find matching pairs, and progress through increasingly difficult levels.\n\nEach round introduces new cards, patterns, and environments, offering a fresh experience every time you play. Designed with intuitive VR controls, you\u2019ll use your hands to flip, select, and match cards naturally \u2014 just like in real life.\n\n\ud83c\udf1f Key Features:\n\u2022 Relaxing and immersive 3D environments\n\u2022 Progressive difficulty with new levels and card types\n\u2022 Improves memory, focus, and reaction time\n\u2022 Simple gameplay perfect for all ages\n\nEnter a peaceful virtual space where time slows down, focus takes over, and matching the perfect pair feels more satisfying than ever.\n\nGet ready to train your brain, relax your mind, and experience the joy of discovery \u2014 only in Card Match VR.", "short_description": "Discover the ultimate memory challenge in Card Match VR! Flip, match, and test your focus in a fully immersive virtual world. Enjoy relaxing gameplay that sharpens your mind as difficulty increases each level. Simple to play, hard to master \u2014 the perfect blend of fun and brain training for all ages."}]}, "developer_name": null, "developer_privacy_policy_url": "https://alikaanbashan.org/vr_privacy.txt", "developer_terms_of_service_url": null, "publisher_name": "Ali Kaan BA\u015eHAN", "support_website_url": null, "website_url": "https://www.instagram.com/alikaan_1998_/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "377978390272490"}, {"id": "1023770785722696"}, {"id": "459781504968480"}, {"id": "1592192974313489"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "33021558287458145"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sjc6-1.oculuscdn.com/v/t64.5771-25/491820887_1365392275229461_5840964558837448335_n.png?stp=dst-png_s360x360&_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=92ixBfzjy7wQ7kNvwHUMahV&_nc_oc=AdkrB4J1II4bDmPQvbqwtJQ6FtB2GSqPs6AV6TWhSiotr1Z5lq-TuLShO6UTcQqLDXE&_nc_zt=3&_nc_ht=scontent-sjc6-1.oculuscdn.com&oh=00_AfmIHbj-PG5vXuFdQH