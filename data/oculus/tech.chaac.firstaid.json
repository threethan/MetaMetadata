h=00_Afep2TjgU96I7dmGCCI-Ty-Os9MFdEb4yAalLliScDEldw&oe=68FB58C1"}, {"file_name": "tutorial_2_1.png", "id": "1773059633308930", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/491819417_1773059636642263_1249842756487671690_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=aja-CyZw2xIQ7kNvwErD7tc&_nc_oc=AdmS8p9pG8-GSsDNgQ1j3xgRM9JH7vLlxnFAd9HbJH6AlD1yNDjDRMrSMbWDNmda3y0&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_Affd4UBNLjK2-Wns-cUbSW5mAuv1E6v4vGK5ylJgNXBPxg&oe=68FB4FEA"}, {"file_name": "tutorial_3.png", "id": "671040079255517", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/491824202_671040082588850_7751685470772367233_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=KiQWenv3_AIQ7kNvwHQg9h7&_nc_oc=AdlZpk8nOyAzPt72eTG_KfdGk3qL9k0wJz3jp3nsYoPCeBq9iMwxiAKJMQ2-2BWR7EE&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfdVxMLShQqOASrny2A94GapE4xGg5VtnAwG8ou0ceW-cA&oe=68FB7AAF"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "CoverImage.png", "id": "779497757477543", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/38982602_779497760810876_157322345244620332_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=avanqbZkWNYQ7kNvwF9KrO2&_nc_oc=AdmAxOBhmtv9sZdlx8jWIq3SVYV9TV29Jn29r6fCTypUiaaT7LLNjMbdsCro1sgocmA&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfdqRdCoJsZiZp7yqiK5yopS0aQ35gVScftzQZ1x1YQ7Eg&oe=68FB6B1C"}, "display_name": "First Aid VR", "keywords": [], "long_description": "Welcome to our VR First Aid app, a transformative learning tool. This application is meticulously designed to teach users essential emergency skills, including how to dial 911, use an AED, and perform CPR. Through realistic, immersive virtual reality scenarios, users gain hands-on experience in managing these critical situations with precision and calm. The VR First Aid app guides you through each step, all within a safe, virtual space. Ideal for individuals, educators, and healthcare professionals, this app is your ultimate resource for becoming a proficient first responder in today\u2019s world.", "short_description": "Equip yourself with life-saving skills in VR! Learn to call 911, use an AED, and perform CPR effectively with our immersive first-aid training app."}]}, "developer_name": null, "developer_privacy_policy_url": "https://docs.google.com/document/d/1B3KNESmF2S8bFCdivo6S0UMBaS68peIqZfzvLoRR6-8/edit?usp=sharing", "developer_terms_of_service_url": null, "publisher_name": "Chaac Technologies", "support_website_url": null, "website_url": "https://chaac.tech/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "514087707868786"}, {"id": "478089301798119"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9200522273384018"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/491822045_745008811328157_6598126243229672152_n.png?stp=dst-png_s360x360&_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=icdUYYISxScQ7kNvwHe0Kkl&_nc_oc=AdnQGMsys2r5_Ev3OgT9LnPhM7I9yX0kevc14YDhzfvu9xBk1MwekzPuD_Wq8elv0WM&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfdhFiI4QSBlXxLOxp-NvDLr9nwvxinXgBUFSQcCan5Q6A&oe=68FB5DC5"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "First Aid VR", "platform": "ANDROID_6DOF", "context_category": {"context_category": "APPS", "id": "362692211442873"}, "context_topics": [{"topic_name": "Hands-On Training", "id": "514087707868786"}, {"topic_name": "