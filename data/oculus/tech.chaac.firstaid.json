h=00_AfYwvehKrIOtL92Z8eyjpMnUg_af1xTBaeS03K8ujTtJwA&oe=68DFA941"}, {"file_name": "tutorial_2_1.png", "id": "1773059633308930", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/491819417_1773059636642263_1249842756487671690_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=gLDKOqHqqr4Q7kNvwGsAF4w&_nc_oc=Adm6TQAbj9N3wRW2FCBaYa295JndqKwFaJdIzuLYwnd9XEh3mpjWhMHy9Unb3s_QgDw&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfboLeKRtFRPaJ65Ybce4u5aHkNU-lHZ55j8PtrXMu2D_Q&oe=68DFA06A"}, {"file_name": "tutorial_3.png", "id": "671040079255517", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/491824202_671040082588850_7751685470772367233_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=FJJOmk3VCFsQ7kNvwE5oEjj&_nc_oc=AdmPYAUKQ9gMum4j7_lU5tLip4V5V-ObuBk5plo3WBvvl_TOU5yDaAn3ju3bmoGFju4&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfabUTNBIGvplkSACWz249vUizlqFyVGX2zGN7ea8A_BZg&oe=68DFCB2F"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "CoverImage.png", "id": "779497757477543", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/38982602_779497760810876_157322345244620332_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=b8iAsaJIn08Q7kNvwGkK3o4&_nc_oc=AdknUsBO9Uawvc9gThy_7SRu2UDbXMMnSr743kYzRFrbz8pZ1EQsUWS8FVORNCVEJNk&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfbKfowu2eaHJSUbQsrjcZcf_f38Mfs_YpwOiA9qozcJHw&oe=68DFBB9C"}, "display_name": "First Aid VR", "keywords": [], "long_description": "Welcome to our VR First Aid app, a transformative learning tool. This application is meticulously designed to teach users essential emergency skills, including how to dial 911, use an AED, and perform CPR. Through realistic, immersive virtual reality scenarios, users gain hands-on experience in managing these critical situations with precision and calm. The VR First Aid app guides you through each step, all within a safe, virtual space. Ideal for individuals, educators, and healthcare professionals, this app is your ultimate resource for becoming a proficient first responder in today\u2019s world.", "short_description": "Equip yourself with life-saving skills in VR! Learn to call 911, use an AED, and perform CPR effectively with our immersive first-aid training app."}]}, "developer_name": null, "developer_privacy_policy_url": "https://docs.google.com/document/d/1B3KNESmF2S8bFCdivo6S0UMBaS68peIqZfzvLoRR6-8/edit?usp=sharing", "developer_terms_of_service_url": null, "publisher_name": "Chaac Technologies", "support_website_url": null, "website_url": "https://chaac.tech/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "514087707868786"}, {"id": "478089301798119"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9200522273384018"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/491822045_745008811328157_6598126243229672152_n.png?stp=dst-png_s360x360&_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=SsGLQJlfu8MQ7kNvwEr4UK1&_nc_oc=Adkwh7pCiUzYr0WQNIWiNv9gmqq9BPR0Z7V9jsT0nmetZwJLFuLpAOKzVsH74N73GzQ&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfZrXkQL8ewUy8PXGZfMBetsvgG7cfiRNOjnDlrM4UBIxg&oe=68DFAE45"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "First Aid VR", "platform": "ANDROID_6DOF", "context_category": {"context_category": "APPS", "id": "362692211442873"}, "context_topics": [{"topic_name": "Hands-On Training", "id": "514087707868786"}, {"topic_name": "