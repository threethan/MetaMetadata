kills in VR! Learn to call 911, use an AED, and perform CPR effectively with our immersive first-aid training app."}]}, "developer_name": null, "developer_privacy_policy_url": "https://docs.google.com/document/d/1B3KNESmF2S8bFCdivo6S0UMBaS68peIqZfzvLoRR6-8/edit?usp=sharing", "developer_terms_of_service_url": null, "publisher_name": "Chaac Technologies", "support_website_url": null, "website_url": "https://chaac.tech/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "514087707868786"}, {"id": "478089301798119"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9200522273384018"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ord5-2.oculuscdn.com/v/t64.5771-25/491822045_745008811328157_6598126243229672152_n.png?stp=dst-png_s360x360&_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=THT-dHLv23AQ7kNvwGnW-mc&_nc_oc=Adlg8kRT6odC2pZEG6HPo0q7QnTk-C1Egx2pH-RDR_QsFpbexrNk7w-T5QLHFVKX24s&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AfXg3_f9TWM57Uzd0QUlAcZRkoODf9G9UovwCKVhBwkbqw&oe=68A45AC5"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "First Aid VR", "platform": "ANDROID_6DOF", "context_category": {"context_category": "APPS", "id": "362692211442873"}, "context_topics": [{"topic_name": "Hands-On Training", "id": "514087707868786"}, {"topic_name": "Medical", "id": "478089301798119"}], "release_date": 1714417320, "lastRevision": {"nodes": [{"id": "6499778216791784", "releas