d worlds with a computer server you're trying to loot and destroy. "}]}, "developer_name": null, "developer_privacy_policy_url": "https://i.stack.imgur.com/lXzGI.jpg", "developer_terms_of_service_url": null, "publisher_name": "World Wide Chaos", "support_website_url": null, "website_url": "http://www.worldwidechaosinc.com/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_FEW", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "459781504968480"}, {"id": "644354873106712"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ACTION", "ADVENTURE", "SHOOTER"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "27497852889861052"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/38974469_1724204525089306_1108077164007516831_n.png?stp=dst-png_s360x360&_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=mf4opiUorE4Q7kNvwHdcWhH&_nc_oc=AdkwvUMbbvTGtE-EMXZjCFDdFG0SKcmLvnm9RB5vYBaG7MsSwSUvmn7aaZicK4rXlms&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_AfrpXos6_16dVy070eFz7iwYcbH6t7lCHw-QPNDP9J8jhw&oe=696F8227"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "QOR (QuestORama)", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Shooter", "id": "3744040075834813"}, {"topic_name": "Action", "id": "459781504968480"}, {"topic_name": "Survival", "id": "644354873106712"}], "release_date": 1620930011, "lastRevision": {"nodes": [{"id"