nc_ohc=GwpUuz7YxZIQ7kNvwGt6g_9&_nc_oc=Adm7nrksM0FZZy9DY5sHSgVBHHDruBk_hee6rtB9K4qIkPIsXF8n8D1HvbrNwBzPcgI&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_Afn9EcbP3z32aeTUFeaeWtTSSGM8C2yrd3nUGsszooxhSw&oe=695A3F7A"}, {"file_name": "bb01.png", "id": "1447438466232317", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/473394123_1447438469565650_7623719739275182226_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=MJtyG87D_dkQ7kNvwFnAC0T&_nc_oc=AdnRz6Pk09sQiS1byeOoqKetEsSciMRPHiVSt6Da486vLL6s4hOtwmsX3RQ91Dz6gnw&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfkOaZMQn1invDnG-zdZGvnx6CH2jYIuledYTxSNhbUWhA&oe=695A555C"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "trailerCover01.png", "id": "1053900703428215", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/473396251_1053900710094881_492633272722356443_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=5kO8ccRQhvoQ7kNvwHGloII&_nc_oc=AdkvRsoujtnOzxXFnpHvlewdUzyTpDpQJ_cF5dT4hqxaEVsdXdEk2Tq7aOWX88SV32A&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_Afl2Rc4QzC6257tOtpL9lp4dfpjPU2Uiu0KH8KxiND6RpQ&oe=695A4C10"}, "display_name": "Marble Mechanics", "keywords": ["marbles", "marble", "simulation", "sandbox", "physics"], "long_description": "**Marble Mechanics** is an immersive VR sandbox, simulation, and puzzle experience where players design and craft intricate marble runs. With intuitive controls and realistic physics, it empowers imagination and creativity. Whether you're a builder who thrives on innovation or a spectator who enjoys the calming flow of marbles gliding through complex tracks, the game's soothing soundscapes and tranquil environment provide a engaging yet relaxing escape.\n\nFeatures:\n- Interactive tutorials and puzzles\n- Free play where players can edit, duplicate, group, save, load and delete tracks\n- Turn on/off track auto-alignment\n- Infinite undo/redo\n- Adjustable gravity settings \n- Extensive tool tips to help you find the right buttons\n", "short_description": "\u201cMarble Mechanics\u201d is a VR game where you build and simulate dynamic marble runs. Dive in for an immersive marble adventure with four tutorials, five puzzles, over 30 achievements to unlock and four Leaderboards to climb!\n\nWith auto-aligning, easy editing, and options like duplicate, delete, and group track elements, it offers a powerful user-friendly interface. The game features an infinite undo/redo and allows saving and re-simulation of your favorite marble runs. \n\nWhat can you make?"}]}, "developer_name": null, "developer_privacy_policy_url": "https://entonosgames.com/privacy.html", "developer_terms_of_service_url": null, "publisher_name": "Entonos", "support_website_url": "https://entonosgames.com/", "website_url": "https://entonosgames.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1990104051447093"}, {"id": "377978390272490"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "24555933147405717"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/473393450_2323008681416269_4778931494870764786_n.png?stp=dst-png_s360x360&_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=OLPxidV-D5oQ7kNvwEeMm7q&_nc_oc=AdmrNXJ1TgB6L0NjuRWhALHxca2wsdLtrUPIf3yiTwxkIxptwHWCw_nmg85McNxXrv0&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfkI68SCPih-czjtsGApDnfA5lTQwtwMYJGkYQ814PeCXw&oe=695A6FC9"}, "is_concept": false, "