nc_ohc=_839rIWExM8Q7kNvwEAHjyl&_nc_oc=AdkGK-wkJVG3xxgzF6LQtGXI6Lp5Fdny7n1Gf__YUtStYMmdCUAGRbMhKwmlIfgncQ0&_nc_zt=3&_nc_ht=scontent-atl3-2.oculuscdn.com&oh=00_AfofnpWFLYlsiSRhYtBmbQMNFEjtFajPd4v4BJ0Yvr8nQg&oe=698204BA"}, {"file_name": "bb01.png", "id": "1447438466232317", "uri": "https://scontent-atl3-1.oculuscdn.com/v/t64.5771-25/473394123_1447438469565650_7623719739275182226_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=k_oY_6P5IdEQ7kNvwFtzfs-&_nc_oc=AdnTu_pa_DNNwv3nkcE-DNqpVeVw-N3MvIwDFNpFlvWhbizgBfzE1MeVUF2UOuueWQU&_nc_zt=3&_nc_ht=scontent-atl3-1.oculuscdn.com&oh=00_AfoFUpgGtJ3xVrPvl30fXkISw8DZVQ2-7gDm8uGIp6i5tg&oe=6981E25C"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "trailerCover01.png", "id": "1053900703428215", "uri": "https://scontent-atl3-2.oculuscdn.com/v/t64.5771-25/473396251_1053900710094881_492633272722356443_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=tQlSvHKvXHIQ7kNvwHeq2y2&_nc_oc=Admm87Cup4gH3C0gcawBuQF1oPaNCVcJJN11FjABQg85bMK_yCMJo4P8mPsaZ0FKHp0&_nc_zt=3&_nc_ht=scontent-atl3-2.oculuscdn.com&oh=00_Afql6tP3IPbvc_TXNp8XEIYqnrvuUkT4tAInNeRKtSpeww&oe=6981D910"}, "display_name": "Marble Mechanics", "keywords": ["marbles", "marble", "simulation", "sandbox", "physics"], "long_description": "**Marble Mechanics** is an immersive VR sandbox, simulation, and puzzle experience where players design and craft intricate marble runs. With intuitive controls and realistic physics, it empowers imagination and creativity. Whether you're a builder who thrives on innovation or a spectator who enjoys the calming flow of marbles gliding through complex tracks, the game's soothing soundscapes and tranquil environment provide a engaging yet relaxing escape.\n\nFeatures:\n- Interactive tutorials and puzzles\n- Free play where players can edit, duplicate, group, save, load and delete tracks\n- Turn on/off track auto-alignment\n- Infinite undo/redo\n- Adjustable gravity settings \n- Extensive tool tips to help you find the right buttons\n", "short_description": "\u201cMarble Mechanics\u201d is a VR game where you build and simulate dynamic marble runs. Dive in for an immersive marble adventure with four tutorials, five puzzles, over 30 achievements to unlock and four Leaderboards to climb!\n\nWith auto-aligning, easy editing, and options like duplicate, delete, and group track elements, it offers a powerful user-friendly interface. The game features an infinite undo/redo and allows saving and re-simulation of your favorite marble runs. \n\nWhat can you make?"}]}, "developer_name": null, "developer_privacy_policy_url": "https://entonosgames.com/privacy.html", "developer_terms_of_service_url": null, "publisher_name": "Entonos", "support_website_url": "https://entonosgames.com/", "website_url": "https://entonosgames.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1990104051447093"}, {"id": "377978390272490"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "24555933147405717"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/473393450_2323008681416269_4778931494870764786_n.png?stp=dst-png_s360x360&_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=fK-ICEp6SKoQ7kNvwFFQjDA&_nc_oc=AdkBm2_iaDm4vC_4ACeoIZvP1wb8ZwNh9IwIlLjkNi7zTPXcAJCuYJKdDwYKzEkEemo&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_AfpiYU68hmxGKJHy1HumAxHHP9P4DFo7yTnxyUp1s96KGA&oe=6981FCC9"}, "is_concept": false, "