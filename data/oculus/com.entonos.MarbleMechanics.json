nc_ohc=vyAapd53mSwQ7kNvwG0E_Qw&_nc_oc=Adly3CpgT7ZS1IgP46ge2B5n5I_ADh60zM-aw-gTcLt_P-x_qnYm8BEPdwu9ehRWD6Q&_nc_zt=3&_nc_ht=scontent-sjc6-1.oculuscdn.com&oh=00_Afk-1fzKJIDHzLuvt9ZrmLxhQ2ZJWKOz-R2Zhl22f0ntPQ&oe=69564AFA"}, {"file_name": "bb01.png", "id": "1447438466232317", "uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/473394123_1447438469565650_7623719739275182226_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=6H5ebQ7VL5oQ7kNvwGR5IrB&_nc_oc=AdmNqVB4lEbJaGG8m1nBp1Cp_OQoXOtNXrCsEeifPAh4JE-xem_2BAgHiilZ6JFYIaw&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_Afl5n4Db4UeifBtP9URe4S2bGFHhUZZZmS-4plM7kedjmw&oe=695660DC"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "trailerCover01.png", "id": "1053900703428215", "uri": "https://scontent-sjc6-1.oculuscdn.com/v/t64.5771-25/473396251_1053900710094881_492633272722356443_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=clTPaZx0R0AQ7kNvwG9ncJc&_nc_oc=Adk5X8ZjSKj51A7G5-zpmf3Z4sRdC6OTRXoYno8uMCxuy0K5Qndr2psulUqeqeAnkg0&_nc_zt=3&_nc_ht=scontent-sjc6-1.oculuscdn.com&oh=00_AfnlU9vGXJ3VQ2PomNtE5QbZQtr1jE3tKlNWu7UNAlGTnw&oe=69565790"}, "display_name": "Marble Mechanics", "keywords": ["marbles", "marble", "simulation", "sandbox", "physics"], "long_description": "**Marble Mechanics** is an immersive VR sandbox, simulation, and puzzle experience where players design and craft intricate marble runs. With intuitive controls and realistic physics, it empowers imagination and creativity. Whether you're a builder who thrives on innovation or a spectator who enjoys the calming flow of marbles gliding through complex tracks, the game's soothing soundscapes and tranquil environment provide a engaging yet relaxing escape.\n\nFeatures:\n- Interactive tutorials and puzzles\n- Free play where players can edit, duplicate, group, save, load and delete tracks\n- Turn on/off track auto-alignment\n- Infinite undo/redo\n- Adjustable gravity settings \n- Extensive tool tips to help you find the right buttons\n", "short_description": "\u201cMarble Mechanics\u201d is a VR game where you build and simulate dynamic marble runs. Dive in for an immersive marble adventure with four tutorials, five puzzles, over 30 achievements to unlock and four Leaderboards to climb!\n\nWith auto-aligning, easy editing, and options like duplicate, delete, and group track elements, it offers a powerful user-friendly interface. The game features an infinite undo/redo and allows saving and re-simulation of your favorite marble runs. \n\nWhat can you make?"}]}, "developer_name": null, "developer_privacy_policy_url": "https://entonosgames.com/privacy.html", "developer_terms_of_service_url": null, "publisher_name": "Entonos", "support_website_url": "https://entonosgames.com/", "website_url": "https://entonosgames.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1990104051447093"}, {"id": "377978390272490"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "24555933147405717"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sjc6-1.oculuscdn.com/v/t64.5771-25/473393450_2323008681416269_4778931494870764786_n.png?stp=dst-png_s360x360&_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=80obzcrq-yQQ7kNvwF6uA7D&_nc_oc=AdkURczV6FLG0k1MbXxlox8w3Nv8yJYk3fanpCXmer8NjMH3bbG9kcLfhD8QUIyesw4&_nc_zt=3&_nc_ht=scontent-sjc6-1.oculuscdn.com&oh=00_AflgjNF3ESk3QKWeWCMO7nAPU0lloHog2QPB-iJ1T4Si9Q&oe=69567B49"}, "is_concept": false, "