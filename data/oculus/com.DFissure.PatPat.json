_trailer": null, "video_trailer_thumbnail": {"file_name": "Frame 6.png", "id": "851657576973695", "uri": "https://scontent.oculuscdn.com/v/t64.5771-25/75330716_851657580307028_764677618217052868_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=ihVtGwI_r8cQ7kNvgHqNKPP&_nc_oc=AdibtyDoObzzG2Anu1LgM_DuUTJ4XlTVyr5EnMkJTbZyHKCat8eRpgiU5DJHzxZ1vBw&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AYEWO3drzUvXu8CCoc9dG9Wv04xr3FAazjQdpeaWpy9SQQ&oe=67D80323"}, "display_name": "PatPat", "keywords": [], "long_description": "This music rhythm game takes you on a fantastic journey through vibrant and dynamic beats. In the game, you'll find yourself immersed in a captivating musical environment, creating wonderful musical echoes by tapping drums on your desktop. Each beat is accompanied by the melody of the music, as if engaging in an intimate conversation with it.\nWhether you're seeking a moment of tranquility amidst busy work schedules or simply unwinding during leisure time, this game offers endless joy and relaxation. Come and explore this new musical world, where your soul dances to the rhythm of the music, and indulge in a journey of pure and delightful melodies!", "short_description": "This is a music rhythm game where you can tap beautiful drums on your desktop, follow the rhythm of the music, and create energetic beats. Come and experience it!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://yx10wesce3.feishu.cn/docx/TisGdeT21olHDax4FBWcgtuTn3e?from=from_copylink", "developer_terms_of_service_url": null, "publisher_name": "D-Fissure", "support_website_url": null, "website_url": "https://d-fissure.com/patpat/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1672577396871977"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recom