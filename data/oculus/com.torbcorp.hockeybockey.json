c=79a-delNfVwQ7kNvwFdsn-q&_nc_oc=Admd35TJuFYiBbCtdsDsc7oMg0CrzZpboCP70tRagI7fODLeK1OPweuOYFuT-oewedE&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_AfmaNX0guCGlNI5L9V4T1FT_nLichvBjCgReFIeltTq9Hw&oe=6943D6A7"}, {"file_name": "customize.png", "id": "976837239545038", "uri": "https://scontent-sin2-3.oculuscdn.com/v/t64.5771-25/75294243_976837242878371_1564667106018058366_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=TsJAzq7WS5cQ7kNvwE7R9sw&_nc_oc=Adlx1eyu4oom9OBbRMpprCH86LiGsOUlNAjJotXQUk_l2B9--EMI7izpiteZjEdz3Vw&_nc_zt=3&_nc_ht=scontent-sin2-3.oculuscdn.com&oh=00_Afm0G5q0QKpiBpg9P0L9fal4blZ5YaUsURSkZ38WdiYp5g&oe=6943F09B"}, {"file_name": "curved_walls.png", "id": "573951296971980", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/38984440_573951300305313_7063127277047755046_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=p5i2fKaiznYQ7kNvwG5AfLK&_nc_oc=Adk5PHDZqiI6WUp6t5BKfDB5AfwhR37tPF3_gdtQq701UH06mVHKtZQJwGvp6EV0-Jk&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfngqysleLqbB-3aJdk8V6bjCov1UkyGet_0n2iP-MkmYw&oe=694401B8"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "hockeybockeyCOVER.png", "id": "1194531554292049", "uri": "https://scontent-sin11-2.oculuscdn.com/v/t64.5771-25/38974517_1194531557625382_2309043283139476511_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=uaUx3lQnCJ0Q7kNvwFN94To&_nc_oc=AdmzBbX_VVxSYJAlj2z4uEhtwWxu8xhpyoEHfjJwQcEVujqAe6RNnYLUCAHRE9YoR28&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_Afmzkzx3DCl1GLijgVcgoshKF9DrU_iMfkxy0arJWHjq0g&oe=69440580"}, "display_name": "Hockey Bockey", "keywords": [], "long_description": "Hockey Bockey is a fluid and fun VR Sport played on a seamless rink with curved boards. Every aspect of the arena is designed to keep the play moving and to allow the players to make spectacular moves and plays with their friends. AI bots are there to insure you never have an empty game, even when you are playing alone!\n\nFeatures:\n- Features 3v3, 2v2, and 1v1 full multiplayer game modes\n- Dynamic AI Bots that can pass, shoot and check will make sure you always have a full game \n- AI goalies that keep the ball moving and out of your net \n- Curved arena walls allow for astounding trick passes and moves\n- Matches are easy to start and join \n- Practice your moves on a goalie in your private practice rink\n- Customize your player, helmet, pads and stick", "short_description": "Welcome to Hockey Bockey...where hockey is MAGIC!  \n\nSkate, pass, shoot, deflect, block, steal, and deke like a pro! Full online multiplayer with 3 game modes! Never have an empty game with full bot support. Your own skill is your only limit!"}]}, "developer_name": "Jason Rosenstock", "developer_privacy_policy_url": "https://sites.google.com/view/hockeybockey-privacypolicy", "developer_terms_of_service_url": null, "publisher_name": "Jason Rosenstock", "support_website_url": null, "website_url": "https://torbcorp.bigcartel.com/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["SPORTS", "RELAXATION_MEDITATION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER", "CO_OP"], "id": "772431210778389"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sin2-1.oculuscdn.com/v/t64.5771-25/75195905_211003350983031_5749559701408537037_n.png?stp=dst-png_s360x360&_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=bjdhurWyA_UQ7kNvwHGNLPN&_nc_oc=AdlgOPxnPblXmbAOhgVimC6qHsYnYc_MtyA3fO1eTDXq40x-vXMQOxcNvjlLjsWNw