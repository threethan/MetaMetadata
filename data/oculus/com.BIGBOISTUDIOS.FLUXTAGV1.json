, and launch yourself through immersive environments in a heart-pounding game of tag unlike any other."}]}, "developer_name": "BIGBOI STUDIOS", "developer_privacy_policy_url": "https://sites.google.com/view/fluxtagv1/home", "developer_terms_of_service_url": null, "publisher_name": "BIGBOITHEBIGGEST", "support_website_url": null, "website_url": "https://sites.google.com/view/fluxtagv1website/home", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "2691890164460465"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER"], "id": "9923694674414192"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1244219040523365"}]}, "baseline_offer": {"id": "1244219040523365"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1749163140, "status": "PUBLISHED", "id": "1244219040523365"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "23899374513086306", "version": "1.1.97"}, "id": "598110766106451", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "9923694617747531", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "23899374513086306"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel