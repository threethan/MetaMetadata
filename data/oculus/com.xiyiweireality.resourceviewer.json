c=4V7Na_CRHjEQ7kNvwFjAvXe&_nc_oc=Adl8J5xA93Y8-gic2UwH1AMthj9sSD_DlGIDyEt9TyEkAIvEZzH3JqcXNDY1vE-NOkk&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AfpNN4wGb6ZH_zfgHutlyzXGc19xQYiJuJzHKFOGjuvc6w&oe=697DF1AB"}, {"file_name": "\u8be6\u60c5\u56fe03.png", "id": "3560246840938186", "uri": "https://scontent-ord5-1.oculuscdn.com/v/t64.5771-25/491822495_3560246844271519_8847290957328570794_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=EZWxw57crw8Q7kNvwH1NgwW&_nc_oc=Adlm8Z3p5GnSi_hrBeNZIAm577KFaHCbNUyPpIbtYRMP0iNE0D2CRPVruPeIRsJ6tL4&_nc_zt=3&_nc_ht=scontent-ord5-1.oculuscdn.com&oh=00_AfppHS8vQDPlAXBQ3cmn49UwbgxIkIZoE7FRGjM4ZUAwMg&oe=697DFC0D"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "20250616-170959.png", "id": "1246807313637747", "uri": "https://scontent-ord5-1.oculuscdn.com/v/t64.5771-25/491823968_1246807316971080_1616604732486704246_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=IPTNNvxoiacQ7kNvwHScYNu&_nc_oc=Adn23Et6NZ6zXHFWZORQG2mQmEuJH3CVqRQcFGHBjvogfIm9cmjZV3wLO2fI0K9OtkY&_nc_zt=3&_nc_ht=scontent-ord5-1.oculuscdn.com&oh=00_AfruC86RWDhkIF1K6a5huwNG5lk_LB6KKv724aVk4EOsCA&oe=697DDD92"}, "display_name": "XR-PPT Player", "keywords": ["XR", "Player", "Slide", "PowerPoint", "PPT"], "long_description": "The XR-PPT Player as the dedicated VR headset client of the XR Resource Collaboration Platform, offers powerful features for a rich and convenient resource viewing experience. \n\nUsers can easily browse various resources on the platform, including VR courseware, images in multiple formats, PPT presentations, stunning panoramic images, realistic panoramic videos, and detailed 3D models. The platform supports one-click import and secondary editing of Office and WPS files. It supports model import and zero-code editing.\nFor 3D models, the XR-PPT Player provides a range of useful operations. Users can flexibly scale models to examine details from different distances, rotate them for a complete view, and perform cross-sectional displays to analyze internal structures. The player also supports animated demonstrations of model principles, as well as features like explosion separation and perspective hiding, meeting diverse exploration and presentation needs in various scenarios.", "short_description": "XR-PPT Player is the VR headset client of XR Resource Collaboration Platform, allowing users to view various resources from the platform within the VR headset."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.xiyiweireality.com/platform-privacy-policy-english.html", "developer_terms_of_service_url": null, "publisher_name": "Meta Quest Store", "support_website_url": null, "website_url": "https://xiyiweireality.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1043107547362485"}, {"id": "1063584910766706"}, {"id": "393222478507326"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["GAMEPAD"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "31759521173647267"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ord5-2.oculuscdn.com/v/t64.5771-25/491820619_1018096540527217_7059025534621381147_n.png?stp=dst-png_s360x360&_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=xqTg3iZX2osQ7kNvwHGORUR&_nc_oc=AdnY6rPcw8mmqAdmMjtFa-4p6I09sF0wcwdECutKK3gyme-667Ou0GY4NSmMesJ4hB0&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AfoyrvfRY_0lQAb3ia_61ozQcGw9Vz-kdS4CWP2w1krcyA&oe=697DF348"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name":