c=j--bfzzVsKkQ7kNvwHt6c64&_nc_oc=Adnkgdic0PumQOhpT7Oj8IOrUxHCkXqTKQBoBqGUdDGLz6XJpnfkjd4yl6S5Z_rJqsw&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_Afr5PTjLW4MpeaM2qlDy0s79TYQ-vZXa-5LyVPtLvuOTUQ&oe=696B7CAB"}, {"file_name": "\u8be6\u60c5\u56fe03.png", "id": "3560246840938186", "uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/491822495_3560246844271519_8847290957328570794_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=h5AA9F0zMsYQ7kNvwEvjAeg&_nc_oc=AdkX5mlDuYNMr3Irto60ZyLAY0nthMj0ctdVhT-wLdff3sMUMdsZFy2fUpsLO2Deoo0&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfoRv7wTac8-DnbYsU2CAK13fGT7guXTOpn3TckLk-IsTg&oe=696B870D"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "20250616-170959.png", "id": "1246807313637747", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/491823968_1246807316971080_1616604732486704246_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=k2sGUBnDLVsQ7kNvwF43m8A&_nc_oc=Adl0cqlTq9EXoZL1XNm5XNFaBW2ZNkJHUuLugLOVrD6a6ha2qSIKqWqrkEUdZb648Nw&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfrGO9tL-OSiL-JdlNs4sA32fMBRqMPBQp8jr3TykCDpDQ&oe=696B6892"}, "display_name": "XR-PPT Player", "keywords": ["XR", "Player", "Slide", "PowerPoint", "PPT"], "long_description": "The XR-PPT Player as the dedicated VR headset client of the XR Resource Collaboration Platform, offers powerful features for a rich and convenient resource viewing experience. \n\nUsers can easily browse various resources on the platform, including VR courseware, images in multiple formats, PPT presentations, stunning panoramic images, realistic panoramic videos, and detailed 3D models. The platform supports one-click import and secondary editing of Office and WPS files. It supports model import and zero-code editing.\nFor 3D models, the XR-PPT Player provides a range of useful operations. Users can flexibly scale models to examine details from different distances, rotate them for a complete view, and perform cross-sectional displays to analyze internal structures. The player also supports animated demonstrations of model principles, as well as features like explosion separation and perspective hiding, meeting diverse exploration and presentation needs in various scenarios.", "short_description": "XR-PPT Player is the VR headset client of XR Resource Collaboration Platform, allowing users to view various resources from the platform within the VR headset."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.xiyiweireality.com/platform-privacy-policy-english.html", "developer_terms_of_service_url": null, "publisher_name": "Meta Quest Store", "support_website_url": null, "website_url": "https://xiyiweireality.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1043107547362485"}, {"id": "1063584910766706"}, {"id": "393222478507326"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["GAMEPAD"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "31759521173647267"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-iad3-2.oculuscdn.com/v/t64.5771-25/491820619_1018096540527217_7059025534621381147_n.png?stp=dst-png_s360x360&_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=JIR8b5oEmjIQ7kNvwGn4Zx8&_nc_oc=AdmnYe37tsOc7MTPbDMvzW3gb-vbawE_1_TG7BF3Qrvfw485HPHA7CbEPWWgh4VNbzg&_nc_zt=3&_nc_ht=scontent-iad3-2.oculuscdn.com&oh=00_AfoJdHQwdf5M03VyDU6wEjojcAxt4sCebvN-ows8-WQvOg&oe=696B7E48"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name":