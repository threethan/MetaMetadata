a&_nc_ohc=5Omn3fufL7AQ7kNvwHCVQMZ&_nc_oc=AdmZ03J3i2G0uDuArcSd_hePEl9f0yvqcVepp1PYbcNq6z8OyvyAGMgqxxZA3eVcUrM&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_AfqJg6BQJMywfz70tjV5cCHW5vrSNDNZQsGSFlvD14Fpnw&oe=6970C2AB"}, {"file_name": "\u8be6\u60c5\u56fe03.png", "id": "3560246840938186", "uri": "https://scontent-sin6-1.oculuscdn.com/v/t64.5771-25/491822495_3560246844271519_8847290957328570794_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=tHVL-Bg-u2YQ7kNvwE9uKhi&_nc_oc=Adm17O3JiJ4IqKXOIMlDL9hnyTqAp35o5cOP6hT_ceq1xgX_B7lKQpLNA0LsiS8N6bI&_nc_zt=3&_nc_ht=scontent-sin6-1.oculuscdn.com&oh=00_AfoJPl8dDj5ugXKJvSPqHx6YmO1KHnibSzQea-hmTqiOHw&oe=6970CD0D"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "20250616-170959.png", "id": "1246807313637747", "uri": "https://scontent-sin11-2.oculuscdn.com/v/t64.5771-25/491823968_1246807316971080_1616604732486704246_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=XTv5-5Xba1sQ7kNvwHzqWJN&_nc_oc=AdlG-quSbKG_87ZkTfwNl90XTkVsuXn8KbHRXg7JlApsJDKk9HE-iTXYot1hM-IshJQ&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_Afo_zJ42iCvWtiqiAHR5ptlAdGa1w5I1W0g8_m1HcoRl4Q&oe=6970AE92"}, "display_name": "XR-PPT Player", "keywords": ["XR", "Player", "Slide", "PowerPoint", "PPT"], "long_description": "The XR-PPT Player as the dedicated VR headset client of the XR Resource Collaboration Platform, offers powerful features for a rich and convenient resource viewing experience. \n\nUsers can easily browse various resources on the platform, including VR courseware, images in multiple formats, PPT presentations, stunning panoramic images, realistic panoramic videos, and detailed 3D models. The platform supports one-click import and secondary editing of Office and WPS files. It supports model import and zero-code editing.\nFor 3D models, the XR-PPT Player provides a range of useful operations. Users can flexibly scale models to examine details from different distances, rotate them for a complete view, and perform cross-sectional displays to analyze internal structures. The player also supports animated demonstrations of model principles, as well as features like explosion separation and perspective hiding, meeting diverse exploration and presentation needs in various scenarios.", "short_description": "XR-PPT Player is the VR headset client of XR Resource Collaboration Platform, allowing users to view various resources from the platform within the VR headset."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.xiyiweireality.com/platform-privacy-policy-english.html", "developer_terms_of_service_url": null, "publisher_name": "Meta Quest Store", "support_website_url": null, "website_url": "https://xiyiweireality.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1043107547362485"}, {"id": "1063584910766706"}, {"id": "393222478507326"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["GAMEPAD"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "31759521173647267"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sin2-2.oculuscdn.com/v/t64.5771-25/491820619_1018096540527217_7059025534621381147_n.png?stp=dst-png_s360x360&_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=ozn1VJaaGjwQ7kNvwHe_ekA&_nc_oc=AdmQnltcQU3OVWvnGWeLFnfERX5sjrrs0Hu0uxgtAe94GsfXeOyPDGUsAn64Ml324fE&_nc_zt=3&_nc_ht=scontent-sin2-2.oculuscdn.com&oh=00_AfpsiQ5EBL9ZmrfilCurUEwpXxsn-DKxU06jvqusX_DJYA&oe=6970C448"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "disp