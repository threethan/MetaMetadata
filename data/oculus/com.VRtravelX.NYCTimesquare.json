h=00_AfPBGUjG8jhM9OTnp-r5bctLn-0q28Vt_7qM9fD-EUIslA&oe=68501182"}, {"file_name": "Screen5.png", "id": "1082702343325402", "uri": "https://scontent-sin2-1.oculuscdn.com/v/t64.5771-25/39031511_1082702346658735_4946577586307123906_n.png?_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=5X4WO5p2dd8Q7kNvwHrgedA&_nc_oc=Adl6EaB8v6dYlcJW0ViCahhmEco4B9x8wELVfK8kyr6iLaCAJotcxCb-4yCulLLhuM8&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_AfMxGgq1JPpXYXwOJdxTPNiE8WyhnJksWeCwKB_9lViAuA&oe=684FFFE7"}, {"file_name": "Screen6.png", "id": "1705634746668376", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/38982709_1705634753335042_5497159140963386007_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=g8rGkBl7QlwQ7kNvwF16bff&_nc_oc=Adn2AOMuddn7zg8CQM3HWh6tf30MN9XdRiQNkBd-eDteBnJLAwlVPcOAMBeDuiJwwvU&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfPe1SK51jTSkrqCrXPVlhXIf45DO5Ew3QyRkSi_kbB1Bw&oe=684FF442"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "NYC Times Square VR Travel Giant's View", "keywords": ["travel", "sightseeing", "timesquare", "newyork", "usa"], "long_description": "Discover the heart of New York City with a VR experience that immerses you in the iconic Times Square like never before. Witness the grandeur of massive digital billboards, the hustle and bustle of crowds, and the charm of yellow taxis from a towering perspective. By day, enjoy the bright lights and vibrant energy of the square, capturing the essence of this global landmark.\n\nAs night falls, Times Square transforms into a glowing wonderland of neon colors and mesmerizing displays. From the perspective of a giant, you can take in every detail, from the towering advertisements to the electric energy of the nightlife. Feel the pulse of the city without the stress of navigating crowds or dealing with high temperatures.\n\nThis experience is not a TV show or a documentary. It is as if you are truly there, right in the middle of the action. Available anytime, this virtual journey is perfect for anyone longing for the magic of Times Square from the comfort of home.\n\n#travel #tourist #sightseeing #vrtravel #newyork #usa #nyc #timesquare", "short_description": "Step into the iconic Times Square and experience its energy like never before. This VR journey lets you explore the square\u2019s vibrant atmosphere by day and its dazzling neon lights by night\u2014all from the perspective of a giant. Immerse yourself in the sounds and sights of this famous destination, stress-free and without crowds. Pause and continue at any time, bringing the magic of New York City right into your home."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.vrtravelx.net/home/NYC-Times-Square-VR-Travel-Giant-View", "developer_terms_of_service_url": null, "publisher_name": "VRtravelX", "support_website_url": null, "website_url": "http://vrtravelx.net/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "726368587961803"}, {"id": "1043107547362485"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8563364920427897"}}]}, "scheduled_offers": {"count": 5, "nodes": [{"status": "PUBLISHED", "id": "1248235900153883"}, {"status": "PUBLISHED", "id": "1879331709551227"}, {"status": "PUBLISHED", "id": "1669211384033539"}, {"status": "PUBLISHED", "id": "526717853746159"}, {"status": "PUBLISHED", "id": "1151837052951343"}]}, "baseline_offer": {"id": "1151837052951343"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 175