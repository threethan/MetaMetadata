ailer": null, "video_trailer_thumbnail": {"file_name": "[2] MarketAsset_2560_1440_Origin_Global.png", "id": "627680196259217", "uri": "https://scontent-sin2-2.oculuscdn.com/v/t64.5771-25/75411988_627680199592550_5702450325759006192_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=HjKtu30HKAkQ7kNvwH-84CV&_nc_oc=AdmOH7rT-aU2pcHvfjfPKdemlLF8nVJf-nlV_evF18RUhS0ld7NuBqkTFn2zhTyd9Uc&_nc_zt=3&_nc_ht=scontent-sin2-2.oculuscdn.com&oh=00_AfOCvAPSsRy_nEO3kF926nX3_uo_kQwggEzEewQJ0m3DEw&oe=686CF267"}, "display_name": "Vivarium", "keywords": ["vivarium", "pet", "nature", "simulation", "relaxation"], "long_description": "Vivarium is your new virtual space to enjoy raising pets and growing plants while designing the most creative vivariums.\n\nYou might have seen hobbyists making a vivarium and decorating their rooms with their creations. Now you can experience this beautiful hobby and relax at home while interacting with realistic pets and plants in your room in either virtual or mixed reality.\n\n# Design and create your different ecosystems\n\nSelect a glass tank and combine soil, rocks, wood, and mosses to create a nice environment for your pets and plants.\nAdd decoration items to give your vivarium its own narrative.\n\n# Look after your plants and pets\n\nTake good care of your vivarium by watering your plants and feeding your pets. \nYou will also need to manage other elements to maintain your vivarium in good condition.\nMake sure to interact with your pets and show them some love.\n\n# Display and share your creations\n\nSet your created vivariums in your virtual lobby gallery or in your room in mixed reality to appreciate their beauty.\nShare them with others to exchange ideas for future designs.\n\n# Immerse in nature\n\nExpand your vivarium in your room in mixed reality to feel like you are inside of it.\nObserve your pets and plants from up close.\n\n# Challenge yourself\n\nEarn extra rewards by challenging yourself in our mission mode.\n\n# Collect all plants and animals\n\nObtain all the different plants and animals and learn more about them to unlock some additional rewards.", "short_description": "Create unique ecosystems where you can raise virtual pets and grow plants in the most aesthetic ways. \nA game designed to immerse and connect with nature from your room in virtual or mixed reality."}]}, "developer_name": "DoubleMe_StudioX", "developer_privacy_policy_url": "https://www.vivariumxr.com/privacy-policy", "developer_terms_of_service_url": "https://www.vivariumxr.com/terms-of-servic", "publisher_name": "DoubleMe", "support_website_url": "https://vivariumxr.zendesk.com/hc/en-us", "website_url": "https://www.vivariumxr.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}, {"id": "1043107547362485"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "zh_CN", "ko", "fr", "de", "es"], "supported_input_devices": ["OCULUS_TOUCH", "HAND_TRACKING"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9058380784199813"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "616085678167445"}, {"status": "PUBLISHED", "id": "1657202781667054"}]}, "baseline_offer": {"id": "1657202781667054"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1755277200, "status": "PUBLISHED", "id": "616085678167445"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "9995494657155083", "version": "1.2.30"}, "id": "3608441149453442", "channel_name": "LIVE"}]}, "is_for_oc