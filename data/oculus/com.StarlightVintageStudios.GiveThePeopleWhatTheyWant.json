r3VRemFO_loYUU03K_YpZPOA0PjuXHTIePGxj9PCjYa6ZvofIsOZl-31cMiFo&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_AfOTY6XTW47jkHK4yH2IbB9lGxPMQiCEzEa8K09QgSHyiA&oe=686A7BEC"}, {"file_name": "GTP-MetaScreenshot04.png", "id": "1268488450404035", "uri": "https://scontent-sin11-2.oculuscdn.com/v/t64.5771-25/38974924_1268488453737368_7408781568125309811_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=apg5WSwNc3MQ7kNvwEjLDmv&_nc_oc=AdncXBrs2VhkxjjpTq3lYThdk0YhxzoKdNAu0js_-JtFOMuQb-jpp8yBtkk7tDOFs-U&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_AfPRndS-HxwFK2BwQ_3h5HEQOb3ReNC-DLqg7H6vUaA4Lg&oe=686A50AE"}, {"file_name": "GTP-MetaScreenshot01.png", "id": "1132538277408339", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/38982613_1132538280741672_3561373941679093613_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=meTSosi3tT0Q7kNvwHMjIF6&_nc_oc=Adkpe2ZBc0C3UCyX46LPxTIBDaESQLc93afDBcwKgL8gSpuIzeeHuv42sasm1g0x5XU&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfPa-XzmNfKBPyLrwIyGHlHPgaBOkxnIzbmhdTJ8aJtC2A&oe=686A668B"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Cover-Landscape.png", "id": "999779757647287", "uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/39036641_999779760980620_7423226441179076258_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=I0TLsxnecKoQ7kNvwG8Swly&_nc_oc=AdkpAwLmw45Xr-uiijylno8CAC0xi5cZ-9FX64aNj2zyKUsWWBHlSuyMtRJoKLIHivQ&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfPTpCTyjcPz8jNDxDxgMQyM1lDln1o5Vz2PiAeLPfw7Qw&oe=686A5C93"}, "display_name": "Give The People What They Want", "keywords": [], "long_description": "In Give the People What They Want, you play as a lowly mining robot who has been promoted to the service industry. It's a tough job, customers can be very demanding, and you'll need to adapt to whatever people need based on the jobs you're given. You will need to learn how to optimize to get people what they want in the shortest amount of time and deal with their increasingly complex requests to show that you can be a good quality service bot.\n\nKey Features:\n3 different stages with unique environments and items\nBonus challenges that will let you earn extra points\nCustomizable robot body system to be the best bot you can be\n\n\nIt's a tough job, but someone's gotta do it, and apparently, there's nobody else who wants to. So it's up to you to Give the People What They Want. Will you succeed as the best service bot ever? Or are you destined for the scrap heap? ", "short_description": "A VR Game where you take on odd jobs, become your ideal self, and throw things at customers' faces."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.starlightvintagestudios.com/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Starlight Vintage Studios", "support_website_url": null, "website_url": "https://starlightvintagestudios.com/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}, {"id": "1043107547362485"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "1097095507628917"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/38974583_1093614677983865_5973055568815117516_n.png?stp=dst-png_s360x360&_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=SKEClyRZMtwQ7kNvwHEDwEG&_nc_oc=AdkjweZQPZHMW1imzeTlT_PB9aW6up4TrH17d1vCUOxJMzKktZT-GsqZ6CXIZhlVTss&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfNsN2Y4iFm