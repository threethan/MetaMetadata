_oc=AdnvPemVAlOhvc09plWRBKsozXJQOAjLsXTnFz5wRfccxOM7HBmWQ9wfEXrTHmElyaU&_nc_zt=3&_nc_ht=scontent-sin2-2.oculuscdn.com&oh=00_AfrWu2WimQcU1aFHGkv99EJNx3Hp_M9Abq_EtcfxlDkOOA&oe=6970AF8E"}, {"file_name": "Screenshot_1.png", "id": "9502439003216679", "uri": "https://scontent-sin2-1.oculuscdn.com/v/t64.5771-25/491819385_9502439006550012_1566276914293835876_n.png?_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=y41uEAbWsO4Q7kNvwHmnbMg&_nc_oc=Adkdht4LJDzSyDneKQ48Yy5NQ-FvGbwPjpHHGcCauNnBypPGWMiKKIA_8IFqljjsido&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_Afr1e2jznmmrSvtA6C2u6ViFNeHVf8pEwFJumf94Np472A&oe=6970CAC5"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Thumbnail.png", "id": "1058835835758857", "uri": "https://scontent-sin2-1.oculuscdn.com/v/t64.5771-25/499299808_1058835839092190_7370524481922916746_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Niob_uNz9GEQ7kNvwF0DShY&_nc_oc=AdmQ_JLh2YghV0l_JQahmF15N3ixEOffAWTnPUfIBtWXk5GRfZbUfjf-PoRMKTnDytY&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_AfqcqCgeZNIay81cPSsh6jQ-qJRhTWvyJXVIUuTuEAsvYQ&oe=6970D965"}, "display_name": "VR4Life", "keywords": ["Virtual", "Worlds", "Creative", "Creator", "Social"], "long_description": "Where creativity meets immersion.\n\nStep into a universe of limitless possibilities with VR4Life, the ultimate platform where creators and explorers connect through community-made virtual worlds.\n\nWhether you're a 3D artist, developer, or just someone who loves immersive content, VR4Life lets you upload your own virtual environments made in tools like Blender and share them with the world. Build interactive scenes, games, or artistic spaces and make them accessible to a global audience.\n\nNot a creator? No problem. Dive into a growing library of virtual experiences crafted by the community. Explore unique environments, play in experimental worlds, or simply relax and discover what others have imagined.\n\nKey Features:\n- Upload custom-made VR worlds from Blender and other 3D tools\n- Browse and explore a wide variety of user-generated virtual spaces\n- Personalized creator pages and community feedback\n- Fully immersive and optimized for Meta headsets\n- A creative and social platform for both makers and explorers\n\nVR4Life turns creativity into reality. Explore. Create. Live.", "short_description": "VR4Life is a virtual reality hub where users can explore or publish immersive worlds built with tools like Blender. Create, share, and experience the future of VR."}]}, "developer_name": null, "developer_privacy_policy_url": "https://immersegame.com/eula.php?s=Vr4Life&u=vr4life.com", "developer_terms_of_service_url": null, "publisher_name": "Immerse Game", "support_website_url": null, "website_url": "https://vr4life.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "539527055410505"}, {"id": "434961926215971"}, {"id": "1819021644882647"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9919524641416607"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "2416504295414051"}]}, "baseline_offer": {"id": "2416504295414051"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1765475580, "status": "PUBLISHED", "id": "2416504295414051"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "25512109215064898", "version": "0.9.7"}, "id": "1048688033854562", "channel_name": "LIVE"}]}, "is_for_oculus_ke