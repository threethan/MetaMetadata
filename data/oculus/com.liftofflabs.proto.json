_oc=Admd5AAnCYR_OLIWVFyt8ohTpkfWmeRTO5kTr9VUoVIO8RugfN68lZ9GVT3LzY7mnLk&_nc_zt=3&_nc_ht=scontent-lhr6-1.oculuscdn.com&oh=00_AfFqJmFTi38zLBRb9KmvYlANHEy-OFCLxD7-tacTZiW_nQ&oe=67FA4A15"}, {"file_name": "migrated_image.png", "id": "202612000967882", "uri": "https://scontent-lhr8-1.oculuscdn.com/v/t64.5771-25/11162714_255736954802324_1012032607_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=poia_r6eX8YQ7kNvwGXoQ3k&_nc_oc=AdmYR--a1U4fqKAevvwMvwEMFtmLR5TRCPLLdZDIJLWx9o4vUKD6VPjN66K40CsgIFM&_nc_zt=3&_nc_ht=scontent-lhr8-1.oculuscdn.com&oh=00_AfGOOa_MsAFvopDtCRzjOrwAJpF24J0m69eVFeyVNae-iQ&oe=67FA6B38"}, {"file_name": "migrated_image.png", "id": "199060918201507", "uri": "https://scontent-lhr8-1.oculuscdn.com/v/t64.5771-25/12504585_301478893520779_1380575737_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=t-nxCtzksvQQ7kNvwFfXua4&_nc_oc=Adl1-mdBd_3_GQEGEmJotCAeyS0lJToFKXmMiNeVe7ov2QL_tAyCV3gBGT4rKyB32i4&_nc_zt=3&_nc_ht=scontent-lhr8-1.oculuscdn.com&oh=00_AfHvA8eJX7NAIJeWnRgZ_duvqj2u-nuu_jkpcfkEUgE8Ag&oe=67FA4050"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Galaxy Golf", "keywords": ["golf", "space", "puzzle", "physics", "indie"], "long_description": "\u2022 8.5/10 - Road to VR \n\u2022 Made With Unity showcase 2016 \n\nIn a Galaxy far, far away\u2026 Ditch the golf clubs, because this planetary physics puzzler is like no other.\n\nSurvey mini-planets from the sky and defy gravity as you work your way through tricky terrain in as few shots as possible. Will you play it safe, or launch your ball into orbit chasing a hole-in-one?\n\nVR brings a whole new perspective to aiming, and with simple one-touch controls, anyone can get into the game. Swipe up and down to change the angle of your hit, and tap to shoot \u2013 it\u2019s that easy! \u2013 but scoring under par will challenge even the best.\n\nBegin with Pleasant Hills and enjoy a mellow course with a pastoral vibe. Then take it up a notch and test your skills on Frosty Brimstone with its challenges inspired by puzzlers and platformers.\n\nNot enough you say? Take off the training wheels and turn on Pro mode!\n\n\u201cOne of those games that just put a smile on my face the second it started.\u201d", "short_description": "\u2022 8.5/10 - Road to VR \n\u2022 Made With Unity showcase 2016 \n\nGet ready for a planetary physics puzzler like none other! Survey mini-planets from the sky and defy gravity as you work your way through tricky terrain to the flag. Will you play it safe, or launch your ball into orbit chasing a hole-in-one?"}]}, "developer_name": null, "developer_privacy_policy_url": "http://bigfishgames.com/company/privacy.html", "developer_terms_of_service_url": "http://www.bigfishgames.com/company/terms.html", "publisher_name": "Big Fish Games", "support_website_url": null, "website_url": "http://liftoff-labs.com/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": null, "genres": ["CASUAL", "SPORTS", "ARCADE", "PUZZLE", "EXPLORATION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["TOUCHPAD", "GAMEPAD"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "1094640667996983"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-lhr8-1.oculuscdn.com/v/t64.5771-25/12427716_1566176397017214_1276657846_n.png?stp=dst-png_s360x360&_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=MKhFZIiiTVoQ7kNvwGhCz-b&_nc_oc=AdmOnxLzleuVHH1X3YQJMsof3-y7bs5Ir47uOGgltUsR6lVE9g3mtXtu6mdocyfpPsI&_nc_zt=3&_nc_ht=scontent-lhr8-1.oculuscdn.com&oh=00_AfEw9nE4dIM5CMfJkvQWStLWVphA7udbWbFw6y8Kz7x4Og&oe=67FA5519"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Gal