{"data": {"node": {"__typename": "Application", "id": "8016534201745429", "organization": {"id": "155429640495160", "viewer_permissions": [], "application_groupings": {"nodes": [{"viewer_has_update_access": false, "full_applications": {"nodes": [{"id": "29051739481106227"}]}, "id": "1311618783283518"}, {"viewer_has_update_access": false, "full_applications": {"nodes": [{"id": "8016534201745429"}]}, "id": "1167189204400253"}, {"viewer_has_update_access": false, "full_applications": {"nodes": [{"id": "5435258143216402"}]}, "id": "1429956490858983"}]}}, "firstRevision": {"nodes": [{"application": {"id": "8016534201745429", "release_date": 1723492756, "is_concept": false, "platform": "ANDROID_6DOF", "scheduled_offers": {"count": 9}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "24668692206103033"}, "id": "1180558553096668"}]}}, "binary": null, "live_binary": null, "created_date": 1753777335, "id": "24237581275880797", "is_initial_release": false, "name_with_fallback": "v6", "pdp_metadata_with_fallback": {"version": 5, "application": {"is_concept": false, "latest_released_pdp_metadata": {"id": "24247451554893769"}, "release_status": "RELEASED", "id": "8016534201745429"}, "id": "24247451554893769", "release_status": "RELEASED", "status": "APPROVED"}, "release_date": null, "administration_status": "APPROVED", "is_first": false, "release_status": "RELEASED", "release_type": "SCHEDULED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "8016534201745429", "is_first_party": false, "enables_in_app_ads": false, "is_concept": false, "platform": "ANDROID_6DOF"}, "iarc_cert": {"id": "1181104753225999"}, "iarc_email": null, "msrp": null, "translations": {"nodes": [{"images": {"nodes": [{"file_name": "Space-Atlas_Hero-Cover_3000x900.png", "id": "1816492035856333", "image_type": "APP_IMG_HERO", "uri": "https://scontent-lhr6-1.oculuscdn.com/v/t64.5771-25/39003263_1816492039189666_3837