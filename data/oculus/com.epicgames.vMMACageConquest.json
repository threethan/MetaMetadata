nnels": {"nodes": [{"channel_name": "LIVE", "id": "825395256071998"}]}, "change_log": "Bug Fixes.", "created_date": 1756473207, "id": "24083197908030064", "version": "20", "version_code": 20, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": {"id": "24083196808030174"}, "asset_files": {"count": 0}, "debug_symbols": {"count": 0}, "__isAppBinaryWithFileAsset": "AndroidBinary"}, {"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "7240758719367247", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "24105036405846214"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "825395256071998"}]}, "change_log": "Hot Fixes", "created_date": 1756126862, "id": "24049803191369536", "version": "19", "version_code": 19, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": {"id": "24049801508036371"}, "asset_files": {"count": 0}, "debug_symbols": {"count": 0}, "__isAppBinaryWithFileAsset": "AndroidBinary"}, {"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "7240758719367247", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "24105036405846214"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "825395256071998"}]}, "change_log": "Bug fixes.", "created_date": 1756009286, "id": "24039380909078431", "version": "18", "version_code": 18, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": {"id": "24039379225745266"}, "asset_files": {"count": 0}, "debug_symbols": {"count": 0}, "__isAppBinaryWithFileAsset": "AndroidBinary"}, {"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "7240758719367247", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "24105036405846214"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "825395256071998"}]}, "change_log": "We\u2019re excited to introduce a major update to MMA Cage Conquest VR that enhances both gameplay and social interaction.\n\nWhat\u2019s New:\n\nNew Lobby Experience \u2013 Players can now enter a redesigned lobby that includes mini-games such as Power Punch, Punch Puppet, and Punch Beats. These activities allow you to train, build strength, and climb the leaderboard before entering core gameplay.\n\nFitness Zone \u2013 A dedicated training area with energetic soundtracks to motivate your workouts. Here, you can view leaderboards, track your progress, and compete with other players for the top spot.\n\nLogin & Friends System \u2013 A new login feature allows players to create profiles, add friends, and play together in both single-player and multiplayer modes.\n\nEnhanced UI Flow \u2013 The interface has been improved, enabling easier access to menus and gameplay features without needing to travel to specific in-game areas.\n\nThis update makes training, competition, and social play more seamless and engaging than ever before.", "created_date": 1755853384, "id": "24024742913875564", "version": "17", "version_code": 17, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": {"id": "24024741360542386"}, "asset_files": {"count": 0}, "debug_symbols": {"count": 0}, "__isAppBinaryWithFileAsset": "AndroidBinary"}]}}, "viewer": {"user": null}}}