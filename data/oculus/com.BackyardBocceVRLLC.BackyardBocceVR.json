 bocce with your friends across diverse landscapes. Explore wherever you want, throw the pauline wherever you want, play wherever you want!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://backyardboccevr.com/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Backyard Bocce VR LLV", "support_website_url": null, "website_url": "http://www.backyardboccevr.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER", "CO_OP"], "id": "27339701785645666"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "1139682184662865"}, {"status": "PUBLISHED", "id": "484232847993736"}]}, "baseline_offer": {"id": "484232847993736"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1755277200, "status": "PUBLISHED", "id": "1139682184662865"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "27781159698166537", "version": "1.0.10"}, "id": "2873789782785072", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "27339701775645667", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "2