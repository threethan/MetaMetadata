ohc=2ClderA__yIQ7kNvwEI4nXT&_nc_oc=Adk0xqSqxjDPNFLmImFDDG3OB7W8jiUCjdgRORwW3Nwhzs5MHuR2yCIQnzYjf1traB0&_nc_zt=3&_nc_ht=scontent-ams4-1.oculuscdn.com&oh=00_AfKaaNCPeeSZ_Faf7OOwOt3i6CGMQfDd5ggWWsKRowahlw&oe=68403442"}, {"file_name": "Dracon_aset.png", "id": "287024699869917", "uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/57566696_287024703203250_4400771700967467009_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=qyI2h7at_xQQ7kNvwFiLhk6&_nc_oc=AdkON3coAU0Id8guAJq5EP_vipaRSHtiUiNKOjNXFKipeM-kw0MMulAB7qQRYT8A_Ww&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_AfLryK7W9pjfamL7k1xm4Z7YUrGfysPZ9TkaXi6lHnkRVg&oe=68402C8C"}, {"file_name": "cosmo.png", "id": "763958354442334", "uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/38974671_763958361109000_1666867587057029197_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=8jAriFPXXCoQ7kNvwE_YjmB&_nc_oc=AdkKL7FvRi5c1-sEZQoWgwFJqL7KOdYXn5soy4KI3tD37LMEj_yYuRKCLq0WTGny4Bo&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_AfLBEYRzMumr7N0zRLPLmqBu0hFVO691c4FYdWRArodz9g&oe=68404E7A"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Aset_yoko.png", "id": "454948119300156", "uri": "https://scontent-ams2-1.oculuscdn.com/v/t64.5771-25/38974497_454948122633489_7144318561306895112_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=p-U1izpSPegQ7kNvwHrPUo7&_nc_oc=AdkmeoQHb8Q1YhGFKXVbKXqs5NAeiYmfVBv2Ngmcfc0a_TEV1OgzMxUwWOrMgFrNHOs&_nc_zt=3&_nc_ht=scontent-ams2-1.oculuscdn.com&oh=00_AfIX9cSjoH1oZKtAg2aeKZrQHRZ0001zdfrJ8Jo_5-xQZA&oe=68403C62"}, "display_name": "VRGolfRangebeta", "keywords": [], "long_description": "It is a application that allows you to experience the driving range of golf at home. You can check your swing on VR by holding the controller instead of the golf club and swinging. You can also set the controller on a golf club for practice and swing. In that case, use the position adjustment function of the controller.\n\nCurrently installed functions\n\n1. Tutorial\n2. Range mode\n\u30fbSwing data display\n\u30fbSwing record\n\u30fbSwing playback\n\u30fbSetting change\n3. Game mode\n\u30fbBalloon split\n\u30fbBuilding target\n\u30fbDriving contest\n\u30fbClosest to the pin\n\u30fbplay 1 Hole\n4. Lesson mode\n\u30fbSlice correction 01\n\u30fbSlice correction 02\n\u30fbHand floats\n5. Round Play Mode\n\u30fbGDO Chigasaki Golf Links\n6. Optional\n\u30fbVibration setting\n\u30fbClub position adjustment\n\u30fbBall\uff06Ballistic color change\n7.Language change\n\u30fbEnglish\n\u30fbJapanese\n\nPlease note that everything is under development.", "short_description": "This is a golf practice app that you can use at home to objectively check your swing by hitting golf balls in VR space. This product is a beta version for testing.\n\nIn the game mode, you can play a round of golf using data from real golf courses in Japan (one hole only), and also enjoy driving contests and greenies.\n\nPlease note that since this application is a beta version, fine-tuning of swing and ballistic behavior has not yet been completed."}]}, "developer_name": "Abic System Corp.", "developer_privacy_policy_url": "https://www.vrgolfrange.abcs.co.jp/privacypolicy", "developer_terms_of_service_url": "https://www.vrgolfrange.abcs.co.jp/termsofservice", "publisher_name": "Abic System Corp.", "support_website_url": null, "website_url": "https://www.vrgolfrange.abcs.co.jp/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["SPORTS", "CASUAL", "SIMULATION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "ja"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING"],