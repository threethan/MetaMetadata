ohc=_hn67PvFdCcQ7kNvgFXgjpy&_nc_oc=AdmGKZjU1fRK4fQ5mHAuKJXiYNGLop0dUVn1oHr0MHjS7ZxghIUVHSGkQ1Qdpg7GBk4&_nc_zt=3&_nc_ht=scontent-syd2-1.oculuscdn.com&oh=00_AYHgO_XARCBLAebyzqikgeZ-VwU7CYYP_95_P9miiKqzoQ&oe=67ED25C2"}, {"file_name": "Dracon_aset.png", "id": "287024699869917", "uri": "https://scontent-syd2-1.oculuscdn.com/v/t64.5771-25/57566696_287024703203250_4400771700967467009_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=6LFNFpwG0T0Q7kNvgG28Lpw&_nc_oc=AdnihDEdarURURjybsnNbVkCxYSbpWE1_FmPLU8KrKm_dZyjO3xngdY0Lqqq3fwr6_o&_nc_zt=3&_nc_ht=scontent-syd2-1.oculuscdn.com&oh=00_AYFhg8uX2Y-VkOlvIVSy5G4MHeYu-L9EM19oKEBlowAHxQ&oe=67ED1E0C"}, {"file_name": "cosmo.png", "id": "763958354442334", "uri": "https://scontent-syd2-1.oculuscdn.com/v/t64.5771-25/38974671_763958361109000_1666867587057029197_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=37HqQWnEsUQQ7kNvgGZkkwl&_nc_oc=AdmVaevn1jglSFW50yM8T-EgaoveOC6ed-fOa729G7TOOR5b7AUqFAkdRC-AjDzSZ-A&_nc_zt=3&_nc_ht=scontent-syd2-1.oculuscdn.com&oh=00_AYFrXgsbIp8HjDGjRiCzZhNY7lRsuRh1FzAIJLXGAKS6Ew&oe=67ED3FFA"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Aset_yoko.png", "id": "454948119300156", "uri": "https://scontent-syd2-1.oculuscdn.com/v/t64.5771-25/38974497_454948122633489_7144318561306895112_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=OF-LaIPxsM4Q7kNvgFL_KN1&_nc_oc=Adm0YGatnbfTfZGWq02Tz2_suRvYZzlubtEvfAvEBGPPvhEMIbImBeqmG9pr2KJMqbU&_nc_zt=3&_nc_ht=scontent-syd2-1.oculuscdn.com&oh=00_AYHRbXhnoiww5S6XHQ_xTi_HhPcGwv0Ec7KdBmT0vQ2DuA&oe=67ED2DE2"}, "display_name": "VRGolfRangebeta", "keywords": [], "long_description": "It is a application that allows you to experience the driving range of golf at home. You can check your swing on VR by holding the controller instead of the golf club and swinging. You can also set the controller on a golf club for practice and swing. In that case, use the position adjustment function of the controller.\n\nCurrently installed functions\n\n1. Tutorial\n2. Range mode\n\u30fbSwing data display\n\u30fbSwing record\n\u30fbSwing playback\n\u30fbSetting change\n3. Game mode\n\u30fbBalloon split\n\u30fbBuilding target\n\u30fbDriving contest\n\u30fbClosest to the pin\n\u30fbplay 1 Hole\n4. Lesson mode\n\u30fbSlice correction 01\n\u30fbSlice correction 02\n\u30fbHand floats\n5. Round Play Mode\n\u30fbGDO Chigasaki Golf Links\n6. Optional\n\u30fbVibration setting\n\u30fbClub position adjustment\n\u30fbBall\uff06Ballistic color change\n7.Language change\n\u30fbEnglish\n\u30fbJapanese\n\nPlease note that everything is under development.", "short_description": "This is a golf practice app that you can use at home to objectively check your swing by hitting golf balls in VR space. This product is a beta version for testing.\n\nIn the game mode, you can play a round of golf using data from real golf courses in Japan (one hole only), and also enjoy driving contests and greenies.\n\nPlease note that since this application is a beta version, fine-tuning of swing and ballistic behavior has not yet been completed."}]}, "developer_name": "Abic System Corp.", "developer_privacy_policy_url": "https://www.vrgolfrange.abcs.co.jp/privacypolicy", "developer_terms_of_service_url": "https://www.vrgolfrange.abcs.co.jp/termsofservice", "publisher_name": "Abic System Corp.", "support_website_url": null, "website_url": "https://www.vrgolfrange.abcs.co.jp/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["SPORTS", "CASUAL", "SIMULATION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "ja"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING"],