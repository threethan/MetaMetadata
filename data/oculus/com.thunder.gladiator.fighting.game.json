.com/privacy.html", "developer_terms_of_service_url": null, "publisher_name": "Skippy Apps PTY LTD.", "support_website_url": null, "website_url": "https://skippyapps.com/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2710427532541308"}, {"id": "459781504968480"}, {"id": "2691890164460465"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["FIGHTING"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH", "GAMEPAD"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "28519978430983841"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent.oculuscdn.com/v/t64.5771-25/473391952_3064764707007259_2744837324681832296_n.png?stp=dst-png_s360x360&_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=O8wxaeD9NrAQ7kNvwHLJEY1&_nc_oc=AdnDUmSg77Jq5Qs2c0adCkIKDhpWn_wWm_18kbJvmCaZjk9n4IT7umv3_2NNKqYLFHQ&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AfMWn7MJkDl6-xSnkFfMr0AUlhCyMyVsvHfbHbvravRK1Q&oe=68613117"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Thunder Gladiator - Sword Fighting Game", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Fighting", "id": "2710427532541308"}, {"topic_name": "Action", "id": "459781504968480"}, {"topic_name": "Adventure", "id": "2691890164460465"}], "release_date": 1658244802, "lastRevision": {"nodes": [{"id": "7734851126589875", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "773485089