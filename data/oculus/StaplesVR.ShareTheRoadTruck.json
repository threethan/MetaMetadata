c_oc=AdmegmAxxgCvqKVQgUEcgS3WBDaYnFowFFTrk0uLb1uOo7sR15Jyxfxf2NCirjQJF5g&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfGaHOCdTV9dDDF2faKuztPPuegUDcT1h7BBXukB_Kqr-w&oe=6813832E"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Share the Road Truck", "keywords": ["Cycling", "Road", "Safety", "CyclingActionNetwork"], "long_description": "Staples VR has partnered with Cycle Action Network to develop a safe driving virtual reality simulator for truck drivers. The simulation shows drivers what it feels like to be incorrectly followed and passed by a large truck when on a bicycle. The experience is designed to change driver behavior. Spend the next 10 minutes exploring three ideas and behaviors that allow these people to share the road and get home safely. These will be ideas and behaviors that you have control over and can do the next time you are in a vehicle and see a person riding.", "short_description": "With the Share the Road Virtual Reality experience explore ideas and behaviors that allow truck drivers to share the road and get home safely through our VR cycle safety simulator."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.staplesvr.com/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "StaplesVR", "support_website_url": null, "website_url": "https://www.staplesvr.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE"], "supported_tracking_modes": [], "user_interact