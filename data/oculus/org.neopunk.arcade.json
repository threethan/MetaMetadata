ideo_trailer": null, "video_trailer_thumbnail": {"file_name": "landscape.png", "id": "1597754543980218", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/38982481_1597754550646884_2352288393038998942_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=LLtAu3HprKsQ7kNvwExkGJa&_nc_oc=AdnBd1-kQ-uuTZwyuVh3nyr3Am3U59Vwd5BJaeFSoJkYFDaNaC412Iqc8fEr8KPpxSk&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfrOXjWPCPbSTeWYP1yaQnbbfaY5mHQYZ6qjf-mska_vDQ&oe=697CB872"}, "display_name": "Arcade", "keywords": [], "long_description": "Welcome to Arcade!\n\nAfter developing Retro Bricks 1982, I decided to go further and propose a  collection of games inspired by the past...\n\nAt the moment, there are 3 old school ames: \n* Bricks, formelly Retro Bricks 1982, that I improved in several ways\n* Blocks, a funny game where you have to destroy shapes\n* Invasions, a classical invasion game\n\nI hope you will have a lot of fun playing Arcade ;-)\n\nNeoPunk", "short_description": "Welcome to Arcade!\nA collection of games inspired by the past...\nMeet Bricks, Blocks and Invasions!\nAnd above all, have fun!\nNeoPunk"}]}, "developer_name": "NeoPunk", "developer_privacy_policy_url": "https://www.neopunk.org/policy.php", "developer_terms_of_service_url": null, "publisher_name": "NeoPunk", "support_website_url": "https://www.neopunk.org/index.php?page=Feedback", "website_url": "https://www.neopunk.org/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1592192974313489"}, {"id": "377978390272490"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["GAMEPAD", "OCULUS_TOUCH"], "supported_player_modes": ["SIT