lease_channels": {"nodes": [{"channel_name": "LIVE", "id": "1330071677791070"}]}, "change_log": null, "created_date": 1743307964, "id": "9176541342394416", "version": "12.0", "version_code": 28000, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": null, "asset_files": {"count": 0}, "debug_symbols": {"count": 0}, "__isAppBinaryWithFileAsset": "AndroidBinary"}, {"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "5539797106068876", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "9176541342394416"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1330071677791070"}]}, "change_log": "Over the last months, i've been rethinking the way to play Arcade\nmaking the experience more exciting.\n\nSo let me introduce you to the new world challenge competition.\n\nThe world challenge is a year-long competition in which each player\nmust collect as many points as possible over the course of a month.\n\nEach game awards a total of 50750 points divided between\nthe 10 highest-ranked players for each level of difficulty.\n\nYou have 1 month - no more - to reach the top 10. scores are then\nreset to zero for the next month of the competition.\n\nThe name of the winner of the 2025 world challenge will appear on\nthe game's presentation page and / or on the greeting page.\n\nThere are two other changes:\n- i renamed one of the game jigsaw.\n- for maze i have adjusted the speeds for most level of difficulty.\n\nThanks and have fun!\n\nneopunk", "created_date": 1739161678, "id": "8883746421673911", "version": "11.0", "version_code": 27, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": null, "asset_files": {"count": 0}, "debug_symbols": {"count": 0}, "__isAppBinaryWithFileAsset": "AndroidBinary"}, {"__typename": "AndroidBi