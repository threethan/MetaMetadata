trailer": null, "video_trailer_thumbnail": {"file_name": "Landscape_NewLarge.png", "id": "906137537640817", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/38974750_906137540974150_1695977931889531979_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=g94mM_IynxYQ7kNvgHB0tO-&_nc_oc=AdiOL2-qmvJUauFPQDnipR4DHCpocZhlUO_kVKqfquzNNb2Ju-5jLUa5F9LN4jnRq9M&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYDI_aghxlvNBtahxw3sX0Jl0dth81Z28NX1xNfhoXuK0w&oe=67BC5CBF"}, "display_name": "Flight 74", "keywords": ["Flight", "Simulator", "Flying", "Airplane", "Sim"], "long_description": "Do you have what it takes to survive and become the ultimate pilot? Fly missions for the cartel and experience a new genre of VR Flight Simulator, exclusively for the Quest.  Rated one of the Top 50 Games on Quest by Vrelity.\n\nYour plane has experienced engine failure and you're forced to land at an unmarked runway off in the distance. Unfortunately for you, the runway is operated by a notoriously dangerous South American Cartel. Your only chance for survival is to work with the cartel and prove your worth.\n\nFeatures:\n- Complex missions that will test your skills\n\n{2750905198401036}\n\n- Hand Tracking! Put down the controller and play with your hands.  Interactive cockpit. Flip switches, press buttons, punch windows, and more!\n\n{849356023396469}\n\n- Multiplayer! Dogfight against your friends! \n\n{574744495234388}\n\n- Quest 3 Graphics Updates and Improvements\n\n{1503648260255664}\n\n- Make use of your belongings in the cockpit including sun glasses, coffee, and guns!\n\n{498783152559639}\n\n- Solve puzzles in a flight game! Movie style campaign and missions!\n\n{1664355374355543}\n\n- Free Flight Mode with 7 Airports and over 2,000 square miles of scenery\n\n{764494538995077}\n\n- Cars, Birds, Cartels, Explosions and more! \n\n{1643006099819731}", "short_description": "Fly missions for the cartel and experience a new genre of VR Flight Simulator, exclusively for the Quest.  \n\nYour plane has experienced engine failure and you're forced to land at an unmarked runway off in the distance. Unfortunately for you, the runway is operated by a notoriously dangerous South American Cartel."}]}, "developer_name": null, "developer_privacy_policy_url": "https://tasappsystems.com/privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "TAS Systems Corp", "support_website_url": null, "website_url": "https://www.tasappsystems.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}, {"id": "673385183287495"}, {"id": "2691890164460465"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH", "HAND_TRACKING"], "supported_player_modes": ["SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER"], "id": "8400877356680889"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/86615895_466098646181176_383379222211346210_n.png?stp=dst-png_s360x360&_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=DCZZUpUrE0cQ7kNvgGyQ2hV&_nc_oc=AdgdpBlrOZS8J5T9yNAONfHGyG4Vu6wiCgm0ksuHTuVdNFGPnz-L2mmiPqsxU2gEWk4&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYB1yNMaXlAnlZph8UiBDEwZc2lxbVDRmqJ0eIPwSZZWLg&oe=67BC5E9B"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Flight 74", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Simulation", "id": "331529188132278"}, {"topic_name": "Racing", "id": "673385183287495"}, {"t