t that they cannot avoid crashing into, while avoiding the trails left by other players.\n\n# _Current Features_\n- Non PvP Driving Park\n- 6 Player Combat Arena\n- Season Pass with Progression Rewards\n- Cosmetic Shop \n\n## More Coming Soon. . .", "short_description": "Electro Combat Driving "}]}, "developer_name": null, "developer_privacy_policy_url": "https://mystical-mote-389916.web.app/harddrive/policy", "developer_terms_of_service_url": null, "publisher_name": "Red Team Interactive", "support_website_url": null, "website_url": "https://mystical-mote-389916.web.app/", "category": null, "comfort_rating": "COMFORTABLE_FOR_FEW", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "673385183287495"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER"], "id": "7973550066027968"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent.oculuscdn.com/v/t64.5771-25/39003280_517973143980722_935446437994908597_n.png?stp=dst-png_s360x360&_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=fyIgyhOee1IQ7kNvgFPVIIx&_nc_oc=Adgb-U6usVt5k_GIJwKNh3bPNEbZ9DkBDOwtjKpx-zXBaC0NcwVvDb_odveLCyEpgSk&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AYG3kHoi6vYFUUOul8n2X9jahubUx7F67nhG5HoTE6mTVQ&oe=67D81C2A"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Hard Drive", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Racing", "id": "673385183287495"}], "release_date": 1720633049, "lastRevision"