display_name": "Orbs!", "keywords": ["fidget", "chill", "relaxing", "hands", "toy"], "long_description": "Orbs! is a relaxing tabletop simulation that lets you immerse your hands in a pile of thousands of colorful little orbs. It's perfect for when you just want something to focus your attention on, and fiddle with, to take your mind off of things.\n\nThere are no scores, no timers, and no goals. There's just your two hands, buttons to change color, flip gravity, slow time, and a big pile of incredibly satisfying little orbs to knock about and run your fingers through.", "short_description": "Orbs! is a relaxing tabletop simulation where you can immerse your hands in a pile of thousands of colorful little balls. It's perfect for when you just want something to focus your attention on, and fiddle with, to take your mind off of things."}]}, "developer_name": null, "developer_privacy_policy_url": "https://orbs-vr.com/privacy", "developer_terms_of_service_url": "https://orbs-vr.com/terms", "publisher_name": "Charlie Shenton", "support_website_url": null, "website_url": "https://orbs-vr.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}, {"id": "1990104051447093"}, {"id": "1676485315806614"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["HAND_TRACKING"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8197262013710339"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/473394109_1149079993277364_889467