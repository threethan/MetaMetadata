_oc=Adn8IO8o6urdeB7BM0U-LGUw4nmxzOyxJ4N4wQzM4ruj-vpqVJB2JwVFc4rU-F8Z8Dc&_nc_zt=3&_nc_ht=scontent-sjc6-1.oculuscdn.com&oh=00_AflPRfpr6WRaWl9HlZEWYCrjCOEuQOYdS6rQFfHfCvsMBg&oe=6956809D"}, {"file_name": "1.png", "id": "1135274944267833", "uri": "https://scontent-sjc3-1.oculuscdn.com/v/t64.5771-25/57572936_1135274947601166_7513787693106153823_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=i4BmaenNG3EQ7kNvwHFGhnb&_nc_oc=AdkOd1lLkGzqT_izioXidfnnm2x81_-w4WOdPsyFdop0yr7dhgGf8m7cGMLBY2PI-RU&_nc_zt=3&_nc_ht=scontent-sjc3-1.oculuscdn.com&oh=00_AfmqG9PSM5vAcnD9BevKJqSGYvT0L_dDxwT2mGFaeOqdPA&oe=695656DD"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "4.png", "id": "1087976862503576", "uri": "https://scontent-sjc6-1.oculuscdn.com/v/t64.5771-25/75588060_1087976869170242_8690834638362885484_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=OuQhERbAu8kQ7kNvwHhYNBR&_nc_oc=AdmRF-TVB8XEsdxvpVyWAIgEDgp9rumWxthJv3snvYlVS3ZjxJOoVbpKNvcZhQFmix0&_nc_zt=3&_nc_ht=scontent-sjc6-1.oculuscdn.com&oh=00_AflXj3HZkJV74ZfrKOKy9qVbBaVSiSYjHXnyowJYOuriwQ&oe=69564C00"}, "display_name": "VRzomOL", "keywords": [], "long_description": "The VRzomOl is a VRFPS game that focuses on PVP network online biochemical battle mode, an improvement from the original CS's popular biochemical mode. The game features two modes: zombie escape and biochemical infection. In these modes, online players must play as either humans or zombies. Within a limited time frame, humans must either annihilate all zombies or save themselves, while zombies aim to infect all humans through an all-out attack.\n1. Biochemical infection: Within a limited timeframe, humans must eliminate all zombies using firearms or evade them in order to prolong survival, while the zombies actively seek out humans to infect them.\n2. Zombie escape: Within a restricted time frame, humans are required to fend off zombies with firearms for self-preservation and successfully reach the designated endpoint in order to achieve successful escape. The immediate infection of all human players by zombies results in victory.", "short_description": "The first VR Biochemical frenzy online battle in history, Multiplayer shooter in VR with zombies escape, officially opened"}]}, "developer_name": null, "developer_privacy_policy_url": "https://vrdanceyinsi.webnode.page/", "developer_terms_of_service_url": null, "publisher_name": "Unity", "support_website_url": "https://www.bilibili.com/video/BV1JC4y1n7Uq/?spm_id_from=333.999.0.0&vd_source=f44d7fafb67bf1b4c28885f69f021585", "website_url": "https://renrenren.webnode.page/", "category": null, "comfort_rating": null, "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "3744040075834813"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER", "CO_OP"], "id": "8149325901748851"}}]}, "scheduled_offers": {"count": 3, "nodes": [{"status": "PUBLISHED", "id": "1299754398166903"}, {"status": "PUBLISHED", "id": "973220864472036"}, {"status": "PUBLISHED", "id": "1097269831579675"}]}, "baseline_offer": {"id": "1299754398166903"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1755143400, "status": "PUBLISHED", "id": "1299754398166903"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "26162341086687390", "version": "0.5.3"}, "id": "788066986585726", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_