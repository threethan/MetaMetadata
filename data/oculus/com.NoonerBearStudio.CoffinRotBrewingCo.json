.games/coffinrot", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}, {"id": "3744040075834813"}, {"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ACTION", "ARCADE", "SHOOTER"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8619865738130042"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/57572932_547055326990138_5788016700826052575_n.png?stp=dst-png_s360x360&_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=uXA8nayoTI4Q7kNvwGlkXvG&_nc_oc=AdmclWn2Y05Xy8_F7IlMQwdMs15hPwkxqN_p6qkffskxF_ULsnTiDewVbK8pfnr84QE&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfOrCpQq4b3LhkDGNkFpsyXC-X8XDsOEFLDWwWqfqyHpmA&oe=684ACFD0"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Coffin Rot Brewing Co.", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Simulation", "id": "331529188132278"}, {"topic_name": "Shooter", "id": "3744040075834813"}, {"topic_name": "Action", "id": "459781504968480"}], "release_date": 1657566126, "lastRevision": {"nodes": [{"id": "625094502198803", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "4382898511826807", "is_first_party": false, "enables_in_app_ads": false, "is_concept": false, "platform": "ANDROID_6DOF"}, "iarc_cert": {"id": "693418858444816"}, "iarc_email": n