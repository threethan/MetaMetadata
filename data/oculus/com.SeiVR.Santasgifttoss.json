_nc_ohc=TxILh3dKM6AQ7kNvwFtp4GS&_nc_oc=AdkIkBsN8Nom4RANO2QLAACIwph_gfogLRyX5L6McLZxjKs6rQh_zKT6oi0jpQ2kQ0I&_nc_zt=3&_nc_ht=scontent-ord5-1.oculuscdn.com&oh=00_AfmsyUUWMo-_h41CYU8QhV5wNJhrpJ2rysHP-yWvn3TmSQ&oe=69455A01"}, {"file_name": "s2.png", "id": "1125799312536770", "uri": "https://scontent-ord5-1.oculuscdn.com/v/t64.5771-25/39031584_1125799315870103_6617510981775345797_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=MkRFPcAUcgYQ7kNvwH9yb3e&_nc_oc=AdnPAp_bnZ33vOL7pnv8UR_MCr8GGFUB6yppu42yGfwxybIUMRTd8cQG1H8YRouPPj4&_nc_zt=3&_nc_ht=scontent-ord5-1.oculuscdn.com&oh=00_AflhlsYKujws-yZeLiRIRwQI6UZ_mNERdrbz3A0fFnoI3Q&oe=694542E0"}, {"file_name": "s1.png", "id": "959909006016960", "uri": "https://scontent-ord5-1.oculuscdn.com/v/t64.5771-25/38974526_959909009350293_7437404938336637750_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=dDCYGKTZ-pYQ7kNvwFYgRQG&_nc_oc=Adn7lRQuw4b00wGwxybDxOx1ETRC6JLv1JRXZvJuRRmxHspFZDHxOeoa14Azf9wERFs&_nc_zt=3&_nc_ht=scontent-ord5-1.oculuscdn.com&oh=00_Afl5x9kulcYzh_hmLFlZabFYlHH1NwjekrObQgk6j3yicg&oe=694555F0"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Santa's Gift Tooss", "keywords": ["SantaClaus", "ChristmasVRgame", "Gifttossinggame", "Tossing", "throw"], "long_description": "Become Santa Claus: Transform into the legendary gift-giver in this enchanting VR experience on Meta Quest. Feel the magic as you take on Santa's role during Christmas Eve.\n\nDeliver Joy to every one: Your mission is to toss presents into chimneys, ensuring every one wakes up to the excitement of Christmas morning. Precision and timing are key to spreading holiday cheer!\n\nRide the Sleigh: Glide through snowy landscapes atop Santa\u2019s sleigh, pulled by faithful reindeer. Soar across rooftops, navigating through festive winter scenes that bring the holiday spirit to life.\n\nOvercome Challenges: Watch out for obstacles like blizzards and cheeky snowbirds that can disrupt your deliveries. Stay focused to ensure no child is left without a gift!\n\nEarn Points and Rewards: Every successful gift delivery earns you points. Aim for high scores as you master the art of Santa\u2019s gift toss and unlock festive surprises along the way.\n\nFestive Atmosphere: Immerse yourself in a world of dazzling visuals, cheerful holiday music, and intuitive VR controls, making this a magical.\n\nPerfect for Family Fun: Whether playing solo or sharing the joy with friends and family, Santa's Gift Toss is the ultimate holiday VR game to celebrate the Christmas spirit.", "short_description": "Santa's Gift Tooss is a delightful VR game on Meta Quest where you become Santa Claus. Your mission is to deliver gifts to every one by tossing presents into chimneys on Christmas Eve."}]}, "developer_name": null, "developer_privacy_policy_url": "https://sites.google.com/view/privacypolicyforsantasgifttoos/trang-ch%E1%BB%A7", "developer_terms_of_service_url": null, "publisher_name": "Jone Sei", "support_website_url": null, "website_url": "https://sites.google.com/view/sei-vr-santasgifttoss/trang-ch%E1%BB%A7", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2691890164460465"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE", "STANDING", "SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9135096346512341"}}]}, "scheduled_offers": {"count": 4, "nodes": [{"status": "PUBLISHED", "id": "1192095626352791"}, {"status": "PUBLISHED", "id": "4301190586767105"}, {"status": "PUBLISHED", "id": "1701246190762380"}, {"status": "PUBLISHED", "id": "9454169