=00_AfFQHxL-YREDB1oMJUsqsRnm3u2byApfxV9aqwnGh82F3A&oe=67FF9104"}, {"file_name": "5.png", "id": "467916352871409", "uri": "https://scontent-fra3-1.oculuscdn.com/v/t64.5771-25/39003282_467916356204742_1871921057092171900_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=UVKJHr-xipAQ7kNvwHJdo9Y&_nc_oc=Adlb_bqAMQmaaehrsWGCFFr_QB1vQHptUaVl9X0O91SYf8BI7xblnKowAf2plSzDdLI&_nc_zt=3&_nc_ht=scontent-fra3-1.oculuscdn.com&oh=00_AfEn8BN_eTq_oBTlkFd4P8BcKyRJoFiMC2JPxLNfMiDdng&oe=67FFADE2"}, {"file_name": "3.png", "id": "1934382610375855", "uri": "https://scontent-fra5-1.oculuscdn.com/v/t64.5771-25/38974473_1934382613709188_7156337812617042679_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=VJLIw2a3MygQ7kNvwEcLepY&_nc_oc=AdkPlPnakJ9babCQ3uq0TokwkhQDnjoM8D5TW0YNtQpZY_k4wCsmnjryH8gDEMvatJ8&_nc_zt=3&_nc_ht=scontent-fra5-1.oculuscdn.com&oh=00_AfGOggTJ3kdfj7u-RvdOWI5kdAFk8vdo5DBn39Cdt01DAw&oe=67FF97F3"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "landscape_coverart.png", "id": "897861905346420", "uri": "https://scontent-fra5-2.oculuscdn.com/v/t64.5771-25/38982672_897861908679753_3996777238831042692_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=p5X067og9p4Q7kNvwEd961W&_nc_oc=AdkWnYKZyE8vQWWjLDSmimxrCm-1IsjidbgzuZ7314vGKLkloxQXCG_OvdOlAi36lK4&_nc_zt=3&_nc_ht=scontent-fra5-2.oculuscdn.com&oh=00_AfHSsg_WqvVbSEJbxVN63F31x4QENLhmEvIUqDwwJcclnA&oe=67FF9A6B"}, "display_name": "Mystery Box VR: Hidden Secrets", "keywords": ["puzzle", "escape", "room", "trivia", "brainteaser"], "long_description": "# AN IMMERSIVE VR PUZZLE EXPERIENCE\nInteract with objects, handles, buttons, and wheels on each side. Crack enigmas and collect hidden puzzle tiles on a random side of the boxes.\n\nYou won't be able to compare reality with fiction.\n\n# DISCOVER INVENTIONS\nOnce you unlock all 8 boxes of a Level Pack, you can compose a puzzle and discover a brief history of an invention.\n\nIt's not always easy to escape the room, so use all your IQ to crack the riddles. There's a logic behind all of them, even where it seems it's not!\n\n# IMMERSIVE SOUNDTRACK\nPlay the game with your headphones to get immersed in this amazing adventure.\n\n# HINTS\nGot stuck on some enigma? No worries, you can still get a helpful hint! You can also get in touch with me in case you really can't make it through a riddle, I'll be glad to help ;)\n\n# MULTILANGUAGE\nAvailable in English, French, Italian, German, Spanish, Russian, and Japanese.\n\nThe game language will change automatically based on the language settings of your device", "short_description": "Crack awesome puzzles around various boxes, and discover the history of the world's most popular inventions!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://xsgames.co/privacy-policy.html", "developer_terms_of_service_url": null, "publisher_name": "XSGames", "support_website_url": "https://xsgames.co/", "website_url": "https://xsgames.co/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2691890164460465"}, {"id": "377978390272490"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "it", "ru", "de", "fr", "ja", "es"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8681944438587156"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-fra3-1.oculuscdn.com/v/t64.5771-25/39035329_1707528070036122_8050080456656857270_n.png?stp=dst-png_s360x360&_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=ff-2R72-PyAQ7kNvwEwQ9xM&_nc_oc=AdlpxpAThIAXRThIPFMVUVitmrMTGUFy2U1pC6bbhUtwsl5a-2U_qrwt3qnr4US7OHo&_n