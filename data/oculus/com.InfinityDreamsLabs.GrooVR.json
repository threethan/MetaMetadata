humbnail": null, "display_name": "GrooVR - Air Drumming (Demo)", "keywords": [], "long_description": "GrooVR is a unique VR rhythm game where your goal is to hit the beats (represented by small plates) as they are coming at the parts of the drum. \n\nEvery beat indicates which stick you need to use and also the direction you need to match. All the music is composed to perfectly fit the handmade levels. \n\nOur goal is to make players learn how to play drums.\n\nEach hit is strongly supported by playing the sound of the drum of the song and visual effects to emphasize the rhythm.\n", "short_description": "GrooVR is a unique VR rhythm game where your goal is to hit the beats (represented by small plates) as they are coming at the parts of the drum. \n\nEvery beat indicates which stick you need to use and also the direction you need to match. All the music is composed to perfectly fit the handmade levels. \n\nOur goal is to make players learn how to play drums.\n\nEach hit is strongly supported by playing the sound of the drum of the song and visual effects to emphasize the rhythm.\n"}]}, "developer_name": null, "developer_privacy_policy_url": "https://infinity-dreams.vercel.app/PrivacyPolicy.pdf", "developer_terms_of_service_url": null, "publisher_name": "Infinity Dreams Labs", "support_website_url": null, "website_url": "https://infinity-dreams.vercel.app/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1672577396871977"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["SIMULATION", "ARCADE", "CASUAL"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", 