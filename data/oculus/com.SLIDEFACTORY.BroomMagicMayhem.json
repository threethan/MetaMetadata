[], "long_description": "Broom Magic Mayhem is a Virtual Reality thrill ride through a magical wizarding world. \n\nIn Broom Magic Mayhem, you can take on the role of a broom-riding wizard and compete online against your friends in a fast-paced thrill ride through a world of fantasy and magic. \n\nCompete with friends as you speed through forests, fly through the clouds, and collect spells and power-ups while trying to capture quick-witted Ghosts in this Virtual Reality Wizarding adventure. \n", "short_description": "A high-flying virtual thrill ride through a magical Wizarding world of adventure. Grab your broom, take flight, and compete online in a fast-paced thrill ride through a world of fantasy and magic. Speed through forests, fly through the clouds, and compete with your friends to capture quick-witted Ghosts in this Virtual Reality Wizarding adventure."}]}, "developer_name": null, "developer_privacy_policy_url": "https://broommagicmayhem.com/privacy.html", "developer_terms_of_service_url": null, "publisher_name": "TinyByte Studios", "support_website_url": null, "website_url": "https://broommagicmayhem.com/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "2691890164460465"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER", "CO_OP"], "id": "1286100085320620"}, "review_messages": ["", ""]}]}, "cover_square_image": {"uri": "https://scontent-fra3-1.oculuscdn.com/v/t64.5771-25/75580058_3391218797874191_583254