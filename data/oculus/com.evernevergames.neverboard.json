, {"file_name": "Neverboard_store_ss04.png", "id": "225513486241486", "uri": "https://scontent-sin2-1.oculuscdn.com/v/t64.5771-25/38974560_225513489574819_2855921463829190666_n.png?_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=yqgKTTv9fR4Q7kNvwHd8ndZ&_nc_oc=Adk-Jci4ZP7g8icnek_PpogwIo2CkmJOESYJIlOQmTsjib0G_e0eafd7Yov0t3gmkEI&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_AfEMSHleTuhAT6EoIwtVmfacpUcvxTjKIr9DEWeKNGIxFw&oe=67F8E7CD"}, {"file_name": "Neverboard_store_ss05.png", "id": "254939093109883", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/38974730_254939096443216_7373439405362586955_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=_tLhQKxeth4Q7kNvwHkBuQz&_nc_oc=AdkX9Ku6Pd5b4GgO_oepE7d0X9Az29loU4SOIS-r32hfrQ_BFCCPZ2nKIm_OiOcmRVg&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfGnB0ent_cJ6HTPh8Q5L463DRz0-YZG4nFIxb9OrwN9Xg&oe=67F90D93"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Neverboard_store_trailercover.png", "id": "215030317207306", "uri": "https://scontent-sin2-3.oculuscdn.com/v/t64.5771-25/38974883_215030320540639_9034172187144613713_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=AqtrBeq8L6AQ7kNvwEXAosc&_nc_oc=AdkbXhP7u7-tl87wx3yPMUR61lgqBAqDbfOeCEgtkFlGYdqWPMS-VHZ0ZhuoSVRONd4&_nc_zt=3&_nc_ht=scontent-sin2-3.oculuscdn.com&oh=00_AfHRTQTx3EjEXvtXpNBRATGIpe8h2FyX0bsZ1Sie2nB4ew&oe=67F91340"}, "display_name": "Neverboard", "keywords": ["board", "tabletop", "cards", "social"], "long_description": "Hang out and play board games in a whole new way! Filled with a growing list of easy to pick up 2-4 player games and delightful interactions, there is something for everyone to discover. \n\n\u2022 Play games from our library\u2014no set up, clean up, or explaining rules!\n\u2022 Talk, eat pizza, toss cards, roll dice, and move your pawn to victory!\n\u2022 Easy to host up to 3 of your friends, never will you have to play alone again!\n\nNeverboard is free to play with regular updates that add new games, features, and content. Download includes Crazy 8s FREE. You can practice any game in the library--no purchase required or stock up on purchasable games to host your next game night with friends! Additional content costs in-game currency which is available free each day and to purchase in the in-game store.\n\nStill have questions? FAQs, how-tos, and more are available on our website or by joining the Neverboard Player's Club on Discord.", "short_description": "The place to hang out with your friends and play board games."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.evernevergames.com/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Evernever Games", "support_website_url": null, "website_url": "https://www.evernevergames.com/neverboard", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1023770785722696"}, {"id": "1676485315806614"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["SOCIAL", "CASUAL"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER", "CO_OP"], "id": "486383022826261"}}]}, "scheduled_offers": {"count": 0, "nodes": []}, "baseline_offer": null, "is_approved": true, "firstOffer": {"nodes": []}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "5906694592723895", "version": "1.2.4"}, "id": "2405786452808875", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "