ol your own helicopter with the possibility of collision with real objects."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.alternative-vr.com/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Alternative VR", "support_website_url": null, "website_url": "https://www.alternative-vr.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["GAMEPAD", "OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8850563258332796"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent.oculuscdn.com/v/t64.5771-25/75303841_1636827486892876_1880226749718280975_n.png?stp=dst-png_s360x360&_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=xnpgTuwEptQQ7kNvwGLM8lQ&_nc_oc=AdmIUjj3YmD3ljWh4havicUo0q3Ola6K23F4B5CTwAmuC4iTfO6kvU4-xLnws90gxtk&_nc_zt=3&_nc_ht=scontent.oculuscdn.com&oh=00_AfNQcSkh_A_l5G5vgYZw3OQNW9OKC4_9DEagwh4lXgHxGw&oe=6856AFE0"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "RC Helicopter", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Simulation", "id": "331529188132278"}], "release_date": 1718731212, "lastRevision": {"nodes": [{"id": "7222733687782436", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "7222733654449106", "is_first_par