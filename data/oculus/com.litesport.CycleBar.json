deo_trailer": null, "video_trailer_thumbnail": {"file_name": "CB - Cover - Landscape.png", "id": "479642521355102", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/38974834_479642524688435_7065933209497930161_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=ExRqe89SOGgQ7kNvgF8FdQw&_nc_oc=AdizFZRIn8i9vDzXuuvI3xZeApnYDgkppPzSXyGgj7SJ7yv2BiyQ4DhStsDaMqGwsjw&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AYAoTrUSQWlCZxt7Txn8R0brcZYfnQ1wXaYDchfhB7OBaA&oe=67B99FC3"}, "display_name": "CycleBar", "keywords": ["fitness", "health", "cycling", "spin", "cardio"], "long_description": "Revitalize your fitness routine with CycleBar on Meta Quest! As the largest spin brand in the U.S., CycleBar delivers an unparalleled indoor cycling experience that combines the thrill of high-energy workouts with the innovation of virtual reality. Whether you're a seasoned cyclist or new to the world of spinning, our app offers a captivating and personalized journey designed to elevate your performance and keep you inspired.\n\n**Key Features:**\n\n- **Immersive VR Rides:** Clip into the first indoor cycling VR app on the Quest and feel the rush of riding with top instructors and fellow cyclists from around the world.\n\n- **Expert Instructors:** Ride alongside professional trainers who motivate and guide you through every pedal stroke, ensuring an engaging and effective workout.\n\n- **Variety of Classes:** Choose from a wide range of rides, including high-intensity interval training (HIIT), endurance rides, themed sessions, and more.\n\n- **Personalized Experience:** Customize your workouts to match your fitness goals, with options to adjust intensity and duration for a tailored ride.\n\nEmbark on an exciting journey to fitness with CycleBar on Meta Quest. Discover the power of movement, connect with a supportive community, and transform your ride into an unforgettable experience. Your adventure starts here\u2014get ready to ride!\n{381239874674871}", "short_description": "CycleBar is the first indoor cycling app on Meta Quest! Rock Your Ride with the largest indoor cycling brand in the U.S. Whether you're a VR enthusiast or a cycling lover, our app offers exhilarating rides led by top instructors, designed to boost your endurance, strength, and motivation. Join a vibrant community, track your progress, and transform your fitness journey with CycleBar's cutting-edge virtual reality experience. Get ready to ride like never before!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://xponential.com/privacy-policy", "developer_terms_of_service_url": "https://www.xponential.com/terms-of-use-xplus", "publisher_name": "Litesport", "support_website_url": "https://xpoplus.app.link/e/help", "website_url": "https://xponential.plus/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "810139357995560"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH", "HAND_TRACKING"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "7481683875287190"}}]}, "scheduled_offers": {"count": 3, "nodes": [{"status": "PUBLISHED", "id": "7263126590478399"}, {"status": "PUBLISHED", "id": "2323874867953586"}, {"status": "PUBLISHED", "id": "456846966954433"}]}, "baseline_offer": {"id": "456846966954433"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1741885200, "status": "PUBLISHED", "id": "7263126590478399"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "8299946236794279", "version