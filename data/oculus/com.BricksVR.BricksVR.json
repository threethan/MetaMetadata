ription": "BricksVR is a multiplayer sandbox building game in VR in early access."}]}, "developer_name": null, "developer_privacy_policy_url": "https://bricksvr.com/privacy", "developer_terms_of_service_url": null, "publisher_name": "BricksVR Studios Inc.", "support_website_url": null, "website_url": "https://bricksvr.com/", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1990104051447093"}, {"id": "1023770785722696"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["CASUAL", "ART_CREATIVITY", "SOCIAL"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER", "CO_OP"], "id": "454232459568442"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-sin2-1.oculuscdn.com/v/t64.5771-25/38974781_1151057708650253_8305288702707347772_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=o1svpXF9b30Q7kNvgFHHdon&_nc_oc=Adl6cuumC9VgKzLYIhmdT9T93k4t-LX502yPX3S3k_KhpBKOuxEL2mBfH9LdEfGWz54&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_AYEjcfnUgVC-ETBAyVL-V1s-L5EwBRLiVIJMjWlKC83gkg&oe=67EA7B8F"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "BricksVR", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Sandbox", "id": "1990104051447093"}, {"topic_name": "Hangout", "id": "1023770785722696"}], "release_date": 1623176274, "lastRevision": {"nodes": [{"id": "235697371585959", "release_type": "SCHEDULED", "administration_status": 