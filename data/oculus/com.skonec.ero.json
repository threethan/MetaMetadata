t=3&_nc_ht=scontent-lhr8-1.oculuscdn.com&oh=00_AfSWB40r7gHrbGaoKH6Hnxv85UuZ8LAAOHvYoaEa5NcKkQ&oe=688A02A8"}, {"file_name": "screenshot_02.png", "id": "255320403658184", "uri": "https://scontent-lhr8-2.oculuscdn.com/v/t64.5771-25/38974852_255320406991517_7897575683643396095_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=AbpORY4SNtMQ7kNvwEy_mTE&_nc_oc=AdmngQTNr0ukas4fcsje00LZcCcMdVAMzJzpGiayEXvEJrI0n0OWG28RY8J9OgISYYw&_nc_zt=3&_nc_ht=scontent-lhr8-2.oculuscdn.com&oh=00_AfTl6yfr8P04hhW03YHqhZVd7fW9Jz5ZiIQ7Q7yuGKbF6g&oe=688A13B4"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Cover_image.png", "id": "835637507428998", "uri": "https://scontent-lhr8-2.oculuscdn.com/v/t64.5771-25/38982584_835637510762331_6004386185149400704_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=GTXNifvwrRsQ7kNvwHjjWAT&_nc_oc=Adm32LLKKb7yAQU-Hzo_vo7-I6maYMIsACbc0LP7hTvg2t0Hpg8aJKSMgksBe1LsN7E&_nc_zt=3&_nc_ht=scontent-lhr8-2.oculuscdn.com&oh=00_AfR8MnpjAjg1_k9W8atIGZj6GeEh6V0PEeDb4aCLhFeVyg&oe=6889F528"}, "display_name": "Escape Room Online", "keywords": [], "long_description": "\ub2e8 \ud558\ub098\uc758 \ubaa9\uc801, \ubb38\uc744 \uc5f4\uace0 \ud0c8\ucd9c\ud558\ub77c! \n\n\uc800\uc8fc\ubc1b\uc740 \uc778\ud615\uc758 \ubc29. \n\ubc29 \uc548\uc5d0 \ubaa8\ub4e0 \uac83\uc774 \ub2e8\uc11c\uac00 \ub41c\ub2e4. \n\uc2dc\uac04 \ub0b4\uc5d0 \ud0c8\ucd9c\ud558\uc9c0 \ubabb\ud558\uba74 \ub2eb\ud600\ubc84\ub9b0 \uc544\uc774\uc758 \ub9c8\uc74c\ucc98\ub7fc \uc774 \ubc29\ub3c4 \uc2a4\uc2a4\ub85c \uad73\uac8c \uc7a0\uaca8\ubc84\ub824 \ub354 \uc774\uc0c1 \uc5f4\ub9ac\uc9c0 \uc54a\ub294\ub2e4. \n\n - \uac8c\uc784\uc744 \ub354\uc6b1 \uc7ac\ubbf8\uc788\uac8c \uc990\uae30\ub824\uba74?\n\n\ud63c\uc790\uc11c\ub3c4 \uc990\uac81\uc9c0\ub9cc \uac8c\uc784\uc774 \uc5b4\ub835\uac70\ub098 \ubb34\uc11c\uc6b4 \uac83\uc774 \uc2eb\ub2e4\uba74 \uce5c\uad6c\ub97c \ucd08\ub300\ud574 \ubcf4\uc138\uc694.\n\uc5b4\ub824\uc6b4 \ud37c\uc990\ub3c4 \ubcf4\uc774\uc2a4 \ucc57\uc744 \ud1b5\ud574 \uc11c\ub85c\uc758 \uc0dd\uac01\uc744 \uacf5\uc720\ud558\uace0 \uba38\ub9ac\ub97c \ub9de\ub300\uc5b4 \ud568\uaed8 \uadf9\ubcf5\ud569\ub2c8\ub2e4.\n\ucd5c\ub300 4\uc778\uae4c\uc9c0 \ud611\ub825\ud558\uc5ec \ud78c\ud2b8\ub97c \ucc3e\uc544 \ud574\uacb0\ud574 \ubcf4\uc138\uc694!\n\n - \uce5c\uad6c\uc640 \ud568\uaed8 \uac8c\uc784\uc744 \ud558\uace0 \uc2f6\uc5b4\uc694!\n\n\uc6b0\uc120 \ud50c\ub808\uc774\ub97c \ud568\uaed8 \ud560 \uce5c\uad6c\uc5d0\uac8c \uc624\ub79c\ub9cc\uc5d0 \uc5f0\ub77d\ud569\ub2c8\ub2e4. \n\uad00\uacc4\uac00 \uc5b4\uc0c9\ud558\ub2e4\uba74 [\uce5c\uad6c\uc5d0\uac8c \uc120\ubb3c\ud558\uae30]\ub97c \ud574\ubcf4\uc138\uc694.\n\n\uac8c\uc784\uc744 \uad6c\uc785\ud55c \uce5c\uad6c\uc5d0\uac8c \ubc29\uc744 \ub9cc\ub4e0 \ud6c4\uc5d0 [\ubc29 \ucf54\ub4dc]\ub97c \uc54c\ub824\uc8fc\uc138\uc694.\n\uac8c\uc2a4\ud2b8\ub85c \ucd08\ub300\ub41c \uce5c\uad6c\ub294 [\ubc29 \ucf54\ub4dc]\ub97c \uc785\ub825\ud558\uc5ec [\uc900\ube44]\ub9cc \ud558\uba74 \ub429\ub2c8\ub2e4.\n\ub05d\ub0ac\uc2b5\ub2c8\ub2e4! \uac8c\uc784\uc744 [\uc2dc\uc791]\ud558\uc5ec \uce5c\uad6c\uc640 \ud568\uaed8 \ubc29\ud0c8\ucd9c\uc744 \uc990\uae30\uc138\uc694.\n\n - \ub2e4\ub978 \ub098\ub77c\uc758 \uce5c\uad6c\ub4e4\uacfc\ub3c4 \ud568\uaed8 \ud50c\ub808\uc774\ud558\uace0 \uc2f6\uc5b4\uc694.\n\n\ub2e4\ub978 \ub098\ub77c\uc758 \uce5c\uad6c\ub4e4\uacfc\ub3c4 \uac19\uc774 \ud50c\ub808\uc774\ud560 \uc218 \uc788\ub3c4\ub85d \uc5c5\ub370\uc774\ud2b8\ub418\uc5c8\uc2b5\ub2c8\ub2e4. \ud568\uaed8 \uc990\uaca8\uc8fc\uc138\uc694!\n\n\uadc0\ud558\uc758 \uc758\uacac\uc744 \uae30\ub2e4\ub9ac\uaca0\uc2b5\ub2c8\ub2e4. \n\ub2e4\uc591\ud55c \uc758\uacac\uc744 \uc218\ub834\ud574 \uc810\ucc28 \uae30\ub2a5\uc744 \uac1c\uc120\ud574 \ub098\uac08 \uc608\uc815\uc774\uba70, \uc815\uc2dd \ubc84\uc804\uc5d0\uc11c\ub294 \ub354 \ub9ce\uc740 \uc5d0\ud53c\uc18c\ub4dc\ub85c \uc5ec\ub7ec\ubd84\uc744 \ucc3e\uc544\ubd59\uaca0\uc2b5\ub2c8\ub2e4.", "short_description": "\uacf5\ud3ec \ubc29\ud0c8\ucd9cVR \uac8c\uc784 (\uc815\uc2dd\ubc84\uc804\uc744 \uc704\ud55c \ud2f0\uc800)\n-\ud63c\uc790\uc11c \ud50c\ub808\uc774 \ud558\uac70\ub098 \uce5c\uad6c\ub97c \ucd08\ub300\ud558\uc5ec \uc990\uae38 \uc218 \uc788\uc2b5\ub2c8\ub2e4"}]}, "developer_name": "Skonec Entertainment", "developer_privacy_policy_url": "https://vrsquare.skonec.com/privacy%20policy%20Skonec_01.htm", "developer_terms_of_service_url": null, "publisher_name": "Skonec Entertainment", "support_website_url": null, "website_url": "http://www.skonec.com/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2691890164460465"}, {"id": "377978390272490"}, {"id": "275629210202852"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "ko", "zh_CN"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "CO_OP"], "id": "757190609421279"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-lhr8-1.oculuscdn.com/v/t64.5771-25/38982716_3423691451202090_5405275208404891212_n.png?stp=dst-png_s360x360&_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=7UtLeDfkkn0Q7kNvwEpaqgS&_nc_oc=AdkqfbX0BkEXb4xCON2fggVKCnEASSkXJcuNGJVRe1XbZs-617N9FqSqKTzu0P5tmJQ&_nc_zt=3&_nc_ht=scontent-lhr8-1.oculuscdn.com&oh=00_AfRAgS_nLk7H29DUg2ujKswZzfn59zLNQTsMLFPc8ur2Og&oe=688A0D5A"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Escape Room Online", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Adventure", "id": "2691890164460465"}, {"topic_name": "Puzzle", "id": "377978390272490"}, {"topic_name": "Strategy", 