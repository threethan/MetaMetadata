s, VRelax can provide relief at any time and place for any kind of stress."}]}, "developer_name": null, "developer_privacy_policy_url": "https://vrelax.com/en/privacy-statement/", "developer_terms_of_service_url": "https://vrelax.com/wp-content/uploads/2022/06/VRelax_Algemene-Voorwaarden_NL-C00421-1679622868-61-002_ver.-WeFact.pdf", "publisher_name": "VRelax B.V.", "support_website_url": "https://vrelax.com/en/support", "website_url": "https://vrelax.com/", "category": null, "comfort_rating": null, "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "810139357995560"}], "external_subscription_type": "REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "nl"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "7811059505609195"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PENDING", "id": "1192180082907043"}, {"status": "PUBLISHED", "id": "1016878043103727"}]}, "baseline_offer": {"id": "1016878043103727"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 0, "status": "PENDING", "id": "1192180082907043"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "25508913982063808", "version": "4.8.2"}, "id": "494317693471141", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "7811059492275863", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "25508913982063808"}, "release_status"