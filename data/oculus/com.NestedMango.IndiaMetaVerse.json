h=00_AfVRvucmkDIk5kXzFv_QmeVKORdP36rjYIpCksSjx5dmeQ&oe=68A31782"}, {"file_name": "6.png", "id": "1369529621044898", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/491819375_1369529624378231_5935905682631166629_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=X6MAy38D5ckQ7kNvwGc9nlB&_nc_oc=Adke2mR9Zz7C5TMGvPK_du9K4or9nMAMwccEnGAnA-um8jb7zEBG1HKya0YAYizcCQw&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfU8-DP9xJoMQPj_AZbeokRiyooWHdhwYgrabEYTvN7isA&oe=68A32F62"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "2560.png", "id": "1822581361926230", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/499308005_1822581365259563_6406833574819652106_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=7L318CbTC8IQ7kNvwHj3AFW&_nc_oc=AdlqNkC9r3uMS8ZiFzH_XbbNJRGs4CXHHPYl2nbS7G2wCIBHLP4H3JPn1baHpTIjfv8&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfUg5CtIgUPebT-w716IZwOY2Y8DiEwcRiu0UxB7J1QWrw&oe=68A2FEB7"}, "display_name": "India Metaverse", "keywords": ["IndiaMetaverse", "VirtualIndia", "IndiaGateVR", "VirtualTour", "Travel"], "long_description": "India Metaverse is your gateway to the rich cultural heritage and majestic landmarks of India. Immerse yourself in a digital world that brings to life the awe-inspiring beauty of India's most famous tourist spots. Begin your journey at a unique combination of an Airport and Railway station before embarking on a virtual tour to India Gate, the Lotus Temple, Ram Mandir in Ayodhya, and the Qutub Minar in Delhi.\n\nOur immersive VR experiences will transport you to these iconic locations, offering a deeper understanding of India's vibrant history, architecture, and spiritual significance. Future updates will include even more destinations and interactive experiences, allowing you to explore India like never before.\n\nStep into the world of India Metaverse, where the spirit of Indian culture and history awaits you. Explore, learn, and engage with the diverse beauty of India through our immersive VR platform.", "short_description": "Explore iconic Indian landmarks and cultural heritage in a stunning Metaverse experience. Visit India Gate, Lotus Temple, Ram Mandir Ayodhaya , Shiva Temple  and Qutub Minar in VR. "}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.nestedmango.com/privacy-policy/india-metaverse", "developer_terms_of_service_url": null, "publisher_name": "Nested Mango", "support_website_url": null, "website_url": "https://www.nestedmango.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "726368587961803"}, {"id": "1043107547362485"}, {"id": "1228796255027646"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER"], "id": "30115894054722922"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sin2-1.oculuscdn.com/v/t64.5771-25/499618093_1711723756379023_2164095482661861943_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=XtNlfytvsQcQ7kNvwHOVNpz&_nc_oc=Adlz7epyFm3mh-skPJy2niq8KirEIiShN-duGygPtQSRqGaAH1S8K5537BWhbn3Ek5g&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_AfXyuDFc-TjMVXp32d-mM0rfgqycWWmqDvpFcRFPzcbjkA&oe=68A32EF3"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "India Metaverse", "platform": "ANDROID_6DOF", "context_category": {"context_category": "APPS", "id": "362692211442873"}, "context_topics": [{"topic_name": "Travel & Exploration", "id": "726368587961803"}, {"topic_name": "Learning", "id": "1043107