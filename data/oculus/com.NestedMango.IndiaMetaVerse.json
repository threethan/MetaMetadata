h=00_AfpQyPZgOKjhYCiB2hvvSa3ShOzJp6SqEGirhDa6bt1QWw&oe=695CFB02"}, {"file_name": "6.png", "id": "1369529621044898", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/491819375_1369529624378231_5935905682631166629_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=F9VXqBO3FFkQ7kNvwH6BQzJ&_nc_oc=AdlTer4uPZn2FXBCDvB2Ef71pa9-XKzSaBJV1_HNUbFNL9KvUU6pUuu08WSIcJsPxDc&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfonsNWhQwXnHmKTSERJS0kU16SPnVq17QZVSH8lHZPXcw&oe=695D12E2"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "2560.png", "id": "1822581361926230", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/499308005_1822581365259563_6406833574819652106_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=bcOqOv1dtNEQ7kNvwExhr5C&_nc_oc=AdlBYZ4uW2Fd2_6oQiqK0IkxGbL0PVy2xujoAmM8f1my3KToDmA9GY_lZp2_sX8In5Q&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfqohptnzMct4FpdUHD-TAywYzGLPGQHGUpF6Dh-fZ3BKw&oe=695CE237"}, "display_name": "India Metaverse", "keywords": ["IndiaMetaverse", "VirtualIndia", "IndiaGateVR", "VirtualTour", "Travel"], "long_description": "India Metaverse is your gateway to the rich cultural heritage and majestic landmarks of India. Immerse yourself in a digital world that brings to life the awe-inspiring beauty of India's most famous tourist spots. Begin your journey at a unique combination of an Airport and Railway station before embarking on a virtual tour to India Gate, the Lotus Temple, Ram Mandir in Ayodhya, and the Qutub Minar in Delhi.\n\nOur immersive VR experiences will transport you to these iconic locations, offering a deeper understanding of India's vibrant history, architecture, and spiritual significance. Future updates will include even more destinations and interactive experiences, allowing you to explore India like never before.\n\nStep into the world of India Metaverse, where the spirit of Indian culture and history awaits you. Explore, learn, and engage with the diverse beauty of India through our immersive VR platform.", "short_description": "Explore iconic Indian landmarks and cultural heritage in a stunning Metaverse experience. Visit India Gate, Lotus Temple, Ram Mandir Ayodhaya , Shiva Temple  and Qutub Minar in VR. "}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.nestedmango.com/privacy-policy/india-metaverse", "developer_terms_of_service_url": null, "publisher_name": "Nested Mango", "support_website_url": null, "website_url": "https://www.nestedmango.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "726368587961803"}, {"id": "1043107547362485"}, {"id": "1228796255027646"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER"], "id": "30115894054722922"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sin2-1.oculuscdn.com/v/t64.5771-25/499618093_1711723756379023_2164095482661861943_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=UJBL4Wa8ZlIQ7kNvwGMsVgi&_nc_oc=AdkBzqGMU1PH0D9N_teMY3yWD5k1K8YjB6rWtQvTk79z8xDDTwWNPBRrt3m1ALJNldU&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_AfoeGNkRvuji0BTZjuyXFfkuZCzDSmLYkYKZNqtlX9sIRw&oe=695D1273"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "India Metaverse", "platform": "ANDROID_6DOF", "context_category": {"context_category": "APPS", "id": "362692211442873"}, "context_topics": [{"topic_name": "Travel & Exploration", "id": "726368587961803"}, {"topic_name": "Learning", "id": "1043107