eo_trailer_thumbnail": {"file_name": "2560x1140.png", "id": "852154746925360", "uri": "https://scontent-fra3-1.oculuscdn.com/v/t64.5771-25/473393275_852154756925359_7555902654768221372_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=3etBbWZo7-UQ7kNvwH5HmMm&_nc_oc=Adm5DjMbDGNPUsG_ObvWP_PfJ23OOqdZy3JXqxx3xkE0XA2ZgvTeTzrKw6BaRzAQtfg&_nc_zt=3&_nc_ht=scontent-fra3-1.oculuscdn.com&oh=00_AffhdGYk_C-qyLvei0DAceJlG6kgQnV6RN77fxDFK31raw&oe=68F627BF"}, "display_name": "Truck Battle", "keywords": [], "long_description": "Truck Battle\" is an immersive VR shooter game. In this game, players will find themselves in an engaging 360-degree environment, engaging in battles with customizable weapons that allow them to choose from various ammunition styles.\n\nPlayers must develop their combat skills by using a variety of weapons and ammunition to defeat enemies and gain strategic advantages. The game offers an exciting VR experience with realistic graphics, detailed world design, and dynamic gameplay mechanics.\n\nWhile providing players with the opportunity to survive challenging missions, \"Truck Battle\" also allows them to experiment with different ammunition styles. Fast-paced action, strategic thinking, and sharpshooting skills are essential elements for players to succeed in this challenging world.\n\n\"Truck Battle\" stands out as a game that pushes the boundaries of VR, immersing players in a realistic war atmosphere and delivering an unforgettable gaming experience. Get ready to join the struggle for survival and become a legendary war hero in the world of \"Truck Battle!", "short_description": "Truck Battle\" is a stunning first-person shooter VR game that invites you to an adrenaline-packed virtual reality experience. Engage in battles against your enemies with customizable weapons by choosing from various ammunition styles. Navigate a dangerous world using tactical and sharpshooting skills. Survive legendary wars by selecting powerful weapons strategically and defeating your opponents. Push the boundaries of VR with \"Truck Battle\" and embark on a journey to become a true war hero!"}]}, "developer_name": null, "developer_privacy_policy_url": "https://drive.google.com/file/d/1wXW4Hyq5Rqr6HxU8MEokie4qwH6c4Wz9/view?usp=share_link", "developer_terms_of_service_url": null, "publisher_name": "Simofun", "support_website_url": null, "website_url": "https://www.simofun.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "3744040075834813"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9054042554679376"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-fra3-2.oculuscdn.com/v/t64.5771-25/473391831_1158132989246103_6940266468356204951_n.png?stp=dst-png_s360x360&_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=8Ec09OTrNyYQ7kNvwHJJ4DO&_nc_oc=AdlE5qmRwnW4x5zLp2yk2LbNBSJkcmCqCIcU7-5Gp3OIHKZXCubM7pDd8zhrofWfe2I&_nc_zt=3&_nc_ht=scontent-fra3-2.oculuscdn.com&oh=00_AfcQKW6a1Tz_GEul5GcUyOEa2d62SJpn27g58RA-Iu0BKA&oe=68F60A29"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Truck Battle", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Action", "id": "459781504968480"}, {"topic_name": "Shooter", "id": "3744040075834813"}], "release_date": 1737011220, "lastRevision": {"nodes": [{"id": "7045240088892976", "release_type": "SCHEDULED", "