o_trailer_thumbnail": {"file_name": "Trailer cover image.png", "id": "623668253052301", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/38974759_623668256385634_4968901980199422805_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=SE1R0JPl2QEQ7kNvwEE_2Zo&_nc_oc=AdmMSG4d3eCtJ1sgbIlurMF057IeSNslpyzMEoHjowPb_0mY59oqTuv_RvGCr7M4L1I&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfVgKJMXveuXMVrTe26cC_wP0A2P_bIuj-9kw10cWV8Npw&oe=68A5BAFE"}, "display_name": "Move People", "keywords": [], "long_description": "Play through funny levels which will surely make you burst out laughing and have loads of fun for hours. But not only that, you will also need to use your brain and think carefully to solve tricky puzzles by using your intuition. Every level is individually crafted to present a unique experience in which you move people around by their limbs and throw them around like ragdolls to complete a unique goal and make various poses. With the perfect balance of fun and excitement you'll surely fall in love with this game and the people in it!", "short_description": "Move people by dragging their limbs and perform daily tasks. Embark on an amazing love story and join the couple on a journey to find their true love."}]}, "developer_name": null, "developer_privacy_policy_url": "http://paurau.com/privacy/move-people", "developer_terms_of_service_url": null, "publisher_name": "Pau Rau", "support_website_url": null, "website_url": "http://paurau.com/move-people", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "377978390272490"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_i