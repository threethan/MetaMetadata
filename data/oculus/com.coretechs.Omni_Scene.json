o_trailer_thumbnail": {"file_name": "OmniScene - TrailerCoverImage.png", "id": "546201834679276", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/75462207_546201838012609_4631670019349215498_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=6zVS_9okVhUQ7kNvwExQotn&_nc_oc=AdkRrEwkGwqzfdWu9fY53bW0TYrePhs3PWTgnRMzOJk8VnMic_wIgsKTrbTth_lk_ic&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfqpsgPEdjXOJlHF4FEKNI9E2Nk914sk7NFC1cpfguLQDg&oe=696B80AD"}, "display_name": "OmniScene", "keywords": [], "long_description": "OmniScene is an advanced VR model viewer application that transforms how you interact with 3D content. Designed for both collaboration and education, OmniScene brings your models to life in a fully immersive, virtual environment. With its multiplayer feature, multiple users can join the same scene in real time, making collaboration and presentations seamless, whether you're working remotely or in the same room. Save lessons and create engaging learning experiences with OmniScene's lesson-saving feature, allowing you to capture key insights, set up tailored walkthroughs, and share information effectively. Users can easily annotate models to highlight critical areas, add notes, or give detailed explanations, making OmniScene perfect for design reviews, educational purposes, or complex presentations. Its immersive model viewing lets you explore and interact with objects as if they were real, offering an unparalleled level of detail and understanding. Whether you're a designer, educator, or student, OmniScene empowers you to visualize, communicate, and learn in ways that were never before possible\u2014all within a user-friendly, virtual space.", "short_description": "OmniScene is a VR model viewer application designed for immersive visualization of 3D models. It allows users to interact with and explore detailed 3D scenes in a virtual reality environment, providing intuitive navigation, scaling, and manipulation tools. Ideal for architects, designers, and creatives, OmniScene supports multiple file formats, offering a powerful way to experience digital content in a more engaging and interactive manner."}]}, "developer_name": null, "developer_privacy_policy_url": "https://docs.google.com/document/d/11nNzgwQLcCBPiEvsC4chppsm6yHe92gh", "developer_terms_of_service_url": null, "publisher_name": "CoreTechs Solutions Ltd.", "support_website_url": null, "website_url": "https://coretechs.eu/hu", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8569628916454777"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1129736038315219"}]}, "baseline_offer": {"id": "1129736038315219"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1736339340, "status": "PUBLISHED", "id": "1129736038315219"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "25099106669747074", "version": "1"}, "id": "7559168830804094", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "7748842988533378", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "25099106669747074"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "7559