red by Gorilla Tag. \nFind a way out\u2026 before what lurks inside finds you"}]}, "developer_name": "ECLIPSE STUDIOS", "developer_privacy_policy_url": "https://sites.google.com/view/eclipsstudios/privacy-policy?authuser=0", "developer_terms_of_service_url": null, "publisher_name": "ARMY", "support_website_url": null, "website_url": "https://sites.google.com/view/eclipsstudios/games?authuser=0", "category": null, "comfort_rating": "COMFORTABLE_FOR_FEW", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER"], "id": "9943589865748998"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1990815911448627"}]}, "baseline_offer": {"id": "1990815911448627"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1749748380, "status": "PUBLISHED", "id": "1990815911448627"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "9950629061711745", "version": "1"}, "id": "4044826949083747", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "9695074243933896", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "9950629061711745"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "4044826949083747"}