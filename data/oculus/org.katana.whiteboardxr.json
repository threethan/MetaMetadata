s or chalkboards."}]}, "developer_name": null, "developer_privacy_policy_url": "https://odachivr.art/privacy-policy.md", "developer_terms_of_service_url": null, "publisher_name": "OdachiVR", "support_website_url": null, "website_url": "https://odachivr.art/", "category": null, "comfort_rating": null, "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1819021644882647"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "7353648504715921"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "817983180235387"}]}, "baseline_offer": {"id": "817983180235387"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1716583020, "status": "PUBLISHED", "id": "817983180235387"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "8930354940378595", "version": "1.4"}, "id": "788471716530653", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "7353648491382589", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "8930354940378595"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "788471716530653"}]}, "change_log": null, "created_date": 1735888478, "id": "8930354940378595", "version": "1.4", "version_code": 10, "release_status": "RELEASED", "status":