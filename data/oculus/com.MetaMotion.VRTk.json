ler": null, "video_trailer_thumbnail": {"file_name": "Cover_Image.png", "id": "858757850373276", "uri": "https://scontent-lhr8-2.oculuscdn.com/v/t64.5771-25/499619113_858757853706609_7421597840703610131_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=sLCEm41f75sQ7kNvwHQ42Pe&_nc_oc=Adks-CFSN8_2p3vvqcx-ODuPL8c_9f496I-ooih6tPN5m2W212YZV8VvTEKYMCqgvqA&_nc_zt=3&_nc_ht=scontent-lhr8-2.oculuscdn.com&oh=00_AfrUQWtrtwGp_jvOTf03LpQ8EhXzYHADOnuYVk5Ge4CJZQ&oe=6971FF44"}, "display_name": "STEPMASTER", "keywords": [], "long_description": "STEPMASTER \u2014 Your AI Motion Coach in VR\n\nSTEPMASTER is an AI-powered VR training application that lets you view, practice, and master a variety of motion-based exercises. From Taekwondo and Taichi to Yoga and K-POP Dance, STEPMASTER combines real expert demonstrations, motion-captured precision, and real-time AI analysis to deliver an immersive learning experience.\n\nAll-in-One Motion Training\n\nLearn multiple disciplines in a single app.\n\u2022 Taekwondo, Taichi, Yoga, K-POP Dance\n\u2022 Fitness, Rehabilitation, and Dance content coming soon\n\u2022 Designed for both beginners and advanced learners\n\nExpert Demonstrations\n\nExperience real human movement captured with high-fidelity motion capture technology.\n\u2022 Precise joint and posture calibration\n\u2022 Authentic demonstration by certified instructors\n\u2022 Realistic animation in immersive VR environments\n\nAI Motion Analysis & Feedback\n\nTrain with your virtual motion coach.\n\u2022 Real-time comparison with expert movements\n\u2022 Feedback on posture, timing, rhythm, and accuracy\n\u2022 Performance scoring and personal progress tracking\n\nEasy to Start, Exciting to Master\n\n\u2022 Interactive tutorials and structured learning paths\n\u2022 Continually expanding content\n\u2022 Gamified, motivating training experience in VR\n\nMeet your AI Motion Coach in STEPMASTER.\nTrain smarter, move better, and enjoy learning like never before.\n{25528059580152256}", "short_description": "Learn exercises and train toward perfection in STEPMASTER.\n\nWatch high-quality demonstrations from real experts, then receive AI-powered feedback to improve your form. Follow step-by-step VR lessons in Taekwondo, Taichi, Yoga, Dance and more, and turn every practice session into real progress."}]}, "developer_name": null, "developer_privacy_policy_url": "https://metamotion.io/info01_en.html", "developer_terms_of_service_url": "https://metamotion.io/info02_en.html", "publisher_name": "https://metamotion.io/", "support_website_url": null, "website_url": "https://stepmaster.io/", "category": null, "comfort_rating": "COMFORTABLE_FOR_FEW", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "810139357995560"}, {"id": "2776542286000603"}, {"id": "514087707868786"}, {"id": "1043107547362485"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "ko"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "25278478291834526"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-lhr8-2.oculuscdn.com/v/t64.5771-25/38974483_501717836125229_6606651892356965275_n.png?stp=dst-png_s360x360&_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=dQ9zQqIIme0Q7kNvwE514as&_nc_oc=AdlxIwMTcZEYMbqAcKMfinvqcdnSS7X1ypl-5mZsqBrdi7sHdkyo3F1mQZVLYnZSwBQ&_nc_zt=3&_nc_ht=scontent-lhr8-2.oculuscdn.com&oh=00_AfoeySNKofgraD-4yX20Io3oWj6NV5H-uc1YOy7EJQnwsw&oe=697220A4"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "STEPMASTER", "platform": "ANDROID_6DOF", "context_category": {"context_category": "APPS", "id": "3626922114428