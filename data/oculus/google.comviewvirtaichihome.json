, "keywords": [], "long_description": "Virtual Tai Chi is an immersive app designed to teach the fundamentals of Tai Chi in the comfort of your home. Perfect for beginners and enthusiasts alike, it offers a comprehensive guide to fundamental stances and movements, leading up to the mastery of a Yang-style 51 form sequence. The app is optimized for practice within a standard 11' x 12' space, making it accessible for small rooms or dedicated meditation areas. A built-in meditation timer helps you keep track of your sessions, creating a balanced experience that blends movement, mindfulness, and meditation for overall well-being.\n\n\n\n\n\n\n", "short_description": "Virtual Tai Chi is an app designed to teach Yang Style Tai Chi, guiding users through fundamental movements and forms. Perfect for all skill levels, it offers an accessible and immersive way to practice Tai Chi at home, enhancing balance, relaxation, and mindfulness.\n\n\n\n\n\n\n"}]}, "developer_name": null, "developer_privacy_policy_url": "https://sites.google.com/view/vir-tai-chi/home", "developer_terms_of_service_url": null, "publisher_name": "Virtaichi", "support_website_url": null, "website_url": "https://sites.google.com/view/vir-tai-chi/home", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1043107547362485"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "27903496495963403"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1764588937620675"}]}, 