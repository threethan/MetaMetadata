l, "video_trailer_thumbnail": {"file_name": "MainMenu2560x1440.png", "id": "990293449567737", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/38974832_990293452901070_8840274522652524177_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=yz9b3KauhwQQ7kNvwFKcUFW&_nc_oc=Adm3YgNqyRTmBjvsDH08KTC3_ec-iOtC1ER9-airqcVsWn8_khZpP89yywOo0QQd1GQ&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_Afm9bNCbviEk_IpCB4VjT0ppouuhxZFfh4S9jMOg50cGdQ&oe=6953DAA0"}, "display_name": "Virtual Tai Chi", "keywords": [], "long_description": "Virtual Tai Chi is an immersive app designed to teach the fundamentals of Tai Chi in the comfort of your home. Perfect for beginners and enthusiasts alike, it offers a comprehensive guide to fundamental stances and movements, leading up to the mastery of a Yang-style 51 form sequence. The app is optimized for practice within a standard 11' x 12' space, making it accessible for small rooms or dedicated meditation areas. A built-in meditation timer helps you keep track of your sessions, creating a balanced experience that blends movement, mindfulness, and meditation for overall well-being.\n\n\n\n\n\n\n", "short_description": "Virtual Tai Chi is an app designed to teach Yang Style Tai Chi, guiding users through fundamental movements and forms. Perfect for all skill levels, it offers an accessible and immersive way to practice Tai Chi at home, enhancing balance, relaxation, and mindfulness.\n\n\n\n\n\n\n"}]}, "developer_name": null, "developer_privacy_policy_url": "https://sites.google.com/view/vir-tai-chi/home", "developer_terms_of_service_url": null, "publisher_name": "Virtaichi", "support_website_url": null, "website_url": "https://sites.google.com/view/vir-tai-chi/home", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1043107547362485"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_a