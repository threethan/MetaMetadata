, "uri": "https://scontent-sin11-2.oculuscdn.com/v/t64.5771-25/38974530_504041684387056_489153490427308851_n.png?_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=YC79O53dmOsQ7kNvgGSA7XK&_nc_oc=Admgv5lHKiiAiezksYQP-3MNIFl83i3Kar7d7BfAMu0fDvcVUVfsen2PhzFbVzxgckE&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_AYED_aWJUXoEF6hukBtVZPBDbaU--h8fXMxLEHQBxYdQ2g&oe=67E2A147"}, {"file_name": "GeograVR_Screenshot6.png", "id": "642861553799923", "uri": "https://scontent-sin6-1.oculuscdn.com/v/t64.5771-25/39003240_642861557133256_8235503438044789680_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=wW5CGRPMrRAQ7kNvgGcNEcc&_nc_oc=AdkVIVM2wOk0EtwKZDjEYzN6UildM3reWf_0EPy7OtX0OJoTWn9OkblKjrapVFmUaFw&_nc_zt=3&_nc_ht=scontent-sin6-1.oculuscdn.com&oh=00_AYGnuUOqgQJNzfA6wOrrN1TbkdABu5j8W8YNXyBLg8mrAQ&oe=67E28DAD"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "Trailer - Cover Image.png", "id": "645812243281461", "uri": "https://scontent-sin2-2.oculuscdn.com/v/t64.5771-25/39001681_645812246614794_2470528681344526397_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=N9TvtZ1YpOMQ7kNvgFiNRh8&_nc_oc=AdmAI9W6XsHt_SH_uUtMWr7WSNJxsZz3osde89Hp-lUsWiZpTrzMh0cNpVVV24akQ2A&_nc_zt=3&_nc_ht=scontent-sin2-2.oculuscdn.com&oh=00_AYHbhJ74gqiMbahlV40FYE0lbK3tSU4Y56429cPuxZgirw&oe=67E287C1"}, "display_name": "GeograVR", "keywords": [], "long_description": "GeograVR is an educational game in which you can gain an understanding of world geography through fast-paced, competitive gameplay. Given the name of a capital city, you must pinpoint its location on a 3D globe, gaining points for accuracy and speed. From Abu Dhabi to Zagreb, you'll race to drop a pin on the right spot. Surpass progressively higher point thresholds to advance through ten challenging levels. Play casually as a means of learning country capitals and their locations, or take to the leaderboards to take on challengers around the globe on score, speed, or accuracy. How far can you go?", "short_description": "GeograVR is a fast-paced game that challenges you to locate capital cities around the globe. From Abu Dhabi to Zagreb, you'll race to drop a pin on the right spot. Earn points for speed and accuracy through a series of progressively more challenging rounds. Play individually to learn, or compete against players around the world for the highest score, fastest time, or longest streak of accurate pins. How far can you go?"}]}, "developer_name": null, "developer_privacy_policy_url": "https://hyperboreanstudios.com/privacy", "developer_terms_of_service_url": null, "publisher_name": "Hyperborean", "support_website_url": null, "website_url": "http://hyperboreanstudios.com/GeograVR", "category": "ENTERTAINMENT", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "1043107547362485"}, {"id": "1592192974313489"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ARCADE", "EDUCATIONAL", "TRAVEL"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "973942873558422"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "3101398433446867"}]}, "baseline_offer": {"id": "3101398433446867"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1651550460, "status": "PUBLISHED", "id": "3101398433446867"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "5053779887978408", "version": "1.0"}, "id": "185923896758619", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "An