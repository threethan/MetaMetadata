inish line. "}]}, "developer_name": null, "developer_privacy_policy_url": "https://figmentproductions.box.com/s/vvdefd1b685sr0u01atzdtepo98r168d", "developer_terms_of_service_url": null, "publisher_name": "Figment Productions Ltd", "support_website_url": null, "website_url": "https://figmentproductions.co.uk/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "434961926215971"}, {"id": "726368587961803"}, {"id": "1819021644882647"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9612851228734860"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "592421326561496"}]}, "baseline_offer": {"id": "592421326561496"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1739957640, "status": "PUBLISHED", "id": "592421326561496"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "9966958076657505", "version": "1.16"}, "id": "1098819958261193", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "9208997645786889", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "9966958076657505"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1098819958261193"}]}, "change_log": 