!"}]}, "developer_name": "Waterbel", "developer_privacy_policy_url": "https://waterbel.wixsite.com/watergames/kopie-van-chimpanzee-chase", "developer_terms_of_service_url": null, "publisher_name": "Waterbel", "support_website_url": null, "website_url": "https://waterbel.wixsite.com/watergames", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER"], "id": "7243531959066853"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "1074280313794849"}]}, "baseline_offer": {"id": "1074280313794849"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1711130220, "status": "PUBLISHED", "id": "1074280313794849"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "7231364686950247", "version": "0.1.8"}, "id": "747332423964833", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "6957730590980326", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "7231364686950247"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "747332423964833"}]}, "change_log": null, "created_date": 1711200076, "id": "7257597180993664", "version": "0.1.9", "version_