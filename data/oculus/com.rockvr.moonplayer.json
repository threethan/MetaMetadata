and watch the best VR videos for FREE! Moon VR can also stream virtual reality videos from your PC."}]}, "developer_name": null, "developer_privacy_policy_url": "https://moonvrplayer.com/privacy.html", "developer_terms_of_service_url": null, "publisher_name": "InnoVision", "support_website_url": null, "website_url": "https://moonvrplayer.com/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "434961926215971"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["PRODUCTIVITY", "UTILITY", "MEDIA_PLAYER"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "470518061388466"}}]}, "scheduled_offers": {"count": 12, "nodes": [{"status": "PUBLISHED", "id": "1064998555109360"}, {"status": "PUBLISHED", "id": "533035556251446"}, {"status": "PUBLISHED", "id": "1167071534953623"}, {"status": "PUBLISHED", "id": "663805132670049"}, {"status": "CANCELLED", "id": "1172484297598302"}, {"status": "CANCELLED", "id": "422178024285161"}, {"status": "PUBLISHED", "id": "3890244201190226"}, {"status": "PUBLISHED", "id": "842546687951593"}, {"status": "PUBLISHED", "id": "512129734789852"}, {"status": "PUBLISHED", "id": "1103068890662257"}, {"status": "PENDING", "id": "1315068202714798"}, {"status": "PUBLISHED", "id": "645537876534568"}]}, "baseline_offer": {"id": "1103068890662257"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1755277200, "status": "PUBLISHED", "id": "1064998555109360"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_resu