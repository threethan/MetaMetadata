ree Buddhism app which using the most advanced Virtual Reality technology, and can help you take advantage of transit time, lunch breaks and other instances of downtime to study Buddhism."}]}, "developer_name": "Chesstar studios", "developer_privacy_policy_url": "http://www.chesstar.cn/site/privacy-policy-and-terms-of-service/", "developer_terms_of_service_url": "http://www.chesstar.cn/site/privacy-policy-and-terms-of-service/", "publisher_name": "Chesstar studios", "support_website_url": null, "website_url": "http://www.chesstar.com/", "category": "APPS", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": null, "context_topics": [], "external_subscription_type": null, "genres": ["EDUCATIONAL", "UTILITY", "SIMULATION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": null, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "zh_TW"], "supported_input_devices": ["GEAR_VR_CONTROLLER", "TOUCHPAD"], "supported_player_modes": [], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "658297428935291"}}]}, "scheduled_offers": {"count": 1, "nodes": [{"status": "PUBLISHED", "id": "234796950432020"}]}, "baseline_offer": {"id": "234796950432020"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1526576400, "status": "PUBLISHED", "id": "234796950432020"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "1514307415363793", "version": "1.0.8"}, "id": "240991173130683", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "1467264753401393", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "1514307415363793"}, "release_status": "