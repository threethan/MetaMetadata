le": "en_US", "screenshots": [{"file_name": "\u622a\u56fe01.png", "id": "2712968612237013", "uri": "https://scontent-atl3-1.oculuscdn.com/v/t64.5771-25/499307416_2712968615570346_3377453002036751838_n.png?_nc_cat=100&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=GHNjT5VDomEQ7kNvwGAkU_K&_nc_oc=Adl-6cXoi944d1O3S8vWszNk-adynNqHwDbhI9BBf2Ma2FfD6HRXxCpW1uqGtaiZUoI&_nc_zt=3&_nc_ht=scontent-atl3-1.oculuscdn.com&oh=00_Afp45prYjj2nAbS28cPx7UGJE9H2Mr3JT8ld1cDgB6pJcA&oe=6981EE92"}, {"file_name": "\u622a\u56fe02.png", "id": "740495315024114", "uri": "https://scontent-atl3-1.oculuscdn.com/v/t64.5771-25/499307609_740495321690780_1334096046333367957_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=i_Vdqh4cMkUQ7kNvwE5VpG5&_nc_oc=Adk-KnWd9sQFHeqE8TaZT_NjXicTMPRWCg6ipTgfR_b52D-dSFHCVOObrLYikwcM5GU&_nc_zt=3&_nc_ht=scontent-atl3-1.oculuscdn.com&oh=00_Afphs0aujeJBHJValmzHSjhoCjnDGh5MHWiLk09EK-kakg&oe=6981EA61"}, {"file_name": "\u622a\u56fe03.png", "id": "1541949097215236", "uri": "https://scontent-atl3-2.oculuscdn.com/v/t64.5771-25/499619369_1541949100548569_9182861963868378113_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=vT3zGlRDrTAQ7kNvwHgCXQ6&_nc_oc=Adntz-B7Vov37daPwwM7kHH8ZzHslSu6SENr9o97UIcXkubNCycirZZKVuLvGG9ZtDQ&_nc_zt=3&_nc_ht=scontent-atl3-2.oculuscdn.com&oh=00_Afq4hiPf4Y1Mrvnz_vrSZfBjgSHoo3xNrfyWKuRh_dcsSg&oe=6981E80F"}, {"file_name": "\u622a\u56fe04.png", "id": "1234942441166754", "uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/499619331_1234942444500087_5640312055778376073_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=FwIpNCwmRvkQ7kNvwEJ1uYi&_nc_oc=AdkFRVN_wul852-Y7iOTuzTf-ggqtQFU22Tqd2a2ImfsEmr5tTuU-4MGZOo2iWShtkk&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_Afr3i5PBx4VWIuV3jLkUHGIMJx4EZdfecZRmf5-8XL4D2A&oe=6981D19B"}, {"file_name": "\u622a\u56fe05.png", "id": "644337698679044", "uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/499305396_644337702012377_909718798155004862_n.png?_nc_cat=110&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=jHhA5GNPNVAQ7kNvwFGQpDi&_nc_oc=AdnEv80hoWtnC4lOdRxcW4jhisZSYDDTxPxQ81LeNSqEUNeC2RZqiCg0rCVQYERHtEQ&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_AfqyyuiIVChcDGfNkmkp_CfJKZAyEOyxud-4YQC2FErqTA&oe=6981E1E3"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Lethal Animal", "keywords": ["animal", "animalcompany", "company", "pvp", "gorilla"], "long_description": "# Why Play Lethal Animal\uff1f\n**\ud83c\udfae Pixel Worlds & Chaotic Adventures**\n\nWhether solo or in public/private lobbies, join friends to explore gorgeously crafted pixel art caves, dungeons, and forests. Collect loot, overcome traps, and outwit monsters. The stunning pixel art style brings these vibes to life, making every exploration feel both thrilling and enjoyable.\n{1271471937662192}\n\ud83d\udd2b **Fully Customizable Characters  & Weapons Arsenal**\n\nStand out by fully customizing your look \u2014 choose your animal, equip exclusive cosmetics. Choose from rich weapons, each with unique effects and playstyles \u2014 from ranged to melee to traps, mining, tailor your style. In PvP battles, pick from a wild arsenal and pranks: Take a red **chili pepper** \u2014 it lets you breathe fire, scaring off nearby players in a blaze of fury! Eat a **Surstr\u00f6mming** (stinky fish) and vomit uncontrol.\n{24117353204612533}\n**\ud83e\udddf Happy Trolling Tools & Smarter, Deadlier Monsters**\n\nEvery encounter is different thanks to unique mechanics for each monster. Expect the unexpected and stay on edge as no two hunts are ever the same \u2014 and now with hilarious tools in unexpected ways. The stone golem is slow but powerful. Even when you defeat it, it doesn't truly die \u2014 it shatters into smaller stone golems that keep attacking.\n{1649121499094530}\n\ud83d\udd14 **Fresh Updates & Community Events**\n\n\ud83d\udccc Wishlist now to be the first beast! Join our Discord to get test access and rewards: https://discord.gg/6PdeDAKgSV\n", "short_description": "Welcome to the Pixel battlefield of Lethal Animal \u2014 a wild multiplayer VR action game filled with unpredictable monsters, bizarre weapons, and absolute chaos. Explore, mining\uff0cfight smarter beasts, troll your friends with ridiculous gadgets, and customize your look \u2014 all in a constantly evolving world. With fast-paced updates and sneak previews for our Discord, there\u2019s always something new to experience. https://discord.gg/6PdeDAKgSV"}]}, "developer_name": "Suplife Games", "developer_privacy_policy_url": "https://vrsuplife.com/privacyPolicy/", "developer_terms_of_service_url": null, "publisher_name": "Suplife Games", "support_website_url": null, "website_url": "https://linktr.ee/suplife_vr", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "1023770785722696"}, {"id": "2691890164460465"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER", "CO_OP"], "id": "23963943959961396"}}]}, "scheduled_offers": {"count": 2, "nodes": [{"status": "PUBLISHED", "id": "874065865157547"}, {"status": "PENDING", "id": "2095560510986564"}]}, "baseline_offer": {"id": "874065865157547"}, "is_approved": true, "firstOffer": {"nodes": [{"start_time": 1766080800, "status": "PUBLISHED", "id": "874065865157547"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "25949540354735070", "version": "2.04.06"}, "id": "734480439027764", "channel_name": "LIVE"}]}, "is_for_ocu