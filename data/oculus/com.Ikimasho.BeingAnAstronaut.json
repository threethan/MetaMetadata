b=1-7&_nc_sid=6e7a0a&_nc_ohc=CtqrmLU2TA8Q7kNvwGK4xqH&_nc_oc=AdmKLCKE8DT8RECcRWw4FLNF1e9adsKqIzAFXlBNBHyqqFPLEVRPRtwa5eCS7boDJvY&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_AfqJxc3iqLhZUf3vipneMoEZmdxNp4UPz1ns94Ij737hPg&oe=695D00C5"}, {"file_name": "Pesquet_2560x1440_SCREENSHOT3.png", "id": "1062208108646798", "uri": "https://scontent-sin2-2.oculuscdn.com/v/t64.5771-25/38982687_1062208115313464_8001165291027202430_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=UFh2STaeJdoQ7kNvwFu4fXe&_nc_oc=AdmA-yjtFfi6LQILJXBMkhv79iqrvRX3ZcJOGtx2h8jDTrrdC8-frtDhki0ATMaX_i8&_nc_zt=3&_nc_ht=scontent-sin2-2.oculuscdn.com&oh=00_Afr_1gu1DWaM4s07uMKzLXZ6UFapN-ByAX5gZyW8qUKBlQ&oe=695D1460"}, {"file_name": "Pesquet_2560x1440_SCREENSHOT5.png", "id": "496646209647193", "uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/38974450_496646212980526_8980009219719461363_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=imaOar8ho6gQ7kNvwGMnRmS&_nc_oc=Adnz20AjZFefRdJG19ZKWcC17HTp_DbbIE_p6mKtMENlEAHrO90lajSyCvwBZGAgIe8&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_Afpqqc_LfdE25gF7W1oulBPvbqL1Pt-oIbMLKX15zCZejQ&oe=695CFEA7"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "Being an Astronaut", "keywords": ["pesquet", "space", "film", "360"], "long_description": "**PART 1 - TRAINING**\n\nExperience the astronaut preparation tests alongside Thomas Pesquet: the centrifuge experience in Cologne, the exercise simulations in the Soyuz in Moscow or even diving into NASA's gigantic pool in Houston intended for spacewalk simulations.\n\n**PART 2 - THE MISSION**\n\nExperience the Soyuz rocket takeoff, the capsule docking with the space station and, thanks to the first images filmed in space in 360\u00b0, the daily life in zero gravity. They will admire the Earth from the Cupola before taking a spacewalk and an extraordinary journey into the cosmos.\n\n# CREDITS #\n- Directed by: Pierre-Emmanuel Le Goff and J\u00fcrgen Hansen\n- Nationality : French\n- \u200bGenre: Docu-Fiction\n- \u200bDuration: 15 min + 17 min / 22 min / 5 min\n- Production: La Vingt-Cinqui\u00e8me-Heure, Prospect TV and DV Group and France TV New Writings.\n\n\u200bWith the support of the Normandy Region and the Medienboard.\n\n\u200bIn partnership with ESA and NASA.", "short_description": "Discover Thomas Pesquet\u2019s adventure in space in two 360\u00b0 virtual reality 15 minutes films. Follow the French astronaut and American Peggy Whitson from their training on Earth all the way to spacewalk thanks to the first virtual reality images ever shot in space."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.25eheure.com/thomaspesquetvr-confidentialite", "developer_terms_of_service_url": null, "publisher_name": "25eHeure", "support_website_url": null, "website_url": "https://www.25eheure.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "726368587961803"}, {"id": "2687655338230691"}, {"id": "434961926215971"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "zh_CN", "fr"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9005121362882289"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sin11-2.oculuscdn.com/v/t64.5771-25/39031508_879184400991457_3956335973008078823_n.png?stp=dst-png_s360x360&_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=croblrCDQcYQ7kNvwFOm06e&_nc_oc=Adn-lgNH9KlBgUF0W3__lE9bvIakYRej4ec1XNEhsWa1hL__XyJLm-kPawSf8NiWXak&_nc_zt=3&_nc_ht=scontent-sin11-2.oculuscdn.com&oh=00_AfpqzOR1btulLiUTNgYaHNJ0HqxKWy