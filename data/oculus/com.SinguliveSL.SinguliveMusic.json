, {"file_name": "Singulive_Hall.png", "id": "647521440270670", "uri": "https://scontent-hkg4-2.oculuscdn.com/v/t64.5771-25/75203623_647521443604003_678387908026998473_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=Y3r3NWSpxL0Q7kNvwHGvOO-&_nc_oc=Adnhu9BaQztVXioh2GZTAgqRTRGd6kV1ZgX40_9-NcAoOjSloFODHKM2T0WsFSwLRPM&_nc_zt=3&_nc_ht=scontent-hkg4-2.oculuscdn.com&oh=00_AfMJ6NAWbuaLwmtWFIw0GBGzJ3PLHRfMht2aa8Viz5mQ-Q&oe=686BCFF1"}, {"file_name": "Singulive_Curtain.png", "id": "636115357922248", "uri": "https://scontent-hkg1-2.oculuscdn.com/v/t64.5771-25/38974918_636115361255581_2832842183392918583_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=gPXHGtQfHuAQ7kNvwEZFTkn&_nc_oc=AdnDgvYkzu2h9J2RGjxdvwbfDWR6PlPdt9porWs7UpKmu4_dxpWvkOUv1nSIEx1sVHA&_nc_zt=3&_nc_ht=scontent-hkg1-2.oculuscdn.com&oh=00_AfNXRUpyQCuNqHaTk8NWtmz7MpiG6KUKyk5fU5H43I33Cw&oe=686B9FD7"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "store_capsule_main.png", "id": "1284736782300032", "uri": "https://scontent-hkg4-1.oculuscdn.com/v/t64.5771-25/75262742_1284736785633365_4087174543594366927_n.png?_nc_cat=108&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=ZjSiSgn_BHsQ7kNvwFu9K94&_nc_oc=AdmI3_XZiHw2Tu9bfaV2ZlxlTwO-EbmKccpZH3acHAUq2umBw9xvll_KRJ5Ik3viXnc&_nc_zt=3&_nc_ht=scontent-hkg4-1.oculuscdn.com&oh=00_AfOjFsJIeuE6Yg0sHmFaGLe79Jv03h9KGQl4cDxgCqdssQ&oe=686BCA36"}, "display_name": "Singulive", "keywords": [], "long_description": "Singulive is an immersive experience that takes you to concert halls and live music experience! It is now a reality that artists' tours stop at your home. \n\nYour virtual reality device is now part of the tour schedules, something that has never happened before. Singulive is a virtual place that coexists with the physical concert halls that we all visit in real life to experience live music. It is a new way of understanding music by artists and spectators. \n\nYou can live the experience alone or accompanied (up to four), look at the web premieres, write them down in your agenda and appoint your companions, with weekly releases Singulive will ensure that your contact with live music is constant and vibrant. \n\nPrepare your mind and passion for music for what Singulive offers you, social networks, news, premieres, multiplayer experience. Feel yourself an active part of the live shows and venues you will enjoy.", "short_description": "Singulive is the gateway for artists and audiences to the world of virtual reality with music as the main protagonist. A place designed to enjoy the live experience and take a step forward with spectacular possibilities. "}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.singulive.com/singulive-app-privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "Singulive SL", "support_website_url": null, "website_url": "https://singulive.com/", "category": "ENTERTAINMENT", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "2687655338230691"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["MUSIC"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "es"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER"], "id": "621419909224615"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-hkg4-2.oculuscdn.com/v/t64.5771-25/43931338_2409046166069594_6377288763838200282_n.png?stp=dst-png_s360x360&_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=2S4t2Z7KnqUQ7kNvwFidKsH&_nc_oc=AdnQuYSPdJ-HNMsV7mc-w3cXtS_RTi_b0cC2qS9IZDKEf-TxcB8myaO3gB2SKKrM-gw&_nc_zt=3&_nc_ht=scontent-hkg4-2.oculuscdn.com&oh=00_AfMqwAuRgVo1mt7OVChBasieeH04W1DNnrki1Y9LnLlF6w&oe=686BABDE"}, "i