eo_trailer_thumbnail": {"file_name": "Cover_Landscape_Xtadium (1).png", "id": "2458743454512592", "uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/473391881_2458743457845925_7872907343413808248_n.png?_nc_cat=107&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=FNk6FHoQvXoQ7kNvwFlOUI9&_nc_oc=AdkiwpfNgjYB2N57qfqX-OKOUQBNCHnYJ9-fvMlDlLXrgbelTdvJN4IUuEcHW6xLvns&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_Afb6JwY-fnPECTzJ7EJkf9zOuvosDDOX5sI1fqXsKAc0BA&oe=68CE9AB1"}, "display_name": "Xtadium", "keywords": ["sport", "stadium", "ufc", "nascar", "nba"], "long_description": "**Xtadium \u2013 Immersive Sports Streaming in VR & MR**\n\nExperience sports like never before with **Xtadium**, the free VR & MR app that puts you front row for the world\u2019s biggest events. Watch **NBA, UFC, PSG, EuroLeague Basketball, X Games, NHL, WNBA**, and more in stunning **180\u00ba and 360\u00ba video** \u2014 live and on-demand.\n\nPowered by **YBVR** and featured as one of Meta\u2019s top Mixed Reality apps, Xtadium transforms your **Meta Quest 2, Quest 3, or Quest Pro** into the ultimate sports arena. Switch between **multiple camera angles**, check **real-time stats**, and host **Watch Parties** with friends \u2014 all from your living room.\n\n**Top Experiences:**\n\n\ud83c\udfc0 **NBA Courtside** \u2013 Free immersive games + multi-game MR mode for League Pass.\n\n\ud83e\udd4a **UFC & ONE Championship** \u2013 Step into the ring with exclusive fight coverage.\n\n\u26bd **PSG: Champions Season** \u2013 Matches and behind-the-scenes training.\n\n\ud83c\udfd2 **NHL, WNBA, EuroLeague** \u2013 Iconic competitions in VR & MR.\n\n\ud83d\udef9 **X Games** \u2013 Skate, BMX, skiing, snowboarding in high adrenaline 180\u00ba/360\u00ba.\n\n**Tech Features:**\n\n- VR & MR modes with up to 8K resolution\n- Multi-camera switching during events\n- Hand tracking for controller-free navigation\n- Social media multitasking\n- In-event overlays and leaderboards\n\n**Download Xtadium free** and turn every match, fight, or race into an immersive event. Your living room is now the best seat in the house.\n\nhttps://xtadiumvr.com | @XtadiumVR", "short_description": "Xtadium is the free VR & MR sports app that puts you front row for NBA, UFC, PSG, EuroLeague, X Games, NHL, WNBA, and more in stunning 180\u00ba/360\u00ba video. Watch live or on-demand with multi-camera angles, real-time stats, and Watch Parties with friends. Enjoy up to 8K resolution, VR & MR modes, hand tracking, and social media multitasking \u2014 all on your Meta Quest 2, Quest 3, or Quest Pro."}]}, "developer_name": null, "developer_privacy_policy_url": "https://ybvr.com/privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "Yerba Buena VR, Inc.", "support_website_url": null, "website_url": "https://www.xtadiumvr.com/", "category": "APPS", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "434961926215971"}, {"id": "2776542286000603"}, {"id": "2687655338230691"}], "external_subscription_type": "REQUIRED_FOR_SOME_CONTENT", "genres": ["SPORTS", "EXPERIENCE_360_NON_GAME"], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "en_US", "en_GB"], "supported_input_devices": ["OCULUS_TOUCH", "HAND_TRACKING"], "supported_player_modes": ["SITTING", "ROOM_SCALE", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER", "MULTI_USER"], "id": "30872411499040868"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/473393543_916202967113688_7214627581993945162_n.png?stp=dst-png_s360x360&_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=fDST67T1eD0Q7kNvwFiRgio&_nc_oc=Adn7z5VJgiowtZdEYn0RrHgg6sWHbrcj2fGSEiF2lpXdHlcKY64buD71PQ7RRBE5oBA&_nc_zt=3&