ile_name": "BeamScr.png", "id": "709799163435706", "uri": "https://scontent-ord5-3.oculuscdn.com/v/t64.5771-25/57568668_709799166769039_755234254166772854_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=DRm7NOS0G2gQ7kNvwEYOazE&_nc_oc=AdnwsZRNBkBuHQAboSkbTrWL79GuslZLIhn0diK_gwE1GdJfqf9YKfOIs812w6k0WRw&_nc_zt=3&_nc_ht=scontent-ord5-3.oculuscdn.com&oh=00_AfpBY2AnLtkGtMf8Bj55L4ODT8sJEPv9IQW6Lh1aGBA0CQ&oe=6960EBB6"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "ThumbClip.png", "id": "1677276539310066", "uri": "https://scontent-ord5-2.oculuscdn.com/v/t64.5771-25/38982476_1677276542643399_1599604260977971870_n.png?_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=8I39vhfene0Q7kNvwHGCD3v&_nc_oc=AdmLPvQvJP3cW_pMaacM0KihVzQ80sJk4xbQBgEik5LUk1KE5yE1-qmskce8rIwLcWA&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AfpHoNZ_bcZZxqS9Nj7RJmTpQDPdvaXJg1odihFZpzjFvA&oe=6960DF92"}, "display_name": "Ekeko - Virtual Memory Space", "keywords": [], "long_description": "Ekeko is a virtual memorial project, where children and young people add scans of real objects to a virtual memory collection. These can then be accessed through a virtual reality experience as part of supported bereavement workshops run by Cruse Bereavement Care throughout Wales, United Kingdom.\n\nThis has been collaboration with Cruse Bereavement Care, Playframe & Amgueddfa Cymru, part of the Hands on Heritage project, funded through the National Heritage Lottery Fund.\n\nWith thanks to our participants, and the memories they kindly donated.\n\nNational Museum Wales https://museum.wales/ \nCruse Bereavement Care offer support after the death of someone close. https://www.cruse.org.uk/", "short_description": "This is a virtual memorial space. Ekeko is the guardian of memories of real lost love ones, photoscanned and donated by children and young people as part Cruse Bereavement Care workshops throughout Wales, United Kingdom.\n\nPick up each object to read or hear it's story, and replace objects using the beam of light to curate your own selection of memory objects. "}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.playframe.co.uk/playframe/Aboutus/privacy-policies/ekeko-app-privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "PlayFrame", "support_website_url": null, "website_url": "https://www.playframe.co.uk/", "category": "APPS", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1819021644882647"}, {"id": "434961926215971"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["DOCUMENTARY_HISTORY", "RELAXATION_MEDITATION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en_GB"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "765405151282674"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-ord5-2.oculuscdn.com/v/t64.5771-25/38982439_716071956289840_6383755117689779733_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=DBD-ib06xq8Q7kNvwFP3_ev&_nc_oc=AdkVnVoCzu7tpdqzP49maaQiXcdRpKh5n9RYA_BxuPnM7Fj3Uw15_2ZI1tPLDbxFqE8&_nc_zt=3&_nc_ht=scontent-ord5-2.oculuscdn.com&oh=00_AfpOcRA6prVMaCuhoe32fKJKWTPcwcOmBeu26IiSks8KwA&oe=69610693"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Ekeko - Virtual Memory Space", "platform": "ANDROID_6DOF", "context_category": {"context_category": "APPS", "id": "362692211442873"}, "context_topics": [{"topic_name": "Creativity", "id": "1819021644882647"}, {"topic_name": "Media", "id": "434961926215971"}], "release_date": 1656603505, "lastRevision": {"nodes": [{"id": "657148935273563", "release_type": "SCHEDULED", "admini