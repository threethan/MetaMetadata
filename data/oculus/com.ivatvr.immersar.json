scontent-lhr6-2.oculuscdn.com&oh=00_AfkGc2Ks8pWwUJrUzX5tZyo7R8I6xsls0t37cWZWaj5cAw&oe=69469C51"}, "display_name": "Immersar", "keywords": ["immersar", "tade", "music", "vrmusic", "cyberpunk"], "long_description": "This game-like experience invites users to explore intricate cyberpunk cityscapes and ethereal space scenes, each environment synchronized with the album\u2019s dynamic soundscapes. Through the seamless integration of interactive VR features, Immersar pushes the boundaries of how music is experienced, transforming sound into a fully immersive 3D world, breaking free from traditional formats and redefining the way we connect with music in the digital age.", "short_description": "Immersar is a VR platform enabling musicians to create immersive, interactive 3D music experiences. Transforming traditional music videos into dynamic VR journeys, the platform debuts with Tade's new album, Beyond. Available on Oculus Quest, Beyond combines ambient electronic music with cinematic, cyberpunk-inspired visuals, offering an unparalleled audio-visual adventure."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.vreximmersive.com/privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "VRex immersive", "support_website_url": null, "website_url": "https://www.vreximmersive.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "434961926215971"}, {"id": "1228796255027646"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_US