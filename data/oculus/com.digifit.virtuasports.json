sports.virtuagym.com/webshop/terms-and-conditions", "developer_terms_of_service_url": null, "publisher_name": "VirtuaSports", "support_website_url": null, "website_url": "https://www.virtuasports.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2776542286000603"}, {"id": "459781504968480"}, {"id": "1592192974313489"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "SITTING"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER", "SINGLE_USER"], "id": "25824222697207777"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-iad3-1.oculuscdn.com/v/t64.5771-25/491818925_1470776471146651_7193465605441863216_n.png?stp=dst-png_s360x360&_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=aB3L-8nrJiMQ7kNvwE625Lh&_nc_oc=Adlay_l5c7YD72HGKkpxyogwJV6e87lYV7eeyiHgZIvcm0EZU9JynVXU5X1VGzRS-JI&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfsI8n61Fxqr2gPMEU92Zju5zTDiXdeWzHTzPLd4oqI02w&oe=699713F0"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "VirtuaSports", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Sports", "id": "2776542286000603"}, {"topic_name": "Action", "id": "459781504968480"}, {"topic_name": "Arcade", "id": "1592192974313489"}], "release_date": 1764849720, "lastRevision": {"nodes": [{"id": "24732833923013332", "release_type": "SCHEDULED", "administration_status": "APPROVED", "pdp_metadata": {"application": {"default_locale": "en_US", "id": "2473283388301