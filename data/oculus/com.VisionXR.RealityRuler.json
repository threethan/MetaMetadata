er": null, "video_trailer_thumbnail": {"file_name": "IconFinal_2560x1440.png", "id": "966040199063430", "uri": "https://scontent-lhr6-2.oculuscdn.com/v/t64.5771-25/473391858_966040202396763_1867969647845781413_n.png?_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=_E7BbdMUva0Q7kNvwEmnVlx&_nc_oc=Adm5XAbC8CNogxnOiApCk74Yb14Bh6M4Qii_FZSUHQTG9Vk6QKWivNvF43GPxN1pl60&_nc_zt=3&_nc_ht=scontent-lhr6-2.oculuscdn.com&oh=00_AfSlu71vEIlFTtjeesOG5CgcgIqXukixBsF4JHDDRazqqA&oe=6874F169"}, "display_name": "Reality Ruler", "keywords": ["Measure", "Ruler", "Length", "Layout", "Measurement"], "long_description": "Unlock the potential of spatial comprehension with Reality Ruler: 3D Measurement in Mixed Reality (MR)! Our app harnesses the pass-through mode of your device, transforming your surroundings into a digital workspace for precise measurement.\n\nKey Features:\nMeasure Line Lengths: Sketch lines in real-world space and instantly obtain their lengths.\nArea Calculations: Assess the area of polygons, rectangles, and circles with intuitive gestures.\nAngles: Obtain accurate angles between intersecting lines in three-dimensional space.\nVolumetrics: Estimate the volume of 3D shapes such as cuboids, cylinders, cones, and spheres.\nReality Ruler boasts an intuitive interface and streamlined controls, ensuring accessibility for users of all backgrounds. Whether you're a student mastering geometry, a craftsman planning a project, or simply intrigued by spatial awareness, Reality Ruler offers a precise and interactive means to measure the world around you.\n\nTransform your headset into a sophisticated measurement tool with Reality Ruler: 3D Measurement in MR.\n\nEnhanced Features:\n\nSave and Load Measurements: Preserve and retrieve your measurements effortlessly.\nHeight Estimation: Determine the height of objects or rooms with ease.\nCustomization: Personalize line colors and widths to suit your preferences.\nRoom Layout Visualization: Gain insight into room layouts based on measured dimensions.\nExperience the fusion of digital precision and physical reality with Reality Ruler: 3D Measurement in MR.", "short_description": "Measure real-world objects in 3D using your Headset's pass-through mode. Calculate length, area, angles, and volume effortlessly with Reality Ruler."}]}, "developer_name": null, "developer_privacy_policy_url": "https://www.visionxr.co.in/privacypolicy", "developer_terms_of_service_url": null, "publisher_name": "VisionXR", "support_website_url": null, "website_url": "https://www.visionxr.co.in/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "393222478507326"}, {"id": "1063584910766706"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH", "HAND_TRACKING"], "supported_player_modes": ["ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9515283451868806"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-lhr6-2.oculuscdn.com/v/t64.5771-25/473392105_1378508366512897_3580154677933748330_n.png?stp=dst-png_s360x360&_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=AJ1gUeXvYUUQ7kNvwEzFRer&_nc_oc=AdlCbO5yjlSTXL7XC9e67M709Qu0Awj5olKwDmv5vcanRPar4Qhw5QaDnls_VZ9DBNk&_nc_zt=3&_nc_ht=scontent-lhr6-2.oculuscdn.com&oh=00_AfTjDn_c0yxx3BB1hT5AAm4UC9O1qmElM1S8M_jXzkyCmQ&oe=6874FF78"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Reality Ruler", "platform": "ANDROID_6DOF", "context_category": {"context_category": "APPS", "id": "362692211442873"}, "context_topics": [{"topic_name": "Utilities", "id": "393222478507326"}, {"topic_name": "Productivity", "id": 