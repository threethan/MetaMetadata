Amygdala Protocol."}]}, "developer_name": null, "developer_privacy_policy_url": "https://titanzgamez.blogspot.com/2025/09/privacy-policy-for-titanz-games-last.html", "developer_terms_of_service_url": null, "publisher_name": "TitanzGames", "support_website_url": null, "website_url": "https://titanzgamez.blogspot.com/2025/09/privacy-policy-for-titanz-games-last.html", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "821934236779161"}, {"id": "377978390272490"}, {"id": "644354873106712"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["GAMEPAD", "HAND_TRACKING", "OCULUS_TOUCH", "TRACKED_KEYBOARD"], "supported_player_modes": ["STANDING", "SITTING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9090419221082758"}}]}, "scheduled_offers": {"count": 16, "nodes": [{"status": "PUBLISHED", "id": "1430750468399644"}, {"status": "PUBLISHED", "id": "1577533050157281"}, {"status": "PUBLISHED", "id": "1440056230795121"}, {"status": "PUBLISHED", "id": "1404933221000143"}, {"status": "PUBLISHED", "id": "1927896068134583"}, {"status": "PUBLISHED", "id": "695021070158708"}, {"status": "PUBLISHED", "id": "2199300920564052"}, {"status": "PUBLISHED", "id": "1375597430960596"}, {"status": "PUBLISHED", "id": "1385139290067894"}, {"status": "PUBLISHED", "id": "1230940949092625"}, {"status": "PUBLISHED", "id": "1736717543493030"}, {"status": "PUBLISHED", "id": "1436939594060694"}, {"status": "PUBLISHED", "id": "685208187945018"}, {"status": "PUBLISHED", "id": "683323618106104"}, {"status": "PUBLISHED", "id": "24534096729576184"}, {"status": "PUBLISHED"