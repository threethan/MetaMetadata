b=1-7&_nc_sid=6e7a0a&_nc_ohc=r64uTxfZkLYQ7kNvwG3cZyA&_nc_oc=AdlmiVSY3pgiT2Zp0V5xQYzQNWbmTkxcEQ-Ghb9GzU4SpzvfO6To20Zgu6LJZoY_iZI&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_AfqLVeOhM1s52ZODYBjJSigcbI2VIAPSv79gIa4w1HxCWg&oe=6964D56D"}, {"file_name": "resized_image_1.png", "id": "1632169797385916", "uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/473391862_1632169800719249_7130548385774956603_n.png?_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=NSZup7TVKGsQ7kNvwH2DJ0U&_nc_oc=AdlFJDEhLBXyYP94ydSO8QRjrDWphq7sHgSXDcuU00ytRoPtpp9xBcTeXl4TOfnfzaw&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfoJozmdwg6gDfsQxYQxJ01OYDbOEWaWb4SF2tAAAfXsrg&oe=6964E35D"}, {"file_name": "resized_image_5.png", "id": "1636158713956270", "uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/473393507_1636158717289603_5195548983704297921_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=0Upt2md0WXoQ7kNvwHiC58U&_nc_oc=AdmlRFl_p2pTQFf8VTtzVbuLmStGSpsbfgQNgCWep0svuJM8NsvR1NOPThwsPkwkUQQ&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfqucIktMSLJjOTRA5aSFVp8LI0K05fESyAjjh5tdGVX0w&oe=6964D0AD"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "LockDownVR", "keywords": [], "long_description": "Lockdown VR thrusts players into the eerie depths of a massive, decaying high-security facility that has been mysteriously abandoned. As the protagonist, you wake up inside its cold, desolate halls with no memory of how you arrived. The facility hides chilling secrets, its walls whispering tales of failed experiments and ominous disappearances. With only a flashlight and your wits, you must traverse through labyrinthine corridors, solve intricate puzzles, and uncover the dark truth. Beware\u2014the facility is not empty. Sinister entities roam the shadows, drawn to your presence. Your mission is simple: survive, escape, and uncover the horrifying reality of Lockdown VR. This immersive game is designed to test your courage and puzzle-solving skills in a high-stakes survival setting.\n", "short_description": "Lockdown VR is a spine-chilling horror experience set in an abandoned high-security facility. Players must uncover dark secrets, solve cryptic puzzles, and survive the lurking terrors while navigating through claustrophobic environments.\n\n"}]}, "developer_name": null, "developer_privacy_policy_url": "https://sites.google.com/view/lock-down-vr-privacy-policy/home", "developer_terms_of_service_url": null, "publisher_name": "SaucerTheDev", "support_website_url": null, "website_url": "https://sites.google.com/view/lock-down-vr-privacy-policy/home", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "1592192974313489"}, {"id": "1990104051447093"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["MULTI_USER"], "id": "8372830086151398"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sin2-1.oculuscdn.com/v/t64.5771-25/473393810_3885457001710491_4841642217589620957_n.png?stp=dst-png_s360x360&_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=v523hTg-VJAQ7kNvwHxcXcS&_nc_oc=Adm4o34nXRj2OdHsGRB7xFB5CeioJlbbEMtxepNTOXFYWKVAXHfoIGAeosy6X89dTUQ&_nc_zt=3&_nc_ht=scontent-sin2-1.oculuscdn.com&oh=00_AfocBBBo6M-PVqkLQDPJFVm7yiH0A58EEhhKuunpr91i7Q&oe=6964CB1A"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "LockDownVR", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name"