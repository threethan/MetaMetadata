d": true, "firstOffer": {"nodes": [{"start_time": 1711651260, "status": "PUBLISHED", "id": "1364308970948956"}]}, "liveChannel": {"nodes": [{"latest_supported_binary": {"__typename": "AndroidBinary", "binary_test_results": {"nodes": []}, "id": "25646544218295970", "version": "8.0.0"}, "id": "1068226080929879", "channel_name": "LIVE"}]}, "is_for_oculus_keys_only": false, "primary_binaries": {"nodes": [{"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "7526177777425891", "is_concept": false, "latest_released_binary": {"__typename": "AndroidBinary", "id": "25646544218295970"}, "release_status": "RELEASED"}, "binary_release_channels": {"nodes": [{"channel_name": "LIVE", "id": "1068226080929879"}]}, "change_log": "Welcome to version 2.0 of BasketBallAR! Thank you for all your support so far. BasketBallAR v2 includes the following improvements:\n- You can now place the basket and shoot the ball with both controllers.\n- Keep track of your best round with high scores.\n- 3-point mode at 7 seconds left in the game.\n- You can move the basket position after placing the basket and in-between games.\n- Improved basket graphics.\n- Added basic sound effects.\n- Switched to Meta's controller models.\n- Will fallback to a default environment and still work if you do not give the app permission to access your scene data.\n- Added tooltips to help explain how to place the basket and shoot the ball.\n- Other minor bug fixes.", "created_date": 1765868678, "id": "25646544218295970", "version": "8.0.0", "version_code": 8, "release_status": "RELEASED", "status": "APPROVED", "binary_test_results": {"nodes": []}, "obb_binary": null, "asset_files": {"count": 0}, "debug_symbols": {"count": 0}, "__isAppBinaryWithFileAsset": "AndroidBinary"}, {"__typename": "AndroidBinary", "__isAppBinary": "AndroidBinary", "binary_application": {"__typename": "Application", "id": "7526177777425891", "is_concept": false, "latest_release