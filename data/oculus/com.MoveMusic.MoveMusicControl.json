_nc_oc=Adlb0tTCFCgg2zzRfDGrVnuKJXh1FWjZyBSkzBqn5cLD8BDUXOX8t5tE5q6s4zwJmnk&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_AfoV_y3cUJjftNh0xoVespoBTEfvE2ogGCUD87xPAEkqcA&oe=6981E29C"}, {"file_name": "HitZone-Controller.png", "id": "496921249035004", "uri": "https://scontent-atl3-3.oculuscdn.com/v/t64.5771-25/75375748_496921255701670_3308875651558871130_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=So_LodeR8w0Q7kNvwF15Yxy&_nc_oc=AdlzOsqg4h7_YTFEyM7nT4oi-HsCV2EaxVkCkhR9otsR24Bm2GcfoFC0B-hrrOiwDcg&_nc_zt=3&_nc_ht=scontent-atl3-3.oculuscdn.com&oh=00_AfoHM0dCDaq1HYqlzMG3LRNRiv8-UJF-2TDUT3im5eq4nQ&oe=6981D479"}, {"file_name": "Inspector.png", "id": "695691598746733", "uri": "https://scontent-atl3-2.oculuscdn.com/v/t64.5771-25/38974766_695691605413399_93646395706859290_n.png?_nc_cat=102&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=gJEZLjBiyzcQ7kNvwHX5Vqa&_nc_oc=AdkoBVk9ekf3-KauYR4_76Ii0m3UAes6XWJQU2IqNM03PDGJLPcJelfnBeVAcqX7zQ8&_nc_zt=3&_nc_ht=scontent-atl3-2.oculuscdn.com&oh=00_Afqr0Be0roXGqY9yjCmgcCaFxEzrw6q7RymFJ9EFWqtWeQ&oe=6981E82C"}], "video_trailer": null, "video_trailer_thumbnail": null, "display_name": "MoveMusic Control", "keywords": ["MIDI", "Controller", "DAW", "Music", "MoveMusic"], "long_description": "Make Music with Movement! MoveMusic Control is a VR MIDI controller that lets you wirelessly control music software on another computer by interacting with a 3D environment you create. Bring expressive, full-body movement to your digital music workflow with this performance-oriented controller that helps you show an audience your music through movement. Use custom 3D controls to create in interesting ways and explore novel sonic possibilities. Supports Passthrough so you can see the world around you while performing in mixed reality. Use our advanced controller building system to customize your 3D MIDI controller. Save your project files to open later or share with a friend.\n\n**NOTE: MIDI features require connection to a computer with MIDI-capable software (e.g. DAW such as Ableton Live). Computer must be capable of a Wi-Fi connection and be able to run the MMConnect companion app (see movemusic.com/downloads).**", "short_description": "MoveMusic is a VR MIDI controller that lets you wirelessly control music software on another computer by interacting with a 3D environment you create. Bring expressive, full-body movement to your digital music workflow with this performance-oriented controller that helps you show an audience your music through movement. Use custom 3D controls to create in interesting ways and explore novel sonic possibilities. Supports Passthrough.\n\nNOTE: MIDI features require connection to a computer."}]}, "developer_name": null, "developer_privacy_policy_url": "https://movemusic.com/privacy", "developer_terms_of_service_url": "https://movemusic.com/eula", "publisher_name": "MoveMusic LLC", "support_website_url": "https://movemusic.com/support", "website_url": "https://movemusic.com/", "category": "APPS", "comfort_rating": "COMFORTABLE_FOR_SOME", "comfort_tags": [], "context_category": {"id": "362692211442873", "context_category": "APPS"}, "context_topics": [{"id": "1055187466124150"}, {"id": "1819021644882647"}, {"id": "393222478507326"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["MUSIC", "UTILITY"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9916137995065827"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-atl3-2.oculuscdn.com/v/t64.5771-25/38974859_572433421748264_1167677742593970660_n.png?stp=dst-png_s360x360&_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=9qg6s7t1zpAQ7kNvwHoHLCZ&_nc_oc=AdlymunitPmxFJs1jY6