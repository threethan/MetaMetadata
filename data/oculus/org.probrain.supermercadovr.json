content-sin11-1.oculuscdn.com/v/t64.5771-25/38974713_332652802543365_2480158436641273391_n.png?_nc_cat=104&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=SBqCiMkSBEsQ7kNvwGEF5Oi&_nc_oc=AdlDRYWgR-iYHqJ5kOSVm2cPVTVhWCU6Lsc791h1Cp66FeF0VikHhFqcKBLj4ovirQ4&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AfrKUlDdsXDXsEahv3qu5xLa_y0KbbAe4HGOfZrv7E4S6g&oe=6970D415"}, {"file_name": "screenshot_5.png", "id": "699474982224730", "uri": "https://scontent-sin6-2.oculuscdn.com/v/t64.5771-25/38982595_699474985558063_3453974348672181496_n.png?_nc_cat=109&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=coyJS4-o7wkQ7kNvwH7kxcO&_nc_oc=AdkKzSBJhQUauYCra0-vEPoIKnaZKbf96AepvCiCgc0lGBkS9Pj-De2vLzysR-xAza8&_nc_zt=3&_nc_ht=scontent-sin6-2.oculuscdn.com&oh=00_Afog7hOzcsPWIVWwwQsRIBFiEsd37p7tXQaoUHSCQPNn-g&oe=6970AD32"}], "video_trailer": null, "video_trailer_thumbnail": {"file_name": "video_cover_image.png", "id": "3483732985208245", "uri": "https://scontent-sin6-1.oculuscdn.com/v/t64.5771-25/38974786_3483732988541578_4502993567047372991_n.png?_nc_cat=111&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=SdRCXok3nVkQ7kNvwGWvKYn&_nc_oc=Adm_RE6wvDwmg_wZBUS7IyR0yvB0bVZZC7GCabEK41katGaZPil3GwZGkoFcEJ2ewR0&_nc_zt=3&_nc_ht=scontent-sin6-1.oculuscdn.com&oh=00_Afo3pZ-v07JaPhFdWYRyOZ4P_PE-dZRup9u32vB6seXDnA&oe=6970D9EA"}, "display_name": "Supermarket VR", "keywords": [], "long_description": "Welcome to Supermarket VR, the supermarket of the future! In this immersive VR experience, you'll travel to a space station where you can shop for groceries.\n\nSupermarket VR is a fun and challenging way to train your cognitive skills in a virtual reality environment. It's perfect for those who want to improve their auditory memory, auditory-visual association and listening in challenging situations, such as understanding speech with distorted sounds or with competitive noises in the environment (auditory closure and auditory figure-ground discrimination).\n\nThis game is designed to be challenging but fun, and it's perfect for players of all ages.", "short_description": "Supermarket VR is a game that transports you to a supermarket on a space station. Train your cognitive skills by memorizing the names of the products on the list, and identifying sounds in a noisy environment. Fun and challenging for players of all ages."}]}, "developer_name": null, "developer_privacy_policy_url": "https://termos.probrain.com.br/pub/policies/policies_eng.pdf", "developer_terms_of_service_url": null, "publisher_name": "ProBrain Solu\u00e7\u00f5es Neurotecnol\u00f3gicas para Sa\u00fade e Educa\u00e7\u00e3o Ltda", "support_website_url": null, "website_url": "https://www.probrain.com.br/", "category": null, "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}, {"id": "1043107547362485"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "NO_MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en", "pt_BR"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "6564857710296707"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-sin6-3.oculuscdn.com/v/t64.5771-25/38982522_2830046897137602_4393294089462604349_n.png?stp=dst-png_s360x360&_nc_cat=106&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=ToXsBUsUQXQQ7kNvwHjuH6r&_nc_oc=Adk9MkltpL4auK-I2pV9kZOAUCReJNecYN0ltjd5ZroUhdE1PwLchAAua_DEmZGAPjM&_nc_zt=3&_nc_ht=scontent-sin6-3.oculuscdn.com&oh=00_AfqwOWqyY5dcClt9ZCcXmvw1hMUaMIVsY8vyq1Gah5-j4A&oe=6970D4FD"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Supermarket VR", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [