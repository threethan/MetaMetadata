nc_ohc=2788UOvFip8Q7kNvwGu6zhm&_nc_oc=Adn942XgP85-8eO6CTMf4x41kf7jtErf5jZYAAOyP-yVMwc8pSWY5v-wsTWUKxWkIv0&_nc_zt=3&_nc_ht=scontent-iad3-1.oculuscdn.com&oh=00_AfGgV9EEe8fy7x7W234vJNbytsggpsz8AZ73oJ2NzhxhzQ&oe=6804E5E5"}, "display_name": "NightClub Simulator VR", "keywords": [], "long_description": "Night Club Simulator is a highly immersive VR game with a great degree of freedom. It uses a fully physics-based animation system to simulate NPC behaviors and reactions, allowing you to interact with them as if you were in the real world. NPCs have realistic AI, displaying emotions such as happiness, anger, fear, or affection. Their emotions will change depending on your actions. You can do whatever you want here, cause chaos, but you have to watch out for the police. Experience a lifelike and interactive night club adventure with Night Club Simulator.  ", "short_description": "Experience a virtual nightclub with a high degree of freedom."}]}, "developer_name": null, "developer_privacy_policy_url": "https://mandegames.com/privacy-policy.html", "developer_terms_of_service_url": null, "publisher_name": "mandegames", "support_website_url": null, "website_url": "https://mandegames.com/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_FEW", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}, {"id": "1990104051447093"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ACTION", "FIGHTING", "SIMULATION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["ROOM_SCALE", "STANDING"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "9483948668354431"}, "review_messages": ["", "", ""]}]}, "cover_square_image"