 movement controls to slip into your pigeon-self!"}]}, "developer_name": "Maestro Pivetta / Studio Seufz", "developer_privacy_policy_url": "https://vrkiwi.org/what-vr/eula-privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "VRKiwi (MeKiwi)", "support_website_url": null, "website_url": "https://vrkiwi.org/portfolio/vr-pigeons/", "category": "GAMES", "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "459781504968480"}, {"id": "1592192974313489"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["ARCADE", "CASUAL", "SIMULATION"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "8686874058044401"}, "review_messages": [""]}]}, "cover_square_image": {"uri": "https://scontent-sin11-1.oculuscdn.com/v/t64.5771-25/38974693_462721473297291_583541181875223647_n.png?stp=dst-png_s360x360&_nc_cat=105&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=AVKraEkZMB4Q7kNvgGGVmsy&_nc_oc=AdgX-uhO_oij9jMCDFmjYaDFmVK4r_PG7KUQJSVNomtqMd342rSWi9CCla00Qcb8cCc&_nc_zt=3&_nc_ht=scontent-sin11-1.oculuscdn.com&oh=00_AYG3OJv86Lf3Ji_vC3gMLq6R6f0o6TJ4mQ1HpeOpMkcrWA&oe=67DC1CE4"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "VR Pigeons", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Action", "id": "459781504968480"}, {"topic_name": "Arcade", "id": "1592192974313489"}], "release_date": 1625615578, "lastRevision": {"nodes": [{"id": "1930789997059699", "release_type": "SCHEDULED", "administ