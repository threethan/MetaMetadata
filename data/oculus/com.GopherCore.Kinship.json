words": [], "long_description": "Kinship is a VR Delivery Game set in the Post-Post Climate Apocalypse. Different communities have formed throughout the world playing specific roles, and it\u2019s you (the Bosun) and your ship Jay\u2019s duty to deliver their goods to each other to ensure their continued survival. Listen to Jay as they instruct you on how to interact with and repair them, while also learning about the history of the world through their dialogue and notes. Featuring full voice-acting, dynamic music and multiple levels.", "short_description": "Vehicle driving & maintenance sim about delivering mutual aid between communities on a post-post climate apocalypse earth. "}]}, "developer_name": null, "developer_privacy_policy_url": "https://finnjc.me/kinship-privacy-policy", "developer_terms_of_service_url": null, "publisher_name": "N/A", "support_website_url": null, "website_url": "https://finnjc.me/kinship", "category": "GAMES", "comfort_rating": "NOT_RATED", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "2691890164460465"}, {"id": "331529188132278"}], "external_subscription_type": "NOT_REQUIRED", "genres": ["EXPLORATION", "SIMULATION", "ADVENTURE"], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "5106103786173640"}, "review_messages": ["", ""]}]}, "cover_square_image": {"uri": "https://scontent-lhr8-2.oculuscdn.com/v/t64.5771-25/38974714_1090786098462342_2618548836719358895_n.png?stp=dst-png_s360x360&_nc_cat=103&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=c1AFot9TVwMQ7kNvgE3n7yF&_nc_oc=Adi6tD5a4DJlKWpQeUo3t-QlhdU0wM87R7QOK631YJzfXSbUuFkX