_name": "Battleline - The Range", "keywords": ["Battleline", "Shooter", "Gun", "Weapon", "Shooting"], "long_description": "Battleline - The Range Demo \n\nThe release of the free demo serves as both a training ground for players and a testing ground for future features, allowing the community to shape the evolution of the game. Explore the interactive training range and dynamic killhouse, where you can master weapons meticulously recreated from iconic eras  and  conflicts.\n\nAs a labor of passion, Battleline strives to push the boundaries of VR realism and storytelling, providing players with a glimpse of the full game\u2019s potential. Your feedback will help refine and expand this ambitious project into a fully-fledged historical VR experience.", "short_description": "Battleline - The Range\nIndie VR shooter created by a solo developer, offering a free and immersive first look at the groundbreaking weapon systems and mechanics at the heart of the Battleline experience."}]}, "developer_name": null, "developer_privacy_policy_url": "https://rushgamestudio1.wordpress.com/privacy-policy/", "developer_terms_of_service_url": null, "publisher_name": "Rush Games Studio", "support_website_url": null, "website_url": "https://rushgamestudio1.wordpress.com/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "3744040075834813"}, {"id": "459781504968480"}, {"id": "2710427532541308"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["GAMEPAD"], "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "96463