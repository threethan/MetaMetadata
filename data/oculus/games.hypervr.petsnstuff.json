pets and lots of creativity boosting gadgets to play with."}]}, "developer_name": "HyperVR Games", "developer_privacy_policy_url": "https://hypervr.games/privacy/petsnstuff.html", "developer_terms_of_service_url": null, "publisher_name": "HyperVR Games", "support_website_url": null, "website_url": "https://hypervr.games/", "category": null, "comfort_rating": "COMFORTABLE_FOR_MOST", "comfort_tags": [], "context_category": {"id": "2743326035925534", "context_category": "GAMES"}, "context_topics": [{"id": "331529188132278"}, {"id": "1990104051447093"}, {"id": "1592192974313489"}], "external_subscription_type": "NOT_REQUIRED", "genres": [], "has_in_app_ads": null, "internet_connection": "NOT_REQUIRED", "play_area": "MOVEMENT", "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "supported_in_app_languages": ["en"], "supported_input_devices": ["OCULUS_TOUCH"], "supported_player_modes": ["STANDING", "ROOM_SCALE"], "supported_tracking_modes": [], "user_interaction_modes": ["SINGLE_USER"], "id": "24038327485767743"}, "review_messages": []}]}, "cover_square_image": {"uri": "https://scontent-atl3-2.oculuscdn.com/v/t64.5771-25/39031358_2020302285088213_4522937233069189893_n.png?stp=dst-png_s360x360&_nc_cat=101&ccb=1-7&_nc_sid=6e7a0a&_nc_ohc=QBHMoo4qBwAQ7kNvwH05cXD&_nc_oc=Adl8FPZZkBgHhSLSttxzU7MFi4jfViyJB93Ghk3oHoeo-32Ut5vIMcy9jc0p2SmCXIM&_nc_zt=3&_nc_ht=scontent-atl3-2.oculuscdn.com&oh=00_AfL0eDuE_ikz6Ui7OB6xQLD2YingJCRsT2219zcMPXxiwg&oe=683309D4"}, "is_concept": false, "is_test": false, "is_trial_offer_valid": false, "display_name": "Pets & Stuff", "platform": "ANDROID_6DOF", "context_category": {"context_category": "GAMES", "id": "2743326035925534"}, "context_topics": [{"topic_name": "Simulation", "id": "331529188132278"}, {"topic_name": "Sandbox", "id": "1990104051447093"}, {"topic_name": "Arcade", "id": "1592192974313489"}], "release_date": 1724929200, "lastRevision": {"nodes": [{"id": "7818625494831200", "release_type": "SCHEDULED",